@임신해제(ARG)
IF CFLAG:ARG:임신경과일수 > 0
    SELECTCASE CFLAG:ARG:임신경과일수
        CASE IS >= 30
            MAXBASE:ARG:체력 += 500
            MAXBASE:ARG:기력 += 500
        CASE IS >= 20
            MAXBASE:ARG:체력 += 300
            MAXBASE:ARG:기력 += 300
        CASE IS >= 15
            MAXBASE:ARG:체력 += 100
            MAXBASE:ARG:기력 += 100
        CASEELSE
            ;-
    ENDSELECT
    TALENT:ARG:가슴사이즈 -= CFLAG:ARG:가슴랭크업
    CFLAG:ARG:가슴랭크업 = 0
    TALENT:ARG:모유체질 = 0
    CFLAG:ARG:임신경과일수 = 0
    CFLAG:ARG:출산휴가중 = 0
    CFLAG:ARG:임신자각상태 = 0
    TALENT:ARG:임신 = 0
    TALENT:ARG:회복속도 = CFLAG:ARG:회복속도다운 - 2
	CFLAG:ARG:회복속도다운 = 0
ENDIF

@물가적용(ARG)
#FUNCTION
RETURNF (ARG * FLAG:물가)/100


@저택에거주(ARG)
#FUNCTION
IF 저택거주:ARG:0 == 1
    RETURNF 1
ENDIF

RETURNF 0


@저택이야(ARG)
#FUNCTION
IF INRANGE(ARG,1401,1445)
    RETURNF 1
ENDIF
RETURNF 0
