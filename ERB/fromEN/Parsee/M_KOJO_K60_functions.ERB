    CASE 38, 49, 55, 97, 17
        ;No honorific because Parsee dislikes them
        RETURNF @"%CALLNAME:LOCAL%"

	;Everyone else: Yuugi gets -san too because Parsee feels awkward around her crush
	CASEELSE
		RETURNF @"%CALLNAME:LOCAL%さん"
ENDSELECT


