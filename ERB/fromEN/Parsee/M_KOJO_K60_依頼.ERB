;依頼口上・一般依頼16：お悩み相談
;==================================================
@M_KOJO_IRAI_16_K60(ROLE, SCENE, CLIENT_ID)
#DIMS ROLE     ;キャラの立場、의뢰자かそうでないか
#DIMS SCENE    ;セリフの表示されるタイミング
#DIM  CLIENT_ID;의뢰자IDだけ抜き出したもの
;--------------------------------------------------
;RETURN 0 の部分は共通の口上が表示されます
;依頼ごとの口上を表示する部分は RETURN 0 を RETURN 1 にしてください
;--------------------------------------------------
SELECTCASE SCENE
	CASE "의뢰 제시시"

		RETURN 0
	CASE "의뢰비수탁시"

		RETURN 0
	CASE "의뢰 수탁시"

		RETURN 0
	CASE "의뢰 확인시"

		RETURN 0
	CASE "의뢰 파기시"

		RETURN 0
	CASE "의뢰실행 직전"

		RETURN 0
	CASE "의뢰실행 직후"
		IF TFLAG:193 == 1
		;正解の選択肢
		ELSEIF TFLAG:193 == 2
		;ハズレの選択肢だったが逆に正解
		ELSEIF TFLAG:193 == -1
		;ハズレの選択肢
		ELSEIF TFLAG:193 == -2
		;正解の選択肢だったが逆にハズレ
		ENDIF
		RETURN 0
	CASE "성공 보고시"
;보수：野菜か果物

		RETURN 0
	CASE "실패 보고시"

		RETURN 0
;CASE "의뢰 보고 불필요"
;＜この依頼にはありません＞
;	RETURN 0
	CASE "口上委託率"
;＜この依頼特有の分岐です。(RESULT:100の値)％の確率で"口上専用"が発生します＞
		RESULT:100 = 0

;＜RESULT:YYに0以外の数値を入れると、該当のお悩みを発生させなくできます（詳しくは一般依頼16_お悩み相談参照）＞
;1,2:弾幕ごっこの悩み 3,4:晩酌の悩み 5,6:本の悩み 7,8:香料の悩み
;9,10:音楽の悩み 11,12:服の悩み 13:髪型の悩み 14:参拝の悩み 15:ダイエットの悩み
;16,17:菜園の悩み 18:休日の悩み 19:人気の悩み 20:集中力の悩み 21:御三家の悩み 22:進化の悩み
		RESULT:1 = 0
		RESULT:2 = 0
		RETURN 1
	CASE "口上専用"
;＜この依頼特有の分岐です。(口上委託率)％の確率で発生します＞
;16,お悩み相談 の場合
;RESULT        発生判定,0だと発生しません
;RESULT:1      正解の選択肢（ただしRESULT:2が0だと、一定確率で逆にハズレる）
;RESULT:2      逆張り正解／ハズレ判定の消去,1だと逆張りが無くなります
;RESULTS:0     お悩み文章（自動で『』で囲まれます）
;RESULTS:1～3  選択肢１～３
;何も発生しない状態になると無限ループになるので注意してください
;●追記メモ
;RESULT:1を4～6にするとそれぞれ1～3がハズレ（残り2つが正解）になります
;RESULT:1を7にするとどれも正解になります
;RESULT:1を8にするとどれもハズレになります
;1～3以外の場合は逆張り判定が自動で無くなります
		RESULT = 0

;▼サンプル
[SKIPSTART]
	SELECTCASE RAND:5
	CASE 0
		;このお悩みは、恋慕で発生し、親密によって正解が変わります
		SIF TALENT:연모
			RESULT = 1
		RESULTS:0 = 赤ちゃん作るなら何人がいいかな？
		RESULTS:1 = ５人でバスケチームを作ろう
		RESULTS:2 = ９人で野球チームを作ろう
		RESULTS:3 = １１人でサッカーチームを作ろう
		IF ABL:친밀 >= 100
			RESULT:1 = 3
		ELSEIF ABL:친밀 >= 20
			RESULT:1 = 2
		ELSE
			RESULT:1 = 1
		ENDIF
	CASE 1
		;このお悩みは、無条件で発生し、正解は絶対に2番で固定化されます
		SIF 1
			RESULT = 1
		RESULTS:0 = どのジョブがいいかな？
		RESULTS:1 = かっこいしナイト
		RESULTS:2 = シーフが便利だよ
		RESULTS:3 = 青魔道士最強！
		RESULT:1 = 2
		RESULT:2 = 1
	CASE 2
		;このお悩みは、夏の月に発生し、正解がランダムで変わります
		SIF DAY:2 == 2
			RESULT = 1
		RESULTS:0 = どの水着を買おうかな？
		RESULTS:1 = かわいいワンピース
		RESULTS:2 = クールなセパレート
		RESULTS:3 = 色っぽいビキニ
		RESULT:1 = RAND:3 + 1
	CASE 3
		;このお悩みは、無条件で発生し、1番と3番が正解になります（5 - 3 = 2番がハズレになります）
		SIF 1
			RESULT = 1
		RESULTS:0 = ワンちゃんの名前は何にしよう？
		RESULTS:1 = ボロンゴ
		RESULTS:2 = ゲレゲレ
		RESULTS:3 = プックル
		RESULT:1 = 5
	CASE 4
		;このお悩みは、無条件で発生し、どれを選んでも正解になります
		SIF 1
			RESULT = 1
		RESULTS:0 = 夕飯は何がいい？
		RESULTS:1 = 고기 감자 조림
		RESULTS:2 = 고로케
		RESULTS:3 = 생선구이
		RESULT:1 = 7
	ENDSELECT
[SKIPEND]

		SIF !RESULT
			RESTART
		RETURN 1
ENDSELECT

