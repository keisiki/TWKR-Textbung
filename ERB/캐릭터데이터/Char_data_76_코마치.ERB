@CHARAMOVE_DATA_76(ARG,ARG:2)
LOCAL = 76
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;야쿠모집 , 축생계는 가지 않는다(입구 부분은 나올 때 용)
			CASE 630, 631, 640, 641
				RETURN -10
			CASE 630 TO 639, 640 TO 652
				RETURN -100
			;타인의 집에는 들어가지 않는다
			CASE 608, 609
				RETURN -100
			;에이키님의 방
			CASE 615
				RETURN -5
			;법정
			CASE 612
				RETURN -10
			;삼도내의 모래 강변~피안에 자주(잘) 있다
			CASE 602 TO 604
				RETURN 10
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 6, 7
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 23, 61,62,64,65, 71,72,73,75
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터76
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터76(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "코마치")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "머리 장식"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "나막신"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "버선"))
CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "속속곳"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "띠"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "어른의 매력이 넘치는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "탕가 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "레이스 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[코마치]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/스파이스/일식/튀겨짐/안주/푸짐/정키/농후/호화/선명한/호쾌/영장/명계풍/매운/매운/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "건강한/화학적/시큼한/시큼한/검소/검소/개구리/개구리/개구리/벌레고기/벌레고기/벌레고기/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "명계풍/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/아름다운/일본풍/화려/" + "사생/술/" * 3 + "사신의/피안의/저승길의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "서양풍/섬세/로리타/매니악/" + "넥타이/" * 3)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰76(ARG, O_DATA, V_NAME)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_76
DISH_NAME = 도깨비불 솜사탕
RESULTS = 디저트
CALL 재료설정("방황하는영혼")
DISH_TASTE = 일식/단/담백/담백/차가운/검소/푹신푹신/영장/명계풍/
DISH_COMMENT = 명계에서 인기가 희박한 솜사탕。간담이 서늘해진다。
RETURN 1

