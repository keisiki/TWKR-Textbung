@CHARAMOVE_DATA_8(ARG,ARG:2)
LOCAL = 8
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;대체로 유적에 있다
			CASE 36 TO 50
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,6, 21,22,23,27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;引力設定
	CASE 8
		TCVAR:LOCAL:인력점 = TCVAR:[[유메미]]:인력점
ENDSELECT

@EXIST_캐릭터데이터8
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터8(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "치유리")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지＋후드) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：바지"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("바지", "쇼트 바지"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "세라복"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "푸른 라인이 들어간")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "세일러풍 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "세일러풍 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/스파이스/양식/카레/튀겨짐/푸짐/농후/수수께끼 요리/사각사각/마력/화학적/" + "매운/기능적/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/담백/" + "단/" * 2 + "개구리/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "화학적/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흰빛/기술/근사함/미래/파워풀/지적/" + "도서/" * 3 + "닻의/아티팩트/기계/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "레트로/섬세/로리타/위엄/" + "손목호구/담뱃대/" * 3)

ENDSELECT

