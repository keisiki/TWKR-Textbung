@CHARAMOVE_DATA_71(ARG, ARG:2)
LOCAL = 71
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;主に居間に居る
			CASE P9거실
				RETURN 5
			;私室にはあまり入らない
			CASE P15레이무방, P52별채
				RETURN -5
			;他所
			CASE P27삼요정의집・객실 TO P35삼요정의집・지하실, P37몽환유적・1층통로 TO P51몽환유적・창고, P56아운의거처
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 지장길, 수양버들아래, 명련사경내, 신령묘광장
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터71
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터71(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "신묘마루")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "밥그릇"))
CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "속속곳"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "띠"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "크든 작든 딱 맞는 사이즈인")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "난쟁이의 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "난쟁이의 캐미숄")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[신묘마루]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/알/일식/이파리/정키/호화/품위있는/선명한/푹신푹신/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/매운/매운/수수께끼 요리/수수께끼 요리/수수께끼 요리/" + "개구리/벌레고기/오니의마력/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "특기요리＊품위있는＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흰빛/질서/근사함/멋쟁이/팝/섬세/주술/" + "장식/" * 3 + "소인의/작디작은/바늘/찻잔/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "파워풀/지적/" + "담뱃대/" * 3 + "오니의/귀력의/" * 10)

;농민 관련
;코로보쿠르
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "머위 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "머위")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "머위밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "하쿠레이 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 350)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 3)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_71
DISH_NAME = 난쟁이빵
RESULTS = 경식
CALL 재료설정("범용")
DISH_TASTE = 양식/빵/단/시원한/기능적/사각사각/마력/
DISH_COMMENT = 먹으면 주변 지형을 파악할 수 있는 소인을 위해 만든 마법의 빵
RETURN 1
; 번역 주, 일본의 빵집 체인점 こびとパン의 패러디로 추정

;IMAGE_I71_特殊服装(CHARA)-------------------------------------------------------------------
