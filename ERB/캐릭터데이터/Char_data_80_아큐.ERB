@CHARAMOVE_DATA_80(ARG, ARG:2)
LOCAL = 80
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		;日曜日は連動
		IF YOUBI_MATCH(DAY, "일")
			RESULT:1 = [[아큐]]
			RESULT:2 = [[코스즈]]
		ENDIF
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;稗田邸,阿求私室
			CASE P221히에다저택, P229아큐방
				RETURN 5
			;慧音の家,鈴奈庵,小鈴の部屋
			CASE P224케이네댁, P213스즈나안, P226코스즈방
				RETURN 5
			;私室には行かない
			CASE P208라이코방, P209야츠하시방, P210벤벤방
				RETURN -100
			CASE P231반키방, P232렌코방, P233메리방, P234유키방, P235마이방, P239미요이방
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;小鈴の仕事日はちょいちょい会いに行く
		SIF YOUBI_MATCH(DAY, "수목") && INRANGE(TIME, 720, 1020) && CFLAG:LOCAL:오늘의운세 % 2 == 0
			TCVAR:LOCAL:인력점 = P213스즈나안
ENDSELECT

@EXIST_캐릭터데이터80
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터80(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "아큐")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "머리 장식"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "남자용치마"))
CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "피부에 순한 비단 100%의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "심리스 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/일식/이파리/시원한/건강한/담백/예술적/품위있는/가정적/뭉근뭉근/외계풍/단/단/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "매운/푸짐/정키/농후/" * 2 + "개구리/스파이스/명계풍/지옥풍/" * 3 + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "담백한 맛/히에다가 비전의 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "찻빛/아름다운/예술적/레트로/지적/" + "문구/도서/" * 3 + "기억의/지식의/기록/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/파워풀/로리타/" + "불/날붙이/생체품/손목호구/담뱃대/쥐의/" * 3 + "사신의/죽음의/망각의/" * 10)

;농민 관련
;마을의 톱으로 군림한다
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "쌀 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "쌀")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "논")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "마을")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 206)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 550)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 9)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_80
DISH_NAME = 구문잡탕죽
RESULTS = 경식
CALL 재료설정("고급식재")
DISH_TASTE = 일식/국물/쌀/야채/콩/시큼한/건강한/담백/따뜻한/검소/검소/가정적/뭉근뭉근/비술/약선/
DISH_COMMENT = 히에다가에 전해지는、기억력을 증폭시킨다는 잡탕죽(雑炊)。건강에도 좋다。
RETURN 1

;IMAGE_I80_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I80_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:0 % 2 && !TCVAR:CHARA:발정
			RESULTS = _日替わり
	ENDIF
ENDIF

