@CHARAMOVE_DATA_15(ARG, ARG:2)
LOCAL = 15
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;納屋には入らない、屋敷外には出ない
			CASE P305헛간, P334폐양관・입구 TO P348안개의호수・바닥
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 안개의호수, 정문, 객실, 식당, 대도서관, 지하실
				RETURN 1
			CASE 숲의입구, 향림당, 키리사메마법점, 인형양관
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터15
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터15(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "사쿠야")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "화이트브림"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "가터 스타킹"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
	ENDIF
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "에이프런 드레스"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "리본"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "가터가 달린 품위있는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "고급 실크 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "고급 실크 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[사쿠야]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "홍마관출장서비스")
CASE "고유커맨드 실행 가능"
	IF TFLAG:출장청소 && (FLAG:물주기대행 || !ITEM:150)
	ELSEIF GET_MAPNAME(MAIN_MAP) == "홍마관"
	ELSE
		CALLF MAKE_INT(V_NAME, 1)
	ENDIF

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/스파이스/중화/조림/단/담백/호화/품위있는/가정적/사각사각/현란/" + "시큼한/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/야취/짠/짠/벌레고기/벌레고기/벌레고기/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "숨김맛/비술/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/아름다운/심플/서양풍/시크/" + "시계/식기/" * 3 + "산뜻한/메이드의/시간의/시공의/회중시계/나이프/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "파워풀/로리타/" + "담뱃대/" * 3)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰15(ARG, O_DATA, V_NAME)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_15
DISH_NAME = 홍마 런치
RESULTS = 주식
CALL 재료설정("고급식재")
DISH_TASTE = 양식/어린이 런치/빵/우유/단/푸짐/농후/따뜻한/호화/품위있는/선명한/비술/메이드류/
DISH_COMMENT = 사쿠야가 아가씨를 위해 창작한 어린이 런치。
RETURN 1

