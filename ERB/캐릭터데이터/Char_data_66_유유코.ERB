@CHARAMOVE_DATA_66(ARG,ARG:2)
LOCAL = 66
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		RETURN 0
	;이동 확률
	CASE 2
		;악천후시, 밖이라면 이동하기 쉽고
		SIF OUTROOF(CFLAG:(NO:66):현재위치) && ((TIME:5 & 4) || (TIME:5 & 8))
			RETURN 10
		RETURN 70
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;시비곡직청, 축생계는 가지 않는다(입구 부분은 나올 때 용)
			CASE 610, 613, 640, 641
				RETURN -10
			CASE 610 TO 618, 640 TO 652
				RETURN -100
			;타인의 집에는 들어가지 않는다
			CASE 608, 609
				RETURN -100
			;특히 이루어
			CASEELSE
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,15, 21,22,23, 61,62,63,64,65,66
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터66
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터66(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "유유코")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "속속곳"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "띠"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "벚꽃 무늬가 선명한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "일본식 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "일본식 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[유유코]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/고기 요리/튀겨짐/안주/푸짐/농후/예술적/품위있는/가정적/쫄깃한/명계풍/달구경/일식/일식/단/단/시큼한/시큼한/" + "봄철/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "짠/시원한/담백/천계풍/천계풍/천계풍/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "명계풍/특기요리＊봄철＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "핑크/아름다운/예술적/일본풍/화려/섬세/" + "사생/유령/부채/식기/버찌의/잠의/" * 3 + "흑사나비의/벛꽃의/망령의/봄의/명계의/저승길의/죽음의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "서양풍/파워풀/로리타/" + "신령/날붙이/생체품/손목호구/" * 3 + "불멸의/" * 10)

;농민 관련
;석류로 설정한 것은 독단
;명계(죽음)＆여성에게 인연이 있는 과일로서 선택
;처음은 쌀 농가로 했지만 피안에서 쌀을 재배하고 있는 것은 매우 위화감이
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "석류 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "석류")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "석류밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "명계")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 6)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 630)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 500)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 13)
ENDSELECT

;IMAGE_I66_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I66_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:0 % 2
			RESULTS = _日替わり
	ENDIF
ENDIF

