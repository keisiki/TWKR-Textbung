@CHARAMOVE_DATA_151(ARG, ARG:2)
LOCAL = 151
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;地霊殿には行かない
			CASE P901지령전앞 TO P930옛혈지지옥
				RETURN -100
			;酒場と鬼の溜まり場に行きやすい
			CASE P933술집, P938오니소굴
				RETURN 5
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			;Lots of occult tomes in the library and Suzunaan
			CASE 대도서관, 상가거리
					RETURN 1
			;Looks for magic artifacts at Kourindou
			CASE 향림당
					RETURN 1
			;Her friend Sariel visits these places sometimes
			CASE 지옥의깊은길, 옛지옥가도
				RETURN 1
			;spooky places
			CASE 묘지, 무명의언덕, 재고의길, 무연총
				RETURN 1
			;Sightseeing before "the things that devour and dissolve" consume Gensokyo
			CASE 삼도천, 몽환유적, 현무의늪, 홍룡동, 고요의바다
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
		;夜はよく酒場
		SIF CFLAG:LOCAL:오늘의운세 % 4 == 0 && INRANGE(TIME, 1200, 1440)
			TCVAR:LOCAL:인력점 = P933술집
ENDSELECT

@EXIST_캐릭터데이터151
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터151(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "유겐마간")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(원피스) 세트")

CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))	
CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "소매"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "리본"))

CASE "고유 속옷 형용사"
	;CALLF MAKE_STR(V_NAME, "electrified")
	CALLF MAKE_STR(V_NAME, "대전된")
	
CASE "고유 하반신속옷명"
	;CALLF MAKE_STR(V_NAME, "conductive panties")
	CALLF MAKE_STR(V_NAME, "전도성 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	;CALLF MAKE_STR(V_NAME, "conductive bra")
	CALLF MAKE_STR(V_NAME, "전도성 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[유겐마간]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "수수께끼 요리/마계풍/선명한/외계풍/마력/인챈트/역전/불온/예상외/실험적/" + "몰카맛/금단/마법/" * 3 + "불가사의/" * 5)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "담백한 맛/영장/비술/심플/자비/" + "과학적/화학적/" * 3 + "귀여운/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "마계풍/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "호러한/요염한/괴기한/에니그마틱한/매니악한/환수의/어둠의/법계의/저주의/전자공학의/" + "고식한/레트로한/미스테리어스한/전위적인/오컬틱한/외계의/" * 3 + "유현한/신비적인/기묘한/섬뜩한/크레이지한/금기의/환영의/" * 5 + "사악한/광기의/" * 10)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "홍마의/무녀의/천공의/평화의/육체의/촌스러운/시공의/퇴마의/" + "모던한/오리엔탈한/천륜의/" * 3 + "큐트한/봉마의/봉주(封呪)의/" * 5)
ENDSELECT

