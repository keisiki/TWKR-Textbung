@CHARAMOVE_DATA_144(ARG, ARG:2)
LOCAL = 144
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 지옥, 피안, 중유의길, 삼도천
				RETURN 1
			CASE 작열지옥터, 옛지옥가도, 옛지옥온천, 지옥의깊은길
				RETURN 1
			CASE 신사경내, 신사툇마루, 신사본당
				RETURN 1
			CASE 산록의수해, 선인의저택, 깊은숲속
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
ENDSELECT

