@CHARAMOVE_DATA_13(ARG, ARG:2)
LOCAL = 13
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;三妖精宅内に行きやすい
			CASE P27삼요정의집・객실 TO P34스타방
				RETURN 5
			;よく居る
			CASE P343호숫가, P345치르노의움집, P346대요정의거처, P347안개의호수
				RETURN 5
			;納屋と地下
			CASE P303수위실, P304빈오두막, P305헛간, P328지하복도 TO P333대욕탕
				RETURN -100
			;チルノ以外の私室には入らない
			CASE P311파츄리방, P312소악마방, P320사쿠야방, P321빈방, P326레밀리아방, P327작은발코니
				RETURN -100
			CASE P338폐양관・목욕탕, P340루나사방, P341메를랑방, P342리리카방, P344레티의움집
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 요정의큰나무
				RETURN 1
			CASE 안개의호수, 얼음집, 정문
				RETURN 1
			CASE 숲의입구, 고목의큰나무, 재고의길
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터13
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터13(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "대요정")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "직접 만든 듯한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "털실 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "털실 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[대요정]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/과일/이파리/파이/시큼한/시원한/건강한/담백/검소/선명한/가정적/푹신푹신/반짝임/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "푸짐/매운/매운/" + "벌레고기/스파이스/농후/그로테스크/" * 3 + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "담백한 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "초록빛/식물/귀여운/섬세/로리타/" + "바람/" * 3 + "바람의/들꽃의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "무서움/화려/파워풀/위엄/" + "날붙이/탄환/손목호구/담뱃대/" * 3)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰13(ARG, O_DATA, V_NAME)
ENDSELECT

;IMAGE_I13_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I13_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF NUDITYCHECK(CHARA) == 1
			RESULTS = _パンイチ
			SIF FLAG:스탠딩종류
				RESULTS = _半裸
		ELSEIF NUDITYCHECK(CHARA) == 2
			RESULTS = _ブラ
			SIF FLAG:스탠딩종류
				RESULTS = _下着
		ELSEIF NUDITYCHECK(CHARA) == 3
			RESULTS = _下着
		ELSEIF (BETWEENTIME(CFLAG:CHARA:취침시간-180, CFLAG:CHARA:기상시간+30) && CFLAG:현재위치 ==  CFLAG:CHARA:초기위치) || CFLAG:CHARA:수면
			RESULTS = _パジャマ
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ENDIF

