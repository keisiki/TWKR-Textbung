@CHARAMOVE_DATA_158(ARG,ARG:2)
LOCAL = 158
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 수면시간(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

