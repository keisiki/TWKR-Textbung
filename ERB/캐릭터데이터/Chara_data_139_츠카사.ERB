@CHARAMOVE_DATA_139(ARG, ARG:2)
LOCAL = 139
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		;たまに連動
		SIF CFLAG:LOCAL:오늘의운세 % 8 == 0
			RETURN [[메구무]]
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;천계には行かない
			CASE P830현운해 TO P842히나나위저택헛간
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 숲의입구, 향림당, 고목의큰나무, 키리사메마법점
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 간헐천지하센터
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내, 홍룡동
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터139
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터139(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "츠카사")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지＋후드) 세트")

CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "차이나드레스(바지)"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "버선"))
	
CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "고혹적인")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "순백 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "파수관 여우 브래지어")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[츠카사]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/고기 요리/고기 요리/알/알/알/튀겨짐/단/단/매운/매운/가벼운/담백/호화/품위있는/선명한/뭉근뭉근/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "시큼한/시큼한/농후/검소/검소/야취/야취/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "숨김맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "고급/흰빛/혼돈/귀여운/심플/시크/지적/" + "순백의/여우의/약병/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "흑빛/질서/화려/위엄/" + "싸구려/부적/" * 3 + "너구리의/손목호구/" * 10)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_139
DISH_NAME = 유부밥
RESULTS = 주식
CALL 재료설정("당근")
DISH_TASTE = 일식/쌀/야채/짠/가벼운/담백/따뜻한/검소/가정적/
DISH_COMMENT = 삶은 유부와 당근을 섞은 밥。그리운 맛。
RETURN 1

;IMAGE_I139_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I139_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용 && FLAG:스탠딩종류
	IF CFLAG:CHARA:의상일시변경 == 55 || (CFLAG:CHARA:기본복장세트 == 55 && CFLAG:CHARA:의상일시변경==0)
		SIF !CHECK_NUDE(CHARA)
			RESULTS = _バニー
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:의상일시변경 == 55 || (CFLAG:CHARA:기본복장세트 == 55 && CFLAG:CHARA:의상일시변경==0)
			IF FLAG:스탠딩종류
				;顔絵
				RESULTS = _バニー
			ELSE
				;立ち絵
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

;IMAGE_I139_体形(CHARA)-----------------------------------------------------------------------
;キャラクター特有の体形表示条件
;エフェクトを共有する体形差分はこちら
;服装を指定しても自動で表示されるので注意
;使用時注意点:
;	RESULTSに体形を格納すること
;	書式は %体形%%服装% 適宜"_"等入れること
;	"巨乳"は本編で実装済み %体形%巨乳%服装% で追記可能
;-------------------------------------------------------------------------------------------
@IMAGE_I139_体形(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF FLAG:스탠딩종류
		;顔絵
	ELSE
		;立ち絵
	ENDIF
ELSE
	;通常画像
	IF FLAG:스탠딩종류
		;顔絵
	ELSE
		;立ち絵
		SIF TALENT:CHARA:임신 && CFLAG:CHARA:임신경과일수 >= 41
		RESULTS = 妊娠
	ENDIF
ENDIF

;IMAGE_I139_特殊表情(CHARA)--------------------------------------------------------------------
;キャラクター特有の表情表示条件
;時間停止中でも維持する表情はこちら
;使用時注意点:
;	RESULTS:0に表情を格納
;	RESULTS:1にマウスオーバー時の表情を格納すること
;	"通常","笑顔","怒り","発情","睡眠"は本編で実装済み
;-------------------------------------------------------------------------------------------
@IMAGE_I139_特殊表情(CHARA)
#DIM CHARA

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF FLAG:스탠딩종류
		;顔絵
		IF RESULTS:0 == "通常"
			IF CFLAG:CHARA:우후후 || CFLAG:CHARA:욕구불만도 >= 500
				RESULTS:0 = 半目
				RESULTS:1 = 半目
			ELSEIF TALENT:CHARA:기분 > 0
				RESULTS:0 = 悪い顔
				RESULTS:1 = 悪い顔
			ENDIF
		ENDIF
	ELSE
		;立ち絵
	ENDIF
ELSE
	;通常画像
	IF FLAG:스탠딩종류
		;顔絵
	ELSE
		;立ち絵
	ENDIF
ENDIF

