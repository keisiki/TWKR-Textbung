@CHARAMOVE_DATA_89(ARG, ARG:2)
LOCAL = 89
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室は入らない
			CASE P113나즈린방, P114무라사방, P115쇼방, P116이치린방, P117누에방, P118뱌쿠렌방, P109조온방, P136쿄코방
				RETURN -100
			;神霊廟メンバーの私室は入らない
			CASE P125토지코방, P127후토방, P128미코방
				RETURN -100
			;묘지によく居る
			CASE P121묘지
				RETURN 5
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 지장길, 수양버들아래, 묘지, 신령묘광장
				RETURN 1
			CASE 재고의길, 무연총
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
		;よく묘지に行く
		SIF CFLAG:LOCAL:오늘의운세 % 4 == 0 && INRANGE(TIME, 0, 360)
			TCVAR:LOCAL:인력점 = P121묘지
ENDSELECT

@EXIST_캐릭터데이터89
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터89(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "요시카")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "신사부적"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "윗도리"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "청아가 입혔다고 생각되는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "삼단 프릴 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "프릴 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)


;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/중화/튀겨짐/정키/농후/차가운/검소/야취/뭉근뭉근/요력/영장/단/단/시큼한/시큼한/푸짐/푸짐/" + "그로테스크/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "따뜻한/신력/" + "짠/시원한/담백/" * 2 + "건강한/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "미지의맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/혼돈/무서움/기묘/" + "사생/요술/생체품/" * 3 + "사령의/불멸의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/지적/" + "빛/신령/도서/부적/기계/" * 3)
ENDSELECT

