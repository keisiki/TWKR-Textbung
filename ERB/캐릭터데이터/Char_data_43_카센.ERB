@CHARAMOVE_DATA_43(ARG,ARG:2)
LOCAL = 43
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;하천 하우스 위층
			CASE 724 TO 728
				;7~17시는 피하는, 그 이외도 그다지 가지 않는다
				;※카센은 일로 도장에 가므로 탈출은 간단
				IF BETWEENTIME(420,1020)
					RETURN -10
				ELSE
					RETURN -5
				ENDIF
			;밖으로 나오기 쉽게 저택내는 피한다
			CASE 720 TO 723
				RETURN -10
			CASE 729, 730
				RETURN -5
			;각 개인방
			CASE 704, 706, 709, 715 TO 719, 733, 734
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 7, 8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23, 71,72,73,75, 81,82,83,84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;引力設定
	CASE 8
		;평일에는 3분의 2로 나돌아다니다
		IF YOUBI_MATCH(DAY, "월화수목금") && INRANGE(TIME, 600, 960)
			SELECTCASE CFLAG:LOCAL:오늘의운세 % 3
			CASE 1
				TCVAR:LOCAL:인력점 = 703
			CASE 2
				TCVAR:LOCAL:인력점 = 711
			CASEELSE
				TCVAR:LOCAL:인력점 = 0
			ENDSELECT
		ENDIF
ENDSELECT

