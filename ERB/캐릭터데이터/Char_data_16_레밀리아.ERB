@CHARAMOVE_DATA_16(ARG, ARG:2)
LOCAL = 16
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
		;最近のお嬢様はサマータイム制などと申して起床・就寝時間をずらしております。
		IF DAY:2 == 2
			CFLAG:LOCAL:생활시간보정 = 60
		ELSEIF DAY:2 == 4
			CFLAG:LOCAL:생활시간보정 = -60
		ELSE
			CFLAG:LOCAL:생활시간보정 = 0
		ENDIF
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;納屋には入らない、屋敷外には出ない
			CASE P305헛간, P334폐양관・입구 TO P348안개의호수・바닥
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 요정의큰나무
				RETURN 1
			CASE 정문, 객실, 식당, 대도서관, 지하실
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터16
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터16(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "레밀리아")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제) 세트")

CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "우산"))
CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "굉장히 고급스러운")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "풀 레이스 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "캐미숄")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[레밀리아]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고급식재/국물/케이크/농후/호화/품위있는/선명한/푹신푹신/메이드류/외계풍/달구경/양식/양식/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "일식/시원한/정키/담백/야취/" + "싸구려식재/개구리/벌레고기/검소/" * 3 + "매운/" * 4  + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "특기요리＊양식＊/현란/")

;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "고급/보석/혼돈/귀여운/기묘/서양풍/화려/로리타/위엄/" + "빨강빛/피/미술품/인형/" * 3 + "영원히빨간/박쥐의/악마의/홍마의/만월의/피의/운명의/우산/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "푸른빛/질서/심플/일본풍/섬세/" + "싸구려/물/빛/부적/손목호구/담뱃대/메이드의/" * 3 + "해/태양석의/" * 5)
;농민 관련
;서양 같기 때문에 밀의 공급 바탕으로
;안개의호수 부근은 냉해가 무섭다
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "밀 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "밀")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "밀밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "홍마관")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 3)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 301)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 600)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 16)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_16
DISH_NAME = 진홍의 블러드 수프
RESULTS = 경식
CALL 재료설정("토마토")
DISH_TASTE = 양식/국물/야채/짐승고기/단/농후/따뜻한/품위있는/선명한/선명한/선명한/그로테스크/현란/피의마력/
DISH_COMMENT = 생피를 홀짝이듯 붉게 물든 수프。
RETURN 1

