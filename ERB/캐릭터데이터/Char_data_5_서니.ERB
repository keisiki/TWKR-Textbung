@CHARAMOVE_DATA_5(ARG, ARG:2)
LOCAL = 5
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		RESULT:1 = [[서니]]
		RESULT:2 = [[루나]]
		RESULT:3 = [[스타]]
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P27삼요정의집・객실
				RETURN 5
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루, 요정의큰나무
				RETURN 1
			CASE 안개의호수, 얼음집, 정문
				RETURN 1
			CASE 숲의입구, 향림당, 고목의큰나무, 키리사메마법점, 인형양관, 재고의길, 무연총
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
		;平日は5分の4で神社に遊びに行ったり,ルナ、スターと連動
		IF YOUBI_MATCH(DAY, "월화수목금") && INRANGE(TIME, 600, 960)
			SELECTCASE CFLAG:LOCAL:오늘의운세 % 5
			CASE 1
				TCVAR:LOCAL:인력점 = P2경내
			CASE 2
				TCVAR:LOCAL:인력점 = P10툇마루
			CASE 3
				TCVAR:LOCAL:인력점 = P11마당
			CASE 4
				TCVAR:LOCAL:인력점 = P25수호신의숲
			CASEELSE
				TCVAR:LOCAL:인력점 = 0
			ENDSELECT
		ENDIF
ENDSELECT

@EXIST_캐릭터데이터5
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터5(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "서니")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "화이트브림"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "태양 마크가 수놓인")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "원포인트 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "하프 톱 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[서니]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;농민 관련
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "양상추 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "양상추")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "양상추밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "하쿠레이 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 25)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 200)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛" ;KR판 추가주석 눅진눅진(トロトロ) 단독사용됨
	CALLF MAKE_STR(V_NAME, "우유/버섯/양식/튀겨짐/매운/푸짐/정키/농후/따뜻한/호화/야취/호쾌/눅진눅진/반짝임/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/시큼한/시큼한/야채/야채/야채/그로테스크/그로테스크/그로테스크/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "진한 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "주홍빛/열매/귀여운/팝/화려/파워풀/로리타/" + "해/생체품/" * 3 + "태양의/햇빛의/트리니테리언의/반사의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "시크/지적/" + "어둠/별/달/도서/담뱃대/" * 3)

ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_5
DISH_NAME = 서니 사이드 업
RESULTS = 경식
CALL 재료설정("범용")
DISH_TASTE = 양식/알/알/단/시원한/선명한/호쾌/
DISH_COMMENT = 한쪽 면을 구운 계란 프라이。화려한 장식 첨부。
DISH_TIME = 10
RETURN 1

;IMAGE_I5_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I5_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

