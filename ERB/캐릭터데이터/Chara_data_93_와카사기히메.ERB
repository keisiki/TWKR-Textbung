@CHARAMOVE_DATA_93(ARG, ARG:2)
LOCAL = 93
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		;プールを改造していないと動けない
		IF !GETBIT(FLAG:풀장, 3)
			SELECTCASE ARG:2
				CASE P347안개의호수, P348안개의호수・바닥
					RETURN 0
				CASEELSE
					RETURN -100
			ENDSELECT
		;動ける状態の時
		ELSE
			SELECTCASE ARG:2
				;湖畔と안개의호수によく居る
				CASE P343호숫가, P347안개의호수
					RETURN 5
				;かまくら、大ちゃん家には行かない
				CASE P344레티의움집, P345치르노의움집, P346대요정의거처
					RETURN -100
				;紅魔館内部、폐양관には行かない
				CASE P303수위실 TO P342리리카방
					RETURN -100
			ENDSELECT
		ENDIF
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 안개의호수
				RETURN 1
			;改造していないと動けない
			CASE 정문
				SIF GETBIT(FLAG:풀장, 3)
					RETURN 1
			CASE 죽림입구, 미혹의샛길, 사각의죽림
				SIF GETBIT(FLAG:풀장, 3)
					RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터93
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터93(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "와카사기히메")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：기모노"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("기모노", "기모노"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "띠"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "인어의 왕도인")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "패각 비키니")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "패각 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[와카사기히메]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 99)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/이파리/시큼한/가벼운/건강한/담백/차가운/검소/야취/가정적/말랑말랑/맛있는물/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "푸짐/정키/매운/매운/" + "그로테스크/수수께끼 요리/" * 5 + "생선 요리/" * 10)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "맛있는물/특기요리＊담백＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/광물/귀여운/예술적/팝/시크/" + "물/보석/물고기/" * 3 + "물고기의/인어의/심해어의/고대어류의/비늘/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/무서움/화려/위엄/" + "얼음/천둥/날붙이/탄환/손목호구/담뱃대/" * 3 + "불/" * 5)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_93
DISH_NAME = 머메이드 파르페
RESULTS = 디저트
CALL 재료설정("얼음의비늘")
DISH_TASTE = 양식/아이스크림/파르페/과일/단/단/차가운/여름철/호화/예술적/선명한/말랑말랑/반짝임/
DISH_COMMENT = 인어 공주를 본뜬 블루 오션스러운 파르페。보기만 해도 시원하다。
RETURN 1

;IMAGE_I93_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;   RESULTSに特殊服装を格納すること
;   書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I93_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF CFLAG:CHARA:우후후 && TALENT:CHARA:가슴사이즈 > 0
			RESULTS = _はだけ
	ENDIF
ENDIF

