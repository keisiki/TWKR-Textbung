@CHARAMOVE_DATA_143(ARG, ARG:2)
LOCAL = 143
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、명계、八雲邸には行かない(入り口部分は出る時用）
			CASE P612법정 TO P639야쿠모가거실
				RETURN -100
			;他人の家には入らない
			CASE P608우루미집, P609에이카집, P606히사미집, P607잔무의집
				RETURN -100
			;영장원には行かない
			CASE P642영장원 TO P645마유미의방
				RETURN -100
			;他所の風呂には行かない
			CASE P652사키댁목욕탕, P646야치에댁목욕탕
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 중유의길, 삼도천, 피안, 지옥, 축생계
				RETURN 1
			CASE 옛지옥가도, 작열지옥터
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터143
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터143(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "유마")
;こっから衣装設定
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(치마＋후드) 세트")
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "숟가락"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "복슬복슬한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "양 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "양 브래지어")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;料理関連
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "중화/정키/농후/축생류/" + "매운/푸짐/호쾌/" * 3)
CASE "요리：싫어하는 맛"
	;例外的に嫌いな味が極端に少ない
	CALLF MAKE_STR(V_NAME, "검소/가벼운/담백/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "축생류/")
;贈り物関連
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흰빛/혼돈/무서움/지적/" + "강욕(強欲)의/피/" * 3  + "양의/독수리의/강욕(剛欲)의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/화려/예술적/" + "신령/" * 3 + "하니와의/수달의/늑대의/거북의/용의/하니와/" * 10)
ENDSELECT

