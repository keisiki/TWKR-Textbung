@CHARAMOVE_DATA_82(ARG, ARG:2)
LOCAL = 82
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		RESULT:1 = [[렌코]]
		RESULT:2 = [[메리]]
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;遺跡によく行く
			CASE P36몽환유적앞 TO P51몽환유적・창고
				RETURN 5
			;蓮メリの部屋,광장
			CASE P232렌코방, P233메리방, P202중앙광장
				RETURN 5
			;私室には行かない
			CASE P221히에다저택, P224케이네댁, P222서당, P226코스즈방, P229아큐방
				RETURN -100
			CASE P208라이코방, P209야츠하시방, P210벤벤방
				RETURN -100
			CASE P231반키방, P234유키방, P235마이방, P239미요이방
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루, 몽환유적
				RETURN 1
			CASE 지장길, 수양버들아래, 명련사경내, 묘지, 신령묘광장
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 죽림입구, 미혹의샛길, 사각의죽림, 토끼동굴, 무명의언덕, 태양의밭
				RETURN 1
			CASE 중유의길, 삼도천
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터82
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터82(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "메리")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "침착한 인상의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "스트라이프 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "스트라이프 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/치즈/일식/조림/케이크/시원한/담백/예술적/품위있는/가정적/푹신푹신/몽환풍/단/단/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "정키/야취/매운/매운/영장/영장/벌레고기/벌레고기/벌레고기/지옥풍/지옥풍/지옥풍/" + "그로테스크/수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "화학적/특기요리＊양식＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "보랏빛/아름다운/기묘/일본풍/섬세/지적/불가사의/" + "자연/정신/미술품/에니그마틱한/" * 3 + "매지(魅知)의/미지의/경계의/꿈의/꿈과 현실의/수첩/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "파워풀/위엄/" + "어둠/요괴/생체품/손목호구/넥타이/" * 3 + "외계의/악몽의/" * 10)
ENDSELECT

;IMAGE_I82_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;   RESULTSに特殊服装を格納すること
;   書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I82_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

