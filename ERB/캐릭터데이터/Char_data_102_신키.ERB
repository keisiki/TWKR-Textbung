@CHARAMOVE_DATA_102(ARG, ARG:2)
LOCAL = 102
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;あんまり入らない
			CASE P512마리사댁・침실, P521인형저택・앨리스방, P523인형저택・유메코방
				RETURN -5
			;引きこもらない
			CASE P522인형저택・신키방
				RETURN -10
			;リビング, 研究室に行きやすい
			CASE P524인형저택・연구실, P525인형저택・거실
				RETURN 5
			;他所の私室には入らない
			CASE P511마리사댁・거실, P515마리사댁・창고, P535엘렌댁・침실, P536엘렌댁・창고, P537엘렌댁・욕실
				RETURN -100
			CASE P541토키코의거처 TO P544나루미의오두막
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 숲의입구, 고목의큰나무, 키리사메마법점, 인형양관, 재고의길, 무연총
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터102
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터102(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "신키")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "이브닝 드레스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "무로부터 창조된")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "마계신님의 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "마계신님의 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "빵/스파이스/양식/나베/파이/시원한/건강한/호화/품위있는/가정적/쫄깃한/마력/메이드류/마계풍/단/단/매운/매운/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "중화/푸짐/검소/시큼한/시큼한/" + "개구리/벌레고기/수수께끼 요리/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "마계풍/어머니의 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흰빛/고급/혼돈/아름다운/위엄/장대/" + "마법/요술/" * 3 + "마계나비의/마신의/마계의/신화의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/로리타/매니악/" + "싸구려/주술/" * 3)
ENDSELECT

