@CHARAMOVE_DATA_87(ARG, ARG:2)
LOCAL = 87
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113나즈린방, P114무라사방, P116이치린방, P117누에방, P118뱌쿠렌방, P109조온방, P136쿄코방
				RETURN -5
			;離れ、マミゾウの棲家には入らない,神霊廟内部は行かない
			CASE P119별채, P137마미조의거처, P125토지코방 TO P134곳간, P138종루, P139사선루
				RETURN -100
			;居間に行きやすい
			CASE P108거실
				RETURN 5
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 지장길, 수양버들아래, 명련사경내, 명련사방장, 묘지
				RETURN 1
			CASE 광장, 상가거리
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;白蓮が本堂にいる時はやってくる
		SIF CFLAG:[[뱌쿠렌]]:현재위치 == P103본당 && !CFLAG:[[뱌쿠렌]]:우후후 && SHIRAHU([[뱌쿠렌]])
			TCVAR:LOCAL:인력점 = P103본당
ENDSELECT

@EXIST_캐릭터데이터87
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터87(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "쇼")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "법의"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "날개옷"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "그녀다운")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "호랑이무늬 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "호랑이무늬 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[쇼]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/고기 요리/튀겨짐/푸짐/농후/호화/야취/가정적사각사각/신력/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "빵/가벼운/담백/외계풍/화학적/매운/매운/스파이스/스파이스/스파이스/그로테스크/그로테스크/그로테스크/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "행운/특기요리＊고기 요리＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "노랑빛/금빛/질서/귀여운/심플/대륙풍/화려/" + "빛/보석/주술/법구/" * 3 + "호랑이의/정의의/이빨/손톱/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/시크/로리타/매니악/" + "어둠/생체품/" * 3)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_87
DISH_NAME = 헝그리 타이거 스테이크
RESULTS = 주식
CALL 재료설정("짐승고기")
DISH_TASTE = 양식/스테이크/고기 요리/스파이스/짠/푸짐/농후/따뜻한/호화/야취/호쾌/
DISH_COMMENT = 사찰 음식을 견딜 수 없게 된 결과、번뇌에 찌들어버린 고기 요리。
RETURN 1

