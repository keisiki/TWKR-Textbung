CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "지옥풍/마력/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/푸른빛/노랑빛/고급/혼돈/무서움/기묘/화려/파워풀/위엄/장대/매니악/" + "어둠/보석/요술/명왕의/달의/지상의/이계의/" * 3 + "판데모닉한/감주의/여신의/지옥의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "심플/레트로/시크/섬세/" + "싸구려/" * 3)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_113
DISH_NAME = 삼색 지옥 소보로 덮밥
RESULTS = 주식
CALL 재료설정("새고기")
DISH_TASTE = 일식/쌀/야채/알/매운/푸짐/농후/따뜻한/예술적/가정적/선명한/그로테스크/그로테스크/지옥풍/
DISH_COMMENT = 피처럼 붉은 다진 닭고기、새파란 나물、계란의 삼색 덮밥。
RETURN 1

;IMAGE_I113_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I113_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:의상일시변경 == 55 || CFLAG:CHARA:기본복장세트 == 55
			IF FLAG:스탠딩종류
				;顔絵
				RESULTS = _バニー
			ELSE
				;立ち絵
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

