@CHARAMOVE_DATA_113(ARG, ARG:2)
LOCAL = 113
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;와타츠키정には行かない
			CASE P1020와타츠키정・대문 TO P1044응접실
				RETURN -100
			;고요의바다は渡らない
			CASE P1001괴안통로 TO P1015모래사장
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 중유의길, 삼도천, 피안, 지옥
				RETURN 1
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 고요의바다
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터113
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터113(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "헤카티아")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：액세서리"
