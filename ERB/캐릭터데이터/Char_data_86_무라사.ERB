@CHARAMOVE_DATA_86(ARG,ARG:2)
LOCAL = 86
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;명련사 멤버의 방(강당과 창고 제외하다)
			CASE 113, 115, 116, 117, 118
				RETURN -5
			;별채와 묘지와 신령묘내부
			CASE 119, 121, 125 TO 133
				RETURN -100
			;거실에 가기 쉽다
			CASE 108
				RETURN 5
			;뱌쿠렌이 본당에 있을 때 유행해 온다
			CASE 103
				SIF CFLAG:55:현재위치 == 103
					RETURN 10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1,3,6,8
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 15, 31, 61,62, 81,83
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터86
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터86(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "무라사")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：바지"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("바지", "쇼트 바지"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "세라복"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "이름 대신에 닻의 마크가 쓰여진")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "보이쉬 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "탱크톱 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[무라사]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/스파이스/국물/카레/정키/농후/차가운/기능적/호쾌/그로테스크/말랑말랑/매운/매운/매운/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "튀겨짐/단/단/개구리/개구리/개구리/벌레고기/벌레고기/벌레고기/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "자극적/맛있는물/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/혼돈/무서움/" + "물/유령/요술/" * 3 + "바다의/심해의/닻의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "지적/위엄/" + "빛/불/부적/손목호구/" * 3)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_86
DISH_NAME = 심해 요리
RESULTS = 주식
CALL 재료설정("맛있는물고기")
DISH_TASTE = 생선 요리/짠/푸짐/담백/야취/호쾌/그로테스크/영장/
DISH_COMMENT = 심해어를 사용한 약간 그로테스크한 요리。
RETURN 1

