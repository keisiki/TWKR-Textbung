@CHARAMOVE_DATA_153(ARG, ARG:2)
LOCAL = 153
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;私室には入らない
			CASE P704캇파의아지트, P706액신의집, P709릴리네집, P716아키자매의집・봉당 TO P730선인저택・마당
				RETURN -100
			CASE P733세이자은신처, P737마네키네코의집, P740야마와로의아지트, P736야만바의집
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 중유의길, 삼도천, 피안, 지옥, 축생계
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 깊은숲속
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터153
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터153(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "미천")
;こっから衣装設定
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))
CASE "普段着：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "금륜"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "서유기풍의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "원숭이바지")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "원숭이브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "면류/푸짐/농후/호화/호쾌/선명한/사각사각/신력/축생류/" + "중화/매운/짠/" * 2 + "감자튀김/고기 요리/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/시큼한/시큼한/담백/담백/검소/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "축생류/특기요리：중화/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/질서/멋쟁이/대륙풍/파워풀/장대/" + "신령/수달의/" * 3 + "원숭이의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/서양풍/섬세/지적/" + "늑대의/독수리의/개의/" * 10)
ENDSELECT

