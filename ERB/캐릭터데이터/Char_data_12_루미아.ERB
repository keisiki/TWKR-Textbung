@CHARAMOVE_DATA_12(ARG,ARG:2)
LOCAL = 12
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;각 개인방, 목욕탕
			CASE 512, 514, 521, 522, 523, 526, 531, 535, 537
				RETURN -100
			;거실, 리빙, 2층
			CASE 511, 525, 530
				RETURN -5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11,12,15, 51,53,56,57, 61,62
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터12
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터12(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "루미아")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "민소매 윗도리"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "어둠을 굳힌 듯한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "칠흑의 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "칠흑의 캐미숄")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[루미아]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/고기 요리/튀겨짐/단/농후/호화/야취/사각사각/푸짐/푸짐/" + "개구리/짠/그로테스크/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/시큼한/담백/검소/시원한/시원한/시원한/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "진한 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "무서움/로리타/" + "흑빛/어둠/생체품/" * 3 + "요마의/땅거미의/어둠의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "지적/" + "흰빛/빛/해/조명/" * 3)

ENDSELECT

