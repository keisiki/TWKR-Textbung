CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "날씨조작")
CASE "고유커맨드 실행 가능"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "과일/스파이스/중화/면류/파이/푸짐/정키/농후/호화/품위있는/선명한/푹신푹신/천계풍/단/단/매운/매운/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/시원한/건강한/담백/검소/야취/시큼한/시큼한/개구리/개구리/개구리/벌레고기/벌레고기/벌레고기/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "천계풍/현란/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/무지갯빛/혼돈/아름다운/기묘/팝/화려/장대/위엄/" + "흙/날붙이/" * 3 + "요석의/복숭아의/천사의/천인의/극광의/천계의/비상(緋想)의/비상비비상의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/심플/시크/섬세/지적/" + "천둥/사생/도서/생체품/넥타이/" * 3 + "악마의/" * 5)

;농민 관련
;복숭아
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "복숭아 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "복숭아")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "복숭아 농원")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "천계")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 8)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 833)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 600)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 7)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
