@CHARAMOVE_DATA_34(ARG, ARG:2)
LOCAL = 34
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;他所の家には行かない
			CASE P807모미지댁, P808아야댁, P809하타테댁, P851쿠다카의오두막, P852코마쿠사의오두막, P853미스마루의오두막, P854메구무댁, P855홍룡동, P856모모요의거처
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내, 천계
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;平日は3分の2で出歩く,紫苑と連動
		IF YOUBI_MATCH(DAY, "월화수목금") && INRANGE(TIME, 600, 960)
			SELECTCASE CFLAG:LOCAL:오늘의운세 % 3
			CASE 1
				TCVAR:LOCAL:인력점 = P811경내
			CASE 2
				TCVAR:LOCAL:인력점 = P802다리
			CASEELSE
				TCVAR:LOCAL:인력점 = 0
			ENDSELECT
		ENDIF
ENDSELECT

@EXIST_캐릭터데이터34
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터34(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "텐시")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "복숭아"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "일절 꿰맨 자리가 없는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "복숭아색 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "브라 캐미숄")
CASE "고유 속옷 착용 조건"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
