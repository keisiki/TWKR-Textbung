@CHARAMOVE_DATA_155(ARG, ARG:2)
LOCAL = 155
SELECTCASE ARG
	;連動設定（RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;外に出る道中は行く
			CASE P930옛혈지지옥, P929지옥의인공태양, P928작열지옥터, P926안뜰, P9051층복도, P901지령전앞
				RETURN 0
			;残りの地霊殿には行かない
			CASE P901지령전앞 TO P930옛혈지지옥
				RETURN -100
			;家には入らない
			CASE P936유기의집, P940파르시의집, P941야마메의집, P942키스메의집
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 지장길, 수양버들아래, 묘지
				RETURN 1
			CASE 상가거리, 술집소로
				RETURN 1
			CASE 중유의길, 삼도천, 피안, 지옥, 축생계
				RETURN 1
			CASE 환상풍혈, 지옥의깊은길, 옛지옥가도, 지령전, 작열지옥터
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
		;あまりにも奥に居るので昼間は表に出てくる,1/8でそのまま
		SIF INRANGE(TIME, 660, 840) && CFLAG:LOCAL:오늘의운세 % 8
			TCVAR:LOCAL:인력점 = P931큰길
ENDSELECT

@EXIST_캐릭터데이터155
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터155(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "치야리")
;こっから衣装設定
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지＋후드) 세트")
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "지옥T셔츠"))
CASE "평상복：바지"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("바지", "반바지"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "원념의 소용돌이")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "도깨비불 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "도깨비불 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "국물/드링크/짠/짠/시큼한/시큼한/정키/농후/호화/선명한/그로테스크/뭉근뭉근/영장/지저풍/" + "고기 요리/" * 3 + "피의마력/" * 5)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "단/단/담백/검소/예술적/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "피의마력/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/요술/혼돈/무서움/기묘/매니악/" + "불/피/독수리의/츄파카브라의/" * 3 + "피의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "푸른빛/질서/멋쟁이/미래/" + "물/" * 3 + "늑대의/수달의/" * 10)
ENDSELECT

