@CHARAMOVE_DATA_38(ARG,ARG:2)
LOCAL = 38
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;인공 태양에는 가지 않는다
			CASE 930
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 1, 2, 3, 8, 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 11 TO 16, 21,22,23,27, 35,36,37,38, 84,85, 91,92,93,96
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;引力設定
	CASE 8
		;거의 매 턴으로 불리기 때문에 시작 시간이 더 불안정하다. 최대 20시까지
		IF INRANGE(TIME, 600 + RAND:120, 700 + (CFLAG:LOCAL:오늘의운세 / 2))
			SELECTCASE CFLAG:LOCAL:오늘의운세 % 6
			CASE 1
				TCVAR:LOCAL:인력점 = 925
			CASE 2
				TCVAR:LOCAL:인력점 = 929
			CASE 3
				TCVAR:LOCAL:인력점 = 933
			CASE 4
				TCVAR:LOCAL:인력점 = 939
			CASE 5
				TCVAR:LOCAL:인력점 = 945
			CASEELSE
				TCVAR:LOCAL:인력점 = 0
			ENDSELECT
		ENDIF
ENDSELECT

@EXIST_캐릭터데이터38
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터38(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "코이시")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	IF !RAND:2
		IF RAND:1000 == 514
			CALLF MAKE_STR(V_NAME, "전라")
		ELSE
			CALLF MAKE_STR(V_NAME, "팬티만")
		ENDIF
	ELSE
		CALLF MAKE_STR(V_NAME, "파자마(바지＋후드) 세트")
	ENDIF

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "아마 코이시의 물건은 아닌")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "섹시 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "섹시 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[코이시]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "스파이스/치즈/양식/튀겨짐/케이크/정키/농후/가정적/예술적/말랑말랑/지저풍/단/단/매운/매운/" + "수수께끼 요리/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/짠/짠/야채/야채/야채/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "지저풍/몰카맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "잿빛/귀여운/기묘/팝/로리타/불가사의/" + "정신/" * 3 + "하트의/장미의/지저의/마음의/이드(id)의/에고(ego)의/나이프/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/심플/지적/" + "손목호구/" * 3 + "눈동자의/" * 10)
ENDSELECT

;IMAGE_I38_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I38_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF  NUDITYCHECK(CHARA) == 1
			RESULTS = _下着P
			SIF FLAG:스탠딩종류
				RESULTS = _半裸
		ELSEIF NUDITYCHECK(CHARA) == 2
			RESULTS = _下着B
		ELSEIF NUDITYCHECK(CHARA) == 3
			RESULTS = _下着
			SIF FLAG:스탠딩종류
				RESULTS = _下着B
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ENDIF

