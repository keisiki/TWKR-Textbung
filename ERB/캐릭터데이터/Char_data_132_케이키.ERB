@CHARAMOVE_DATA_132(ARG,ARG:2)
LOCAL = 132
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 수면시간(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、八雲邸には行かない（入り口部分は出る時用）
			CASE 610, 613, 630, 631
				RETURN -10
			CASE 610 TO 618, 630 TO 639
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 608, 609
				RETURN -100
			;組長宅には行かない
			CASE 646 TO 652
				RETURN -100
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 5, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 1, 2, 3, 4, 6, 21, 22, 23, 27, 51, 52, 53, 54, 56, 57, 61, 62, 64, 65, 68, 69
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터132
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터132(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "케이키")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(원피스) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "두건"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))
CASE "평상복：외투"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("외투", "작업 앞치마"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "샌들"))
CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "목걸이"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "조형술로 만든")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "크리에이터 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "크리에이터 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[케이키]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/빵/국물/케이크/시원한/호화/예술적/품위있는/푹신푹신/단/단/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야취/영장/짠/짠/스파이스/스파이스/스파이스/축생류/축생류/축생류/축생류/축생류/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "색배합/숨김맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/질서/아름다운/예술적/레트로/위엄/" + "흙/츠쿠모가미/주술/미술품/인형/기하학모양의/" * 3 + "귀형의/하니와의/조형술의/피규어/조각/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/심플/" + "요괴/생체품/" * 3 + "늑대의/수달의/독수리의/" * 10)
ENDSELECT

