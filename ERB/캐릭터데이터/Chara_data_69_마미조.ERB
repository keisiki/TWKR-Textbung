@CHARAMOVE_DATA_69(ARG, ARG:2)
LOCAL = 69
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;命蓮寺メンバーの私室はあんまり入らない
			CASE P113나즈린방, P114무라사방, P115쇼방, P116이치린방, P117누에방, P118뱌쿠렌방, P109조온방, P136쿄코방
				RETURN -5
			;神霊廟メンバーの私室はあんまり入らない
			CASE P125토지코방, P127후토방, P128미코방, P139사선루
				RETURN -5
			;本堂、枯山水、調薬室には行かない
			CASE P103본당, P131고산수, P132조제실
				RETURN -100
			;よく居る
			CASE P108거실, P129응접실
				RETURN 5
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 지장길, 수양버들아래, 명련사경내, 묘지, 신령묘광장
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터69
