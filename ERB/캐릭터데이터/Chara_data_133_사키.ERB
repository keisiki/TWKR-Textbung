@CHARAMOVE_DATA_133(ARG, ARG:2)
LOCAL = 133
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、冥界、八雲邸には行かない（入り口部分は出る時用）
			CASE P612법정 TO P639야쿠모가거실
				RETURN -100
			;他人の家には入らない
			CASE P608우루미집, P609에이카집, P606히사미집, P607잔무의집  
				RETURN -100
			;영장원には行かない
			CASE P642영장원 TO P645마유미의방
				RETURN -100
			;他所の風呂には行かない
			CASE P646야치에댁목욕탕, P655유마댁목욕탕
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 중유의길, 삼도천, 피안, 지옥, 축생계
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터133
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터133(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "사키")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "카우보이 모자"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "롱 부츠"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "스카프"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "편자 도장이 찍힌")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "블랙 쉐이프 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "검은날개 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[사키]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/스파이스/중화/나베/푸짐/정키/농후/따뜻한/호화/야취/호쾌/사각사각/축생류/" + "매운/당근/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/" + "단/시큼한/가벼운/담백/" * 2 + "천계풍/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "축생류/특기요리＊호쾌＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/빨강빛/혼돈/근사함/심플/파워풀/장대/" + "늑대의/" * 3 + "독수리의/천마의/스카프/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/섬세/지적/" + "신령/도서/기계/" * 3 + "하니와의/수달의/독수리의/거북의/용의/하니와/" * 10)
ENDSELECT

