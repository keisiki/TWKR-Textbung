@CHARAMOVE_DATA_30(ARG, ARG:2)
LOCAL = 30
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;小町の私室はあまり入らない
			CASE P616코마치의방
				RETURN -5
			;八雲邸、축생계は行かない
			CASE P630야쿠모가현관 TO P655유마댁목욕탕
				RETURN -100
			;他人の家、他宅の私室等には入らない
			CASE P608우루미집, P609에이카집, P623유유코의방, P624요우무의방, P627백옥루목욕탕
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 지장길, 수양버들아래, 명련사경내
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 중유의길, 삼도천, 피안, 백옥루정원, 백옥루, 지옥
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

