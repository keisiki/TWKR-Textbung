@CHARAMOVE_DATA_1(ARG, ARG:2)
LOCAL = 1
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P2경내, P10툇마루
				RETURN 5
			;他所
			CASE P30삼요정의집・목욕탕, P39몽환유적・욕실, P56아운의거처
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터1
