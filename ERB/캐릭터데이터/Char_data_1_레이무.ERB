;オブジェクト本体
@캐릭터데이터1(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "레이무")

;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "소매"))
CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "하쿠레이의 무녀옷"))

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "운세뽑기계약")
CASE "고유커맨드 실행 가능"
	CALLF MAKE_INT(V_NAME, !FLAG:운세뽑기계약)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/양식/이파리/시원한/담백/검소/품위있는/가정적/푹신푹신/" + "단/시큼한/" * 2 + "봄철/센베이/" * 3)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "매운/요력/" * 2 + "그로테스크/수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "담백한 맛/특기요리＊일식＊/")
;선물 관련
CASE "감성：좋아함" ;JP판 수정요망, 존재하지 않는 형용사 蓮華蝶の. 연꽃나비(蓮花蝶の) 오타로 추정
	CALLF MAKE_STR(V_NAME, "빨강빛/질서/귀여운/일본풍/" + "신령/주술/부적/여우의/봄의/영원한/" * 3 + "蓮華蝶の/벛꽃의/무녀의/낙원의/봉마의/바늘/불제봉/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/서양풍/로리타/매니악/" + "손목호구/기계/" * 3 + "요괴/" * 5 + "모리야의/요괴의/" * 10)

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "작고 붉은 리본이 프런트에 달린")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "프런트 리본 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "리본 브래지어")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;농민 관련
;신사는 토지를 가지고 있음、칸다
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "쌀 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "쌀")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "논")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "하쿠레이 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 400)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 5)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 103)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰1(ARG, O_DATA, V_NAME)

;고유 카운터 테스트


ENDSELECT

