@CHARAMOVE_DATA_119(ARG, ARG:2)
LOCAL = 119
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;自宅によく居る
			CASE P736야만바의집
				RETURN 5
			;私室には入らない
			CASE P704캇파의아지트, P706액신의집, P709릴리네집, P716아키자매의집・봉당 TO P730선인저택・마당
				RETURN -100
			CASE P733세이자은신처, P737마네키네코의집, P740야마와로의아지트
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 깊은숲속
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터119
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터119(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "네무노")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))

;굳이 색은 결정하지 않는다
CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "색의 농담이 재미있는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "손수 물들인 훈도시")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "손수 물들인 사라시")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[네무노]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 3)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/버섯/일식/나베/안주/푸짐/농후/가을철/검소/야취/가정적/뭉근뭉근/짠/짠/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "빵/치즈/양식/가벼운/담백/외계풍/단/단/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "어머니의 맛/특기요리＊가을철＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "찻빛/아름다운/심플/시크/" + "질서/날붙이/" * 3 + "산의/성역의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/서양풍/로리타/" + "기계/" * 5)
ENDSELECT

