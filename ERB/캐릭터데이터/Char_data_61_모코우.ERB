@EXIST_캐릭터데이터61
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터61(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "모코우")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：바지"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("바지", "사시누키하카마"))
CASE "평상복：상반신윗도리2"
	IF DAY:2 == 2
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "반소매 블라우스"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
	ENDIF
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "서스펜더"))
CASE "평상복：그외2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외2", "부적"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "자각 없는 멋쟁이")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "부적 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 0)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/스파이스/일식/나베/시원한/따뜻한/검소/선명한/가정적/푹신푹신/매운/매운/매운/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "아이스크림/푸짐/농후/차가운/단/단/시큼한/시큼한/수수께끼 요리/수수께끼 요리/수수께끼 요리/달구경/달구경/달구경/달도시풍/달도시풍/달도시풍/달도시풍/달도시풍/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "대화력/특기요리＊새고기＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "귀여운/일본풍/파워풀/장대/" + "빨강빛/불/주술/부적/" * 3 + "봉황의/불멸의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "팝/섬세/로리타/매니악/" + "사생/기계/" * 3 + "물/얼음/달/왕녀의/봉래의/" * 5)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_61
DISH_NAME = 닭고기 꼬치구이
RESULTS = 경식
CALL 재료설정("새고기", "산새")
DISH_TASTE = 일식/안주/고기 요리/새고기/짠/농후/따뜻한/가정적/
DISH_COMMENT = 불사조처럼 선명하게 구운 닭고기。맛은 지극히 보통이다。
RETURN 1

;IMAGE_I61_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I61_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

