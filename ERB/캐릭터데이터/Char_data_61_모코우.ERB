@CHARAMOVE_DATA_61(ARG,ARG:2)
LOCAL = 61
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;몽환관에는 가지 않는다
			CASE 450
				RETURN -10
			CASE 450 TO 466
				RETURN -100
			;영원정 안으로는 가지 않는다
			CASE 405, 407 TO 430
				RETURN -100
			;남의 집에는 들어가지 않는다
			CASE 432, 437, 439, 440, 442, 443
				RETURN -100
			;斜角の竹林,迷いの小道,夜雀の屋台,自宅,竹林入口によく居る
			CASE 433, 434, 444, 445, 447
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 4
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 41 TO 47
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT
