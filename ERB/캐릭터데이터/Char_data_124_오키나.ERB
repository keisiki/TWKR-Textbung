@CHARAMOVE_DATA_124(ARG, ARG:2)
LOCAL = 124
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;未設定
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 숲의입구, 향림당, 고목의큰나무, 키리사메마법점 , 재고의길, 무연총
				RETURN 1
			CASE 중유의길, 삼도천, 마요이가
				RETURN 1
			CASE 지하실
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터124
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터124(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "오키나")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "관모"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "롱 부츠"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "도사옷"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "밤하늘을 투영한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "별의 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "별의 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:3
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/조림/건강한/호화/예술적/품위있는/말랑말랑/매운/매운/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "검소/짠/짠/" + "개구리/벌레고기/수수께끼 요리/" * 3 + "텐구류/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "인챈트/가호/현란/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "노랑빛/고급/혼돈/아름다운/예술적/레트로/화려/지적/위엄/" + "주술/미술품/사계의/" * 3 + "칠성의/후호(後戸)의/숨겨진/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "서양풍/시크/로리타/" + "싸구려/" * 3 + "텐구의/바캉스의/" * 10)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_124
DISH_NAME = 마타라보카
RESULTS = 주식
CALL 재료설정("짐승고기")
DISH_TASTE = 양식/고기 요리/야채/시큼한/푸짐/호화/품위있는/신력/
DISH_COMMENT = 살팀보카라는 생햄 요리를 어레인지한 멋진 요리。
RETURN 1
