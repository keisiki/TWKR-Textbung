@CHARAMOVE_DATA_11(ARG, ARG:2)
LOCAL = 11
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;神社のよくいるところ
			CASE P2경내, P10툇마루
				RETURN 5
			;よく居る
			CASE P502향림당, P507버섯군생지
				RETURN 5
			;引きこもらない
			CASE P512마리사댁・침실
				RETURN -10
			;アリス以外の私室には入らない
			CASE P522인형저택・신키방, P523인형저택・유메코방, P526인형저택・욕실, P531인형저택・빈방
				RETURN -100
			CASE P535엘렌댁・침실, P537엘렌댁・욕실, P541토키코의거처, P542리글의거처, P543루미아의거처, P544나루미의오두막
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루, 몽환유적
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 안개의호수, 정문, 객실, 식당, 대도서관, 지하실
				RETURN 1
			CASE 죽림입구, 미혹의샛길, 사각의죽림, 영원정, 몽환관
				RETURN 1
			CASE 숲의입구, 향림당, 고목의큰나무, 키리사메마법점, 인형양관, 재고의길, 무연총
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
		;水木金15～19時は店番,たまにサボる
		IF YOUBI_MATCH(DAY, "수목금") && INRANGE(TIME, 900, 1140) && CFLAG:LOCAL:오늘의운세 > 200
			TCVAR:LOCAL:인력점 = P510키리사메마법점
		;19～22時はまぁまぁな頻度で研究部屋にこもる
		ELSEIF INRANGE(TIME, 1140, 1320) && CFLAG:LOCAL:오늘의운세 % 3
			TCVAR:LOCAL:인력점 = P513마리사댁・연구실
		ENDIF
ENDSELECT

@EXIST_캐릭터데이터11
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터11(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "마리사")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "가터 스타킹"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "민소매 윗도리"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "소녀틱한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "프릴드로워즈")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "슈미즈")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;농민 관련
;농가라고 할까 버섯 스폿을 가르쳐 주었으려나
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "버섯 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "버섯")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "버섯 농원")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "마법의숲")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 5)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 507)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 450)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 6)

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "사람돕기")
CASE "고유커맨드 실행 가능"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "생선 요리/버섯/면류/푸짐/야취/선명한/호쾌/사각사각/마계풍/외계풍/일식/일식/단/단/매운/매운/겨울철/겨울철/겨울철/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "빵/양식/시큼한/시큼한/" + "그로테스크/수수께끼 요리/" * 3)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "인챈트/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/질서/귀여운/기묘/파워풀/장대/" + "마법/별/도서/까마귀의/마녀의/겨울의/" * 3 + "버섯의/혜성의/별빛부스러기의/사랑색의/마도서/비/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "섬세/" + "요괴/" * 5)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_11
DISH_NAME = 마법의버섯 소테
RESULTS = 경식
CALL 재료설정("마법의버섯")
DISH_TASTE = 양식/미지의맛/시원한/건강한/따뜻한/예술적/선명한/마력/
DISH_COMMENT = 마법의 버섯을 사용한 소테。먹기 전까진 맛을 알 수 없다。
RETURN 1

;IMAGE_I11_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I11_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:0 % 2 && DAY:2 != 4
			RESULTS = _日替わり
	ENDIF
ENDIF

