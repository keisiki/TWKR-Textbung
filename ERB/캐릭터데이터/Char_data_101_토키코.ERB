@CHARAMOVE_DATA_101(ARG, ARG:2)
LOCAL = 101
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;自宅によく居る
			CASE P541토키코의거처
				RETURN 5
			;魔理沙家,アリス家,エレン家,妖怪の棲み処
			CASE P510키리사메마법점 TO P515마리사댁・창고, P521인형저택・앨리스방 TO P531인형저택・빈방
				RETURN -100
			CASE P535엘렌댁・침실 TO P537엘렌댁・욕실, P542리글의거처, P543루미아의거처, P544나루미의오두막
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 안개의호수, 정문, 대도서관
				RETURN 1
			CASE 숲의입구, 향림당, 고목의큰나무
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터101
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터101(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "토키코")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "흰 바탕에 연분홍색을 칠한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "연분홍 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "연분홍 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/벌레고기/일식/이파리/시원한/담백/검소/야취/가정적/뭉근뭉근/단/단/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "매운/매운/알/알/알/" + "새고기/수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "담백한 맛/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/질서/귀여운/심플/일본풍/시크/섬세/지적/" + "새/도서/" * 3 + "지혜의/지식의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "팝/파워풀/위엄/" + "날붙이/탄환/손목호구/" * 3)
ENDSELECT

