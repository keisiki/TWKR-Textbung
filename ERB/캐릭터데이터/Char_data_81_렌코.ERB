@CHARAMOVE_DATA_81(ARG, ARG:2)
LOCAL = 81
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		RESULT:1 = [[렌코]]
		RESULT:2 = [[메리]]
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;遺跡によく行く
			CASE P36몽환유적앞 TO P51몽환유적・창고
				RETURN 5
			;蓮メリの部屋,광장
			CASE P232렌코방, P233메리방, P202중앙광장
				RETURN 5
			;私室には行かない
			CASE P221히에다저택, P224케이네댁, P222서당, P226코스즈방, P229아큐방
				RETURN -100
			CASE P208라이코방, P209야츠하시방, P210벤벤방
				RETURN -100
			CASE P231반키방, P234유키방, P235마이방, P239미요이방
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루, 몽환유적
				RETURN 1
			CASE 지장길, 수양버들아래, 명련사경내, 묘지, 신령묘광장
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 죽림입구, 미혹의샛길, 사각의죽림, 토끼동굴, 무명의언덕, 태양의밭
				RETURN 1
			CASE 중유의길, 삼도천
				RETURN 1
		ENDSELECT
	;引力設定
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터81
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터81(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "렌코")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：외투"
	SIF DAY:2 == 1
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("외투", "케이프"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "하반신이 바싹 조여보이는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "도트 무늬 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "누브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/스파이스/중화/면류/드링크/시원한/정키/기능적/품위있는/가정적/사각사각/달도시풍/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "시큼한/시큼한/벌레고기/벌레고기/벌레고기/그로테스크/그로테스크/그로테스크/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "화학적/특기요리＊정키＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/고급/근사함/심플/레트로/시크/지적/불가사의/매니악/" + "별/달/문구/유물/에니그마틱한/" * 3 + "양자의/미지의/매지(魅知)의/환상(幻想)의/수첩/넥타이/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/화려/로리타/" + "손목호구/" * 3 + "외계의/" * 10)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_81
DISH_NAME = 영양 효율 보충제
RESULTS = 경식
CALL 재료설정("영양드링크", "스포츠드링크")
DISH_TASTE = 시원한/건강한/기능적/기능적/기능적/약효/외계풍/
DISH_COMMENT = 영양을 응축한 정제。요리인데도 통 먹을 생각이 안 난다。
DISH_MAXLIMIT = 600
RETURN 1

;IMAGE_I81_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;   RESULTSに特殊服装を格納すること
;   書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I81_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF DAY:2 == 2
			RESULTS = _夏服
	ENDIF
ENDIF

