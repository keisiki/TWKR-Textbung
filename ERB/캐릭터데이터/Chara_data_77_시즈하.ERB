@CHARAMOVE_DATA_77(ARG, ARG:2)
LOCAL = 77
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
		RESULT:1 = [[시즈하]]
		RESULT:2 = [[미노리코]]
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;各私室
			CASE P704캇파의아지트, P706액신의집, P729선인저택・현관 TO P730선인저택・마당
				RETURN -100
			CASE P733세이자은신처, P736야만바의집, P737마네키네코의집, P740야마와로의아지트
				RETURN -100
				;畑によく居る
			CASE P715아키자매의집・밭
				RETURN 5
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 광장, 상가거리, 술집소로
				;秋の間だけ
				SIF DAY:2 == 3
					RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 깊은숲속
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터77
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터77(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "시즈하")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "머리 장식"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "가을의 종언을 체현한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "단풍 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "단풍 브래지어")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/쌀/스파이스/이파리/시원한/건강한/담백/예술적/야취/선명한/사각사각/단/단/매운/매운/가을철/가을철/가을철/가을철/가을철/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "정키/농후/짠/짠/" + "개구리/벌레고기/겨울철/" * 3 + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "현란/특기요리＊가을철＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "아름다운/예술적/일본풍/화려/섬세/" + "주홍빛/바람/식물/미술품/" * 3 + "단풍의/가을의/가을바람의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "위엄/" + "얼음/날붙이/탄환/손목호구/기계/" * 3 + "여름의/벛꽃의/해바라기의/수박의/" * 5  + "봄의/겨울의/봄바람의/" * 10)

;농민 관련
;포도가 달린 건 여동생 쪽이지만
;포도라면 멋있고 단풍도 들고 과일도 물들고
;바꾼다면 미노리코를 포도로 시즈하를 옻(비식용의 유용식물, 또한 단풍도 든다) 이라던지???
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "머루 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "머루")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "머루밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "요괴의 산·산록")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 715)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 400)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 4)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_77
DISH_NAME = 단풍튀김
RESULTS = 경식
CALL 재료설정("산나물")
DISH_TASTE = 일식/튀겨짐/덴푸라/야채/짠/시원한/건강한/담백/따뜻한/가을철/검소/가정적/야취/야취/사각사각/
DISH_COMMENT = 산의 단풍을 그대로 튀김으로 만든 요리。맛은 덤덤하다。
RETURN 1

;IMAGE_I77_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I77_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF CFLAG:CHARA:의상일시변경 == 55 || (CFLAG:CHARA:기본복장세트 == 55 && CFLAG:CHARA:의상일시변경==0)
			IF FLAG:스탠딩종류
				;顔絵
				RESULTS = _バニー
			ELSE
				;立ち絵
				RESULTS = _バニー
			ENDIF
		ENDIF
	ENDIF
ENDIF

