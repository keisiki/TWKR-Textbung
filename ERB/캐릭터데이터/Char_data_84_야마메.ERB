@CHARAMOVE_DATA_84(ARG,ARG:2)
LOCAL = 84
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;가지 않는다
			CASE 901 TO 930, 935, 936, 937, 939, 940, 942, 943
				RETURN -100
			;큰 길과 술집에 가기 쉽다
			CASE 931, 933
				RETURN 5
			;더러움에 매료된다
			CASEELSE
				RETURN YOGORE:(ARG:2) / 300
		ENDSELECT
	;일시간대
	CASE 5
		RETURN 0
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 9
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 91,92,93
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터84
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터84(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "야마메")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "윗도리"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "점퍼스커트"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "검은 천에 거미집을 자수한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "거미무늬 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "거미무늬 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[야마메]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/일식/국물/드링크/농후/검소/야취/그로테스크/뭉근뭉근/지저풍/짠/짠/시큼한/시큼한/" + "벌레고기/" * 5)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "매운/매운/빵/빵/빵/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "지저풍/특기요리＊벌레고기＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "흑빛/찻빛/귀여운/시크/" + "벌레/독/생체품/" * 3 + "거미줄의/거미의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "화려/파워풀/로리타/" + "동물/새/기계/" * 3)
ENDSELECT


