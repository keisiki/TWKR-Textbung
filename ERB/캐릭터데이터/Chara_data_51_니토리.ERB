@CHARAMOVE_DATA_51(ARG, ARG:2)
LOCAL = 51
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;沢によく居る
			CASE P703현무의늪
				RETURN 5
			;私室には入らない
			CASE P706액신의집, P709릴리네집, P716아키자매의집・봉당 TO P730선인저택・마당
				RETURN -100
			CASE P733세이자은신처, P736야만바의집, P737마네키네코의집, P740야마와로의아지트
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 간헐천지하센터
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

