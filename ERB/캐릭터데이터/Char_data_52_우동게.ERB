@CHARAMOVE_DATA_52(ARG, ARG:2)
LOCAL = 52
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;몽환관には行かない
			CASE P450몽환관현관 TO P466몽환관정원
				RETURN -100
			;他人の家には入らない
			CASE P432카게로의집, P437메디슨의집, P439라바의집, P443미스티아의집, P445모코우의집
				RETURN -100
			;来客用トイレ,永琳私室,輝夜私室,裏庭はあんまり行かない
			CASE P406손님용화장실, P413에이린방, P414카구야방, P422뒷마당
				RETURN -5
			;영원정入口,待合室,診察室,調剤室,てゐ私室,鈴仙私室,庭がちょっと好き
			CASE P401영원정입구, P403대합실, P404진찰실, P405조제실, P410테위방, P411우동게방, P421마당
				RETURN 3
			;縁側,居間によく居る
			CASE P409툇마루, P416거실
				RETURN 5
			
			;ドレミー宅には行かない
			CASE P1050도레미댁・거실 TO P1055도레미댁・화장실
				RETURN -100
			;門番詰め所は行かない
			CASE P1023문지기초소
				RETURN -100
			;二階はあまり行かない
			CASE P10402층회랑 TO P1044응접실
				RETURN -5
			;玉兎街方面,玉兎식당,玉兎詰め所によく居る
			CASE P1003옥토거리, P1004광장, P1010식사처 TO P1013야채가게, P1024옥토식당, P1025옥토초소
				RETURN 5
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로, 주택가
				RETURN 1
			CASE 죽림입구, 미혹의샛길, 사각의죽림, 토끼동굴, 영원정
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
		;たまーにてゐと連動
		SIF CFLAG:LOCAL:오늘의운세 % 10 == 0
			TCVAR:LOCAL:인력점 = TCVAR:[[테위]]:인력점
ENDSELECT

@EXIST_캐릭터데이터52
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터52(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "우동게")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "와이셔츠"))
CASE "평상복：상반신윗도리1"
;여름은 블레이저 코트를 벗는다
	SIF DAY:2 != 2
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "블레이저 코트"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "넥타이"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "토끼 꼬리에 리본으로 묶은")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "줄무늬 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "줄무늬 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/과일/이파리/건강한/품위있는/기능적/가정적/쫄깃한/옥토끼풍/달도시풍/달구경/단/단/시큼한/시큼한/당근/당근/당근/당근/당근/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야취/매운/매운/" + "개구리/짐승고기/스파이스/벌레고기/그로테스크/" * 3 + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "옥토끼풍/특기요리＊건강한＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "빨강빛/질서/귀여운/섬세/지적/" + "토끼의/지상의/광기의/약병/넥타이/병적으로 귀여운/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/무서움/기묘/화려/로리타/" + "날붙이/생체품/손목호구/" * 3)

;특수 의상
CASE "집 데이트 의상"
	SIF MAIN_MAP == 2
		CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "의상세트", "우동게 약장수세트"))
CASE "마을 데이트 의상"
	CALLF MAKE_INT(V_NAME, OBJNAME_TO_ID(ARG, "GET", "의상세트", "우동게 약장수세트"))

CASE "고유커맨드"
	CALLF MAKE_STR(V_NAME, "약을산다")
CASE "고유커맨드 실행 가능"
	CALLF MAKE_INT(V_NAME, 1)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_52
DISH_NAME = 광기의 붉은 계란 프라이
RESULTS = 경식
CALL 재료설정("범용")
DISH_TASTE = 알/짠/시원한/건강한/따뜻한/가정적/선명한/그로테스크/요력/옥토끼풍/
DISH_COMMENT = 붉은 눈동자 같은 계란 프라이。겉보기에는 괴상하지만 영양가는 높다。
RETURN 1

;IMAGE_I52_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I52_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		SIF EQUIP:CHARA:상반신윗도리1 != 4
			RESULTS = _ブラウス
	ENDIF
ENDIF

