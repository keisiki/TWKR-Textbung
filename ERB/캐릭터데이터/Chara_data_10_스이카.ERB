@CHARAMOVE_DATA_10(ARG, ARG:2)
LOCAL = 10
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P2경내, P10툇마루, P11마당
				RETURN 5
			CASE P701수해도입구, P703현무의늪, P711휴게소, P801산정입구, P806폭포뒤, P830현운해
				RETURN 10
			;私室にはあまり入らない
			CASE P15레이무방, P52별채
				RETURN -5
			;他所
			CASE P27삼요정의집・객실 TO P35삼요정의집・지하실, P37몽환유적・1층통로 TO P51몽환유적・창고, P56아운의거처
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사툇마루
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내, 천계
				RETURN 1
			CASE 환상풍혈, 지옥의깊은길, 옛지옥가도
				RETURN 1
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터10
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터10(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "스이카")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "리본"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "민소매 블라우스"))
CASE "평상복：그외1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외1", "구속도구"))
CASE "평상복：그외2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("그외2", "각진 끈"))

;어떻게 봐도 원작이 노브라
CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "매우 튼튼한")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "오니의 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 0)
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[스이카]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)

;농민 관련
;이 아가씨의 경우는 그 근방의 숲을 영차영차 밭으로 일궈 주었으려나
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "수박 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "수박")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "수박밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "하쿠레이 신사")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 300)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 4)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/스파이스/일식/조림/안주/매운/푸짐/농후/호화/야취/호쾌/쫄깃한/텐구류/지옥풍/짠/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "단/담백/" + "수수께끼 요리/소인족 비전의 맛/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "진한 맛/자극적/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "금빛/혼돈/근사함/화려/파워풀/" + "사슬의/식기/술/" * 3 + "수박의/오니의/귀력의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "질서/섬세/" + "도서/기계/" * 3 + "캇파의/텐구의/" * 5)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰10(ARG, O_DATA, V_NAME)
ENDSELECT

;IMAGE_I10_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I10_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
			IF GROUPMATCH(CFLAG:CHARA:대체적용, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _変T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 90
					RESULTS = _茨歌仙
				ENDIF
			ELSE

			ENDIF
		ELSE
			;立ち絵
			IF GROUPMATCH(CFLAG:CHARA:대체적용, 1)
				IF CURRENT_CLOTHES_SET(CHARA) == 55
					RESULTS = _バニー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 62
					RESULTS = _変T
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
					RESULTS = _セーラー
				ELSEIF CURRENT_CLOTHES_SET(CHARA) == 90
					RESULTS = _茨歌仙
				ENDIF
			ELSE

			ENDIF
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ENDIF

