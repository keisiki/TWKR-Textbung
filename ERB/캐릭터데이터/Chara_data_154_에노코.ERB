@CHARAMOVE_DATA_154(ARG, ARG:2)
LOCAL = 154
SELECTCASE ARG
	;連動設定(RETURN=依存するキャラ, RESULT:1～3=対等に連動するキャラ）
	CASE 1
	;特殊生活時間設定(CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;魔理沙の家の寝室と風呂には入らない
			CASE P512마리사댁・침실, P514마리사댁・욕실
				RETURN -100
			;アリス家,エレン家,妖怪の棲み処
			CASE P521인형저택・앨리스방 TO P531인형저택・빈방
				RETURN -100
			CASE P535엘렌댁・침실 TO P537엘렌댁・욕실, P541토키코의거처, P542리글의거처, P543루미아의거처, P544나루미의오두막
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 중유의길, 삼도천, 피안, 지옥, 축생계
				RETURN 1
			CASE 숲의입구, 향림당, 고목의큰나무, 키리사메마법점, 재고의길, 무연총
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 깊은숲속
				RETURN 1
		ENDSELECT
	;引力設定(CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터154
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터154(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "에노코")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(네글리제＋캡) 세트")
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "오프숄더"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：액세서리"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("액세서리", "목걸이"))
CASE "평상복：완부의복"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("완부의복", "곰덫"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "함정의 마크가 달린")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "강아지 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "강아지 브라")
CASE "고유 속옷 착용 조건"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/고기 요리/고기 요리/일식/조림/안주/짠/짠/시큼한/시큼한/가벼운/농후/검소/야취/가정적/뭉근뭉근/축생류/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "단/단/푸짐/정키/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "축생류/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "잿빛/질서/귀여운/멋쟁이/시크/지적/" + "불/늑대의/" * 3 + "개의/정적의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/화려/위엄/" + "사생/" * 3 + "수달의/독수리의/" * 10)
ENDSELECT

