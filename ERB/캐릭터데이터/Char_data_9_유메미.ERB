@CHARAMOVE_DATA_9(ARG, ARG:2)
LOCAL = 9
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;대체로 유적에 있다
			CASE 36 TO 50
				RETURN 5
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,6, 21,22,23,27
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;引力設定
	CASE 8
		TCVAR:LOCAL:인력점 = 0
		SIF YOUBI_MATCH(DAY, "일") && INRANGE(TIME, 600, 960)
			TCVAR:LOCAL:인력점 = 2
ENDSELECT

