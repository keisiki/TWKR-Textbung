@CHARAMOVE_DATA_23(ARG, ARG:2)
LOCAL = 23
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;으흥 예의 바르기 때문에 사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 51
				RETURN -100
			;정원, 부엌에 자주 있다
			CASE 621, 626
				RETURN 5
			;유유코님의 방
			CASE 623
				RETURN -5
			;타인의 집, 타인의 방 등에는 들어가지 않는다
			CASE 608, 609, 615, 616, 617, 633, 635, 636, 638, 643 TO 652
				RETURN -100
			;시비곡직청, 축생계는 가기 어렵다
			CASE 610, 613, 640, 641
				RETURN -10
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 0, 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 1,2,4, 21,22,23,27, 61,62,64,65,66
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터23
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터23(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "요우무")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "캬츄샤(헤어밴드)"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "치마"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：외투"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("외투", "베스트"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "활동성 중시의")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "이너 스패츠")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "스포츠브라")
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "알/쌀/조림/시큼한/담백/봄철/검소/품위있는/가정적/쫄깃한/요력/명계풍/일식/일식/단/단/단/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "빵/양식/영장/매운/매운/개구리/개구리/개구리/벌레고기/벌레고기/벌레고기/" + "그로테스크/수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "섬세한맛/명계풍/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "초록빛/질서/귀여운/일본풍/시크/" + "유령/사생/날붙이/" * 3 + "명계의/유명(幽明)의/유령의/영혼의/삶과죽음의/비수/호신용 단검/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/무서움/기묘/서양풍/지적/위엄/" + "탄환/생체품/담뱃대/기계/" * 3)

;여기는 의뢰 ID가 관련하지 않는 정보
;의뢰 발생율 스카2/일반3/고유 5
CASE "의뢰 없음 발생율"
	CALLF MAKE_INT(V_NAME, 2)
CASE "일반의뢰발생율"
	CALLF MAKE_INT(V_NAME, 3)
CASE "고유의뢰발생율"
	CALLF MAKE_INT(V_NAME, 5)
CASE "고유의뢰최종 요소"
	CALLF MAKE_INT(V_NAME, 101)
CASEELSE
	;고유의뢰관련의 데이터에 바톤 터치
	SIF STRCOUNT(O_DATA, "의뢰")
		CALLF 고유의뢰23(ARG, O_DATA, V_NAME)
ENDSELECT

