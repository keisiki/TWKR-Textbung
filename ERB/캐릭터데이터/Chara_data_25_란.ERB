@CHARAMOVE_DATA_25(ARG, ARG:2)
LOCAL = 25
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;紫様の私室はあまり入らない
			CASE P633유카리의방
				RETURN -5
			;是非曲直庁、축생계は行かない
			CASE P610시비곡직청홀 TO P618시비곡직청빈방, P640이계의문 TO P655유마댁목욕탕
				RETURN -100
			;他人の家には入らない
			CASE P608우루미집, P609에이카집, P606히사미집, P607잔무집
				RETURN -100
			;私室等には入らない
			CASE P623유유코의방, P624요우무의방, P627백옥루목욕탕
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루
				RETURN 1
			CASE 술집소로
				RETURN 1
			CASE 중유의길, 삼도천, 마요이가, 백옥루정원, 백옥루
				RETURN 1
			CASE 산록의수해, 현무의늪, 큰두꺼비의연못, 깊은숲속
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터25
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터25(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "란")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "니삭스"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "도사옷"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "큰 꼬리구멍이 뚫린")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "O 백 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "풀컵 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[란]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "고기 요리/알/일식/중화/튀겨짐/푸짐/품위있는/가정적/기능적/쫄깃한/매운/매운/시큼한/시큼한/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "짠/짠/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "요력/요력/특기요리＊일식＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "푸른빛/질서/아름다운/일본풍/시크/지적/" + "요술/도서/" * 3 + "여우의/식신의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "팝/로리타/매니악/" + "물/손목호구/" * 3 + "너구리의/" * 10)

;농민 관련
;유부
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "대두 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "대두")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "대두밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "마을")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 2)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 205)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 600)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 5)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_25
DISH_NAME = 유부초밥
RESULTS = 경식
CALL 재료설정("범용")
DISH_TASTE = 일식/튀겨짐/쌀/시큼한/가벼운/담백/중립온도/검소/가정적/
DISH_COMMENT = 가마니를 본떠서 밥을 유부로 싼 초밥。유부초밥。
; 한판 주 : 요리 이름은 お稲荷さん, 설명의 유부초밥은 いなり寿司
RETURN 1

;IMAGE_I25_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I25_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용 == 1 && FLAG:스탠딩종류
	IF IS_BUNNY(CHARA)
		IF TALENT:CHARA:가슴사이즈 > 1
			RESULTS = _爆バニー
		ELSE
			RESULTS = _バニー
		ENDIF
	ENDIF
ENDIF

;IMAGE_I25_体形(CHARA)-----------------------------------------------------------------------
;キャラクター特有の体形表示条件
;エフェクトを共有する体形差分はこちら
;服装を指定しても自動で表示されるので注意
;使用時注意点:
;   RESULTSに体形を格納すること
;   書式は %体形%%服装% 適宜"_"等入れること
;   "巨乳"は本編で実装済み %体形%巨乳%服装% で追記可能
;-------------------------------------------------------------------------------------------
@IMAGE_I25_体形(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용 == 1 && FLAG:스탠딩종류 && CHECK_NUDE(CHARA) && TALENT:CHARA:가슴사이즈 > 1
	RESULTS = 爆乳
ENDIF

;IMAGE_I25_特殊エフェクト(CHARA)--------------------------------------------------------------------
;キャラクター特有のエフェクト表示条件
;使用時注意点:
;	RESULTS配列に特殊エフェクトを格納
;	RETURNで追加するエフェクト数を返却すること
;-------------------------------------------------------------------------------------------
@IMAGE_I25_特殊エフェクト(CHARA)
#DIM CHARA
#DIM DYNAMIC エフェクト数 = 0
VARSET RESULTS

IF CFLAG:CHARA:대체적용 == 1 && FLAG:스탠딩종류
	IF !CHECK_NUDE(CHARA) && (!BEDROOM() || !BATHROOM(CFLAG:MASTER:현재위치))
		RESULTS:エフェクト数 = 帽子
		エフェクト数 += 1
	ENDIF
	IF TCVAR:CHARA:발정 && CFLAG:CHARA:욕구불만도 > 25 && !MARK:CHARA:반발각인 && TALENT:CHARA:기분 >= 0
		RESULTS:エフェクト数 = 湯気
		エフェクト数 += 1
	ENDIF
	;顔絵
	IF TALENT:CHARA:모유체질 && NOWEX:CHARA:분유 && CHECK_NUDE(CHARA)
		IF FLAG:거유차이표시 && TALENT:CHARA:가슴사이즈>=2
			RESULTS:エフェクト数 = 爆乳母乳
		ELSE
			RESULTS:エフェクト数 = 母乳
		ENDIF
		エフェクト数 += 1
	ENDIF
	IF CHECK_NUDE(CHARA) && !BEDROOM() && !BATHROOM(CFLAG:MASTER:현재위치)
		RESULTS:エフェクト数 = 前掛け
		エフェクト数 += 1
	ENDIF
	IF NOWEX:CHARA:TotalEX >= 5
		IF !RAND:2
			CSTR:CHARA:범용이펙트 = ～～
		ELSE
			CSTR:CHARA:범용이펙트 = んお゛お゛
		ENDIF
	ENDIF
ENDIF

RETURN エフェクト数

;IMAGE_I25_特殊表情(CHARA)--------------------------------------------------------------------
;キャラクター特有の表情表示条件
;時間停止中でも維持する表情はこちら
;使用時注意点:
;   RESULTS:0に表情を格納
;   RESULTS:1にマウスオーバー時の表情を格納すること
;   "通常","笑顔","분노","발정","수면"は本編で実装済み
;-------------------------------------------------------------------------------------------
@IMAGE_I25_特殊表情(CHARA)
#DIM CHARA
IF CFLAG:CHARA:대체적용 == 1 && FLAG:스탠딩종류
	IF TCVAR:CHARA:발정 && CFLAG:CHARA:욕구불만도 > 25 && !MARK:CHARA:반발각인 && TALENT:CHARA:기분 >= 0
		IF !CFLAG:CHARA:우후후
			RESULTS:0 = 発情Ａ
			RESULTS:1 = 発情Ｂ
		ELSEIF !NOWEX:CHARA:TotalEX && !FLAG:70
			RESULTS:0 = 発情Ｃ
			RESULTS:1 = 発情Ｃ
		ENDIF
	ENDIF
ENDIF
