@CHARAMOVE_DATA_78(ARG,ARG:2)
LOCAL = 78
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		;연동 처리(시즈하과 함께)
		IF TALENT:78:연모 || (TALENT:78:사모 && !TALENT:77:사모)
			CFLAG:LOCAL:현재위치 = CFLAG:78:만난위치 ?  CFLAG:78:만난위치 # SUKIMA()
		ELSE
			CFLAG:LOCAL:현재위치 = CFLAG:77:만난위치 ?  CFLAG:77:만난위치 # SUKIMA()
		ENDIF
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;사람이 있는 집의 목욕탕이라든지 화장실에는 가지 않는다
			CASE 12, 13, 14, 18, 30, 35, 39, 40, 43, 44, 51, 52
				RETURN -100
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				SIF FLAG:15 != LOCAL
					RETURN -100
			;개인 방
			CASE 717
				;기상 후와 취침전 이외는 피한다
				IF BETWEENTIME(1200,420)
					RETURN 10
				ELSE
					RETURN -10
				ENDIF
			;전
			CASE 715
				;밭에 자주(잘) 있다
				IF BETWEENTIME(540,1400)
					RETURN 10
				ELSE
					RETURN -5
				ENDIF
			;아지트, 액가, 치유리 집, 카센가, 은둔지, 오두막
			CASE 704, 706, 709, 720 TO 730, 733, 734
				RETURN -100
			CASEELSE
				;가을은 활발하게 된다
				IF DAY:2 == 3
					CFLAG:LOCAL:이동율보정 = 5
				ELSE
					CFLAG:LOCAL:이동율보정 = -5
				ENDIF
				RETURN 0
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 7, 8
				RETURN 1
			CASE 2
				SIF DAY:2 == 3
					RETURN 1
				RETURN 0
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 71,72,73, 81,82,83,84
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

@EXIST_캐릭터데이터78
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터78(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "미노리코")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(바지) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "모자"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "에이프런 드레스"))

CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "희미하게 단 향기가 나는")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "군고구마색 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "군고구마색 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(1 + (2 * 승부속옷판정([[미노리코]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "야채/고구마/쌀/버섯/조림/푸짐/농후/호화/야취/가정적/쫄깃한/단/단/짠/짠/" + "가을철/" * 5)
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "담백/검소/시큼한/시큼한/" + "개구리/벌레고기/겨울철/" * 3 + "수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "풍요/특기요리＊가을철＊/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "주홍빛/귀여운/멋쟁이/화려/" + "흙/식물/열매/식기/" * 3 + "포도의/단풍의/가을의/풍요의/씨앗/가을바람의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "얼음/날붙이/탄환/손목호구/기계/" * 3 + "여름의/벛꽃의/해바라기의/수박의/" * 5  + "봄의/겨울의/봄바람의/" * 10)

;농민 관련
;米でもいいが텐구と被るのだよ
CASE "농민：설명"
	CALLF MAKE_STR(V_NAME, "감자 농가")
CASE "농민：작물"
	CALLF MAKE_STR(V_NAME, "감자")
CASE "농민：밭의 종류"
	CALLF MAKE_STR(V_NAME, "감자밭")
CASE "농민：밭의 맵"
	CALLF MAKE_STR(V_NAME, "요괴의 산·산록")

CASE "농민：맵 ID"
	CALLF MAKE_INT(V_NAME, 7)
CASE "농민：장소 ID"
	CALLF MAKE_INT(V_NAME, 715)
CASE "농민：보수"
	CALLF MAKE_INT(V_NAME, 400)
CASE "농민：금리"
	CALLF MAKE_INT(V_NAME, 4)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_78
DISH_NAME = 스위트 포테이토
RESULTS = 디저트
CALL 재료설정("고구마")
DISH_TASTE = 양식/야채/단/농후/따뜻한/가을철/야취/가정적/
DISH_COMMENT = 고구마를 사용한 일본식 양과자。해외에서는 유통되지 않는다。
RETURN 1

;IMAGE_I78_特殊服装(CHARA)-------------------------------------------------------------------
