@CHARAMOVE_DATA_141(ARG, ARG:2)
LOCAL = 141
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;未設定
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 벚꽃길토리이, 신사경내, 신사본당, 신사툇마루
				RETURN 1
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 정문, 객실, 식당
				RETURN 1
			CASE 구천의폭포, 절경의언덕, 산의호수, 모리야신사경내, 홍룡동
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터141
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터141(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "치마타")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(원피스) 세트")

CASE "평상복：모자"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("모자", "캬츄샤(헤어밴드)"))
CASE "평상복：외투"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("외투", "망토"))
CASE "평상복：원피스"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("원피스", "원피스"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "롱 부츠"))
	
CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "스페셜")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "레인보우 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "레인보우 브래지어")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[치마타]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 1)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "스파이스/스파이스/스파이스/과일/과일/과일/조림/케이크/단/단/시큼한/시큼한/호화/예술적/선명한/말랑말랑/반짝임/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "매운/매운/검소/수수께끼 요리/수수께끼 요리/수수께끼 요리/")
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "색배합/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "무지갯빛/질서/아름다운/예술적/화려/불가사의/장대/매니악/" + "주술/일곱빛깔의/" * 3 + "무지개의/달무지개의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/시크/" + "어둠/" * 3)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_141
DISH_NAME = 레인보우 타르트
RESULTS = 디저트
CALL 재료설정("딸기", "수박", "선도")
DISH_TASTE = 양식/케이크/단/농후/호화/예술적/선명한/말랑말랑/색배합/반짝임/
DISH_COMMENT = 무지개빛으로 채색된 과일 타르트。진을 만들면 마법의 무지개가 뜬다。
RETURN 1
