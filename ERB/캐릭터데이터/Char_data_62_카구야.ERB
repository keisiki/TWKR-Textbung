@CHARAMOVE_DATA_62(ARG, ARG:2)
LOCAL = 62
SELECTCASE ARG
	;연동설정 (RETURN=의존하는 캐릭터, RESULT:1~3=대등하게 연동되는 캐릭터)
	CASE 1
	;특수생활시간 설정 (CFLAG:LOCAL:생활시간보정 을 설정)
	CASE 3
	;이동경향 (±~10=가기 쉽다/가기 어렵다, -100=가지 않는다)
	CASE 4
		SELECTCASE ARG:2
			;몽환관には行かない
			CASE P450몽환관현관 TO P466몽환관정원
				RETURN -100
			;他人の家には入らない
			CASE P432카게로의집, P437메디슨의집, P439라바의집, P443미스티아의집, P445모코우의집
				RETURN -100
			;来客用トイレは行かない
			CASE P406손님용화장실
				RETURN -100
			;診察室,調剤室,てゐ私室,鈴仙私室,裏庭,二階はあんまり行かない
			CASE P404진찰실, P405조제실, P410테위방, P411우동게방, P422뒷마당, P4262층복도, P427창고, P4282층빈방
				RETURN -5
			;縁側,輝夜私室,居間,庭によく居る
			CASE P409툇마루, P414카구야방, P416거실, P421마당
				RETURN 5

			;와타츠키정には行かない
			CASE P1020와타츠키정・대문 TO P1044응접실
				RETURN -100
			;他人の家には行かない
			CASE P1005사구메댁・거실, P1006사구메댁・침실, P1008링고댁, P1009세이란댁
				RETURN -100
			;달의도시には行かない
			CASE P1002달의도시
				RETURN -100
		ENDSELECT
	;출현장소
	CASE 7
		SELECTCASE ARG:2
			CASE 광장, 상가거리, 술집소로
				RETURN 1
			CASE 죽림입구, 미혹의샛길, 사각의죽림, 영원정, 영원정안채
				RETURN 1
		ENDSELECT
	;인력설정 (CFLAG:LOCAL:인력점을 설정한다)
	CASE 8
ENDSELECT

@EXIST_캐릭터데이터62
#LOCALSIZE 1
#LOCALSSIZE 1

@캐릭터데이터62(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "통칭"
	CALLF MAKE_STR(V_NAME, "카구야")
;여기서부터 의상 설정
CASE "오늘의 파자마"
	CALLF MAKE_STR(V_NAME, "파자마(주반) 세트")

CASE "평상복：신발"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("신발", "신발"))
CASE "평상복：양말"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
CASE "평상복：치마"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("치마", "롱 스커트"))
CASE "평상복：상반신윗도리2"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리2", "블라우스"))
CASE "평상복：상반신윗도리1"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("상반신윗도리1", "윗도리"))

;신이 짠 천바지나 달은 보통이라고 생각해
CASE "고유 속옷 형용사"
	CALLF MAKE_STR(V_NAME, "별빛을 섞어 짠")
CASE "고유 하반신속옷명"
	CALLF MAKE_STR(V_NAME, "시즈(직물) 팬티")
CASE "고유 상반신속옷"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 상반신속옷명"
	CALLF MAKE_STR(V_NAME, "시즈(직물) 브라")
CASE "고유 속옷 착용 조건"
	SIF !RAND:(3 - (2 * 승부속옷판정([[카구야]])))
		CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(앞)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 비쳐보이는(뒤)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "고유 속옷 옷감 두께"
	CALLF MAKE_INT(V_NAME, 2)

;요리 관련
CASE "요리：좋아하는 맛"
	CALLF MAKE_STR(V_NAME, "쌀/팥소/과일/일식/나베/시원한/담백/예술적/품위있는/선명한/쫄깃한/달도시풍/달구경/단/단/짠/짠/")
CASE "요리：싫어하는 맛"
	CALLF MAKE_STR(V_NAME, "야취/시큼한/시큼한/" + "검소/외계풍/" * 3 + "개구리/벌레고기/수수께끼 요리/" * 5)
CASE "요리：조수 효과"
	CALLF MAKE_STR(V_NAME, "달도시풍/색배합/품위있는/")
;선물 관련
CASE "감성：좋아함"
	CALLF MAKE_STR(V_NAME, "무지갯빛/고급/질서/아름다운/예술적/불가사의/위엄/" + "달/보석/유물/" * 3 + "봉래의/영원한/수유(須臾)의/" * 5)
CASE "감성：싫어함"
	CALLF MAKE_STR(V_NAME, "혼돈/심플/팝/" + "불/싸구려/사생/생체품/손목호구/" * 3 + "봉황의/" * 10)
ENDSELECT

;-------------------------------------------------
;고유요리설정
;DISH_NAME          요리명
;RESULTS            요리의 종류(경식/주식/디저트）
;CALL 재료설정("")  재료의 설정。"범용"을 넣으면 자동으로 범용 식재료가 된다
;DISH_TASTE         맛의 설정
;DISH_COMMENT       (임의）한줄 코멘트
;DISH_TIME          (임의）조리 소요 시간
;DISH_MAXLIMIT      (임의）요리의 지속 시간
;-------------------------------------------------
@SPECIAL_MENU_62
DISH_NAME = 봉래의 얼음구슬 가지
RESULTS = 디저트
CALL 재료설정("선도")
DISH_TASTE = 아이스크림/단/단/차가운/호화/예술적/선명한/달도시풍/색배합/
DISH_COMMENT = 가지 모양의 막대기에 꽂힌 알록달록하고 둥근 아이스。무척 달다。
RETURN 1

;IMAGE_I62_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I62_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:CHARA:대체적용
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:스탠딩종류
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:스탠딩종류
			;顔絵
			IF TCVAR:CHARA:미약 || CFLAG:CHARA:욕구불만도 > 850
				RESULTS = _欲情
			ENDIF
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;夏服を置き換え
			IF TCVAR:CHARA:미약 || CFLAG:CHARA:욕구불만도 > 850
				RESULTS = _夏服
			ENDIF
	ENDIF
ENDIF

