	SENSE = 조악/
	SELL_AREA = AREA_SETBIT("달이외")
CASE 2108
	NAME_ADJECT = 거꾸로된
	MAGNIF_PRICE = 50
	SENSE = 조악/기묘/
	SELL_AREA = AREA_SETBIT("달이외")
CASE 2109
	NAME_ADJECT = 녹슨
	MAGNIF_PRICE = 60
	SENSE = 광물/조악/레트로/섬세/
	SELL_AREA = AREA_SETBIT("달이외")
CASE 2110
	NAME_ADJECT = 망가진
	MAGNIF_PRICE = 10
	SENSE = 기술/조악/
	SELL_AREA = AREA_SETBIT("달이외")
CASE 2111
	NAME_ADJECT = 장난감
	MAGNIF_PRICE = 20
	SENSE = 기술/조악/귀여운/팝/로리타/로리타/로리타/로리타/로리타/
	SELL_AREA = AREA_SETBIT("달이외")

CASE 9999
	MAGNIF_PRICE = 300
	SENSE = 
	SELL_AREA = AREA_SETBIT("")
[SKIPSTART]
CASE 
	NAME_ADJECT = 
	MAGNIF_PRICE = 
	SENSE = 
	SELL_AREA = AREA_SETBIT("")
[SKIPEND]
CASEELSE
	VARSET RESULTS
	VARSET RESULT
	RETURN 0
ENDSELECT

VARSET RESULTS
VARSET RESULT
;분포
IF INRANGE(NOW_AREA, 0, 10) && GETBIT(SELL_AREA, NOW_AREA)
	RESULT:0 = 1
;分布無視
ELSEIF NOW_AREA == 99
	RESULT:0 = 1
ENDIF

;옷감 제품은 광물・기술이 적용되지 않는다
SIF STRFIND(SENSE_MAIN, "옷감") >= 0 && STRFIND(SENSE, "광물") + STRFIND(SENSE, "기술") >= -1
	RESULT:0 = 0
;철제 제품은 섬유가 적용되지 않는다
SIF STRFIND(SENSE_MAIN, "철제") >= 0 && STRFIND(SENSE, "섬유") >= 0
	RESULT:0 = 0
;초상 은 광물、섬유、기술가 적용되지않는다
SIF STRFIND(SENSE_MAIN, "초상") >= 0 && STRFIND(SENSE, "광물") + STRFIND(SENSE, "섬유") + STRFIND(SENSE, "기술") >= -2
	RESULT:0 = 0
;향수전용
SIF STRFIND(SENSE_MAIN, "향수") >= 0 && STRFIND(SENSE, "광물") + STRFIND(SENSE, "섬유") + STRFIND(SENSE, "기술") + STRFIND(SENSE, "모양") >= -3
	RESULT:0 = 0
;생체품은 생물이 적용되지않는다
SIF STRFIND(SENSE_MAIN, "생체품") >= 0 && STRFIND(SENSE, "식물") >= 0
	RESULT:0 = 0

SENSE += NAME_ADJECT + "/"

RESULT:1 = MAGNIF_PRICE
RESULT:2 = SELL_AREA
RESULTS:0 = %NAME_ADJECT% 
RESULTS:1 = %SENSE%
RETURN RESULT:0
