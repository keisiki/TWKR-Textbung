@DAIRY_CONDITION_EV19
SIF RAND:10 > 0
	RETURN -1
SIF FLAG:이상기상
	RETURN -1
;여름과 겨울만
SIF !GROUPMATCH(DAY:2, 2,4)
	RETURN -1
CALL DAIRY_EV19
RETURN 1

@DAIRY_EV19
FLAG:이상기상 = RAND:2 + 9

SELECTCASE DAY:2
	CASE 2
		FLAG:이상기상 = 9
		TIME:5 = 5
		PRINTFORML 바람이 엄청 분다……
	CASE 4
		FLAG:이상기상 = 10
		TIME:5 = 9
		PRINTFORML 오늘밤은 냉기가 심하다……
ENDSELECT

CALL COLORMESSAGE(@"내일은 %조사처리(GET_WEATHER(),"가")% 될 것 같다……", C_YELLOW, 2,1)

