@DO_T_SKILL(사용자, 스킬번호)
#DIM DYNAMIC 사용자
#DIM DYNAMIC 스킬번호
#DIM DYNAMIC 스킬타입
;스킬의 시전시 처리, 대사만 넣으면 된다
CALL CAN_T_SKILL(사용자, 스킬번호)
IF RESULT == 0
    ;시전불가
    PRINTL 시전할수 없습니다
    RETURN 0
ELSE
    ;시전성공
    CALL PRINT_SPELLNAME(스킬번호)
    ;이 아래의 대사 작성
    CALLFORMF T_SKILL{스킬번호}("타입")
    스킬타입 = RESULT
    RESULT = 0
    CALLFORM AFTER_T_SKILL{스킬번호}(사용자)
    IF RESULT == 0
        IF 사용자 == T_대상1
            CALL T_시스템처리(250,스킬번호,스킬타입)
        ELSEIF 사용자 == T_대상2
            CALL T_시스템처리(251,스킬번호,스킬타입)
        ENDIF
    ENDIF
    RETURN 1
ENDIF

