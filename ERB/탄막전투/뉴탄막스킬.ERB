        IF T_턴주인 == MASTER
            CALL T_시스템처리(122)
        ELSE
            CALL T_시스템처리(123)
        ENDIF
        RETURN 1
    ELSE
        IF T_턴주인 == MASTER
            CALL T_시스템처리(102)
        ELSE
            CALL T_시스템처리(103)
        ENDIF
        PRINTFORML 그러나 빗나가고 말았다...
        RETURN 0
    ENDIF
ENDIF

@평타명중판정(ARG)
#FUNCTION
#DIM DYNAMIC 보정

SELECTCASE TB_거리()
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 - (3 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 -= 5
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 - (2 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 += 0
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 - (1 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 += 5
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리
        보정 += 10
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 + (1 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 += 5
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 + (2 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 += 0
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 + (3 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 -= 10
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 + (4 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 -= 20
    CASE IS < TA_평타사거리:T_턴주인:TA_최적사거리 + (5 * TA_평타사거리:T_턴주인:TA_사거리범위)
        보정 -= 35
    CASE IS > TA_평타사거리:T_턴주인:TA_최대사거리
        보정 -= 95
ENDSELECT

RETURNF 명중판정(MAX(ARG + 보정, 0))


@스킬명중판정(명중률,스킬번호,추가인수 = 0,지점 = 0,대상 = 0,디코이 = 0)
#FUNCTION
#DIM DYNAMIC 명중률
#DIM DYNAMIC 스킬번호
#DIM DYNAMIC 추가인수
#DIM DYNAMIC 보정
#DIM DYNAMIC 최적사거리
#DIM DYNAMIC 최대사거리
#DIM DYNAMIC 사거리범위
#DIM DYNAMIC 속성
#DIM DYNAMIC 유도력
#DIM DYNAMIC 판정
#DIM DYNAMIC 상대속도
#DIM DYNAMIC 지점
#DIM DYNAMIC 거리
#DIM DYNAMIC 대상
#DIM DYNAMIC 대상반대
#DIM DYNAMIC 디코이

IF 대상 == 0
    대상 = T_시전자반대 ;즉, 맞는 쪽
ENDIF
대상반대 = 사용자반대(대상) ; 때리는 쪽

상대속도 = ARG능력치(대상반대,"속도") - ARG능력치(대상,"속도")
IF TB_SUM_REMAIN(대상,TCV_속박)
    상대속도 += ARG능력치(대상,"속도")/2
ENDIF
IF TB_SUM_REMAIN(대상반대,TCV_속박)
    상대속도 -= ARG능력치(대상반대,"속도")/2
ENDIF
IF TB_SUM_REMAIN(대상,TCV_봉쇄)
    상대속도 += ARG능력치(대상,"속도")/2
ENDIF
IF TB_SUM_REMAIN(대상반대,TCV_봉쇄)
    상대속도 -= ARG능력치(대상반대,"속도")/2
ENDIF

IF 지점 == 0
    거리 = TB_거리_은신(대상)
ELSE
    거리 = TB_거리(대상,지점)
    IF 디코이
        거리 = TB_거리_은신(대상,지점)
    ENDIF
ENDIF

IF 추가인수 == 0
    CALLFORMF T_SKILL{스킬번호}_RANGE("최적사거리")
    최적사거리 = RESULT
    CALLFORMF T_SKILL{스킬번호}_RANGE("최대사거리")
    최대사거리 = RESULT
    CALLFORMF T_SKILL{스킬번호}_RANGE("범위변동단위")
    사거리범위 = RESULT
    CALLFORMF T_SKILL{스킬번호}_RANGE("조준속성")
    속성 = RESULT
    CALLFORMF T_SKILL{스킬번호}_RANGE("유도력")
    유도력 = RESULT
ELSE
    CALLFORMF T_SKILL{스킬번호}_{추가인수}_RANGE("최적사거리")
    최적사거리 = RESULT
    CALLFORMF T_SKILL{스킬번호}_{추가인수}_RANGE("최대사거리")
    최대사거리 = RESULT
    CALLFORMF T_SKILL{스킬번호}_{추가인수}_RANGE("범위변동단위")
    사거리범위 = RESULT
    CALLFORMF T_SKILL{스킬번호}_{추가인수}_RANGE("조준속성")
    속성 = RESULT
    CALLFORMF T_SKILL{스킬번호}_{추가인수}_RANGE("유도력")
    유도력 = RESULT
ENDIF
SELECTCASE 속성
    CASE T_근거리
        보정 += LIMIT(상대속도 / 20, -8 , 8)
    CASE T_고정탄
        SELECTCASE 거리
            CASE IS < 최적사거리 - (5 * 사거리범위)
                보정 -= 25
            CASE IS < 최적사거리 - (4 * 사거리범위)
                보정 -= 10
            CASE IS < 최적사거리 - (3 * 사거리범위)
                보정 -= 5
            CASE IS < 최적사거리 - (2 * 사거리범위)
                보정 += 0
            CASE IS < 최적사거리 - (1 * 사거리범위)
                보정 += 5
            CASE IS < 최적사거리
                보정 += 10
            CASE IS < 최적사거리 + (1 * 사거리범위)
                보정 += 5
            CASE IS < 최적사거리 + (2 * 사거리범위)
                보정 += 0
            CASE IS < 최적사거리 + (3 * 사거리범위)
                보정 -= 5
            CASE IS < 최적사거리 + (4 * 사거리범위)
                보정 -= 10
            CASE IS < 최적사거리 + (5 * 사거리범위)
                보정 -= 25
            CASE IS > 최대사거리 ;불가능하지만 뭐
                보정 -= 80
        ENDSELECT
        보정 += LIMIT(상대속도 / 20, -8 , 8)
    CASE T_조준탄
        SELECTCASE 거리
            CASE IS < 최적사거리 - (5 * 사거리범위)
                보정 -= 25
            CASE IS < 최적사거리 - (4 * 사거리범위)
                보정 -= 10
            CASE IS < 최적사거리 - (3 * 사거리범위)
                보정 -= 5
            CASE IS < 최적사거리 - (2 * 사거리범위)
                보정 += 0
            CASE IS < 최적사거리 - (1 * 사거리범위)
                보정 += 5
            CASE IS < 최적사거리
                보정 += 10
            CASE IS < 최적사거리 + (1 * 사거리범위)
                보정 += 5
            CASE IS < 최적사거리 + (2 * 사거리범위)
                보정 += 0
            CASE IS < 최적사거리 + (3 * 사거리범위)
                보정 -= 5
            CASE IS < 최적사거리 + (4 * 사거리범위)
                보정 -= 10
            CASE IS < 최적사거리 + (5 * 사거리범위)
                보정 -= 25
            CASE IS > 최대사거리 ;불가능하지만 뭐
                보정 -= 80
        ENDSELECT
        보정 += LIMIT(상대속도 / 10 , -15 , 15)
    CASE T_유도탄
        SELECTCASE 거리
            CASE IS < 최적사거리 - (5 * 사거리범위)
                보정 -= 20
            CASE IS < 최적사거리 - (4 * 사거리범위)
                보정 -= 10
            CASE IS < 최적사거리 - (3 * 사거리범위)
                보정 -= 5
            CASE IS < 최적사거리 - (2 * 사거리범위)
                보정 += 0
            CASE IS < 최적사거리 - (1 * 사거리범위)
                보정 += 5
            CASE IS < 최적사거리
                보정 += 10
            CASE IS < 최적사거리 + (1 * 사거리범위)
                보정 += 5
            CASE IS < 최적사거리 + (2 * 사거리범위)
                보정 += 0
            CASE IS < 최적사거리 + (3 * 사거리범위)
                보정 -= 5 - (유도력/5) ;유도력 25부터 보너스
            CASE IS < 최적사거리 + (4 * 사거리범위)
