@HAVE_T_SKILL(사용자, 스킬번호)
#FUNCTION
#DIM 사용자
#DIM 스킬번호
LOCAL = 스킬번호 + 500 ;TALENT.csv 내 스킬번호

SIF TALENTNAME:LOCAL == ""
    RETURNF 0

SIF TALENT:사용자:LOCAL == 1
    RETURNF 1
SIF 탄막디버그 == 1
    RETURNF 1

RETURNF 0

;지금 스킬을 시전할수 있는지
;실행가부 대응
@CAN_T_SKILL(사용자, 스킬번호)
#DIM 사용자
#DIM 스킬번호
;식중함수는 CALLFORM 사용 불가 이기 때문에 수정

IF HAVE_T_SKILL(사용자, 스킬번호)
    ;이 스킬 소유중
    IF 사용자 == MASTER
        IF !IS_MEMORIZED(스킬번호)
            RETURN 0
        ENDIF
    ENDIF
    TRYCALLFORM ONGOING_T_SKILL{스킬번호}(사용자, "남은쿨타임")
    IF RESULT == 0
        ;쿨타임 0 시전가능
        RESULT = 0
        TRYCALLFORM DENY_SILENCE{스킬번호}
        IF TB_MAX_VALUE(사용자,TCV_침묵) > RESULT - 1 && TB_MAX_VALUE(사용자,TCV_침묵)
            RETURN 0
        ENDIF
        CALLFORMF T_SKILL{스킬번호}("HP코스트")
        IF RESULT >= TCVAR:사용자:T_체력
            RETURN 0
        ENDIF
        RESULT = 0
        IF 사용자 == MASTER
            CALLFORMF T_SKILL{스킬번호}("마력코스트")
            IF RESULT > BASE:사용자:마력
                RETURN 0
            ENDIF
        ENDIF
        RESULT = 0

        CALLFORMF T_SKILL{스킬번호}_RANGE("조준속성")
        IF RESULT != T_버프기
            RESULT = 0
            CALLFORMF T_SKILL{스킬번호}_RANGE("최대사거리")
            IF RESULT < TB_거리_은신(사용자반대(사용자))
                RETURN 0
            ENDIF
        ENDIF
        RESULT = 0
        TRYCALLFORM SKILLTYPE_MOVE{스킬번호}
        IF RESULT
            IF !T_MOVEABLE(사용자) && RESULT == 2
                RETURN 0
            ENDIF
            IF !T_TPABLE(사용자) && RESULT == 3
                RETURN 0
            ENDIF
        ENDIF
        RESULT = 0
        CALLFORMF T_SKILL{스킬번호}("행동코스트")
        IF RESULT + 1 > T_행동기회:사용자:T_통상행동
            RETURN 0
        ENDIF
        RESULT = 0
        CALLFORMF T_SKILL{스킬번호}("이동코스트")
        IF RESULT > T_행동기회:사용자:T_이동행동
            RETURN 0
        ENDIF
        RESULT = 0
        RETURN 1
    ELSE
        ;아직 쿨타임이라 시전불가능
        RETURN 0
    ENDIF
ELSE
    ;이스킬 없음, 시전불가
    RETURN 0
ENDIF

