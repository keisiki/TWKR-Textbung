            ;공격형 = bit 1, 방어형 = bit 2, 회복형 = bit 3, 상태이상형 = bit 4, 버프형 = bit 5
            IF 공격점수 >= 회피점수
                CALL HAVE_T_TYPE(ARG, 공격형)
                IF RESULT == 1
                    스킬타입:1 = 1
                ELSE
                    CALL HAVE_T_TYPE(ARG, 상태이상형)
                    IF RESULT == 1
                        스킬타입:1 = 4
                    ELSE
                        GOTO 호전적패턴루프
                    ENDIF
                ENDIF
            ELSEIF 회피점수 > 공격점수
                CALL HAVE_T_TYPE(ARG, 방어형)
                IF RESULT == 1
                    스킬타입:1 = 2
                ELSE
                    CALL HAVE_T_TYPE(ARG, 회복형)
                    IF RESULT == 1
                        스킬타입:1 = 3
                    ELSE
                        CALL HAVE_T_TYPE(ARG, 버프형)
                        IF RESULT == 1
                            스킬타입:1 = 5
                        ELSE
                            GOTO 호전적패턴루프
                        ENDIF
                    ENDIF
                ENDIF
            ENDIF
            IF 해주시도 == 1
                스킬타입:1 = 6
            ENDIF
            SELECTCASE 스킬타입:1
                CASE 0
                    ;모두
                    스킬타입 = 공격형 + 방어형 + 회복형 + 상태이상형 + 버프형
                CASE 1, 2, 3, 4, 5, 6
                    SETBIT 스킬타입, 스킬타입:1
            ENDSELECT
            CALL RANDOM_PICK_T_SKILL(ARG, 스킬타입)
