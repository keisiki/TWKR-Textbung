    IF TCVAR:ARG:T_독 > 5&&RAND:10 == 0
        ;독은 거의 해제 안함
        CALL HAVE_T_TYPE(ARG, 해주형)
        IF RESULT == 1
            행동 = 2
            해주시도 = 1
        ENDIF
    ENDIF
    IF TCVAR:ARG:T_실명
        공격점수 -= 30
        회피점수 += 50
    ENDIF
    IF TCVAR:(사용자반대(ARG)):T_취약
        공격점수 += 20
        회피점수 -= 10
    ENDIF
    IF TCVAR:(사용자반대(ARG)):T_기절
