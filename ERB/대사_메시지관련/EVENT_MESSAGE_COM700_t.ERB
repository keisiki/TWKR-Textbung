	PRINT 
ELSE
	FOR LOCAL, 17,6,-1
		IF GROUPMATCH(LOCAL, 13, 12, 11)
			CONTINUE
		ENDIF
		IF EQUIP:PLAYER:LOCAL 
			PRINTFORM %CLOTHNAME(LOCAL, EQUIP:PLAYER:LOCAL)% 위로부터
			BREAK
		ENDIF
	NEXT
ENDIF
