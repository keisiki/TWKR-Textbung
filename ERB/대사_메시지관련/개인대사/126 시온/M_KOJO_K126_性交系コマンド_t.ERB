@M_KOJO_MESSAGE_COM_K126_70
CALL M_KOJO_MESSAGE_COM_K126_70_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K126_70_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;애널 처녀
		IF !EXP:A성교경험
			IF TALENT:연인
				PRINTFORMW 「…엉덩이는…」
				PRINTFORMW 「나, 스스로도 해본일 없지만…」
			ENDIF
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입 계속
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		IF TALENT:연인
			IF RAND:5 == 0
				PRINTFORMW 「, …」
				PRINTFORMW 「은…격렬해 …%UNICODE(0x2764)%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「 , 미안 , 좀 더, 상냥하고…」
				PRINTFORMW 「윽 흐응 %UNICODE(0x2764)% 격렬하게 하지 않고 %UNICODE(0x2764)%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「사람의 엉덩이를 이런 난폭하게…」
				PRINTFORMW 「%CALLNAME:MASTER%의…변태…%UNICODE(0x2764)%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「안 돼 …%UNICODE(0x2764)% 엉덩이 너무 대단해 …%UNICODE(0x2764)%」
				PRINTFORMW 「이상하고, 되어버린다%UNICODE(0x2764)%」
			ELSE
				PRINTFORMW 「아앙 %UNICODE(0x2764)% 아앙 %UNICODE(0x2764)% 아앙 %UNICODE(0x2764)%」
				PRINTFORMW 「%CALLNAME:MASTER%%UNICODE(0x2764)%  %CALLNAME:MASTER%%UNICODE(0x2764)%」
			ENDIF
		ENDIF
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		RETURN 0
	;연인
	ELSEIF TALENT:연인
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

