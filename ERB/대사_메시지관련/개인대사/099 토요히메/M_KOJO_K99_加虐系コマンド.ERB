;==================================================
;104,눈가리개
;==================================================
@M_KOJO_MESSAGE_COM_K99_104
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K99_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_104_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMDL 
		RETURN 1
	;通常
	ELSE
		;装着
		IF TEQUIP:눈가리개
			;연모
			IF TALENT:연모
				;マゾっ気で分岐
				SELECTCASE ABL:마조끼
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「아…… 기다려、말하지 말고……HPH」","L"
								CALL HPH_PRINT,@"「무엇을 당할지 모르는 편이 두근두근 하기 때문에……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 자발적으로 아이마스크를 장착한 토요히메는 흥분한 모습으로 이 다음 행해질 행위를 기다리고 있다……
							CASE 1
								CALL HPH_PRINT,@"「이거…… 아무것도 안 보여서……HPH」","L"
								CALL HPH_PRINT,@"「굉장히…… 흥분해버려……HPH」","L"
								IF !CFLAG:99:1998
									PRINTFORMDL 토요히메는 무엇을 당할지 모르는 공포에 숨이 거칠어지고 있었다
									PRINTFORMDL %마스터가% 귓가에 일부러 큰 소리를 내어 위협해 주자
									PRINTFORMDW 토요히메는 움찔움찔 몸이 튀어오르더니、가랑이에서 흥분의 꿀이 방울져 떨어져 내리고 있었다……
								ENDIF
							CASE 2
								SIF !CFLAG:99:1998
									PRINTFORMDL %마스터는% 아이마스크를 장착한 토요히메를 껴안으며、그녀의 목덜미를 끈적하게 핥아올렸다
								CALL HPH_PRINT,@"「히아앙HPH 응……후우……앗HPH」","L"
								CALL HPH_PRINT,@"「안 보여서…… 감각이 날카로워져……HPH」","L"
								CALL HPH_PRINT,@"「하아아……HPH 이상해져 버릴 것 같아……HPH」","W"
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「나는… 당신의 얼굴을 보면서 하는 쪽을 좋아하는데……」
								SIF !CFLAG:99:1998
									PRINTFORMDW 아이마스크를 장착한 토요히메는 약간 불만인 것처럼 투덜거렸다
							CASE 1
								PRINTFORML 「……저기、역시 벗어도 좋아？」
								PRINTFORML 「당신의 얼굴이 안 보이는 것은…… 조금 무서워요」
								SIF !CFLAG:99:1998
									PRINTFORMDW 토요히메는 무서워 하고 있다。그 모습이 %CALLNAME:MASTER%의 가학심을 부추겼다
							CASE 2
								PRINTFORML 「이것으로 좋네요。%CALLNAME:MASTER%」
								PRINTFORML 「……%CALLNAME:MASTER%？」
								PRINTFORML 「저기、어째서 입다물고 있는거야？」
								SIF !CFLAG:99:1998
									PRINTFORMDW 잠시 입을 다물고 상태를 보고 있는 것 뿐인데、토요히메느 실로 분위기를 돋우는 반응을 해주었다
						ENDSELECT
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				;マゾっ気で分岐
				SELECTCASE ABL:마조끼
					;マゾっ気２以上
					CASE IS > 1
						SELECTCASE RAND:3
							CASE 0
								CALL HPH_PRINT,@"「아이마스크를 벗고 나서、만약 당신이 없다면……HPH」","L"
								CALL HPH_PRINT,@"「……아앗HPH 그거 굉장히 흥분되는데……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 토요히메는 멋대로 망상을 부풀려 가랑이를 적시고 있다。정말로 방치해 둘까……
							CASE 1
								CALL HPH_PRINT,@"「그런데、다음은 무엇을 당하게 되려나……HPH」","L"
								CALL HPH_PRINT,@"「우후훗HPH 두근두근 해요……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW 토요히메는 다음에 행해질 행위에 기대를 부풀리고 있다……
							CASE 2
								CALL HPH_PRINT,@"「응…… 조금만 기다려줘……HPH」","L"
								SIF !CFLAG:99:1998
									PRINTFORMDW %마스터가% 아이마스크를 부착하라고 지시하자、토요히메는 솔직하게 따라주었다
						ENDSELECT
					;マゾっ気２未満
					CASEELSE
						SELECTCASE RAND:3
							CASE 0
								PRINTFORML 「이것을 쓰면 좋은거야？」
								PRINTFORML 「……응、깜깜해서 아무것도 안 보여요」
								PRINTFORMW 「저기、아직 거기에 있어？」
							CASE 1
								PRINTFORML 「후훗、깜깜하구나。어릴 적에 요리히메와 탐험하던 날의 일이 생각나네요」
								PRINTFORML 「깜깜한 방에 갇혀…… 그 아이、엉엉 울고 있었던가」
								SIF !CFLAG:99:1998
									PRINTFORMDW 추억 이야기도 적당히、%마스터는% 행위를 재개했다
							CASE 2
								PRINTFORML 「이것을 쓰면、졸려져 버릴 것 같은데……」
								PRINTFORML 「후우…… 만약 잠들면 깨워줘요……」
								SIF !CFLAG:99:1998
									PRINTFORMDW 토요히메는 이미 졸린 것 같다
						ENDSELECT
				ENDSELECT
				RETURN 1
			ENDIF
		;解除
		ELSE
			;マゾっ気で分岐
			SELECTCASE ABL:마조끼
				;マゾっ気２以上
				CASE IS > 1
					CALL HPH_PRINT,@"「얼굴을 보면서 하는 것도 좋지만……HPH」","L"
					CALL HPH_PRINT,@"「이따금은、이런 것도 나쁘지 않네요……HPH」","W"
				;マゾっ気２未満
				CASEELSE
					PRINTFORML 「응、눈부시다……」
					SIF !CFLAG:99:1998
						PRINTFORMDW 토요히메는 눈을 깜빡깜빡 하고 있다
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

