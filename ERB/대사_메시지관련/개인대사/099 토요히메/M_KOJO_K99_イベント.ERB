;==================================================
;押し倒した
;==================================================
@M_KOJO_EVENT_K99_10(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;우후후==2（開始時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;MASTERを押し倒した（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORML 
			;연모
			IF TALENT:연모
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「후후훗……오늘은 저에게 맡겨줘요……」
						PRINTFORML 「당신을、기분 좋게 해줄게요……♪」
						PRINTFORMDL 밀려서 넘어지자마자、순식간에 옷이 벗겨지고……
						PRINTFORMDW 깨닫고 보니、%마스터는% 태어난 상태 그대로의 모습이었다
					CASE 1
						PRINTFORML 「저……%CALLNAME:MASTER%……」
						PRINTFORML 「오늘은、당신에게 어리광을 부려도 좋을까……？」
						PRINTFORMDL 토요히메의 풍만한 가슴이 %CALLNAME:MASTER%의 가슴팍을 아찔하게 눌러온다
						PRINTFORMDW 여기까지 와서 거절할 용기를、%마스터는% 갖고 있지 않았다……
					CASE 2
						PRINTFORML 「%CALLNAME:MASTER%……사랑해요……」
						PRINTFORML 「그러니…… 섹스합시다……♪」
						PRINTFORMDW 토요히메는 %CALLNAME:MASTER%에게 올라타、익숙한 솜씨로 옷을 벗기기 시작했다……
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「미안합니다…… 좀 흥분해서……」
						PRINTFORML 「그러니…… 당신의 몸、사용하고 싶어졌어요……♪」
						PRINTFORMDW 토요히메는 %CALLNAME:MASTER%에게 올라타、묻지도 따지지도 않고 옷을 벗기기 시작했다……
					CASE 1
						PRINTFORML 「쉬ー잇…… 괜찮아요……」
						PRINTFORML 「살ー짝、기분 좋아질 뿐이니까……♪」
						PRINTFORMDL 토요히메는 %마스터를% 밀어서 넘어뜨리고
						PRINTFORMDW 혀로 슬쩍 입맛을 다시며、옷에 손을 대기 시작했다……
					CASE 2
						PRINTFORML 「킥킥……그렇게 무방비하면……」
						PRINTFORML 「나ー 쁜 누나에게、먹혀버려요……♪」
						PRINTFORMDL 토요히메가 그렇게 속삭이며、귓가에 후우ー 하고 달콤한 입김을 불어온다
						PRINTFORMDW 몸서리 쳐지는 섬뜩한 감각에、%마스터는% 저항할 힘을 모두 잃어버렸다
				ENDSELECT
			ENDIF
			PRINTFORMW 
			;押し倒し口上切り替え用のTCVAR
			TCVAR:99:399 = 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTERに抱きついた（開始時）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;なだめられた（終了時）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;-------------------------------------------------
		;抜け出した
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード満足終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;抱き着きモード強制終了
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;抜け出す失敗
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		;-------------------------------------------------
		;好きにして状態から抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;欲求不満が高くて抜け出せなかった
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;誘ってきた（人目につかない場所に行きたがるやつ）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;ARG:1=TARGETの欲求不満度（最高1000）
		;必要ならばこれで分岐を作ってください
		PRINTFORML
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

