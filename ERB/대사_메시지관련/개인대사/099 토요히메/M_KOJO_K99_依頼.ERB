;--------------------------------------------------
;RETURN 0 부분은 공통 구상이 표시됩니다
;의뢰별 구상을 표시하는 부분은 RETURN 0 을 RETURN 1 로 해주세요
;--------------------------------------------------
SELECTCASE SCENE
CASE "의뢰 제시시"
	
	RETURN 0
CASE "의뢰비수탁시"
	
	RETURN 0
CASE "의뢰 수탁시"
	
	RETURN 0
CASE "의뢰 확인시"
	
	RETURN 0
CASE "의뢰 파기시"
	
	RETURN 0
CASE "의뢰실행 직전"
	
	RETURN 0
CASE "의뢰실행 직후"
	IF TFLAG:193 == 1
		;正解の選択肢
	ELSEIF TFLAG:193 == 2
		;ハズレの選択肢だったが逆に正解
	ELSEIF TFLAG:193 == -1
		;ハズレの選択肢
	ELSEIF TFLAG:193 == -2
		;正解の選択肢だったが逆にハズレ
	ENDIF
	RETURN 0
CASE "성공 보고시"
;보수:野菜か果物
	
	RETURN 0
CASE "실패 보고시"
	
	RETURN 0
;CASE "의뢰 보고 불필요"
;＜이 의뢰에는 없습니다＞
;	RETURN 0
CASE "구상위탁률"
;＜この依頼特有の分岐です。(RESULT:100の値)％の確率で"구상전용"が発生します＞
	RESULT:100 = 0

;＜RESULT:YYに0以外の数値を入れると、該当のお悩みを発生させなくできます（詳しくは一般依頼16_お悩み相談参照）＞
;1,2:弾幕ごっこの悩み 3,4:晩酌の悩み 5,6:本の悩み 7,8:香料の悩み
;9,10:音楽の悩み 11,12:服の悩み 13:髪型の悩み 14:参拝の悩み 15:ダイエットの悩み
;16,17:菜園の悩み 18:休日の悩み 19:人気の悩み 20:集中力の悩み 21:御三家の悩み 22:進化の悩み
	RESULT:1 = 0
	RESULT:2 = 0
	RETURN 1
CASE "구상전용"
;＜この依頼特有の分岐です。(口上委託率)％の確率で発生します＞
;16,お悩み相談 の場合
;RESULT        발생판정, 0이면 발생하지 않는다
;RESULT:1      正解の選択肢（ただしRESULT:2が0だと、一定確率で逆にハズレる）
;RESULT:2      逆張り正解／ハズレ判定の消去,1だと逆張りが無くなります
;RESULTS:0     お悩み文章（自動で『』で囲まれます）
;RESULTS:1～3  選択肢１～３
;何も発生しない状態になると無限ループになるので注意してください
;●追記メモ
;RESULT:1を4～6にするとそれぞれ1～3がハズレ（残り2つが正解）になります
;RESULT:1を7にするとどれも正解になります
;RESULT:1を8にするとどれもハズレになります
;1～3以外の場合は逆張り判定が自動で無くなります
	RESULT = 0

;▼샘플
[SKIPSTART]
	SELECTCASE RAND:5
	CASE 0
		;このお悩みは、恋慕で発生し、親密によって正解が変わります
		SIF TALENT:연모
			RESULT = 1
		RESULTS:0 = 아기를 낳는다면 몇 명이 좋을까？
		RESULTS:1 = ５명으로 농구팀을 만들자
		RESULTS:2 = ９명으로 야구팀을 만들자
		RESULTS:3 = １１명으로 축구팀을 만들자
		IF ABL:친밀 >= 100
			RESULT:1 = 3
		ELSEIF ABL:친밀 >= 20
			RESULT:1 = 2
		ELSE
			RESULT:1 = 1
		ENDIF
	CASE 1
		;このお悩みは、無条件で発生し、正解は絶対に2番で固定化されます
		SIF 1
			RESULT = 1
		RESULTS:0 = 어떤 직업이 좋을까？
		RESULTS:1 = 멋잇는 나이트
		RESULTS:2 = 시프가 편리해
		RESULTS:3 = 청마도사 최강！
		RESULT:1 = 2
		RESULT:2 = 1
	CASE 2
		;このお悩みは、夏の月に発生し、正解がランダムで変わります
		SIF DAY:2 == 2
			RESULT = 1
		RESULTS:0 = 어떤 수영복을 살까？
		RESULTS:1 = 예쁜 원피스
		RESULTS:2 = 쿨한 세퍼레이트
		RESULTS:3 = 섹시한 비키니
		RESULT:1 = RAND:3 + 1
	CASE 3
		;このお悩みは、無条件で発生し、1番と3番が正解になります（5 - 3 = 2番がハズレになります）
		SIF 1
			RESULT = 1
		RESULTS:0 = 강아지 이름은 뭘로 할까？
		RESULTS:1 = 보롱고
		RESULTS:2 = 게레게레
		RESULTS:3 = 푸클
		RESULT:1 = 5
	CASE 4
		;このお悩みは、無条件で発生し、どれを選んでも正解になります
		SIF 1
			RESULT = 1
		RESULTS:0 = 저녁식사로 뭐가 좋아？
		RESULTS:1 = 고기감자조림
		RESULTS:2 = 고로케
		RESULTS:3 = 생선구이
		RESULT:1 = 7
	ENDSELECT
[SKIPEND]

	SIF !RESULT
		RESTART
	RETURN 1
ENDSELECT

