;81,펠라치오
;==================================================
@M_KOJO_MESSAGE_COM_K99_81
CALL M_KOJO_MESSAGE_COM_K99_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;お掃除フェラ
	ELSEIF TCVAR:MASTER:112 == 1 || TCVAR:MASTER:112 == 2
		;연모
		IF TALENT:연모
			CALL HPH_PRINT,@"「할짝……츄……응HPH 할짝……날름……으응HPH」","L"
			SIF !CFLAG:99:1998
				PRINTFORMDL 토요히메는 장대 전체를 혀로 핥으며、두 명이 사귄 증거를 예쁘게 빨아내고 있다……
			CALL HPH_PRINT,@"「츄우우……우웃HPH 응、하아……HPH」","L"
			SIF !CFLAG:99:1998
				PRINTFORMDW 최후로 요도에 남은 정액마저 빨아 올리고서야、토요히메는 만족스럽게 입술을 떼었다
			RETURN 1
		;それ以外
		ELSE
			PRINTFORML 「지금 깨끗하게 해 줄테니까요……」
			PRINTFORML 「날름……츄우……으응、츄파……낼름……으응」
			SIF !CFLAG:99:1998
				PRINTFORMDW 토요히메는 자신의 혀를 사용해 %CALLNAME:MASTER%의 남근을 열심히 핥았다……
			RETURN 1
		ENDIF
	;通常
	ELSE
		;연모
		IF TALENT:연모
			;このコマンドであなたが射精
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「당신의 더러움……전부 받아 들여 줄 테니까……HPH」","L"
						CALL HPH_PRINT,@"「그러니까……가득 줘요……HPH 아음……응HPH 츄루루……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 더러움이라 말하고는 있지만、결국 자신이 정액을 마시고 싶어서일 것이다
							PRINTFORMDW %마스터는% 그런 일을 생각하면서、사양하지 않고  토요히메의 입 안에 대량의 『더러움』을 털어 놓았다
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「이대로……사정해……HPH 츄파……앙HPH」","L"
						CALL HPH_PRINT,@"「당신의、마시고 싶은……HPH 츄……츄루루……룹HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 사랑하는 사람의 아기씨로 목을 적시려는 토요히메는 일심분란하게 육봉에 달라붙어 떼놓지 않는다
							PRINTFORMDW 그 격렬함에 %마스터는% 순식간에 끝까지 달아올라、결국 그녀의 소망대로 입안에 대량의 정령을 발했다……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「날름……응HPH 츄파……츄우……우응HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 부드럽게 입술로 귀두를 훑어지면서、다른 생물과 같이 꿈틀거리는 혀로 민감해진 방울이 꾸짖어진다
						    PRINTFORMDL 마침내 더 이상 참을 수 없게 된 %마스터가% 허리를 띄우고 폭발적으로 사정을 시작하자
						    PRINTFORMDL 토요히메는 목 안쪽까지 장대를 깊숙이 물어 삼키고、자신의 입으로 수컷의 욕망을 모두 받아들였다……
						ENDIF
						CALL HPH_PRINT,@"「으응……으윽HPH 응、후우……꾸우울……꺽HPH」","W"
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「할짝……HPH 츄우……츄파……앙HPH」","L"
						CALL HPH_PRINT,@"「쿠흐……응쿠……웃HPH 어때……？ 기분 좋아……？HPH」","W"
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메는 %마스터가% 느끼는 포인트를 적확하게 몰아세우고 있다
							PRINTFORMDL 기분이 좋지 않을 이유가 없다。%마스터는% 토요히메에게 그렇게 답했다
						ENDIF
						CALL HPH_PRINT,@"「다행이다……HPH 그러면、좀 더 즐겨 주세요……HPH 으응……츄우우우HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「으응……HPH 훗……쿠츄……っHPH」","L"
						CALL HPH_PRINT,@"「꿀꺽……응……으흡……응HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 목 안까지 육봉을 머금고 입술을 사용해 장대 전체를 애무하면서
							PRINTFORMDL 입 안에 모인 쿠퍼액을 소리를 내며 삼킨다
						ENDIF
						CALL HPH_PRINT,@"「하아……앙HPH %CALLNAME:MASTER%의 자지、맛있어요……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 그리고 토요히메는 숙련된 창부 처럼、말없이 구음을 재개했다……
					CASE 2
						CALL HPH_PRINT,@"「아음……츄……츄파……앙HPH」","L"
						CALL HPH_PRINT,@"「응、응……후……츄파……앗HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 처음은 귀두의 부분을 중점적으로 애무하면서 %CALLNAME:MASTER%의 반응을 살핀다
							PRINTFORMDL 기회를 봐 조금씩、조금씩 입의 안쪽까지 물어 간다
							PRINTFORMDW 완급이 붙은 교묘한 봉사에 %마스터는% 완전히 희롱당하고 있다……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「가 버릴 것 같아……？ 할짝……츄……응」
						PRINTFORML 「언제라도……가도 좋으니까……하음……츄루……응」
						IF !CFLAG:99:1998
							PRINTFORMDL 따뜻하게 미끌거리는 혀로 민감한 귀두를 자극하면서 토요히메는 %CALLNAME:MASTER%에게 사정을 재촉한다
							PRINTFORMDW 급속히 사정감이 치밀어 오른 %마스터는% 토요히메의 머리를 붙잡고 그녀의 입 안에 대량의 정령을 방출했다……
						ENDIF
					CASE 1
						PRINTFORML 「츄루……즈즈……응、하아……아음……응」
						IF !CFLAG:99:1998
							PRINTFORMDL 육봉의 첨단으로부터 울컥울컥 넘쳐 나오는 쿠퍼액을 빨아 올려 입 안에서 타액과 혼합한다
							PRINTFORMDL 그것을 그대로 삼키고는、토요히메는 다시 봉사를 재개한다……
						ENDIF
						PRINTFORML 「응……우읏、츄픕、쿠흡、쿠흡、츄팟」
						IF !CFLAG:99:1998
							PRINTFORMDL %마스터가% 사정을 예고하자 토요히메는 깊숙이 육봉을 물어 삼키고
							PRINTFORMDW 『이대로 사정해』 라고 눈짓 해왔다……
						ENDIF
					CASE 2
						PRINTFORML 「츄우……츄……응、츄루룹……으흡……응」
						IF !CFLAG:99:1998
							PRINTFORMDL 끈적한 혀놀림으로 장대 전체가 애무당하자 %마스터는% 허리를 떨었다
							PRINTFORMDL 사정 직전의 육봉은 새빨갛게 충혈해 당장이라도 파열해버릴 것만 같았다……
						ENDIF
						PRINTFORML 「츄푸읍……후훗、기분 좋아……？」
						PRINTFORMW 「참지 않아도 괜찮으니까……쿠흡……츄……츄우우……우응」
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「쿠파……응……우응、츄루……우응……웃」
						IF !CFLAG:99:1998
							PRINTFORMDL 눈깔사탕을 빠는 것 같은 혀 사용으로 토요히메는 %CALLNAME:MASTER%의 남근에 봉사하고 있다
							PRINTFORMDW 눈을 살며시 치켜 뜨고 응시해 오는 토요히메의 머리를 상냥하게 어루만져 주자、그녀는 어쩐지 기뻐보였다……
						ENDIF
					CASE 1
						PRINTFORML 「할짝……츄……웅、하아……음……응」
						PRINTFORML 「응、후우……읍、츄르……흡……응」
						SIF !CFLAG:99:1998
							PRINTFORMDW 토요히메는 목 안까지 육봉을 물어 삼키며 %마스터를% 기분 좋게 보내주려고 하고 있다……
					CASE 2
						PRINTFORML 「당신의 자지……움찔움찔 하고 있어요……」
						PRINTFORML 「츄……끝 부분에서、이렇게나……날름……」
						PRINTFORML 「엣치한 즙……흘러 나와서……우응……츄파……응」
						IF !CFLAG:99:1998
							PRINTFORMDL 장대를 타고 흐르는 쿠퍼액의 물방울을 혀로 빨아내며、구슬방울에 살그머니 입맞춤 한다
							PRINTFORMDW 그리고 그대로、천천히 토요히메의 입 안으로 %CALLNAME:MASTER%의 남근이 삼켜져 들어갔다……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

