;80,수음
;==================================================
@M_KOJO_MESSAGE_COM_K99_80
CALL M_KOJO_MESSAGE_COM_K99_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			;このコマンドであなたが射精
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「후훗HPH 이제 가 버릴 것 같아……？HPH」","L"
						CALL HPH_PRINT,@"「좋아요……참지 않아도……HPH」","L"
						CALL HPH_PRINT,@"「내가 전부 받아 들여줄 테니까……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메가 장대를 훑어주는 속도가 서서히 격렬함을 늘려 간다
							PRINTFORMDL 당연 %마스터는% 거기에 견딜 수 있을 리도 없고
							PRINTFORMDW 입을 열고 대기하는 고귀한 토요히메의 안면에 정액을 뿌리게 되는 것이었다……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「우응ー……조금 아까운 생각도 들지만……HPH」","L"
						CALL HPH_PRINT,@"「그렇지만、당신이 그렇게 하고 싶다면야……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 최후는 질내에。그렇게 생각하고 있던 토요히메는 약간 어딘지 부족한 것 같은 표정을 하고 있었다……
					CASE 2
						SIF !CFLAG:99:1998
							PRINTFORMDL 토요히메의 교묘한 수음으로 %마스터는% 눈 깜짝할 사이에 사정으로 이끌어졌다
						CALL HPH_PRINT,@"「아……HPH 굉장한 양……HPH」","L"
						CALL HPH_PRINT,@"「게다가、이 냄새……하아아……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메는 손바닥에 흠뻑 달라붙은 정액에 얼굴을 가까이 하고
							PRINTFORMDW 한계까지 심호홉 해 농후한 더러운 냄새를 폐 안에 거두었다……
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%의 자지 손대고 있으면……HPH」","L"
						CALL HPH_PRINT,@"「저도……흥분하게 되어 버려……HPH」","L"
						CALL HPH_PRINT,@"「아아、안 돼……참을 수 없어……っHPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 한 손으로 장대를 쓸어올리면서 빈 손으로 스스로를 위로하는 토요히메
							PRINTFORMDW 그 광경은、보고 있는 것만으로 사정해 버릴 것 같을 정도로 음란했다……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「더러움이 모여 있는 것은 여기일까나……？HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 토요히메는 보들보들 음낭을 주물러 풀면서 규칙적으로 장대를 쓸어올리고 있다
						CALL HPH_PRINT,@"「안 되잖아요……이렇게 모아둬서는……HPH」","L"
						CALL HPH_PRINT,@"「내가 깨끗이 해 줄테니까요……HPH」","W"
					CASE 2
						CALL HPH_PRINT,@"「저기、들리나요……？HPH」","L"
						CALL HPH_PRINT,@"「끈적끈적 하고 엣치한 소리가 나고 있는 것……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 선행한 액투성이가 된 손가락으로 장대를 다 잡고서는、상냥하게 위애로 훑어준다
							PRINTFORMDW 직접적인 쾌락에 더해 시각과 소리가 합쳐져、%마스터는% 순식간에 가지 않도록 견디는 것이 고작이었다……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「이제 가 버릴 것 같아……？」
						PRINTFORML 「좋아요……가득 사정해도……♪」
						SIF !CFLAG:99:1998
							PRINTFORMDW 토요히메는 %CALLNAME:MASTER%의 민감한 부분을 중점적으로 쓸어올려 사정으로 이끌었다……
					CASE 1
						PRINTFORML 「이렇게 하면 기분 좋지요……？」
						PRINTFORML 「사정할 것 같으면 언제라도……꺄악」
						SIF !CFLAG:99:1998
							PRINTFORMDL 토요히메가 말을 채 끝내기도 전에 %마스터는% 폭발적인 기색으로 정령을 내뿜었다
						PRINTFORML 「……후훗、수고하셨습니다。잔뜩 나왔네요」
						SIF !CFLAG:99:1998
							PRINTFORMDW 얼굴을 흘러내리는 정액을 닦으며、토요히메는 %CALLNAME:MASTER%에게 상냥하게 미소지었다……
					CASE 2
						PRINTFORML 「지금은 그저 기분 좋아지는 것만 생각하면 좋은거야……」
						PRINTFORML 「그러니까 참지 말고……저에게 몸을 맡기도록……」
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메의 그 말로 %마스터는% 무의식적으로 사정을 참고 있던 일을 알아차렸다
							PRINTFORMDL 그녀가 말한 대로 전신의 힘을 빼 쾌락에 몸을 바치자, 눈 깜짝할 사이에 사정을 끝내버렸다……
						ENDIF
						PRINTFORMW 「……우응♪ 노력하셨군요。훌륭해요」
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「자、어깨의 힘을 빼고……」
						PRINTFORML 「내가 기분 좋게 해 줄 테니까……」
						SIF !CFLAG:99:1998
							PRINTFORMDW 토요히메는 윤활제 대신에 타액을 늘어뜨리더니、천천히 장대를 훑기 시작했다……
					CASE 1
						PRINTFORML 「당신의 자지、굉장히 뜨거워지고 있어……」
						PRINTFORML 「이런 것이 나의 거기에 삽입된다면……아앗」
						PRINTFORMW 「지금、움찔움찔 해버렸죠…… 저와 엣치하는 것 상상해 버렸어……？」
					CASE 2
						PRINTFORML 「저기……좀 더 보여줄래……？」
						PRINTFORML 「당신의 기분 좋다는 표정……♪」
						SIF !CFLAG:99:1998
							PRINTFORMDW 토요히메는 쾌락으로 비뚤어져가는 %CALLNAME:MASTER%의 얼굴을 응시하며 상냥한 손놀림으로 장대를 훑고 있다……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

