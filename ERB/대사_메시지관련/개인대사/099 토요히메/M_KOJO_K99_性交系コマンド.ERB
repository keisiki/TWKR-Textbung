;正常位
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;처녀
		IF TCVAR:파과 == 1
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;挿入継続
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;연모
		IF TALENT:연모
			;このコマンドであなたが사정
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「사정이구나……？HPH 으응HPH 좋아요……HPH」","L"
						CALL HPH_PRINT,@"「당신의 정액……뷰크뷰크해도……HPH」","L"
						CALL HPH_PRINT,@"「나의 자궁에……잔뜩 넣어……줘HPH」","L"
						PRINTFORMDL %CALLNAME:MASTER%의 허리에 양다리를 얽어매듯 단단히 감싸고、씨뿌리기를 재촉해 오는 토요히메
						PRINTFORMDW 그 말에 흥분한 %CALLNAME:MASTER%는、토요히메를 껴안고 사정을 향해 격렬하게 허리를 쳐박아 나갔다
					CASE 1
						CALL HPH_PRINT,@"「앙HPH 아앙HPH 이런HPH 이렇게나HPH」","L"
						CALL HPH_PRINT,@"「행복해요、사랑해요오HPH 후아아HPH HPH HPH」","L"
						PRINTFORMDL 토요히메를 뒤덮고、체중을 맡긴 채 열중해 허리를 쳐박는다
						PRINTFORMDL 용맹한 피스톤의 움직임에 토요히메는 완전히 발정해버려、암컨 그 자체의 표정으로 그저 허덕이고 있다
						PRINTFORMDW 주우욱 안쪽 깊숙이까지 삽입해 정령을 토해내자、토요히메는 다리를 핑 하고 늘어뜨리며 씩씩한 수컷에게 씨뿌리기 당하는 행복에 몸을 떨었다
					CASE 2
						CALL HPH_PRINT,@"「하앗HPH 앙HPH 아아、으응HPH」","L"
						CALL HPH_PRINT,@"「사정해주세、요HPH 나의 거기HPH 임신시켜HPH」","L"
						PRINTFORMDL 지나친 쾌락에 이성의 긴장이 풀려버린 %CALLNAME:MASTER%는、짐승과 같이 허리를 계속 흔든다
						PRINTFORMDW 그리고 사정의 순간、질 안쪽까지 페니스를 쳐박고 펄펄 끓어오르는 정령을 그녀의 한 가운데에 뿜어냈다
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「응、크윽HPH 으응……아아……앗HPH」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%……심술궃게、굴지 말고……오HPH」","L"
						PRINTFORMDL 초조하게 만들듯이 페니스를 그저 출납하기만 하자 참을 수 없게 되었을까
						PRINTFORMDW 토요히메는 부족한 쾌락을 벌충이라도 하듯이 자신의 가슴과 음핵을 만지작거리기 시작했다……
					CASE 1
						CALL HPH_PRINT,@"「이 모습……당신의 얼굴도……연결되고 있는 곳도……HPH」","L"
						CALL HPH_PRINT,@"「전부、잘 보여……으응HPH 응、아앗HPH」","L"
						PRINTFORMDL %CALLNAME:MASTER%는 토요히메를 뒤집기 자세로 하고
						PRINTFORMDW 그녀가 『잘 보인다』라고 한 것과 같게、긴 스트로크로 질내를 범해주었다
					CASE 2
						CALL HPH_PRINT,@"「아……%CALLNAME:MASTER%……HPH」","L"
						CALL HPH_PRINT,@"「……응……후……HPH 후아……으응……앗HPH」","L"
						PRINTFORMDW %CALLNAME:MASTER%는 허리의 움직임을 멈추고、토요히메와 연결된 채로 입맞춤을 주고 받았다
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが사정
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「이대로、사정해도 좋으니까……HPH」","L"
						CALL HPH_PRINT,@"「당신의 자지가 기분 좋아지는 걸……나의 한 가운데로 느끼게 해줄래……？HPH」","L"
						PRINTFORMDL 덮어 누르는 %CALLNAME:MASTER%를 껴안고、이대로 사정하도록 재촉하는 토요히메
						PRINTFORMDW 이제 멈추지 않는다。%CALLNAME:MASTER%는 토요히메를 껴안고 답을 돌려주고、본능대로 허리를 쳐박아 나갔다
					CASE 1
						CALL HPH_PRINT,@"「사정하고 싶은거네요……HPH 좋아요……HPH」","L"
						CALL HPH_PRINT,@"「당신의 더러움……전부 나의 거기에 토해내줘……HPH」","L"
						PRINTFORMDL 섹스의 쾌감에 허덕이는 토요히메의 표정、피스톤이 움직일때마다 튀어오르는 가슴、허리를 당길 때마다 질내로부터 긁어내지는 거품 낀 애액……
						PRINTFORMDW 그 전부를 망막에 새기면서、%CALLNAME:MASTER%는 짐승과 같이 허리를 계속 흔들어 댄 끝에、결국 사정했다
					CASE 2
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%……손을 잡아줘……HPH」","L"
						CALL HPH_PRINT,@"「지금만은……진정한 연인같이……앗HPH」","L"
						PRINTFORMDL 요구에 응해 손을 잡고、연인처럼 손가락을 얽히듯 겹치며
						PRINTFORMDW 아기씨를 요구해 뜨거울 정도로 장대를 포옹해 오는 질내에、%CALLNAME:MASTER%는 마음껏 정령을 발사했다
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「그대로요……HPH 아、응응……으앗HPH」","L"
						PRINTFORMDL 허리를 찔러 넣자 순조롭게 안쪽 깊숙이 받아들여、뽑으려고 하면 질 전체를 가지고 결코 놓치지 않겠다는듯이 휘감겨 온다
						PRINTFORMDW 분명 명기의 꿀단지、%CALLNAME:MASTER%는 그대로 열중해 거듭 범했다……
					CASE 1
						CALL HPH_PRINT,@"「으응……응HPH 하아……아아……응HPH」","L"
						CALL HPH_PRINT,@"「당신의 자지……깊은 곳、닿고 있어……아앙HPH」","L"
						PRINTFORMDW 근본까지 삽입한 페니스로 질 안쪽을 찔러 줄때마다、토요히메는 허리를 띄우며 성대하게 흐느꼈다……
					CASE 2
						CALL HPH_PRINT,@"「당신의 엣치……굉장히 능숙하니까……HPH」","L"
						CALL HPH_PRINT,@"「내 쪽이……으응HPH 먼저 가 버릴 것 같아……응HPH」","L"
						PRINTFORMDL 허리를 띄어、민감한 부분을 쓸어올릴 때 마다 한 단계 높은 교성을 질러대는 토요히메
						PRINTFORMDW %CALLNAME:MASTER%는 교묘하게 허리를 쓰면서、토요히메를 기분 좋게 만들어 주려고 피스톤질을 계속했다……
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			CALL HPH_PRINT,@"「와줘……%CALLNAME:MASTER%……HPH」","L"
			PRINTFORMDL 위를 바라보고 누워있는 토요히메의 비밀의 틈에 육봉을 넣고、서서히 허리를 가라앉혔다
			PRINTFORMDW 근본까지 삽입한 곳에서 %CALLNAME:MASTER%는 토요히메를 덮어 누르고、참고 있던 숨을 길게 토해냈다
			RETURN 1
		;それ以外
		ELSE
			CALL HPH_PRINT,@"「아……삽입하려는 거야……？HPH」","L"
			PRINTFORMDL %CALLNAME:MASTER%가 덮어오자、토요히메는 익숙해진 동작으로 다리를 열고 소중한 것을 드러냈다
			CALL HPH_PRINT,@"「상냥하게 해줘……요、아앙HPH」","W"
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

