			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
;-------------------------------------------------
;정상위
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;처녀
		IF TCVAR:파과 == 1
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;挿入継続
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;연모
		IF TALENT:연모
			;このコマンドであなたが射精
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「사정이구나……？HPH 으응HPH 좋아요……HPH」","L"
						CALL HPH_PRINT,@"「당신의 정액……퓻퓻해도……HPH」","L"
						CALL HPH_PRINT,@"「나의 자궁에……잔뜩 넣어……줘HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL %CALLNAME:MASTER%의 허리에 양다리를 얽어매듯 단단히 감싸고、씨뿌리기를 재촉해 오는 토요히메
							PRINTFORMDW 그 말에 흥분한 %마스터는%、토요히메를 껴안고 사정을 향해 격렬하게 허리를 쳐박아 나갔다
						ENDIF
					CASE 1						
						CALL HPH_PRINT,@"「하앙HPH 아앙HPH 아아、으앙HPH」","L"
						CALL HPH_PRINT,@"「사정해주세、요HPH 나의 거기HPH 임신시켜주세요옷HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메를 뒤덮고、체중을 맡긴 채 열중해 허리를 쳐박는다
							PRINTFORMDL 용맹한 피스톤의 움직임에 토요히메는 완전히 발정해버려、암컨 그 자체의 표정으로 그저 허덕이고 있다
							PRINTFORMDL 사정의 순간、본능에 따라 찰싹 요도구를 자궁구에 밀착시키자
							PRINTFORMDL 토요히메는 사랑하는 사람에게서 나오는 더러움을 모두 그 몸으로 받아들이려고
							PRINTFORMDW 다리로 %CALLNAME:MASTER%의 허리를 꼬옥 껴안았다……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「아앙HPH 아아앙HPH 거기HPH 거기에요오옷HPH」","L"
						CALL HPH_PRINT,@"「너무、행복해요오옷HPH 후아아앙HPH HPH HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 지나친 쾌락에 이성의 긴장이 풀려버린 %마스터는%、짐승과 같이 허리를 계속 흔든다
							PRINTFORMDW 그리고 사정의 순간、질 안쪽까지 페니스를 쳐박고 펄펄 끓어오르는 정령을 그녀의 한 가운데에 뿜어냈다
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						;アイマスク装着中
						IF TEQUIP:눈가리개
							CALL HPH_PRINT,@"「응、크윽HPH 으응……아아……앗HPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL 시각에 의지할 수 없는 만큼、토요히메의 몸은 외부 자극에 의해 민감해져 있다
								PRINTFORMDL 그 상태에서 까칠까칠 음핵을 밀어올리거나、자궁구를 육봉의 끝으로 애무하듯이 돌렸다
								PRINTFORMDW 토요히메는 고개를 뒤로 젖힌 채、미칠것 같이 교성을 내질렀다
							ENDIF
						;それ以外
						ELSE
							CALL HPH_PRINT,@"「이 모습……당신의 얼굴도……연결되고 있는 곳도……HPH」","L"
							CALL HPH_PRINT,@"「전부、잘 보여……으응HPH 응、아아앙HPH」","L"
							IF !CFLAG:99:1998
								PRINTFORMDL %마스터는% 토요히메를 뒤집기 자세로 하고
								PRINTFORMDW 그녀가 『잘 보인다』라고 한 것과 같게、긴 스트로크로 질내를 범해주었다
							ENDIF
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「응、크윽HPH 으응……아아……앗HPH」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%……심술궃게、굴지 말고……오HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 초조하게 만들듯이 페니스를 그저 출납하기만 하자 참을 수 없게 되었을까
							PRINTFORMDW 토요히메는 부족한 쾌락을 벌충이라도 하듯이 자신의 가슴과 음핵을 만지작거리기 시작했다……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「하앙HPH 으앙HPH 역시、당신의……앙HPH」","L"
						CALL HPH_PRINT,@"「크고、멋져요……아앙HPH 우응HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 덮어 씌여、힘찬 피스톤으로 꿀단지를 휘젓는 %CALLNAME:MASTER%의 허리에 다리를 휘감으며
							PRINTFORMDW 토요히메는 녹아버린 표정으로、사랑하는 사람과 하나가 되는 행복에 도취되어 있다……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		;それ以外
		ELSE
			;このコマンドであなたが射精
			IF NOWEX:MASTER:사정
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「이대로、사정해도 좋으니까……HPH」","L"
						CALL HPH_PRINT,@"「당신의 자지가 기분 좋아지는 걸……나의 한 가운데로 느끼게 해줄래……？HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 덮어 누르는 %마스터를% 껴안고、이대로 사정하도록 재촉하는 토요히메
							PRINTFORMDW 이제 멈추지 않는다。%마스터는% 토요히메를 껴안고 답을 돌려주고、본능대로 허리를 쳐박아 나갔다
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「사정하고 싶은거네요……HPH 좋아요……HPH」","L"
						CALL HPH_PRINT,@"「당신의 더러움……전부 나의 거기에 토해내줘……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 섹스의 쾌감에 허덕이는 토요히메의 표정、피스톤이 움직일때마다 튀어오르는 가슴、허리를 당길 때마다 질내로부터 긁어내지는 거품 낀 애액……
							PRINTFORMDW 그 전부를 망막에 새기면서、%마스터는% 짐승과 같이 허리를 계속 흔들어 댄 끝에、결국 사정했다
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%……손을 잡아줘……HPH」","L"
						CALL HPH_PRINT,@"「지금만은……진정한 연인같이……앗HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 요구에 응해 손을 잡고、연인처럼 손가락을 얽히듯 겹치며
							PRINTFORMDW 아기씨를 요구해 뜨거울 정도로 장대를 포옹해 오는 질내에、%마스터는% 마음껏 정령을 발사했다
						ENDIF
				ENDSELECT
				RETURN 1
			;それ以外
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「그대로요……HPH 아、응응……으앗HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 허리를 찔러 넣자 순조롭게 안쪽 깊숙이 받아들여、뽑으려고 하면 질 전체를 가지고 결코 놓치지 않겠다는듯이 휘감겨 온다
							PRINTFORMDW 분명 명기의 꿀단지、%마스터는% 그대로 열중해 거듭 범했다……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「으응……응HPH 하아……아아……응HPH」","L"
						CALL HPH_PRINT,@"「당신의 자지……깊은 곳、닿고 있어……아앙HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 근본까지 삽입한 페니스로 질 안쪽을 찔러 줄때마다、토요히메는 허리를 띄우며 성대하게 흐느꼈다……
					CASE 2
						CALL HPH_PRINT,@"「당신의 엣치……굉장히 능숙하니까……HPH」","L"
						CALL HPH_PRINT,@"「내 쪽이……으응HPH 먼저 가 버릴 것 같아……응HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 허리를 띄어、민감한 부분을 쓸어올릴 때 마다 한 단계 높은 교성을 질러대는 토요히메
							PRINTFORMDW %마스터는% 교묘하게 허리를 쓰면서、토요히메를 기분 좋게 만들어 주려고 피스톤질을 계속했다……
						ENDIF
				ENDSELECT
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			CALL HPH_PRINT,@"「와줘……%CALLNAME:MASTER%……HPH」","L"
			IF !CFLAG:99:1998
				PRINTFORMDL 위를 바라보고 누워있는 토요히메의 비밀의 틈에 육봉을 넣고、서서히 허리를 가라앉혔다
				PRINTFORMDW 근본까지 삽입한 곳에서 %마스터는% 토요히메를 덮어 누르고、참고 있던 숨을 길게 토해냈다
			ENDIF
			RETURN 1
		;それ以外
		ELSE
			CALL HPH_PRINT,@"「아……삽입하려는 거야……？HPH」","L"
			SIF !CFLAG:99:1998
				PRINTFORMDL %마스터가% 덮어오자、토요히메는 익숙해진 동작으로 다리를 열고 소중한 것을 드러냈다
			CALL HPH_PRINT,@"「상냥하게 해줘……요、아앙HPH」","W"
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

