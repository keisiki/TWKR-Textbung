	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;挿入中かつあなたが射精したと同時に豊姫がＢ絶頂
	ELSEIF NOWEX:MASTER:사정 && NOWEX:B절정 && (TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER) 
