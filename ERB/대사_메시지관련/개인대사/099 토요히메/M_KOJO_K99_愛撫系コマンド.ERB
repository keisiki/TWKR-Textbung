;==================================================
@M_KOJO_MESSAGE_COM_K99_3
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K99_3_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_3_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;バイブ挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TEQUIP:바이브
		;연모
		IF TALENT:연모
			SELECTCASE RAND:3
				CASE 0
					SIF !CFLAG:99:1998
						PRINTFORMDL %마스터가% 바이브를 넣었다 뺐다 할 때마다 토요히메의 꿀구멍에서는 질척질척 하는 음란한 소리가 울린다
					CALL HPH_PRINT,@"「하아……앙HPH 저、이런 완구로……HPH」","L"
					CALL HPH_PRINT,@"「자궁、내려와 버리는……아앙HPH 아가짱 갖고 싶어하고 있어……아앙HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 토요히메의 지체는 암컷의 본능에 저항하지 못하고、자궁은 내려 앉아、바이브의 아이를 임신하고자 그 첨단에 입을 대고 달라붙었다……
				CASE 1
					CALL HPH_PRINT,@"「안 돼、요오오……아앙HPH 당신의 것이 아니면……아앙HPH」","L"
					IF !CFLAG:99:1998
