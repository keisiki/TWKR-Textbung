@M_KOJO_MESSAGE_COM_K99_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中かつあなたが射精したと同時に絶頂
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:MASTER:사정 && (TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER) && (NOWEX:C절정 || NOWEX:V절정 || NOWEX:A절정 || NOWEX:B절정 || NOWEX:M절정)
		;연모
		IF TALENT:연모
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「응、하아HPH 이대로……응HPH」","L"
					CALL HPH_PRINT,@"「아앗HPH 응HPH 이대로 사정해……앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 야비할 정도로 아기씨를 요구해 오는 토요히메의 허리를 붙잡고 몇 번이나 밀어올리며
						PRINTFORMDL 빈 손으로는 음란하게 위아래로 흔틀리는 토요히메의 유방을 움켜잡는다
					ENDIF
					CALL HPH_PRINT,@"「흐아아앙HPH 가버려、요오오오HPH HPH HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 결정타로 근본까지 페니스를 찔러 넣고 그대로 사정하자、토요히메도 등들 뒤로 젖히며 동시 절정에 도달했다
				CASE 1
					CALL HPH_PRINT,@"「응、아앗HPH 좋아요HPH %CALLNAME:MASTER% HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 애무하면서 격렬한 피스톤으로 토요히메의 \@ TEQUIP:50 == PLAYER ? 질내 # 장내 \@를 휘저으면서
						PRINTFORMDL %마스터는% 몇 번째인가의 절정을 맞이한 토요히메의 귓가에 대고, %마스터는% 사정을 예고했다
					ENDIF
					CALL HPH_PRINT,@"「우、으응HPH 넣어줘……HPH」","L"
					CALL HPH_PRINT,@"「당신의 정액、저에게 주세……요HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「히아앙HPH 아아앙HPH 나、더는……앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 머리카락을 흐트러뜨리며 허덕이는 토요히메의 몸을 애무하며
						PRINTFORMDL %마스터는% 허리를 움직여 그녀의 약점을 집요하게 자극했다
					ENDIF
					CALL HPH_PRINT,@"「더는、안 돼요、요오옷HPH 후우、아、아―――아아앙HPH HPH HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 그리고 사정 직전、%마스터가% 깊숙이 페니스를 뿌리까지 밀어넣자
						PRINTFORMDW 토요히메도 마치 산소 부족에 허덕이듯 삽입된 육봉을 조여오면서、깊고 깊은 절정에 이르렀다……
					ENDIF
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「으응HPH 거기、좋아……아앙HPH」","L"
					CALL HPH_PRINT,@"「좀 더 만져주며……찔러……요HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDL 토요히메의 약한 곳을 집요하게 자극하면서 %마스터는% 사정을 향해 허리의 움직임을 가속시켜 간다……
					CALL HPH_PRINT,@"「아앙HPH 저도、이제……앗앗HPH」","L"
					CALL HPH_PRINT,@"「안 돼……요HPH 아、아아앗HPH 아아아앗HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%……함께……HPH」","L"
					CALL HPH_PRINT,@"「함께 가는거에……요？HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL %마스터는% 동시 절정을 요구해 오는 토요히메를 껴안으며
						PRINTFORMDL 그녀의 민감한 부분을 애무하면서 격렬하게 허리를 찔러 넣었다……
					ENDIF
					CALL HPH_PRINT,@"「아、아앗HPH 응、크윽HPH ～～～아아아앙HPH」","W"
				CASE 2
					CALL HPH_PRINT,@"「당신의 자지、안에서 커져서……HPH」","L"
					CALL HPH_PRINT,@"「이제 사정하려고 하네요……？HPH 아、으아앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 이대로 사정하는 것도 좋지만、어차피라면 함께 기분 좋아져야 한다
						PRINTFORMDW %마스터는% 토요히메의 지체를 애무하면서、서서히 허리의 움직임을 빠르게 했다……
					ENDIF
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"……으응HPH 달라요……좀 더 밑에……HPH」","L"
					CALL HPH_PRINT,@"「아앙 HPH 거기는 지나쳐요……HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL %마스터는% 토요히메와 서로 장난치면서
						PRINTFORMDW 그녀의 몸을 열심히、시간을 들여 애무했다……
					ENDIF
				CASE 1
					CALL HPH_PRINT,@"「아……응……앗HPH 후아……아아……앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL %마스터는% 굳이 토요히메의 성감대는 건드리지 않고 몸을 애무해 나간다
					    PRINTFORMDL 목에 키스를 해 보거나、흐르는 땀을 혀로 빨아내 보거나……
	     				PRINTFORMDL 비밀스런 장소랄 빠는 것처럼 가장해、허벅지에 혀를 기게 해 보거나……
	     				PRINTFORMDL 그리고、그녀가 초조해 하기 직전에 유두를 살짝 깨물어 주거나
			    		PRINTFORMDW 느긋하게 시간을 들여、%마스터는% 농염한 토요히메의 지체를 풀어나갔다……
					ENDIF
				CASE 2
					CALL HPH_PRINT,@"「당신이 어루만지는 곳이、자꾸자꾸 뜨거워져……HPH」","L"
					CALL HPH_PRINT,@"「몸이、쑤셔서……견딜 수 없는 거야……아앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 토요히메는 %CALLNAME:MASTER%에게 달라붙어 강한 쾌락을 추구하고 있다
						PRINTFORMDL %마스터가% 자극에 민감한 부분을 손가락으로 애무해주면
						PRINTFORMDW 토요히메는 기뻐하는 소리를 높이며、음란하게 몸을 떨었다……
					ENDIF
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「응……응HPH 간지러워요……HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDL 목덜미부터 쇄골、그리고 유방까지 키스의 위치를 내려가며 토요히메의 지체를 애무한다
					CALL HPH_PRINT,@"「히아앙HPH 응、아아……앙HPH 거기、좋아……아앙HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 예민한 부분에 손가락이 닿자、토요히메의 몸은 솔직하게 반응을 돌려주었다
				CASE 1
					CALL HPH_PRINT,@"「우웃、아앙HPH 거기、약하니까……」","L"
					CALL HPH_PRINT,@"「상냥하게 손대요……그래、그렇게 능숙하게……아앙HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 토요히메는 자신의 약한 곳을 모두 드러내어、%CALLNAME:MASTER%에게 몸을 내맡기고 있다……
				CASE 2
					CALL HPH_PRINT,@"「그 상태로……좀 더……HPH」","L"
					CALL HPH_PRINT,@"「아아……앙……앙HPH 츄우……츄흡……응……아앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 토요히메와 입술을 거듭 겹치며 그녀의 풍만한 가슴을 비비면서 돌린다
						PRINTFORMDW 혀를 얽히며 타액을 흘려넣자、토요히메는 솔직하게 그것을 그대로 꿀꺽 삼켰다……
					ENDIF
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
