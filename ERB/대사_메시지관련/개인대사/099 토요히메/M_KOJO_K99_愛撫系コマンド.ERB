		CALL M_KOJO_MESSAGE_COM_K99_0_1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%……HPH 좀 더 강하게 해도 좋으니까……HPH」","L"
					CALL HPH_PRINT,@"「젖가슴의 끝 쪽……당신 손으로 괴롭혀 줄래……？HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDL %마스터가% 졸라대는 토요히메의 유두를 집어올리자
					CALL HPH_PRINT,@"「아파HPH 크、아앗HPH 아아앗HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 소망대로、아픔을 느낄 만큼 강하게 잡아당겨 주었다……
				CASE 1
					CALL HPH_PRINT,@"「으응HPH 그렇게나、꽉 해버리면……HPH」","L"
					CALL HPH_PRINT,@"「젖가슴、무너져 버려요……응HPH 크으으윽HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL %마스터는% 토요히메의 가슴에 손의 형태가 남을 정도로 점점 강하게 비벼대었다
						PRINTFORMDW 폭력적인 애무에 토요히메는 표정이 녹아내려、입가에 군침마저 흘리며 허덕이고 있다……
					ENDIF
				CASE 2
					IF !CFLAG:99:1998
						PRINTFORMDL 토요히메는 %CALLNAME:MASTER%의 손을 붙잡고
						PRINTFORMDL 핑 발기해 존재를 주장하는 유두로 유도했다
					ENDIF
					CALL HPH_PRINT,@"「저기……%CALLNAME:MASTER%……HPH」","L"
					CALL HPH_PRINT,@"「여기도、기분 좋게 해 줄래……？HPH」","W"
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「만지는 느낌은 어때요……？HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 힘을 주면 어디까지나 빠져들만큼 부드럽고、그러면서도 탄력도 있다
						PRINTFORMDL 말로는 표현할 수 없을 만큼의 극상의 만지는 느낌에
						PRINTFORMDW %마스터는% 빠져 버릴 것만 같았다……
					ENDIF
				CASE 1
					CALL HPH_PRINT,@"「주무르거나……비비거나……들이마시거나……HPH」","L"
					CALL HPH_PRINT,@"「당신 좋을대로 만져봐……HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 원래 그럴 생각이었다 라는 듯이 %마스터는% 토요히메의 가슴에 손을 뻗어
						PRINTFORMDW 그녀의 남달리 민감한 유방을 움켜잡거나、발기한 첨단을 손가락으로 집어 보거나、뭉그러뜨려 보았다
					ENDIF
				CASE 2
					SIF !CFLAG:99:1998
						PRINTFORMDL %마스터는% 손바닥 너머로 전해지는 토요히메의 심장 뛰는 소리를 느끼며 열심히 가슴을 애무하였다……
					CALL HPH_PRINT,@"「후훗HPH 젖가슴 정말 좋아하네……HPH」","L"
					CALL HPH_PRINT,@"「저도、당신에게 만져지는 것 좋아해요……HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

