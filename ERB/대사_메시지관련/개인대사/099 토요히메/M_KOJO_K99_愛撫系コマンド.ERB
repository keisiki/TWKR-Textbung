@M_KOJO_MESSAGE_COM_K99_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K99_1_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K99_1_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;膣内射精直後
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		IF TCVAR:102
			;연모
			IF TALENT:연모
				CALL HPH_PRINT,@"「하아……하아……HPH 이렇게나 가득……HPH」","L"
				CALL HPH_PRINT,@"「봐요……깨끗하게 해줘요……HPH 확실하게 거기도요……？HPH」","L"
				IF !CFLAG:99:1998
					PRINTFORMDL %마스터는% 혀를 사용해 깨끗하게 해달라고 요구하는 토요히메의 비밀스런 장소에 달라붙어
					PRINTFORMDW 질내에 남은 정액과 애액의 혼합물을 혀로 긁어내、입안에 머금은 그것을 소리를 내며 삼켜버렸다
				ENDIF
				RETURN 1
			;それ以外
			ELSE
				CALL HPH_PRINT,@"「깨끗하게 해 주겠다니……아、기다려……오오옷HPH」","L"
				CALL HPH_PRINT,@"「으응、응으응HPH 부끄러워요……앙HPH」","L"
				IF !CFLAG:99:1998
					PRINTFORMDL %마스터는% 토요히메의 비밀스런 장소에 혀로 핥으며
					PRINTFORMDW 질구로부터 넘쳐나오는 정액을 열심히 빨아내었다……
				ENDIF
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:시간정지
		PRINTFORMW 
		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「아아……앙……앗HPH 아아……아앗HPH」","L"
					CALL HPH_PRINT,@"「당신에게 빨려지는 거……굉장히 기분 좋아……아앙HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDL 토요히메는 무의식적으로 허리를 들어 올려、%CALLNAME:MASTER%의 얼굴에 비밀스런 장소를 밀어붙여 왔다
					CALL HPH_PRINT,@"「좀 더、빨아줘……아앙HPH 할짝 할짝 해……아앙HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「아……하아……앙HPH 후、우……응HPH」","L"
					CALL HPH_PRINT,@"「그런、소리내서는……앙HPH 후아앙HPH」","L"
					CALL HPH_PRINT,@"「안 돼 HPH 거기、느끼는 곳이……라HPH 으으응HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL 민감한 클리토리스를 빨아올려져、토요히메는 허리를 흠칫거리며 허덕이고 있다
						PRINTFORMDW %마스터는% 그 모습을 눈에 새겨가며、토요히메에게 봉사를 계속했다……
					ENDIF
				CASE 2
					IF !CFLAG:99:1998
						PRINTFORMDL 질꺽질꺽 잘익은 암컷 구멍은 끝도 없이 애액을 흘려보내며 수컷을 권하고 있다
						PRINTFORMDL 거기에 유인되기라도 한 것 처럼 %마스터는% 토요히메의 비밀스런 장소에 달라붙어
						PRINTFORMDL 줄줄 음란한 소리를 내는 감미로운 꿀을 훌쩍였다
					ENDIF
					CALL HPH_PRINT,@"「응……아아아……앙HPH 나의 거기、맛있어……？HPH」","L"
					CALL HPH_PRINT,@"「많이 빨아……기분 좋게 해줘……HPH 아앙HPH」","W"
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT,@"「후아앙HPH 아、하아아……앙HPH」","L"
					IF !CFLAG:99:1998
						PRINTFORMDL %마스터가% 혀를 사용해 봉사하자 토요히메가 허벅지로 비밀의 틈에 얼굴을 꾹 끼워버렸다
						PRINTFORMDL 조금 숨이 괴롭지만、%마스터는% 그대로 봉사를 계속해 나갔다……
					ENDIF
					CALL HPH_PRINT,@"「기분、좋아……아앙HPH 하앙……응……앗HPH」","W"
				CASE 1
					SIF !CFLAG:99:1998
						PRINTFORMDL %마스터는% 토요히메의 비밀의 틈을 손가락으로 벌리고、공기에 드러나 실룩거리는 질의 입구에 천천히 혀를 쑤셔 넣어갔다
					CALL HPH_PRINT,@"「으응……앗HPH 나의 안까지……HPH」","L"
					CALL HPH_PRINT,@"「당신의 혀、들어와서……아앙HPH 앗、아아앗HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 남달리 민감한 질내를 혀로 후벼지는 쾌락에、토요히메늘 허리를 띄우며 즐거워하고 있다……
					
				CASE 2
					CALL HPH_PRINT,@"「잔뜩 적셔요……HPH」","L"
					CALL HPH_PRINT,@"「그러는 편이、반드시 당신도 기분 좋을 테니까……HPH」","L"
					SIF !CFLAG:99:1998
						PRINTFORMDW 토요히메는 사타구니에 얼굴을 묻고 개 처럼 비밀스런 장소를 빨고 있는 %CALLNAME:MASTER%의 머리를 사랑스럽다는듯이 어루만지고 있다……
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1
