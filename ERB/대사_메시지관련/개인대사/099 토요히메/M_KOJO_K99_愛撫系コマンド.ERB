		RETURN 1
	;通常
	ELSE
		;연모
		IF TALENT:연모
			;露出癖で分岐
			SELECTCASE ABL:노출벽
				;露出癖２以上
				CASE IS > 1
					;膣内射精直後
					IF TCVAR:102
						CALL HPH_PRINT,@"「하아……하아……HPH 이렇게나 한 가득……HPH」","L"
						CALL HPH_PRINT,@"「아응……HPH 흘러넘쳐 버려……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 토요히메는 넘쳐 나오는 백탁을 손가락을 떠올려、질내로 되밀고 있다
						CALL HPH_PRINT,@"「모처럼 당신이 넣어 주었는데……아까워요……HPH」","W"
					;それ以外
					ELSE
						CALL HPH_PRINT,@"「저기……보이나요……？HPH」","L"
						CALL HPH_PRINT,@"「나의 거기、당신의 아기 갖고 싶어하는 거야……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메가 손가락을 음순을 나누어 벌리자、그녀의 신성한 부분이 아낌없이 드러났다
							;淫乱分岐の地の文から借用
							PRINTFORMDW 부드러울 것 같은 질내는 충분히 점액으로 젖어、이쪽을 권하는 듯이 벌렁벌렁 꿈틀거렸다……
						ENDIF
						CALL HPH_PRINT,@"「넣어줘……%CALLNAME:MASTER%……HPH」","W"
					ENDIF
				;露出癖２未満
				CASEELSE
					;膣内射精直後
					IF TCVAR:102
						CALL HPH_PRINT,@"「잔뜩 사정하셨네요……HPH」","L"
						CALL HPH_PRINT,@"「이렇게나 넣어주었으니……아기、가져버릴지도……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 방금 전까지 %CALLNAME:MASTER%의 일물을 물고 있던 질의 입구가 뻐끔 벌어져
							PRINTFORMDW 단정히 못한 모습으로 정액을 흘려 보내며 성교의 여운으로 실룩거리고 있다……
						ENDIF
					;それ以外
					ELSE
						CALL HPH_PRINT,@"「아무리 당신이라도、역시 부끄러워요……HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDL 드러난 토요히메의 성기는 예쁜 핑크색을 하고 있다
						CALL HPH_PRINT,@"「응………으응HPH」","W"
						IF !CFLAG:99:1998
							PRINTFORMDL %마스터는% 숨결이 닿는 거리까지 얼굴을 가까이 대고
							PRINTFORMDW 지금부터 행해질 행위를 상상하며 자신의 육봉을 천천히 다뤄나갔다……
						ENDIF
					ENDIF
			ENDSELECT
			RETURN 1
		;それ以外
		ELSE
			;露出癖で分岐
			SELECTCASE ABL:노출벽
				;露出癖２以上
				CASE IS > 1
					;膣内射精直後
					IF TCVAR:102
						CALL HPH_PRINT,@"「이렇게나 사정했구나……HPH」","L"
						CALL HPH_PRINT,@"「나의 거기、그렇게 기분 좋았어……？HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 토요히메는 %CALLNAME:MASTER%에게 씨뿌리기 된 성기를 손가락으로 펼치며 요염하게 미소짓고 있다……
					;それ以外
					ELSE
						CALL HPH_PRINT,@"「봐봐……HPH 나의 여기、당신의 자지 갖고 싶어서……HPH」","L"
						CALL HPH_PRINT,@"「봐요……이렇게 되어 버린……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 듬뿍 애액을 머금은 토요히메의 성기는 물건을 갖고 싶다는 듯이 실룩거렸고、질구는 움찔대며 뻐끔거렸다
							PRINTFORMDW 마치 수컷을 어서 오라고 권하는 그 움직임에 %CALLNAME:MASTER%의 일물은 더욱 단단함을 더했다……
						ENDIF
					ENDIF
				;露出癖２未満
				CASEELSE
					;膣内射精直後
					IF TCVAR:102
						IF !CFLAG:99:1998
							PRINTFORMDL 달의 공주에게 씨뿌리기 했다는 사실을 재차 확인하기 위해、%마스터는% 토요히메에게 질구를 펼쳐보이게 했다
							PRINTFORMDL 드러난 질내는 %CALLNAME:MASTER%의 정령으로 가득 채워져、신선한 씨앗을 자궁에 거두어들이려고 꿈틀대고 있었다……
						ENDIF
						CALL HPH_PRINT,@"「하아……아、하아……앙HPH」","L"
						CALL HPH_PRINT,@"「이렇게나 잔뜩……임신해 버려요……HPH」","W"
					;それ以外
					ELSE
						PRINTFORML 「부끄러우니까、조금만이에요……？」
						SIF !CFLAG:99:1998
							PRINTFORMDW %CALLNAME:MASTER%에게 명령당하자、토요히메는 소극적으로 질내를 펼쳐 보였다
					ENDIF
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

