@M_KOJO_EXTRASOURCE_COM_K99_312
IF CFLAG:99:1000 == 1
	SOURCE:반감 = SOURCE:반감 / 5
ENDIF

@M_KOJO_MESSAGE_COM_K99_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
CALL M_KOJO_COLOR_K99
	;時間停止中
	IF FLAG:시간정지
		PRINTFORMDL %마스터는% 토요히메에 입맞춤을 했다
		PRINTFORMDW 시간이 멈춰진 토요히메는 무저항인 채 입맞춰지고 있다……
	;睡眠中
	ELSEIF CFLAG:수면
		PRINTFORMDL %마스터는% 잠들어 있는 토요히메의 입을、자신의 입으로 덮었다……
		PRINTFORMDW 입술이 떨어지자 토요히메는 조금 미소짓고 있었다……
	;押し倒され
	ELSEIF CFLAG:우후후 == 2
		;どのカウンターに対して実行したかで分岐
		SELECTCASE TCVAR:20
			;前から抱きつく
			CASE 32
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT,@"「얼굴을 이쪽으로 돌려줘요……HPH」","L"
						CALL HPH_PRINT,@"「응ー……츄……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 얼굴을 붉히는 %CALLNAME:MASTER%의 볼에 손을 대고
							PRINTFORMDW 토요히메는 미소를 지으며 부드럽게 입술을 포개왔다……
						ENDIF
					CASE 1
						CALL HPH_PRINT,@"「착한 아이네……HPH 응……츄……HPH」","L"
						CALL HPH_PRINT,@"「자아…… 다음은 당신으로부터……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 토요히메는 눈을 감은 채 %CALLNAME:MASTER%의 키스를 기다리고 있다
							PRINTFORMDW %마스터는% 상냥하게 그녀을 껴안으며、살며시 입술을 맞췄다……
						ENDIF
					CASE 2
						CALL HPH_PRINT,@"「잠깐만…… 당신은 그대로……HPH」","L"
						CALL HPH_PRINT,@"「여긴、저에게 맡겨요……HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 키스를 하려던 %마스터를% 부드럽게 제지하며
							PRINTFORMDW 토요히메는 %마스터를% 끌어안고서、조금 농후하게 입술을 포개왔다……
						ENDIF
				ENDSELECT
			;手淫
			CASE 50
				SELECTCASE RAND:3
					CASE 0
						SIF !CFLAG:99:1998
							PRINTFORMDL 격앙되어 치솟은 육봉을 상냥하게 문질러지며、%마스터는% 토요히메와 입을 맞췄다
						CALL HPH_PRINT,@"「응……앙HPH 당신의 자지、아주 단단해졌어……HPH」","L"
						CALL HPH_PRINT,@"「후훗 HPH 잔뜩 잔뜩 기분 좋게 ……해・줄・게HPH」","L"
						SIF !CFLAG:99:1998
							PRINTFORMDW 귓가에 그렇게 속삭여 오면、%마스터는% 그저 고개를 끄덕끄덕 할 수 밖에 없었다
					CASE 1
						CALL HPH_PRINT,@"「츄우……츄……HPH 날름……하아……HPH」","L"
						CALL HPH_PRINT,@"「어때……？ 기분 좋아졌어……？HPH」","L"
						IF !CFLAG:99:1998
							PRINTFORMDL 할짝할짝 두 방울을 핥아주면서、토요히메가 %CALLNAME:MASTER%에게 그렇게 묻는다
							PRINTFORMDW %마스터가% 고개를 끄덕이자、토요히메는 제대로 말해준 상으로 장대를 강하게 빨아주었다
						ENDIF
