;40,로터
;==================================================
@M_KOJO_MESSAGE_COM_K124_40
CALL M_KOJO_MESSAGE_COM_K124_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		RETURN 1
	;睡姦中
	ELSEIF CFLAG:MASTER:장난 == 2
		PRINTFORML 「흐읏.. 으읏..」
		PRINTFORML 오키나는 잠을 잘 못 자는 듯하다
		RETURN 1
	ELSEIF TCVAR:124:만취 == 1
		RETURN 1
	;연모or연인
	ELSEIF TALENT:124:연인 || TALENT:124:연모 || TALENT:124:애욕
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「이, 이런 장난감으로.. 후, 으응%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「으읏, 기, 기분 좋은데.. 뭔가 석연치 않아..!」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「큭, 으아앗! 어, 어이! 언제까지 누르고 있을 작정이야..!」
			CASEELSE
				PRINTFORMW 「히이익, 싫어, 끄, 끈질기다고! 이제 그만둬..!」
		ENDSELECT
	ENDIF
ENDIF

RETURN 1
