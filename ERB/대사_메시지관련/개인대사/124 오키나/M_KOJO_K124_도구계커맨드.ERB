;44,바이브
;==================================================
@M_KOJO_MESSAGE_COM_K124_44
CALL M_KOJO_MESSAGE_COM_K124_44_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K124_44_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;時姦中
	IF FLAG:70
		IF TEQUIP:바이브
		ENDIF
	;睡姦中
	ELSEIF CFLAG:MASTER:장난 == 2
		RETURN 1
	ELSEIF TCVAR:124:만취 == 1
		RETURN 1
	;연모or연인
	ELSEIF TALENT:124:연인 || TALENT:124:연모
		;解除時
		IF !TEQUIP:바이브
			IF EXP:V경험 > 70
				PRINTFORML 「벌써 빼버리는 건가? 그럼 다음은 %마스터가% 직접..%UNICODE(0x2764)%」
			ELSE
				PRINTFORML 「큭, 으으.. 이거 다음은.. 어떻게 할 작정이야..?」
			ENDIF
		ELSE
			;처녀
			IF TALENT:처녀 == 1
				PRINTFORML 「처음은 %마스터가% 직접.. 아니, 벌써 늦은 건가.. 우우..」
			ELSEIF EXP:V경험 > 70
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「좀 더 엉망진창으로 해 줘, %CALLNAME:MASTER%..%UNICODE(0x2764)%」
					PRINTFORMW 오키나는 기대와 욕망이 가득한 눈으로 %마스터를% 바라보고 있다
				CASEELSE
					PRINTFORML 「흐아아.. 아앙, 굵엇%UNICODE(0x2764)%」
					PRINTFORMW 「아앗%UNICODE(0x2764)% 정말.. 홀딱 반할 것 같앗, 훗, 으읏%UNICODE(0x2764)%%UNICODE(0x2764)%」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「크으윽, 응, 조금 빡빡해..」
					PRINTFORMW 오키나는 괴로운 듯이 얼굴을 일그러뜨리고 있다
				CASEELSE
					PRINTFORML 「응.. 크읏.. 하아앗」
					PRINTFORMW 「이, 이거.. 너무 굵지 않아..?」
				ENDSELECT
			ENDIF
		ENDIF
	;それ以外
	ELSE
		;解除時
		IF !TEQUIP:바이브
			PRINTFORML 「으아앗!.. 하아, 하아, 하앗..」
		ELSE
			;처녀
			IF TALENT:처녀 == 1
				PRINTFORML 「처.. 처음이었는데...」
			ELSE
				PRINTFORML 「으읏, 으으읏! 이, 이거, 싫어...」
			ENDIF
		ENDIF
	ENDIF
ENDIF

RETURN 1
