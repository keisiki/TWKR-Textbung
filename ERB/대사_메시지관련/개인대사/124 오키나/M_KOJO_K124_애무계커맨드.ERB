			;사정した
			IF NOWEX:PLAYER:11
					PRINTFORML 「응, 츄븝.. 푸하앗%UNICODE(0x2764)%」
					PRINTFORML 「아, %CALLNAME:MASTER%.. 나, 나올 것 같은가..? 그럼, 그대로, 싸버려..%UNICODE(0x2764)%」
			ELSE
					PRINTFORML 「하아, 츄응, 으믑~~..」
					PRINTFORMW 「푸핫, 하아.. 하아.. %CALLNAME:MASTER%..%UNICODE(0x2764)%」
			ENDIF
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「츄웁, 츄븝%UNICODE(0x2764)%　하아앗, 하아앗..%UNICODE(0x2764)%」
					PRINTFORMW 오키나는 열중해서 %CALLNAME:MASTER%의 입술을 탐하고 있다
				CASEELSE
 					PRINTFORML 「츗..%UNICODE(0x2764)% 츄웁, 츄븝.. 푸하아%UNICODE(0x2764)%」
					PRINTFORMW 「이러고 있으면.. 배 속이 뜨거워지네..」
			ENDSELECT
		ENDIF
	;それ以外
	ELSE
		;V挿入orA挿入中
		IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
			;사정した
			IF NOWEX:PLAYER:11
					PRINTFORML 「으믑?! 응, 츄, 쮸웁, 푸하%UNICODE(0x2764)%」
					PRINTFORMW 「하아, 하아%UNICODE(0x2764)% 키스하면서는, 안 된다니까앗..%UNICODE(0x2764)%」
			ELSE
				PRINTFORML 「츗, 으믑..%UNICODE(0x2764)% 위도 아래도 바쁘구나.. 후훗」
			ENDIF
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「츄, 츄븝, 할짝.. 쮸릅, 츗」
					PRINTFORMW 「푸하앗, 여, 연인끼리도 아닌데, 이런.. 히윽%UNICODE(0x2764)%」
				CASEELSE
					PRINTFORML 「으읍..?! 하아, 싫, 엇..! ..하앗, 이 변태..!」
			ENDSELECT
			PRINTFORMDW 침입해 오는 %CALLNAME:MASTER%의 혀에 오키나는 몸을 뺐다
		ENDIF
	ENDIF
ENDIF
RETURN 1
