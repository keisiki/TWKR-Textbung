;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:C절정 NOWEX:V절정 NOWEX:B절정 NOWEX:A절정 NOWEX:M절정
;各部位の절정強度。まず0~3が割り当てられ、多重절정により最大16倍される。つまり単部位なら最大3、5重절정で最大48
;NOWEX:TotalEX
;多重절정補正を受ける前の各部位절정強度の合計値。1~15。このテンプレにおける分岐の基準。

;TEQUIP:50
;Vに誰の竿が入ってるか。TEQUIP:50 = MASTERであなたが挿入中となる。
;TEQUIP:51
;Aに誰の竿が入ってるか。TEQUIP:51 = MASTERであなたが挿入中となる。
;NOWEX:사정
;사정量、最大3。たとえばIF NOWEX:MASTER:사정 > 1だと、あなたが大量사정以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手とあなたが同時に절정した場合真となる。절정の部位は問わない。

;-------------------------------------------------
;절정時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K130
#DIM TotalEX
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	IF FLAG:70
		PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
	ELSE
		PRINTFORMW 「응읏%UNICODE(0x2665) *1%」
		PRINTFORMW 「저도 젖이 나오는거네요%UNICODE(0x2665) *1%」
	ENDIF
ENDIF
;-------------------------------------------------
;사정した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計절정数で分岐
;これだけではどの部位が절정したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	;-------------------------------------------------
	;五重절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:오중절정
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
		ELSE
			PRINTFORMW 「핫%UNICODE(0x2665) *1% 흣%UNICODE(0x2665) *1% 흣으응%UNICODE(0x2665) *1%」
			PRINTFORMW 야치에는 제대로 된 소리가 나오지 않는 교성을 지르며 엄청난 쾌락에 몸을 떨고 있다.
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:사중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
			ELSEIF RAND:5 == 0
				PRINTFORMW 「읏%UNICODE(0x2665) *1% 아앗%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「으응%UNICODE(0x2665) *1% 응앗%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「핫%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「읏%UNICODE(0x2665) *1% 아앙%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「응%UNICODE(0x2665) *1% 하앗%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:四重절정
			;C＆V＆A＆B절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆V＆A＆M절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆V＆B＆M절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆A＆B＆M절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆A＆B＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:삼중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
			ELSEIF RAND:5 == 0
				PRINTFORMW 「읏%UNICODE(0x2665) *1% 핫%UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「응앗%UNICODE(0x2665) *1% 간닷%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「흐응%UNICODE(0x2665) *1% 하앗%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「응%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1% 응힛%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「핫%UNICODE(0x2665) *1% 안됏%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;C＆V＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;B＆V＆M절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆V＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;C＆B＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;A＆B＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;C＆A＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;二重절정
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:이중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
			ELSEIF RAND:5 == 0
				PRINTFORMW 「읏%UNICODE(0x2665) *1% 읏%UNICODE(0x2665) *1% 좀더엇%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「응앗%UNICODE(0x2665) *1% 응힛%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「좀더엇%UNICODE(0x2665) *1% 가게해줘엇%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「읏%UNICODE(0x2665) *1% 응앗%UNICODE(0x2665) *1% 핫%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「시럿%UNICODE(0x2665) *1% 읏%UNICODE(0x2665) *1% 안댓%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:二重절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:C절정
		;時姦中
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「거깃%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「거깃%UNICODE(0x2665) *1% 좀더엇%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「클리로 가버렷%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「시럿%UNICODE(0x2665) *1% 그만%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:V절정
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「응읏%UNICODE(0x2665) *1% 간닷%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 깊엇%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 바보가 되버렷%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 거깃%UNICODE(0x2665) *1% 좀더엇%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「아앗%UNICODE(0x2665) *1% 응읏%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;時姦中
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「흐응%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「핫%UNICODE(0x2665) *1% 거깃%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「엉덩이가 약하다니%UNICODE(0x2665) *1% 그럴리갓%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「으응흐읏%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 애널로 간닷%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;時姦中
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 감도는 환부같은 곳보다 훨씬 높다구요%UNICODE(0x2665) *1%」
			PRINTFORMW 야치에는 왠지 자랑스러운 듯이 가슴으로 절정했다.
		ELSEIF RAND:3 == 0
			PRINTFORMW 「안됏%UNICODE(0x2665) *1% 민감하니까요%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응힛%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「좀더엇%UNICODE(0x2665) *1% 가슴%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;時姦中
		IF FLAG:70
			PRINTFORMW 얼어붙은 시간 속에서 야치에는 절정했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW （입으로도 기분이 좋아지는군요……%UNICODE(0x2665) *1%）
		ELSEIF RAND:4 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 응읏%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 으응읏%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「읏%UNICODE(0x2665) *1% 흐앗%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「좀더 기분 좋아지죠%UNICODE(0x2665) *1%
		ENDIF
	ENDIF
ENDIF

;-------------------------------------------------
;奴隷の睡眠절정時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B2_K130
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;睡眠분유
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;睡眠사정した
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;睡眠방뇨
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;睡眠절정
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
;合計절정数で分岐
;これだけではどの部位が절정したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 
		;中
		CASE 2 TO 4
			PRINTFORMW 
		;弱
		CASE 1
			PRINTFORMW 
	ENDSELECT
ENDIF


