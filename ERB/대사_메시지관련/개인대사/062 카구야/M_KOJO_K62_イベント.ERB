;==================================================
@M_KOJO_EVENT_K62_17(ARG,ARG:1)
;말주는 TARGET
;배타와 명기되어 있지 않은 물건은 상황에 따라서는 연속으로 표시되므로 주의
;ARG에 사정 상황
;ARG:1에 사정 상태
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------

IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;ARG=1~3은 배타
	;수수께끼의 쾌감
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;팬츠 분실
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각 입안 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;정액중독저정음
		IF ARG:1 ==  1
		;정액중독저토출
		ELSEIF ARG:1 == 2
		;정액중독저손으로 가린다
		ELSEIF ARG:1 == 3
		;정액중독고정음
		ELSEIF ARG:1 == 4
		;농후정액＆정액중독저토출
		ELSEIF ARG:1 == 5
		;농후정액＆정액중독고정음
		ELSEIF ARG:1 == 6
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각 안면 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;유치 소유＆정액경험 100 미만
		IF ARG:1 == 1
		;통상 닦아 잡기
		ELSEIF ARG:1 == 2
		;얼굴에 사정하기 정액 빨아냄
		ELSEIF ARG:1 == 3
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각 자위 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;닦아 잡기
		IF ARG:1 == 1
		;빨아냄
		ELSEIF ARG:1 == 2
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각 애널 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;A감각 2 이하
		IF ARG:1 == 1
		;그 이외
		ELSEIF ARG:1 == 2
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각 처녀 상실】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각질내 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;비처녀질내 사정 필 없음
		IF ARG:1 == 1
		;비처녀질내 사정 필 있어
		ELSEIF ARG:1 == 2
		;처녀질내 사정
		ELSEIF ARG:1 == 3
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;시간정지 해제 공통
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 1
