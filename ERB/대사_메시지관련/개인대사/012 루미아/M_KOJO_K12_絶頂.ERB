;~
;분기 만들기에 유용할 것 같은 변수 따위의 메모
;~
;NOWEX:C절정 NOWEX:V절정 NOWEX:B절정 NOWEX:A절정 NOWEX:M절정
;각 부위의절정 강도. 우선 0~3을 할당할 수 있어 다중절정에 의해 최대 16배 된다. 즉 단부위라면 최대 3, 5겹절정에서 최대 48
;NOWEX:TotalEX
;다중절정 보정을 받기 전의 각 부위절정 강도의 합계치. 1~15. 이 텐프레에 있어서의 분기의 기준.

;TEQUIP:50
;V에 누구의 장대가 들어가 있을까. TEQUIP:50 = MASTER로 당신이 삽입중이 된다.
;TEQUIP:51
;A에 누구의 장대가 들어가 있을까. TEQUIP:51 = MASTER로 당신이 삽입중이 된다.
;NOWEX:사정
;사정량, 최대 3. 예를 들어 IF NOWEX:MASTER:사정 > 1이라고, 당신이 대량 사정 이상 했을 경우가 된다.
;SYNCED_ORGASM(TARGET)
;상대와 당신이 동시에절정 했을 경우진이 된다.절정의 부위는 묻지 않는다.

;-------------------------------------------------
;절정시의 말
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K12
#DIM TotalEX
;-------------------------------------------------
;분유
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	IF TCVAR:발정
		IF TALENT:연모
			IF ABL:B감각 >=  10
				SELECTCASE RAND:4
					CASE 1
						CALL HPH_PRINT(" 「응읏…HPH 젖가슴 내는 것… 기분이 좋아 HPH」", "W")
					CASE 2
						CALL HPH_PRINT(" 「뷰─뷰─… 는 HPH 네에에 HPH」", "W")
					CASE 3
						CALL HPH_PRINT(" 「좀 더 짜…HPH 이봐요… 아직도 나오기 때문HPH」", "W")
					CASEELSE
						CALL HPH_PRINT(" 「기분 있고─의…HPH 좀 더 내는 것…HPH」", "W")
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL HPH_PRINT(" 「(이)나… 젖가슴 멈추지 않아…HPH」", "W")
					CASE 2
						CALL HPH_PRINT(" 「하아아…HPH 젖가슴 나와 버렸다…」", "W")
					CASEELSE
						CALL HPH_PRINT(" 「또, 나와 버리는…HPH」", "W")
				ENDSELECT
			ENDIF
		;ELSEIF TALENT:사모
		
		ELSE
			SELECTCASE RAND:3
				CASE 1
					CALL HPH_PRINT(" 「싫다아… 멈추지 않아…HPH」", "W")
				CASE 2
					CALL HPH_PRINT(" 「가슴이 근질근질 해…응, 응응HPH」", "W")
				CASEELSE
					CALL HPH_PRINT(" 「보지 말고… 부끄러워…HPH」", "W")
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:연모
			IF ABL:B감각 >=  10
				SELECTCASE RAND:5
					CASE 1
						PRINTFORMW 「응우… 젖가슴 내는 것, 조금 기분이 좋을지도…」
					CASE 2
						PRINTFORMW 「에헤헤… 이렇게 나와 버렸다. 마셔 봐?」
					CASE 3
						PRINTFORMW 「언제라도 어머니가 될 수 있다―. 후훗♪」
					CASE 4
						PRINTFORMW 「%CALLNAME:MASTER%의 아기, 갖고 싶구나?」
					CASEELSE
						PRINTFORMW 「아직 치고 있는 생각이 들어. 좀 더 짰으면 좋구나」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「조금 괴롭지만, %마스터가% 해 준다면…」
					CASE 2
						PRINTFORMW 「익숙해져 준다면 편하게 낼 수 있게 될까나…」
					CASEELSE
						PRINTFORMW 「어때? 흥분했어─? 후후…♪」
				ENDSELECT
			ENDIF
		;ELSEIF TALENT:사모
		
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「이제(벌써)… 빨리 멈추어 주지 않을까…」
				CASE 2
					PRINTFORMW 「가슴은 아프고 옷은 더러워지고… 좋은 일 없네요」
				CASEELSE
					PRINTFORMW 「이상한 눈으로 보지 말아요…」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;-------------------------------------------------
;사정했다
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	;구
	IF TCVAR:2 == 4
		IF TCVAR:발정
			IF TALENT:연모
				IF ABL:C감각 >=  10
					SELECTCASE RAND:5
						CASE 1
							CALL HPH_PRINT(" 「나의 정액을 좋아하는 사람? 좋아하겠지 HPH 지금 주기 때문에… 받아 HPH」", "W")
						CASE 2
							CALL HPH_PRINT(@" 「%CALLNAME:MASTER%의 구 좋아해 있고 HPH 좀 더 내고 싶어…HPH」", "W")
						CASE 3
							CALL HPH_PRINT(" 「들이마셔! 정액 들이마셔 HPH 아… 이제(벌써) 나와 버리는 HPH」", "W")
						CASE 4
							CALL HPH_PRINT(" 「입의 안에 정액 내 HPH 좋지요? 마셔! 전부 마셔 HPH」", "W")
						CASEELSE
							CALL HPH_PRINT(@" 「다시 한번 빨아 HPH %CALLNAME:MASTER%도 한 그릇 더 갖고 싶지요?」", "W")
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
						CASE 1
							CALL HPH_PRINT(@" 「이제 한계…! %CALLNAME:MASTER%의 구에서 가게 해 HPH」", "W")
						CASE 2
							CALL HPH_PRINT(" 「끝 쪽 날름날름 되면… 벌써 가 버려 HPH」", "W")
						CASEELSE
							CALL HPH_PRINT(" 「아아… 정액 꿀꺽꿀꺽 되고 있는HPH」", "W")
					ENDSELECT
				ENDIF
			ELSEIF TALENT:사모
					SELECTCASE RAND:3
						CASE 1
							CALL HPH_PRINT(" 「자지 기분 좋아서… 인내 할 수 없어…HPH」", "W")
						CASE 2
							CALL HPH_PRINT(" 「이대로라면 내 버려…? 정액 먹게 되어 버리는…HPH」", "W")
						CASEELSE
							CALL HPH_PRINT(" 「기분이 좋은 것… 멈출 수 없는 HPH」", "W")
					ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL HPH_PRINT(" 「이제(벌써) 나와 버리기 때문에, 입 놓아 HPH」", "W")
					CASE 2
						CALL HPH_PRINT(" 「지금은 간지 얼마 되지 않았기 때문에… 빨지 말아요…HPH」", "W")
					CASEELSE
						CALL HPH_PRINT(" 「우우… 인내 할 수 없었어요 HPH」", "W")
				ENDSELECT
			ENDIF
		ELSE
			;IF TALENT:연모
			;	SELECTCASE RAND:5
			;		CASE 1
			;			PRINTFORMW 「」
			;		CASE 2
			;			PRINTFORMW 「」
			;		CASE 3
			;			PRINTFORMW 「」
			;		CASE 4
			;			PRINTFORMW 「」
			;		CASEELSE
			;			PRINTFORMW 「」
			;	ENDSELECT
			IF TALENT:사모
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「맛있지 않지요? 무리하지 않아도 괜찮아」
					CASE 2
						PRINTFORMW 「입에 내 버린… 미안해요」
					CASEELSE
						PRINTFORMW 「하아… 자지, 굉장했다…」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORMW 「그것, 맛있지 않을 것입니다?」
					CASE 2
						PRINTFORMW 「사정까지 시키다는… 악취미」
					CASEELSE
						PRINTFORMW 「정액 마시다는… 변태」
				ENDSELECT
			ENDIF
		ENDIF
	ELSEIF TCVAR:2 == 1
	;PRINTFORMW 질
	
	ELSE
	;PRINTFORMW 외
	
	ENDIF
	
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
;합계절정수로 분기
;이것만으로는 어느 부위가절정 했는가는 구별되지 않기 때문에, 나누어 쓰고 싶으면 임의로 분기가 가필되었고.
IF LOCAL
	SELECTCASE NOWEX:TotalEX
		;강
		CASE 5 TO 15
			PRINTFORMW 
		;안
		CASE 2 TO 4
			PRINTFORMW 
		;약
		CASE 1
			PRINTFORMW 
	ENDSELECT
ENDIF

