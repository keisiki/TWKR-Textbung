								CALL HPH_PRINT(" 「이제(벌써) 갈 것 같겠지 HPH 좋아, 이대로 내 HPH 정액 내 HPH」", "L")
								PRINTFORML 
								CALL ASK_YN("저항 한다", "하는 대로")
								
								IF RESULT == 0
									PRINTFORML 
									IF CFLAG:1001 == 2
										PRINTFORML %마스터는% 어떻게든 허리를 당기려고 하지만, 루미아는 더욱 더 강하게 매달려 온다
										CALL HPH_PRINT(" 「놓치지 않는다고, 말한 것이겠지? 포기해 가 버려라 HPH」", "L")
									ELSE
