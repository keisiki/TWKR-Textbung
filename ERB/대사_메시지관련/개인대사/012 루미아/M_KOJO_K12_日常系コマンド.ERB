									CALL HPH_PRINT(" 「푸핫! 굉장한 나왔군요 HPH 마시지 못했던 원」", "L")
									PRINTFORMW 루미아는 %CALLNAME:MASTER%의 페니스에 흘러넘친 정액을 빨아내고 있다…
									PRINTFORML 
									CALL HPH_PRINT(@" 「잘 먹었어요 HPH 또 잘 부탁해, %CALLNAME:MASTER%HPH」", "W")
									EXP:12:펠라티오경험 += 3
									EXP:12:정액경험 += 2
									EXP:12:정음경험 += 2
									EXP:MASTER:사정경험 += 2
									BASE:MASTER:발기 = 0
						ENDIF
					ENDIF
				ENDIF
			ELSE
				SELECTCASE RAND:3
