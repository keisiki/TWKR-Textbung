					CASEELSE
						CALL HPH_PRINT(" 「킥킥… 커지고 있어 HPH 다른 사람에게 발각되면, 대단하다 HPH」", "L")
						PRINTFORMW 루미아는 추잡한 미소를 띄워 %CALLNAME:MASTER%의 사타구니를 더듬고 있다…
				ENDSELECT
			ENDIF
		ELSEIF CFLAG:12:기정사실 >=  1
			SELECTCASE RAND:3
				CASE 1
					CALL HPH_PRINT(" 「이대로 자면… 습격당해 버릴까나……HPH」", "W")
				CASE 2
