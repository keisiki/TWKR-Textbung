					ELSE
						PRINTFORML 루미아는 노출로 한 %CALLNAME:MASTER%의 육봉에 혀를 기게 하고 있다…
						SIF BASE:MASTER:발기 < 1000
							BASE:MASTER:발기 = 1000
						PRINTFORML 
						CALL ASK_YN("저항 한다", "하는 대로")
						
						IF RESULT == 0
							PRINTFORML 
							PRINTFORML %마스터는% 루미아를 떼어냈다
							CALL HPH_PRINT(" 「여기서 끝까지 할 수도 없는 거네. 유감이지만…HPH」", "W")
						ELSEIF RESULT == 1
							PRINTFORML 
							PRINTFORML %마스터는% 루미아의 애무에 몸을 맡기고 있다…
							CALL HPH_PRINT(" 「아는 HPH 펄떡펄떡이구나―… 앙… 응뭇, 응HPH응HPH」", "L")
							PRINTFORML 루미아는 %CALLNAME:MASTER%의 페니스를 빨아, 입술로 훑고 있다…
							PRINTFORML 
							CALL ASK_YN("저항 한다", "하는 대로")
							
							IF RESULT == 0
								PRINTFORML 
								PRINTFORML %마스터는% 루미아를 떼어내려고 했다…
								PRINTFORML 그러나 루미아가 허리를 잡아 떼어 놓지 않는다
								CALL HPH_PRINT(" 「안돼HPH 이제 놓치지 않는……HPH」", "W")
								CFLAG:1001 += 1
							ELSEIF RESULT == 1
								PRINTFORML 
								PRINTFORML %마스터는% 루미아의 입안에 열중하고 있다…
								CALL HPH_PRINT(@" 「HPH %CALLNAME:MASTER%의 자지, 맛있어…HPH」", "W")
							ENDIF
								PRINTFORML 
