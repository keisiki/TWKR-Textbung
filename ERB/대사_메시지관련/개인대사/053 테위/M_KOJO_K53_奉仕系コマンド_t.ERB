;==================================================
@M_KOJO_MESSAGE_COM_K53_80
CALL M_KOJO_MESSAGE_COM_K53_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K53_80_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;24시간 이상 목욕하지 않은 경우 검문당한다
		IF DAY * 1440 + TIME - EX:MASTER:전회입욕시각 > 1440
			PRINTFORMW 「구!조금 이것, 분명하게 씻어? 이런 건 잡게 하다는 정말 최저―」
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;기본 세트
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSE
		CALL K53_AUTO_PVOICE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

