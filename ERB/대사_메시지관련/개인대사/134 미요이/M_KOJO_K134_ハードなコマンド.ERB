;140,이라마치오
;==================================================
@M_KOJO_MESSAGE_COM_K134_140
CALL M_KOJO_MESSAGE_COM_K134_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K134_140_1
;-------------------------------------------------
;기입체크（=0, 非表示, 1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「응! 으응~~~!」
		RETURN 1
	ENDIF
	;기본세트
	;연모
	IF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「으응%UNICODE(0x2665) *1%」
			PRINTFORMW 미요이는 %CALLNAME:MASTER%의 음경에 박히며 신음하고 있다.
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1%응%UNICODE(0x2665) *1%」
			PRINTFORMW 미요이는 %CALLNAME:MASTER%의 음경에 박히며 신음하고 있다.
		ELSE
			PRINTFORMW 「으으읏%UNICODE(0x2665) *1%으응%UNICODE(0x2665) *1%」
			PRINTFORMW 미요이는 %CALLNAME:MASTER%의 음경에 박히며 신음하고 있다.
		ENDIF
	;그 이외
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「으응~!」
			PRINTFORMW 미요이는 %CALLNAME:MASTER%의 음경에 박히며 신음하고 있다.
		ELSEIF RAND:2 == 0
			PRINTFORMW 「으으응~~~!!!」
			PRINTFORMW 미요이는 %CALLNAME:MASTER%의 음경에 박히며 신음하고 있다.
		ELSE
			PRINTFORMW 「으응!!!」
			PRINTFORMW 미요이는 %CALLNAME:MASTER%의 음경에 박히며 신음하고 있다.
		ENDIF
	ENDIF
ENDIF
RETURN 1

