;==================================================
@M_KOJO_MESSAGE_SCOM_K134_1
CALL M_KOJO_MESSAGE_SCOM_K134_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K134_1_1
;-------------------------------------------------
;기입체크 (=0, 비표시、1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「누가 먼저 가게 하는지 승부하죠%UNICODE(0x2665) *1%」
		RETURN 1
	ENDIF
	;기본세트
	;시간(時姦)중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		;정액중독4以上
		IF ABL:정액중독 > 4
			IF RAND:3 == 0
				PRINTFORMW 「움?우우움~%UNICODE(0x2665) *1%」
				PRINTFORMW 미요이는 %CALLNAME:MASTER%의 사정을 예감하고 혀놀림을 능숙하게 하며 그것을 재촉하고 있다.
				PRINTFORMW （조금만 더 있으면 맛있는 몽글몽글 정액 많이 나올 것 같아……%UNICODE(0x2665) *1%）
			ELSEIF RAND:2 == 0
				PRINTFORMW 「응%UNICODE(0x2665) *1% 응츄우%UNICODE(0x2665) *1%」
				PRINTFORMW 「참을 수 없는 몽글몽글한 자지즙, 미요이의 입안에 가득 사정하세요오%UNICODE(0x2665) *1%」
				PRINTFORMW 미요이는 %CALLNAME:MASTER%의 사정을 예감하고 혀놀림을 능숙하게 하며 그것을 재촉하고 있다.
			ELSE
				PRINTFORMW 「으으읏%UNICODE(0x2665) *1%자지 딱딱해졌네요오%UNICODE(0x2665) *1%」
				PRINTFORMW 미요이는 %CALLNAME:MASTER%의 사정을 예감하고 혀놀림을 능숙하게 하며 그것을 재촉하고 있다.
				PRINTFORMW （진한 자지즙 빨리 마시고 싶어%UNICODE(0x2665) *1%）
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「으움」
				PRINTFORMW 미요이는 %CALLNAME:MASTER%의 사정을 예감하고 혀놀림을 능숙하게 하며 그것을 재촉하고 있다.
				PRINTFORMW 「이제 사정해버릴 정도로 자지 딱딱해졌네요%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「으응%UNICODE(0x2665) *1%우움%UNICODE(0x2665) *1%」
				PRINTFORMW 미요이는 %CALLNAME:MASTER%의 사정을 예감하고 혀놀림을 능숙하게 하며 그것을 재촉하고 있다.
			ELSE
				PRINTFORMW 「으으읏%UNICODE(0x2665) *1%이제 쌀 것 같으신가요오%UNICODE(0x2665) *1%」
				PRINTFORMW 미요이는 %CALLNAME:MASTER%의 사정을 예감하고 혀놀림을 능숙하게 하며 그것을 재촉하고 있다.
			ENDIF
		ENDIF
	;혀3以上
	ELSEIF ABL:혀 > 2
		;정액중독4以上
		IF ABL:정액중독 > 4
			IF RAND:3 == 0
				PRINTFORMW 「으응%UNICODE(0x2665) *1%우우움%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「으응%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORMW 「크고 단단하고 우람한 자지네요. 입에 무는 것도 힘들어요오%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「음츄우%UNICODE(0x2665) *1% 낼름%UNICODE(0x2665) *1%」
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「응~%UNICODE(0x2665) *1%」
				PRINTFORMW 「미요이의 입 기분 좋으세요?%UNICODE(0x2665) *1%더 기분 좋게 해드릴게요오%UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「으응%UNICODE(0x2665) *1%미요이한테 맡겨주세요%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「으으읏%UNICODE(0x2665) *1%자지씨 잔뜩 기분 좋게 해드릴게요%UNICODE(0x2665) *1%」
			ENDIF
		ENDIF
	;그 이외
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1% 츄루%UNICODE(0x2665) *1%」
			PRINTFORMW 「이렇게 자지 움찔거려서 귀엽네요%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「낼름%UNICODE(0x2665) *1%으응%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「으응……%UNICODE(0x2665) *1%」
			PRINTFORMW 미요이는 어딘가 어색하게 음경을 입에 머금었다.
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K134_{SELECTCOM}_1
RETURN 1

