;==================================================
;12,パイズリフェラ
;==================================================
@M_KOJO_MESSAGE_SCOM_K65_12
CALL M_KOJO_MESSAGE_SCOM_K65_12_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K65_12_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:발정
			PRINTFORML 「…입과 가슴으로…？」
			CALL HPH_PRINT,@"「아와, %CALLNAME:MASTER%씨 엣찌…HPH」","L"
			PRINTFORML 모미지는 %CALLNAME:MASTER%의 반응을 눈을 치켜뜨고서, 천천히 페니스를 가슴 사이에 넣었다.
			PRINTFORMW 매끄러운 피부가 민감한 부위에 푸릉푸릉 얽혀 있어 실로 좋은 경치이다.
			CALL HPH_PRINT,@"「……HPH%K65_AEGI("控えめ",4)%」","L"
			PRINTFORMW %CALLNAME:MASTER%의 반응에 기분이 좋아진 모미지는 고혹적인 미소를 띄우며, 귀두에 키스를 반복했다…
		ELSE
			PRINTFORML 「…끼운 상태로, 핥는건가요…？」
			PRINTFORMW 「음, 알겠습니다, 해보도록 할게요…」
			PRINTFORML 모미지는 %CALLNAME:MASTER%의 얼굴을 들여다 보며, 천천히 페니스를 가슴 사이에 넣었다.
			PRINTFORMW 매끄러운 피부가 민감한 부위에 푸릉푸릉 얽혀 있어 실로 좋은 경치이다.
			CALL HPH_PRINT,@"「응…이걸로, 괜찮은 것 같네요…%K65_AEGI("控えめ",3)%」","L"
			PRINTFORMW 모미지는 %CALLNAME:MASTER%의 반응을 살피면서, 조심스럽게 귀두에 입을 맞추기 시작했다…
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;技量不足
	ELSEIF ABL:TARGET:가슴 < 2
		IF TCVAR:발정
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「응…읏, 읏？응무으…」
					PRINTFORML 모미지는 %CALLNAME:MASTER%의 귀두에 달라붙으려 하지만,
					PRINTFORMW 가슴 사이로 튀어나와 있는 그것을 좀처럼 잡지 못하고, 불만스럽게 입술을 삐죽거리고 있다…
				CASE 1
					PRINTFORML 「응슈웃, 쿳, 후우읏…」
					PRINTFORML 모미지는 안타깝다는 듯이 페니스를 가슴으로 문지르지만, 그녀가 초조해 할 수록 페니스의 취급이 조잡해지고,
					PRINTFORMW 오히려 %마스터는% 절정으로부터 멀어져 버린다…
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「읏차,…응, 츄, 츄웃……」
					PRINTFORML 모미지는 가슴을 페니스에 문지르거나, 귀두에 키스 해 보기도 하고 노력을 하고 있다.
					PRINTFORMW 미안할 정도로 열심히 하고 있는 것은 알지만…
				CASE 1
					PRINTFORMW 「이거, 꽤 어렵네요……」
			ENDSELECT
		ENDIF
	;発情
	ELSEIF TCVAR:발정
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:정액중독 > 3 && RAND:2
				SELECTCASE RAND:3
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「읏…, 쥬루룻HPH%K65_AEGI("フェラ音",4)%」","L"
						CALL HPH_PRINT,@"（쿠룻…HPH%CALLNAME:MASTER%씨의 %TEXTR("끈적끈적한/에로에로한/")%%TEXTR("정액/정액/")%HPH）","W"
					CASE 2
						CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
						PRINTFORML %CALLNAME:MASTER%의 절정을 예감하고, 모미지는 가슴의 압박을 더욱 강하게 했다.
						PRINTFORMW 탱글탱글한 가슴이 음경에 착 달라붙어, 도망칠 장소가 없는 쾌락이 찾아왔다.
						CALL HPH_PRINT,@"「훗HPH후ー읏HPH%K65_AEGI("フェラ音",3)%…응읏…HPH」","L"
						PRINTFORML 모미지는 혀를 내밀고, 정낭에서 올라오는 것을 파내듯 방울방울을 핥아 올렸다.
						PRINTFORMW %마스터는% 격렬한 쾌감에 몸을 굳히면서, 모미지의 구강에 잔뜩 뜨거운 정액을 내뱉었다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「앗…HPH%CALLNAME:MASTER%씨, 갈 것 같으신가요…？」","L"
						CALL HPH_PRINT,@"「…자지에 츄ー해 드릴 테니까, 이대로 가슴으로 가 주세요…HPH」","W"
				ENDSELECT
			ENDIF
		;勃起しきってない
		ELSEIF BASE:MASTER:발기 < 1000
			;SELECTCASE RAND:2
			;	CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
			;	CASE 1
			;		
			;ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_82_1
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					PRINTFORMW 부드러운 유방에 페니스가 뿌리부터 쓸어올려져, 장대 끝을 향해 강렬한 쾌감이 솟아오른다.
					CALL HPH_PRINT,@"「훗HPH 응앗…HPH %K65_AEGI("フェラ音")%」","L"
					PRINTFORMW 올라오는 쾌락은, 사탕을 빨고 녹이는듯한 혀놀림으로 귀두 전체로 퍼져나간다.
					CALL HPH_PRINT,@"「응읏HPH 아핫…자지의 냄새가 심해서, 머리가 멍해져요…HPH」","L"
					PRINTFORMW 열띤 모미지의 목소리에, %CALLNAME:MASTER%의 사고까지도 녹아들어가는 것 같았다…
				CASE 2
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
					PRINTFORMW 모미지는 기분 좋은 소리를 내며, 가슴의 골짜기로부터 들여다보는 귀두로 리듬감 있게 입맞춤을 반복하고 있다…
				CASE 3
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					CALL HPH_PRINT,@"「하아HPH 읏…HPH%K65_AEGI("フェラ音",3)%」","L"
					CALL HPH_PRINT,@"「응, %CALLNAME:MASTER%씨…%K65_AEGI("控えめ",2)%…좀 더, 정액을, 마시게 해주세요…HPH」","L"
			ENDSELECT
		ENDIF
	;非発情
	ELSE
		;これでMASTERが射精する
		IF NOWEX:MASTER:11
			IF ABL:정액중독 > 3 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
						PRINTFORML 착정에 열중하고 있는 듯, 모미지는 말 없이 %CALLNAME:MASTER%의 페니스에 달라붙어 있다.
						PRINTFORMW 흘러내린 타액으로 미끄러워진 말랑말랑한 피부가, 장대 뿌리부터 끝부분까지 끈적끈적 얽혀 기분이 좋다.
						PRINTFORML 부풀어 오르는 쾌락이 정점을 향하는 순간, 모미지는 빵빵하게 부풀어 오른 그것을 따뜻한 구강 안 깊숙이 맞이했다…
						CALL HPH_PRINT,@"「%K65_AEGI("フェラ音",3)%…읏…응읏…HPH」","W"
						PRINTFORMW 한계까지 짙어진 모취를 입에 가득 머금은 모미지는, 스스로도 절정한 것처럼 부들부들 몸을 떨었다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
					CASE 1
						PRINTFORML 「…아, 갈 것 같으시네요, %CALLNAME:MASTER%씨…」
						CALL HPH_PRINT,@"「그럼, 이대로 가슴으로 문지르면서, 핥아드릴게요…HPH」","L" 
						CALL HPH_PRINT,@"「영, 차앗…HPH%K65_AEGI("フェラ音")%」","W"
					CASE 2
						PRINTFORML 「…응, 푸웃…가슴, 기분 좋으신가요…？」
						CALL HPH_PRINT,@"「후후, 참지 않아도 되니까, 잔뜩 사정해 주세요…HPH」","L" 
						CALL HPH_PRINT,@"「츄웃…%K65_AEGI("フェラ音")%…HPH」","W"
				ENDSELECT
			ENDIF
		;勃起しきってない
		ELSEIF BASE:MASTER:발기 < 1000
			;SELECTCASE RAND:2
			;	CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_{SELECTCOM}_1
			;	CASE 1
			;		
			;ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
					CALLFORM M_KOJO_MESSAGE_COM_K65_82_1
				CASE 1
					CALL HPH_PRINT,@"「%K65_AEGI("フェラ音")%」","L"
					PRINTFORMW （%CALLNAME:MASTER%씨, 기분 좋아 보여…）
					;[献身的]だからね。セフレだろうともっと気持ちよくなってほしいはずだよね。
					SIF RAND:2
						CALL HPH_PRINT,@"（좀 더, 기뻐해 주면 좋겠어…HPH）","W"
				CASE 2
					CALL HPH_PRINT,@"「%K65_AEGI("控えめ")%」","L"
					PRINTFORMW 모미지는 기분 좋은 소리를 내며, 가슴의 골짜기로부터 들여다보는 귀두로 리듬감 있게 입맞춤을 반복하고 있다…
				CASE 3
					CALL HPH_PRINT,@"「와, 아…가슴 사이로 움찔움찔 해서……우뭇…%K65_AEGI("控えめ",2)%」","L" 
					PRINTFORML 모미지는 우뚝 솟은 페니스의 끝을 입슬로 잡고, 육대를 정성스럽게 가슴으로 쓸어 올렸다…
					CALL HPH_PRINT,@"（우우…이렇게 할 때마다, %CALLNAME:MASTER%씨의 냄새가 강해져서…미쳐버릴 것 같아…HPH）","W"
			ENDSELECT
		ENDIF
	RETURN 1
	ENDIF
ENDIF
