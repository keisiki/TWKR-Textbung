;==================================================
@M_KOJO_MESSAGE_COM_K65_315
CALL M_KOJO_MESSAGE_COM_K65_315_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K65_315_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:수면
	ELSE
		IF CFLAG:우후후 == 2
			;うふふ中かつ発情
			IF TCVAR:발정
				SELECTCASE TCVAR:20
					;70=クンニ強制
					CASE 70
						CALL HPH_PRINT,@"「아후읏, 하아앗…HPH%CALLNAME:MASTER%씨의 혀, 기분 죠아아아……HPH」", "L"
						PRINTFORMW 민감한 부분을 핥아내리면, 모미지는 좀 더 애무를 요구하며 %CALLNAME:MASTER%의 얼굴에 비소를 들이밀었다…
					CASEELSE
						SELECTCASE RAND:2
							CASE 0
								CALL HPH_PRINT,@"「꺄앗, 응, 히얏HPH %CALLNAME:MASTER%씨잇HPH 기분 조아앗HPH」", "L"
								PRINTFORMW 약점인 음핵을 간질이면, 모미지는 유열의 표정을 짓고 있다…
							CASE 1
								CALL HPH_PRINT,@"「～～～흐웃HPH 거기잇HPH 조아해요오HPH」", "W"
						ENDSELECT
				ENDSELECT
			;うふふ中かつ非発情
			ELSE
				SELECTCASE TCVAR:20
					;70=クンニ強制
					CASE 70
						PRINTFORML 「아후웃, 응읏…！%CALLNAME:MASTER%씨의, 혀, 가아앗……！」
						PRINTFORMW 민감한 부분을 핥아내리자, 모미지는 기쁜 듯이 꼬리를 흔들었다…
					CASEELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「히얏！거기는, 예민하다고요…」
								CALL HPH_PRINT,@"「꺗, 큐읏, 응, 핫！안돼, 안돼요, 그렇게 괴롭히면, 앗HPH」", "W"
							CASE 1
								PRINTFORMW 「～～～읏！하앗…！거기 만져지면, 저느은…！」
						ENDSELECT
				ENDSELECT
			ENDIF
		;うふふ中でない
		ELSE
			;非うふふ・発情
			IF TCVAR:발정
				;ABL:C감각(4未満=くすぐったい 6未満=ちょっといいかも 6以上=カイカン)
				IF ABL:C감각 < 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「아, 응! 거기, 민감하니까앗…！조금, 상냥하게엣…」
						CASE 1
							CALL HPH_PRINT,@"「꺗, 아얏, 너무 강해요오…HPH」", "W"
					ENDSELECT
				ELSEIF ABL:C감각 < 6
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「꺄응…！거기, 기분 좋아요오…HPH」", "W"
						CASE 1
							CALL HPH_PRINT,@"「히야앗！그렇…게 만지작거리면, 금방 가버려엇…HPH」", "W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「꺄응…！거기, 기분 좋아요오…HPH 좀 더, 좀 더HPH」", "W"
						CASE 1
							CALL HPH_PRINT,@"「하우웃…！이거, 너무 기분 좋아요…읏HPH 허리가앗, 멋대로오…HPH」", "L"
							PRINTFORMW 발정한 모미지는 허리를 들썩이며, %CALLNAME:MASTER%의 손끝으로 민감한 음핵을 문지르도록 하고 있다…
					ENDSELECT
				ENDIF
			ELSE
				;非うふふ・非発情
				;ABL:C감각(4未満=くすぐったい 6未満=ちょっといいかも 6以上=カイカン)
				IF ABL:C감각 < 4
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「안된단, 말이에요옷…후앗, 응읏…！」
						CASE 1
							PRINTFORMW 「거깃, 만지작거리면 싫다고요…！햣, 앗, 앙」
					ENDSELECT
				ELSEIF ABL:C감각 < 6
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「안된단, 말이에요…！그런 곳을, 문지르면, 저느은…HPH」", "W"
						CASE 1
							CALL HPH_PRINT,@"「거기, 민감하니까요오…！앗, 쿠읏, 후앗, 우응…HPH」", "W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「안되는, 데에…HPH 앙, 하읏, 쿠흣HPH」", "L"
							PRINTFORMW %CALLNAME:MASTER%의 손가락 끝이 클리토스를 어루만질 때마다, 모미지는 강렬한 쾌락에 온 몸을 움찔거리고 있다…
						CASE 1
							CALL HPH_PRINT,@"「거기, 만지면…！앗, 응읏…갖고, 싶어진다고요오…HPH」", "W"
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
