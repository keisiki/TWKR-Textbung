					CASE 1
						CALL HPH_PRINT,@"「응, 츄…%K65_AEGI("控えめ,2")%…%K65_AEGI(",2")%」","L"
						CALL HPH_PRINT,@"「%CALLNAME:MASTER%씨, 좀 더…HPH%K65_AEGI("控えめ,4")%」","W"
				ENDSELECT
			ENDIF
		ELSE
			IF TALENT:연모 && RAND:2
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI(,3)%」", "L"
						PRINTFORML 「읏…저기, 너무…보지 말아주세요…」
						PRINTFORMW 「분명 지금 저는, 굉장히 칠칠치 못한 얼굴을 하고 있으니까요…」
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "L"
						PRINTFORML 「후후. 키스는\@ EXP:키스경험>20 ? 몇 번이나 # 몇 번이고 \@하고 있을텐데, 역시 항상 두근두근거리네요」
						CALL HPH_PRINT,@"「……네에.한 번…더…응, %K65_AEGI("控えめ",2)%」","L"
						PRINTFORMW %CALLNAME:MASTER%와 모미지는 부둥켜안고, 서로의 고동을 느끼며 천천히 키스를 했다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「%K65_AEGI()%」", "W"
					CASE 1
						CALL HPH_PRINT,@"「%K65_AEGI(,2)%…읏…」", "L"
						PRINTFORMW 입맞춤을 주고 받으며 손가락에 닿는 좋은 머리카락을 쓰다듬자, 모미지는 \@ TALENT:연모 && RAND:2 ? 녹아내린 눈으로 응시해 왔다 # 기쁜 듯이 미소를 지었다 \@.
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


