			PRINTFORML 「그렇게나 했는데, 아직도 부족하신가요…？」
			CALL HPH_PRINT,@"（우우, %CALLNAME:MASTER%씨는 역시 대단하시구나아...HPH）", "W"
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		PRINTFORML
