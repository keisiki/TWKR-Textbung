@M_KOJO_MESSAGE_COM_K65_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;怒り
	IF CFLAG:빡침 > 0
		PRINTFORML 「……그렇습니까」
