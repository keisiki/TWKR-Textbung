			CASE 0
				CALL HPH_PRINT,@"「하앗…！우, 앗……나온다아앗……HPH」","L"
				PRINTFORMW 격렬한 성교 끝에, %CALLNAME:MASTER%와 모미지는 동시에 절정에 이르렀다.
				PRINTFORML %CALLNAME:MASTER%의 페니스는 한 여자를 타락시킨 것에 대해 기쁨에 떨듯
				PRINTFORMW 움찔움찔 떨면서 모미지의 질내에 정액을 계속 뿜어내고, 그녀의 자궁을 자신의 색으로 물들인다……
				SIF ABL:65:질내사정중독 > 3
					PRINTFORML （뱃속에서, %CALLNAME:MASTER%씨의 뜨거운 것이 촤악 퍼져나가서, 어쩐지 너무 행복해……）
				PRINTFORMW （이제 나, %CALLNAME:MASTER%씨가 없으면 살아갈 수 없어어……）
			CASE 1
				PRINTFORML 「쿳, 하아아아앗…！！」
				PRINTFORML 절정에 이른 모미지가 등을 힘껏 활처럼 젖히는 동시에, 미끈미끈한 질벽이 크게 요동쳤다.
				PRINTFORMW 그 달콤한 자극에 몸을 맡기면서, %마스터는% 모미지의 자궁구로 귀두를 밀어 붙였다.
				PRINTFORML 「아아～～～앗！！…뜨거…웟…！훗, 쿠, 하아아앙！！」
				PRINTFORML 사정 중에도 %마스터는% 허리를 꾸욱꾸욱 밀어 넣어, 모미지의 질내에 정액을 펴 발라 간다.
				CALL HPH_PRINT,@"「아우우우웃…휘저어져서엇…후우, 우아아, 앗……HPH」","W"
			CASE 2
				CALL HPH_PRINT,@"「후아아아아앗…！읏……뜨거워어……HPH」","L"
				PRINTFORMW %마스터는% 거듭되는 절정에 거칠어진 모미지의 자궁에 가차 없이 뜨거운 정액을 토해냈다.
				CALL HPH_PRINT,@"「핫, 하아, 하아ー앗…HPH」","L"
				PRINTFORM 숨이 끊어질 듯한 기색이면서도, 모미지는
					SIF EQUIP:기모노 || EQUIP:원피스
						PRINTFORM 착의 넘어로
				PRINTFORMW 하얀 아랫배를 어루만지며\@ ABL:65:질내사정중독 > 3 ? 행복한 표정을 짓고 있다… # 만족스러운 듯이 한숨을 흘리고 있다… \@
		ENDSELECT
	ELSEIF TALENT:연모
		;恋慕かつ発情
		IF TCVAR:발정
			;何度も射精
			IF TCVAR:350 > 3 && RAND:2 == 0
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「～～～～～흣HPH」","L"
						CALL HPH_PRINT,@"「좀 더…HPH 좀 더, 와주세요오…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「또, 왔다앗……HPH」","L"
						PRINTFORM 「전부, 저언부 안에 부어
							SIF !TCVAR:피임약
								PRINTFORM , 임신하게 해
						CALL HPH_PRINT,@"주셨어…HPH」","W"
				ENDSELECT
			;キスもしてる
			ELSEIF CUP:TARGET:쾌M > 0 && RAND:2 == 0
			;	SELECTCASE RAND:2
			;		CASE 0
						CALL HPH_PRINT,@"「%TEXTR("온닷, 우/하아, 응잇/")%…HPH %CALLNAME:MASTER%씨잇…！」","L"
						PRINTFORML 사정의 순간, 모미지는 당신의 입술을 물어 뜯을까 싶을 정도로 격렬하게 입을 맞추며 혀를 얽혀 왔다.
						CALL HPH_PRINT,@"「%TEXTR("하훗/응무읏/")%…%K65_AEGI(,3)%…훗, 응으～ㅅHPH」","L"
						PRINTFORMW 모미지는 꿀럭꿀럭 쏟아지고 있는 백탁의 맥류에 눈을 가늘게 뜨면서, 더욱 %CALLNAME:MASTER%(을)를 요구하며 열렬한 키스를 계속하고 있다…
			;		CASE 1
						
			;	ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「아핫…HPH %CALLNAME:MASTER%씨의 정액, 나왔구나…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「응읏…HPH 뱃속에서, 꾸ー욱 하고 있는 거 알고 있어요…」","L"
						CALL HPH_PRINT,@"「에헤헤, 기분 좋으셨나요? %CALLNAME:MASTER%씨…HPH」","W"
				ENDSELECT
			ENDIF
		;恋慕かつ非発情
		ELSE
			;何度も射精
			IF TCVAR:350 > 3 && RAND:2 == 0
				SELECTCASE RAND:2
					CASE 0
						CALL HPH_PRINT,@"「～～～～～흣HPH」","L"
						CALL HPH_PRINT,@"「굉장, 하네요…아직도, 이렇게나, 잔뜩…HPH」","W"
					CASE 1
						CALL HPH_PRINT,@"「또, 왔다아……HPH」","L"
						PRINTFORMW 「이거, 정말로 잔뜩 나오네요…」
				ENDSELECT
			;キスもしてる
			ELSEIF CUP:TARGET:쾌M > 0 && RAND:2 == 0
			;	SELECTCASE RAND:2
			;		CASE 0
						CALL HPH_PRINT,@"「%TEXTR("하붓/응무웃/")%…응, 응～읏HPH」","L"
						PRINTFORML %마스터는% 모미지의 연약한 몸을 껴안고 연분홍 입술에 달라붙었다.
						PRINTFORML 계속 흔들리던 허리의 움직임이 딱 멈추면서, 마지막 순간을 맞이했다.
						CALL HPH_PRINT,@"「읏, 후…%K65_AEGI(,3)%…후우…읏HPH」","L"
						PRINTFORMW 잔뜩 정액을 쏟아내고 나서 입을 떼자, 두 사람의 입가는 은빛 실로 연결되어 있었다…
			;		CASE 1
						
			;	ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「하앗~…안에서, 뜨거운 것이…」
					CASE 1
						PRINTFORMW 「하앗~…아…%CALLNAME:MASTER%씨, 정말 좋아해요…」
				ENDSELECT
			ENDIF
		ENDIF
	ELSE
		PRINTFORMW 「아, 나카에…」
	ENDIF
	RETURN 1
	;IF 充填率
ENDIF
RETURN 1

