;### VA공통 기승위 말용 함수 ################
@DIALOGUE_K78_VA_COWGIRL, ARGS

IF K78_BASIC_CONDITION_IN_MATING()
	$VA_COWGIRL_LOOP
	IF TALENT:연모 || TALENT:애욕
		SELECTCASE RAND:9
		CASE 1
			IF !TEQUIP:밧줄 && !TEQUIP:구속 && !TEQUIP:계속키스 && ( ( ABL:욕망 >= 8 && ABL:노출벽 - TALENT:수치심 * 2 >= 4 ) || ( !( CFLAG:1007 && K78_ARMPIT_EXPOSURE() ) && !K78_SWEATY_UNDERARMS() && ABL:욕망 >= 4 && ABL:노출벽 - TALENT:수치심 >= 1 ) )
				IF ABL:욕망 >= 8 && ABL:노출벽 - TALENT:수치심 * 2 >= 4
					IF CFLAG:1007 && K78_ARMPIT_EXPOSURE()
						PRINTFORMDL %타겟은% 양손을 머리의 뒤에서 짜, %K78_ARMPIT_HAIR_MODIFIER()%겨드랑이를 과시하면서 허리를 흔들었다.
						CALL K78_HPH_PRINT,@"「예 HPH %마스터가% 좋아하는 겨드랑이 털 부디 HPH」","W"
					ELSEIF K78_SWEATY_UNDERARMS()
						PRINTFORMDL 양손을 머리의 뒤에 짜 허리를 흔드는%CALLNAME:TARGET%의 겨드랑이아래에는 큰 땀지미가 되어 있었다.
						CALL K78_HPH_PRINT,@"「아앙 HPH 굉장한 땀으로 HPH 부끄러운 HPH」","W"
					ELSE
						PRINTFORMDL %타겟은% 양손을 머리의 뒤에서 짜, 다리를 크게 연 천한모습으로 격렬하게 허리를 거절했다.
						IF ABL:욕망 >= 20 && RAND:2
							CALL K78_HPH_PRINT,@"「응, 호오 HPH 상스러운 나를 봐줘 HPH」","W"
						ELSE
							CALL K78_HPH_PRINT,@"「하앗, 하앙 HPH 상스러운 나를 봐줘 HPH」","W"
						ENDIF
					ENDIF
				ELSE
					PRINTFORMDL %타겟은% 양손을 머리의 뒤에서 포갠다 허리를 흔들었다.
					CALL K78_HPH_PRINT,@"「이런 이야라시이 모습, 좋아하겠지? HPH」","W"
				ENDIF
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 2
			IF ( EQUIP:치마|| EQUIP:기모노|| EQUIP:원피스) && !TEQUIP:밧줄 && !TEQUIP:구속 && !TEQUIP:계속키스 && ABL:노출벽 - TALENT:수치심 >= 2
				PRINTFORMD %타겟은% 스스로 옷자락을 집어 들어 올려
				IF CFLAG:노팬티
					IF CFLAG:1003 && RAND:3
						PRINTFORMD %K78_GENITAL_HAIR_MODIFIER()%고간을
					ELSE
						PRINTFORMD \@ TALENT:MASTER:성별 & 2 ? 페니스 #남근 모양으로 만든 음구 \@를 입에 물고 넣은\@ ARGS == "V" ? 성기 #배설구멍 \@을
					ENDIF
				ELSE
					PRINTFORMD 팬티를 입은 채로 연결된 두 명의 결합부를
				ENDIF
				PRINTFORMDL 보여 왔다.
				CALL K78_HPH_PRINT,@"「저기, 봐봐 HPH 엣찌겠지 HPH」","W"
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 3
			IF ABL:허리 >= 3 && ( ( ARGS == "V" && ABL:질 >= 3 ) || ( ARGS == "A" && ABL:애널 >= 3 ) ) && TALENT:MASTER:성별 & 2 && !TEQUIP:눈가리개
				IF ARGS == "A"
					PRINTFORMDL %타겟은% 허리를 흔들면서 괄약근을 긴장시켜, 쇠퇴해진 항문관에서 타쿠미에 반짝이는 귀두를 자극해 왔다.
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%의 질은 허리 체에 맞추어 꾸불꾸불절묘하게 연동 해, %CALLNAME:MASTER%의 정자를 짜내려 하고 있는 것 같다.
				ENDIF
				CALL K78_HPH_PRINT,@"「후훗 HPH 느끼고 있는 얼굴, 귀여운 HPH」","W"
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 4
			IF !TEQUIP:밧줄 && !TEQUIP:구속 && !TEQUIP:계속키스 && !TEQUIP:눈가리개
				PRINTFORMDL %타겟은% %마스터가% 손을 잡으면(자), 손가락을 걸면서 기쁜듯이 얼굴을 들여다 봐 넣어 왔다.
				CALL K78_HPH_PRINT,@"「당신의 얼굴을 자주 보고 싶은 것 HPH HPH 부끄러워하지 말고 분명하게 여기 봐 HPH」","W"
			ELSE
				GOTO VA_COWGIRL_LOOP
			ENDIF
		CASE 5
			IF !TEQUIP:계속키스 && !TEQUIP:가슴주무르기 && !TEQUIP:유두빨기
				PRINTFORMDL %CALLNAME:TARGET%에 저 편을 향하게 해 %CALLNAME:MASTER%의 위에서 허리를 흔드는 모습을 배면으로부터 보고 즐기기로 했다.
