;### VA공통 대면좌위 말용 함수 ################
@DIALOGUE_K78_VA_FACE_TO_FACE_SITTING, ARGS

IF K78_BASIC_CONDITION_IN_MATING()
	$VA_FACE_TO_FACE_SITTING_LOOP
	IF TALENT:연모 || TALENT:애욕
		SELECTCASE RAND:6
		CASE 1
			IF !TEQUIP:밧줄 && !TEQUIP:구속 && !TEQUIP:계속키스 && !TEQUIP:유두빨기 && ( K78_ARMPIT_EXPOSURE() || K78_SWEATY_UNDERARMS() )
				IF K78_ARMPIT_EXPOSURE()
					PRINTFORMD 무릎 위로 허리를 흔드는 %CALLNAME:TARGET%에 팔을 들게 해
					IF K78_ARMPIT_SWEAT() && RAND:10 >= 7
						PRINTFORMD 땀으로 질척하게 습기찼다
					ELSE
						PRINTFORMD %K78_ARMPIT_HAIR_MODIFIER()%
					ENDIF
					PRINTFORMDL 겨드랑이에 혓바닥을 기어 갔다.
					CALL K78_HPH_PRINT,@"「응 HPH 낯간지럽지만 , 키모치이이일지도 HPH 음, 하아 HPH」","W"
				ELSE
					PRINTFORMDL 무릎 위로 태운 %CALLNAME:TARGET%에 한쪽 팔을 들게 해 땀지미를 할 수 있던 겨드랑이에 얼굴을 시비면서 허리를 움직였다.
					CALL K78_HPH_PRINT,@"「, 아앙 HPH 정말로 변태 같아요 HPH 나, 향기나 너무 냄새 맡고 HPH」","W"
				ENDIF
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 2
			IF TEQUIP:계속키스
				PRINTFORMDL %타겟은% 꿈틀꿈틀 허리를 흔들면서 %CALLNAME:MASTER%에 몸을 밀착시켜, 끊임없이 입맞춤을 요구해 왔다.
				CALL K78_HPH_PRINT,@"「아암, 응…… 쮸웁… 하아 HPH 쭙 HPH ?…아암…… 응, 좀 더 HPH」","W"
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 3
			IF !TEQUIP:계속키스
				CALL K78_HPH_PRINT,@"「하앗 하앗 HPH 응, 하앗, 하아, 하앙 HPH」","L"
				PRINTFORMDW %CALLNAME:TARGET%의 입술로부터 언제보다 더 달콤한 향기가 하는 한숨이 빠져 %CALLNAME:MASTER%의 얼굴나무 걸린다.
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 4
			IF !TEQUIP:밧줄 && !TEQUIP:구속
				PRINTFORMDL %마스터가% %CALLNAME:TARGET%의 허리에 팔을 돌려 꽉하고 꼭 껴안으면(자), 거기에 대답해 %타겟은% 팔을 목에 얽히게하며 왔다.
				CALL K78_HPH_PRINT,@"「에헤헤♪ 꽉 이라고 하는 것 좋다 HPH %CALLNAME:MASTER%의 심장이 두근 두근 해까지 느껴지는 것 HPH」","W"
			ELSE
				GOTO VA_FACE_TO_FACE_SITTING_LOOP
			ENDIF
		CASE 5
			PRINTFORMDL 서로 그다지 허리를 움직일 수 없는 것도 치워 천성 더욱 열정을 높여 귓전에서 끊임없이 사랑을 속삭임 맞았다.
			CALL K78_HPH_PRINT,@"「하아, 하앗, 좋아해, 너무 좋아 HPH 사랑하고 있는 원HPH 당신과 쭉 이렇게 하고 싶어 HPH」","W"
		CASEELSE
			PRINTFORMDL 구깃구깃하고  허리를 그라인드 시키는 %CALLNAME:TARGET%의 선정적모습이, %CALLNAME:MASTER%의 욕정을 흥분되게 한다.
			CALL K78_HPH_PRINT,@"「에헤 HPH 이 움직여 어때?  기분이 좋아져서 주면(자) 기쁘다 HPH」","W"
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			IF ABL:허리 >= 3
				PRINTFORMDL %타겟은% 천천히 온화하게 움직이면서도 확실히%CALLNAME:MASTER%의 쾌감을 높여 간다.
				PRINTFORMW 「이런 느긋한 엣찌도 나쁘지 않네요」
			ELSE
				PRINTFORMW 「나도 움직이는 거야?  으, 응. 그렇지만 꽤 어려워…」
			ENDIF
		CASE 2
			PRINTFORML 「괜찮아?  , 무겁지 않을까?」
			PRINTFORMDW %타겟은% 성숙한 소녀답게 체중을 신경쓰고 있는 것 같다.
		CASEELSE
			PRINTFORMW 「이 모습이라면 얼굴이 가깝고…조금 부끄럽다…」
		ENDSELECT
	ENDIF
ENDIF


