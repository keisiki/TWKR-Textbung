@M_KOJO_EVENT_K78_1(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	CALL K78_HAIR_GROWTH, 78
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;MASTER가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 함께 들어간다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF K78_CONSCIOUS_AND_TOLERANT()
			PRINTFORMW 「아, %마스터가% 들어가고 있었어. 저기, 함께 들어가도 좋아?」
		ENDIF
		SIF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 나와 갔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 내쫓아졌다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출처에서 조우했다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;타캐릭터와 데이트중에 조우했다
	;ARG:1에 데이트상대
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TCVAR:353 != ARG:1 && K78_CONSCIOUS_AND_TOLERANT()
			;시즈하과 데이트
			IF ARG:1 == 77 && FLAG:역극 != 77
				IF TALENT:연인 || ( TALENT:연모 && !TALENT:77:연인 )
					PRINTFORML 「∼　언니(누나) , 너무 끈적끈적 하고」
					PRINTFORMDW %타겟은% %CALLNAME:77%에의 경쟁심을 드러내 달려들었다.
				ELSE
					PRINTFORMW 「언니(누나)와  외출?  즐겨 와」
				ENDIF
			;조금 전과 다른 상대와 데이트
			ELSEIF TCVAR:353 != 0
				IF TALENT:연모 || TALENT:연인
					PRINTFORML 「다시 여자를 자꾸 갈아 치워라고…」
					PRINTFORMDW %CALLNAME:TARGET%에 감시받아 버렸다.
				ELSE
					PRINTFORML 「어째서 데이트의 상대가 조금 전과 바뀌어…」
					PRINTFORMDW %CALLNAME:TARGET%에 기가 막힐 수 있었다.
				ENDIF
			ELSE
				IF TALENT:연모 || TALENT:연인
					PRINTFORML 「대단히 사이야 유혹하는군……」
					PRINTFORMDW 우나츠라의 %마스터를% 본%타겟은% 재미있지 않을 것 같은 모습이다.
				ELSEIF TALENT:사모
					PRINTFORML 「흐응. %마스터는% 얼굴 넓다…」
					PRINTFORMDW %타겟은% 매정한 모습으로 %마스터와% %CALLNAME:(ARG:1)%를 시선을 돌림 했다.
				ELSE
					PRINTFORM 「아, %CALLNAME:MASTER%.
					SELECTCASE TIME:0
					CASE IS <600
						PRINTFORMW 안녕」
					CASE 600 TO 1139
						PRINTFORMW 안녕하세요」
					CASE IS >= 1140
						PRINTFORMW 안녕하세요」
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
		IF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
			TCVAR:78:353 = ARG:1
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


