	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;망가질 때까지 해 준다
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1




