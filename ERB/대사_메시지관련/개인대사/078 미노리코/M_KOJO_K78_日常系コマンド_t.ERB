;421, 발원함 한다
;조건이 복잡해서, COMF421 발원함. ERB 및, EVENT_MESSAGE_COM400.ERB의 발원함 하는 것 항을 참조
;==================================================
@M_KOJO_MESSAGE_COM_K78_421
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K78_421_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_421_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:78:1099 != 0
		LOCAL:1 = GETCOLOR()
		SETCOLOR GETDEFCOLOR()
		CALL ASK_M(@"내친 김에 %CALLNAME:TARGET%의 엉덩이의 손질이나 유두에 대해서도 발원함 하는", 1,"발원함은 이만큼", 1)
		IF RESULT == 0
			PRINTL
			PRINTFORMDL %마스터는% 내친 김에 %CALLNAME:TARGET%의 엉덩이의 손질과 유두에 대해 부탁하기로 했다.

			$PRAYER_LOOP
			VARSET LOCALS

			SELECTCASE CFLAG:78:1001
			CASE 0
				LOCALS:0 = 변화하지 않는다
			CASE 1
				LOCALS:0 = 출산 전후만 변화한다
			CASE 2
				LOCALS:0 = 임신에 관계없이 모유체질에 의해 변화한다
			CASEELSE
				LOCALS:0 = ※CFLAG:78:1001의 값이 부정※
			ENDSELECT

			SELECTCASE CFLAG:78:1002
			CASE 0
				LOCALS:1 = 스카톨로지 없음
			CASE 1
				LOCALS:1 = 스카톨로지 있어
			CASEELSE
				LOCALS:1 = ※CFLAG:78:1002의 값이 부정※
			ENDSELECT

			PRINTL
			PRINTFORMDL 유두 변화의 현재의 설정：%LOCALS:0%　　　스카톨로지 표현의 현재의 설정：%LOCALS:1%
			PRINTL
			CALL ASK_M("유두 변화의 설정을 변경하는", 1,"스카톨로지 표현의 설정을 변경하는", 1,"이것으로 좋은", 1)
			SELECTCASE RESULT
			CASE 0
				IF CFLAG:78:1001 >= 2
					CFLAG:78:1001 = 0
				ELSE
					CFLAG:78:1001 += 1
				ENDIF
				PRINTL
				GOTO PRAYER_LOOP
			CASE 1
				CFLAG:78:1002 = !CFLAG:78:1002
				PRINTL
				GOTO PRAYER_LOOP
			CASE 2
				PRINTFORMDL %CALLNAME:TARGET%의 유두가%LOCALS:0%같게 된 것 같다.
				PRINTFORMD %타겟이%
				IF CFLAG:78:1002 == 0
					PRINTFORMD 엉덩이를 깨끗하게 해 애널 섹스의 준비를 게을리하지 않는다
				ELSE
					PRINTFORMD 애널 섹스의 앞에서도 엉덩이를 그대로 해 둔다
				ENDIF
				PRINTFORMDW 같게 된 것 같다.
				PRINTL
			ENDSELECT
		ENDIF
		SETCOLOR LOCAL:1
	ENDIF
	IF K78_CONSCIOUS_AND_TOLERANT()
		SELECTCASE RAND:9
		CASE 1 TO 4
			PRINTFORMW 「무엇을 부탁했어?」
		CASE 5 TO 8
			PRINTFORMW 「대단히 열심히 기원하고 있었어」
		CASEELSE
			PRINTFORMW 「으응~… 당신이 비고 있는 신님, 어떻게든(어쩐지) 의심스러운 것을 느끼는 것 같은…」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

