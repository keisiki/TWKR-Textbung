@M_KOJO_MESSAGE_PALAMCNG_C_K78
CALL PALAM_MESSAGE_C

IF K78_BASIC_CONDITION_IN_MATING()
	;처녀상실
	IF TCVAR:파과
		;진짜 처녀상실
		IF TCVAR:파과 == 1
		;재생 처녀상실
		ELSEIF TCVAR:파과 == 2
		;무자각비처녀상실
		ELSEIF TCVAR:파과 == -1
		ENDIF
	ENDIF

	;A처녀상실
	IF TCVAR:A파과
		;진짜 A처녀상실
		IF TCVAR:A파과 == 1
		;무자각A처녀상실
		ELSEIF TCVAR:A파과 == -1
		ENDIF
	ENDIF

	;펠라로 사정 후의 청소
	IF (TCVAR:MASTER:112 == 1 || TCVAR:MASTER:112 == 2) && SELECTCOM == 81
		PRINTFORML 
	ENDIF

	;전의 턴으로 TARGET의 아누스에 주인의 페니스 or페니반을 삽입 자제라고 가 지금은 그렇지 않은, 한편 우후후 안
	IF MASTER_POSE(SET_A, SET_P, 1) == TARGET && MASTER_POSE(SET_A, SET_P, 0) != TARGET && CFLAG:우후후
		IF TCVAR:A에질내사정된플래그 && TCVAR:A에질내사정한캐릭터의번호 == MASTER
			IF CFLAG:1002 && ( TALENT:연모 || TALENT:애욕 ) && 충전율(TARGET, 2) >= 30 && !TCVAR:351 && !RAND:5
				PRINTFORMDL %CALLNAME:TARGET%의 항문으로부터 %TEXTR("페니스/음경/페니스/자지")%를 뽑아 내면(자), 자포자기한 거기는 단지 지금 주입된 정액의 자극에 드디어 무너짐 해, 대담한 대변을 무리 무리와 힘차게 냈다.
				CALL DIALOGUE_K78_DEFECATION_IN_MATING
			ELSEIF CFLAG:1002 && !TCVAR:351 && RAND:3
				PRINTFORMDL 주입된지 얼마 안된 %TEXTR("정액/정액/정자")%가 %CALLNAME:TARGET%의 %TEXTR("항문/아누스/엉덩이의 구멍")%로부터 넘쳐 온다.
				PRINTFORMDL 조금 대변 그것은 얇은 황토색에 물이 들고 있다.
			ELSE
				PRINTFORMDL %CALLNAME:TARGET%의 항문으로부터 %TEXTR("페니스/음경/페니스/자지")%를 뽑아 내면(자), 쏟아진지 얼마 안된 %TEXTR("정액/정액/정자/백탁액")%가 넘쳐 나왔다.
			ENDIF
		ELSEIF 충전율(TARGET, 2) >= 60 && !RAND:3
			IF CFLAG:1002
				IF ( TALENT:연모 || TALENT:애욕 ) && !TCVAR:351 && !RAND:5
					PRINTFORMDL 이마에 땀을 띄우면서 편의에 참고 있던%CALLNAME:TARGET%였지만, 마침내 한계가 온 항문은 단숨에 입을 벌리자 마자 브치브치라고 하는 격렬한 소리와 함께 정액  섞인 대변을 흩뿌렸다.
					CALL DIALOGUE_K78_DEFECATION_IN_MATING
				ELSEIF !TCVAR:351 && RAND:2
					PRINTFORMDL 넘치 듯이 정액을 직장에 흘려 넣어졌기 때문에인가, %CALLNAME:TARGET%의 배가 규르규르와 울었다.
					PRINTFORMDL 편의도 좋아면서도 필사적으로 참고 있는 것 같다.
				ELSE
					PRINTFORMDL \@ 충전율(TARGET, 2) >= 100 ? 배가 부푸는 만큼 #대량의 \@정액을 쏟아진%타겟은%, 장액이 섞여 독특한 냄새를 발하는 %TEXTR("정액/정액/정자/백탁액")%를 %TEXTR("항문/아누스/엉덩이의 구멍")%로부터 흘려 보내고 있다.
				ENDIF
			ELSE
				PRINTFORMDL %CALLNAME:TARGET%의 항문으로부터\@ TALENT:MASTER:성별 & 2 ? %TEXTR("페니스/음경/페니스/자지")% #남근 모양으로 만든 음구 \@를 뽑아 내면(자), 직장에 대량으로 쏟아진 %TEXTR("정액/정액/정자/백탁액")%가 천천히 늘어지고 떨어져 내렸다.
			ENDIF
		ELSE
			IF CFLAG:1002 && ( TALENT:연모 || TALENT:애욕 ) && !TCVAR:351 && !RAND:20
				PRINTFORMDL %타겟은% 갑자기 머뭇머뭇 하기 시작했는지라고 생각하면(자), 큰 방귀 소리와 함께 뿌?뿌? 한 설사보고의 변을 항문으로부터 분출했다.
				CALL DIALOGUE_K78_DEFECATION_IN_MATING
			ELSEIF CFLAG:1002 && !RAND:5
				PRINTFORMDL %CALLNAME:MASTER%의 삽입에 자극되어 장이 연동 하기 시작했는지, %타겟은% %TEXTR("새근―/브우/보박/")%과 비를 히는 끝냈다.
				CALL DIALOGUE_K78_FARTING_IN_MATING
			ELSE
				PRINTFORMDL %CALLNAME:TARGET%의 %TEXTR("항문/아누스/엉덩이의 구멍")%는\@ TALENT:MASTER:성별 & 2 ? %TEXTR("페니스/음경/페니스/자지")% #남근 모양으로 만든 음구 \@가 뽑아져도 더 %TEXTR("뻥/빠끔빠끔/벌렁 벌렁")%과 입을 열어 %TEXTR("항내의 붉은 고기/미끌미끌 한 점막")%를 쬐고 있다.
			ENDIF
		ENDIF
		PRINTW
	ENDIF
ENDIF


;탈분말용 함수
@DIALOGUE_K78_DEFECATION_IN_MATING

IF FIRSTTIME("탈분", 0,78)
	CALL K78_HPH_PRINT,@"「싫어!나와버리는 , 똥 나와버리니까 보, 보지 마」","W"
	PRINTFORMDW %마스터는% 갑작스러운 일로 놀랐지만, %타겟이% 어찌할 바를 모르고 떨면서 오물을 다 내는 모습을 물끄러미 관찰했다.
	PRINTFORMDW 눈물로 수치에 기세가 꺾여 있던%CALLNAME:TARGET%였지만, 흐트러져 배설하는 자신을 눈앞으로 한 %마스터가% 항상 없게 흥분하고 있는 것을  깨닫아, 기분을 고쳐 잡은 것 같다.
	CALL K78_HPH_PRINT,@"「어쩌면……나의 똥으로 흥분해 버렸어…?  후훗 HPH 당신은 역시 대단한 변태군요 HPH」","W"
ELSE
	SIF !RAND:4
		PRINTFORMDL 오늘의 그것은 한층 농후한 악취를 발해, 양도 좀 많은 같다.
	SELECTCASE RAND:3
	CASE 1
		CALL K78_HPH_PRINT,@"「아앗, 싫어 HPH 나와버리는 HPH 상상은 해버린다의 HPH」","L"
	CASE 2
		CALL K78_HPH_PRINT,@"「하아 HPH 이제(벌써) 안 돼 , 참을 수 없는 HPH 똥 나와버린다HPH」","L"
	CASEELSE
		CALL K78_HPH_PRINT,@"「으응 HPH 미안해 HPH 똥 오는 , 응 오면 HPH」","L"
	ENDSELECT
ENDIF

TCVAR:78:351 = 1


;방귀 말용 함수
@DIALOGUE_K78_FARTING_IN_MATING

IF FIRSTTIME("방귀", 0,78)
	IF TALENT:연모 || TALENT:애욕
		CALL K78_HPH_PRINT,@"「히잇　…(이)나다 이제(벌써) HPH 부끄러운 HPH」","W"
		PRINTFORMDW %타겟은% 생각하지 않는 방귀에 스스로도 웃으면서 호오를 붉게 물들이고 있다.
	ELSE
		PRINTFORMW 「~~!(이)나다 이제(벌써)……쥐구멍에라도 들어가고 싶다…」
		PRINTFORMDW %타겟은% 빨간 얼굴 한 채로 숙여, %CALLNAME:MASTER%의 시선으로부터 도망치려 하고 있다.
	ENDIF
ELSE
	IF TALENT:연모 || TALENT:애욕
		SELECTCASE RAND:2
		CASE 1
			CALL K78_HPH_PRINT,@"「이나HPH 방귀 나와 버렸다…HPH 안 돼, 냄새 맡지 말고 HPH」","L"
		CASEELSE
			CALL K78_HPH_PRINT,@"「앙 HPH 미안해요, 또 방귀 해 버렸다 HPH」","L"
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「… 또 나와 버렸다…」
		CASEELSE
			PRINTFORML 「, 절대~ 냄새 맡지 말고!」
		ENDSELECT
		PRINTFORMDL %타겟은%\@ !TEQUIP:밧줄 && !TEQUIP:구속 ? 귀까지 새빨갛게 한 얼굴을 양손으로 오옷이라고 있다.  #새빨간 얼굴로 눈을 떼었다.  \@
	ENDIF
ENDIF


