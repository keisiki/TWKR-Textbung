;두 명째 들어가지 않는 경우는 여기서 처리
;두 명째
LOCAL = MASTER_POSE(6,1,1)
LOCAL:1 = RESULT
LOCAL:2 = TARGET
TARGET = LOCAL
TRYCALLFORM M_KOJO_MESSAGE_SCOM_K{TARGET}_13_2
TARGET = LOCAL:2
RETURN LOCAL:1

@M_KOJO_MESSAGE_SCOM_K78_13_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K78_{SELECTCOM}_1
RETURN 1

