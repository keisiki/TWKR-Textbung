;==================================================
;5, 수음 펠라
;==================================================
@M_KOJO_MESSAGE_SCOM_K78_5
CALL M_KOJO_MESSAGE_SCOM_K78_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K78_5_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF K78_BASIC_CONDITION_IN_MATING()
		$HANDJOB_AND_FELLATIO_LOOP
		IF TALENT:연모 || TALENT:애욕
			IF NOWEX:MASTER:사정 && !TEQUIP:MASTER:콘돔 && EXP:정음경험 > 0
				SELECTCASE RAND:3
				CASE 1
					;질
					IF EXP_UP(15, TARGET) == NOWEX:PLAYER:사정 * 3
						CALL K78_HPH_PRINT,@"「응?  이제 내자? HPH 좋아 HPH 입의 안에 가득 내 HPH」","W"
					ELSE
						GOTO HANDJOB_AND_FELLATIO_LOOP
					ENDIF
				CASE 2
					PRINTFORMDL %타겟은% 사정이 가까운 것을 헤아리면(자) 손에 힘을 주고 쓱쓱격렬하게 훑어 냈다.
					CALL K78_HPH_PRINT,@"「자, 내 HPH 나의 손과 입으로 가득 내는 곳(중) 보여 HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「아, 실룩실룩 하고 있는 HPH 는, 이대로 내면 나 HPH 아암…쪽…날름 HPH」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「참지 않아도 좋아요HPH 내고 싶을 때는 내어도 괜찮으니까 HPH」","W"
				CASE 2
					PRINTFORMDL %타겟은% 힘줄로부터 반짝이는 귀두의 근처를 혀 끝으로 쑤시듯이 자극하고 있다.
					CALL K78_HPH_PRINT,@"「쮸웁, , HPH 여기 키모치이이? HPH 그렇지 않으면 이쪽이 괜찮을까?  앙, HPH」","W"
				CASEELSE
					PRINTFORMDL %타겟은% 찌걱찌걱 소리를 내 근원을 격렬하게 훑어 내면서 귀두를 베어 물고 있다.
					CALL K78_HPH_PRINT,@"「응, HPH , , 앙 HPH , 쭈욱, ? HPH」","W"
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 1
				IF TALENT:사모 || TALENT:섹프
					PRINTFORMW 「…쪽…할짝…?… 점점 열중해 올게요, 이것」
				ELSE
					PRINTFORMW 「응…앙, 쮸웁…… 후우, 입과 손이 지쳐 와 버렸다」
				ENDIF
			CASE 2
				IF ABL:손가락 + ABL:혀 >= 3
					PRINTFORMW 「할짝…쪽…쪽… 어때?  상당히 잘 되었겠죠?」
				ELSE
					PRINTFORMW 「 아프지 않아?  가감상태를 잘 몰라요…」
				ENDIF
			CASEELSE
				PRINTFORMW 「낼 것 같은 때는 말해요. 갑자기라고 놀라기 때문에」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K78_{SELECTCOM}_1
RETURN 1

