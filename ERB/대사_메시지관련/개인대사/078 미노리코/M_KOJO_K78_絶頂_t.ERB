;-------------------------------------------------
IF LOCAL
	IF K78_CONSCIOUS_AND_TOLERANT() && !TALENT:무지
		;-------------------------------------------------
		;오중절정
		;-------------------------------------------------
		IF NOWEX:오중절정
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「, 응HPH 하앙 HPH 아아, 아앗, 아″아″아HPH 있고, 있고″우웃 HPH」","L"
				PRINTFORMDL %타겟은% 큰 큰 소리로 외치는 소리를 준 뒤, 실이 끊어진 것처럼 전신을 축 이완시켜,--와 어깨로 숨을 쉬고 있다.
				PRINTFORMW 「후휴, 후, 후 ……도…이제(벌써)…용서(허락)해…」
			CASE 2
				CALL K78_HPH_PRINT,@"「히잇, 아, 아HPH 아앙 HPH 응, 히익 HPH」","L"
				PRINTFORMDL %타겟은% 눈물이나 콧물, 군침 투성이가 된 얼굴도 그대로, 부들부들 경련하면서도다 라고 있다.
				CALL K78_HPH_PRINT,@"「하아, 핫,은 HPH , 아, 하아 HPH 후우, 후우 HPH 으음 HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「죽는 , 죽어버리는 HPH 안 돼 HPH 안 돼 어HPH 호오 , 응 호오 ″″HPH」","L"
				PRINTFORMDL 비명과 같이 소리를 지른%타겟은%, 축 늘어져 해 %CALLNAME:MASTER%에 기대고 걸렸다.
				CALL K78_HPH_PRINT,@"「하아, 하아…… 너무 괴, 요 HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;사중절정
		;-------------------------------------------------
		IF NOWEX:사중절정
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「야, 안 돼 HPH 이제(벌써) , 이제(벌써) 용서(허락)해 HPH 핫, 으흥 아HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「이제(벌써) 무리HPH 기분 너무 좋아 이상해지는 HPH 머리 새하얗게 되어버리니까HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「그만두어 HPH 이제(벌써) 갔나들HPH 기분이야 지나 HPH 나는 것 HPH 날아가 버리는 것 HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;삼중절정
		;-------------------------------------------------
		IF NOWEX:삼중절정
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「이얏 HPH 이것 대단햇 HPH 아앗, 아앙HPH 아히, 응히HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「응, 응HPH 다, 안 돼 HPH 가요 HPH 가득 이HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「응히HPH 히익 HPH 다, 안 돼 HPH 응, ~~HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;이중절정
		;-------------------------------------------------
		IF NOWEX:이중절정
			SELECTCASE RAND:3
			CASE 1
				CALL K78_HPH_PRINT,@"「아앗, 핫, 하앙 HPH 키키우웃 HPH」","W"
			CASE 2
				CALL K78_HPH_PRINT,@"「하아, 좋아요 HPH 핫, 아앗, 후아이HPH」","W"
			CASEELSE
				CALL K78_HPH_PRINT,@"「하앙 , 하앙 , 이이HPH %CALLNAME:MASTER%키모치이이야 HPH」","W"
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;단부위의 절정
		;-------------------------------------------------	
		IF NOWEX:C절정
			;초강 절정 C
			IF NOWEX:C절정 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「\@ TALENT:성별 & 2 ? 자지 #구리 \@기분 좋앗 HPH 아앙 HPH 응, 응HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「흐응 HPH 으응 HPH ,\@ TALENT:성별 & 2 ? 자지 #콩 \@키HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「\@ TALENT:성별 & 2 ? 자지 #콩 \@굉장해, 대단해서 HPH 아아, 하앗, 은 흐응 HPH」","W"
				ENDSELECT
			;강절정 C
			ELSEIF NOWEX:C절정 == 2
				CALL K78_HPH_PRINT,@"「하앗 핫, 있고, 이크HPH」","W" 
			;절정 C
			ELSE
				PRINTFORMW 「아앗, 으응, 하거기, 좋은 , 좋은 것」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:V절정
			;초강 절정 V
			IF NOWEX:V절정 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「흐응, 보지 대단한 HPH 아앗, 하앙 HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「굉장해요 HPH 보지가 이것 HPH 으응,∼~HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「꾸불꾸불해 키모치이이HPH 보지 좀 더 동글동글 해 HPH」","W"
				ENDSELECT
			;강절정 V
			ELSEIF NOWEX:V절정 == 2
				CALL K78_HPH_PRINT,@"「아흑, 아앗, 하앗 (안)중, 키모치이이 HPH」","W"
			;절정 V
			ELSE
				PRINTFORMW 「응, 아앗, 좋은 곳 , 아얏 비친다」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:A절정
			;초강 절정 A
			IF NOWEX:A절정 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「응HPH 온다HPH 엉덩이로 대단하지만 키HPH」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「응, 응히HPH 엉덩이 보지HPH 엉덩이 보지 이크 우웃 HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「아앗, 이얏 HPH 응 치노구멍에서 가요 HPH 하앙, 아앙HPH」","W"
				ENDSELECT
			;강절정 A
			ELSEIF NOWEX:A절정 == 2
				CALL K78_HPH_PRINT,@"「아앗, 엉덩이가 , 후아, 하앗 HPH」","W"
			;절정 A
			ELSE
				PRINTFORMW 「아흑, 응, 으응 아앗, 은 」
			ENDIF
			RETURN 1
		ENDIF
		IF NOWEX:B절정
			;초강 절정 B
			IF NOWEX:B절정 == 3
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「아하HPH 와, 기분HPH 젖가슴 키모치이이의 HPH 」","W"
				CASE 2
					CALL K78_HPH_PRINT,@"「유두 찡하고는 HPH 굉장해요 HPH 응히,∼~HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「가슴이, 젖가슴이 뜨거워 HPH 가요, 젖가슴으로 가요 HPH」","W"
				ENDSELECT
			;강절정 B
			ELSEIF NOWEX:B절정 == 2
				CALL K78_HPH_PRINT,@"「으응 아앗, 가슴 좋은 , 좋은 HPH」","W"
			;절정 B
			ELSE
				PRINTFORMW 「응, 하아, 응, 아앙」
			ENDIF
			RETURN 1
		ENDIF
		[SKIPSTART]
		IF NOWEX:M절정
			;초강 절정 M
			IF NOWEX:M절정 == 3
				PRINTFORMW 
			;강절정 M
			ELSEIF NOWEX:M절정 == 2
				PRINTFORMW 
			;절정 M
			ELSE
				PRINTFORMW 
			ENDIF
		ENDIF
		[SKIPEND]
	ENDIF
ENDIF
RETURN 1
