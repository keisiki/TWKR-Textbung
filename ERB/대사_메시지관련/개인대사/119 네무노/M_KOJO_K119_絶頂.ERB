;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:0～4 CVABM絶頂
;NOWEX:6 二重絶頂
;NOWEX:7 三重絶頂
;NOWEX:8 四重絶頂
;NOWEX:9 五重絶頂
;NOWEX:10 噴乳
;NOWEX:11 射精
;NOWEX:12 放尿
;適当に組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K119
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	IF NOWEX:분유 > 1 
		CALL HPH_PRINT(@"「자아, 많이 마셔라。지금 짤테니까……HPH」","L")
		PRINTFORML 네무노의 젖꼭지에서 하얀 액체가 쏟아져 나왔다.
	ELSE
		PRINTFORML 「햣, 뭔가 온다....뭐지？」
		PRINTFORML 네무노는 모유를 먹이며 절정에 도달했다.
	ENDIF
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:시간정지 && NOWEX:TotalEX
		PRINTFORML 네무노의 몸에서 갑자기 땀이 난다.
		PRINTFORMW 아무래도 정지된 채로 가버린 것 같다.
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;五重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:오중절정
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:사중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정

	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
	ENDIF
	IF NOWEX:C절정
		IF NOWEX:C절정 >= 2
			CALL HPH_PRINT(@"「앗、온다아HPH　아앗……！！ 」","L")
			PRINTFORML 네무노는 요염한 소리를 내고 있다…….
		;絶頂Ｃ
		ELSE
			PRINTFORML 「아앗…………,으…………」
			PRINTFORML 네무노답지 않은 소리를 질렀다…….
		ENDIF
		PRINTFORMW 
	ENDIF
	IF NOWEX:V절정
		;超強絶頂Ｖ
		IF NOWEX:V절정 == 3
			PRINTFORMW 
		;強絶頂Ｖ
		ELSEIF NOWEX:V절정 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;超強絶頂Ａ
		IF NOWEX:A절정 == 3
			PRINTFORMW 
		;強絶頂Ａ
		ELSEIF NOWEX:A절정 == 2
			PRINTFORMW 
		;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;強絶頂Ｂ
		IF NOWEX:B절정 == 3
			PRINTFORMW 
		;強絶頂Ｂ
		ELSEIF NOWEX:B절정 == 2
			PRINTFORMW 
		;絶頂Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;超強絶頂Ｍ
		IF NOWEX:M절정 == 3
			PRINTFORMW 
		;強絶頂Ｍ
		ELSEIF NOWEX:M절정 == 2
			PRINTFORMW 
		;絶頂Ｍ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF
