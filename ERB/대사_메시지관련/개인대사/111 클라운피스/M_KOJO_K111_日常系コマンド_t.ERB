;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && DISH_ASSI == 111
	CALL M_KOJO_COLOR_K111
	;햄버거
	IF DISHNAME == "햄버거"
		IF TCVAR:111:356
			PRINTFORML 「에에!? 햄버거 만들줄 알아!?」
			IF TALENT:111:연모
				PRINTFORML 「에헤헤, %CALLNAME:MASTER% 고마워∼%UNICODE(0x2764)%」
				PRINTFORML 「같이 만들자♪」
			ELSEIF TALENT:111:사모
				PRINTFORML 「%마스터는% 정말, 상냥하구나∼！」
				PRINTFORML 「나한테도 맡겨줘!」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%도 알고 있구나!」
				PRINTFORML 「나도 도와줄게!」
			ENDIF
		ELSE
			PRINTFORML 「뭐야 뭐야? 햄버거 만드는 거야?」
			PRINTFORML 「신난다∼! 나도 도와줄게!」
		ENDIF
		IF ABL:111:요리기능 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「그래! 치즈버거 만들자!」
				DISH_TASTE += "치즈/"
				TCVAR:111:357 = 1
			CASE 2
				PRINTFORML 「그래! 스파이시버거 만들자!」
				DISH_TASTE += "스파이스/"
				TCVAR:111:357 = 2
			CASE 3
				PRINTFORML 「그래! 주인님을 본딴 어비스버거 만들자!」
				DISH_TASTE += "스파이스/그림같은/" * 3
				TCVAR:111:357 = 3
			CASEELSE
				PRINTFORML 「그래! 요정을 표현한 페어리버거 만들자!」
				DISH_TASTE += "귀엽다/"
				TCVAR:111:357 = 4
			ENDSELECT
		ELSEIF ABL:111:요리기능 >= 1
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「그래! 치즈버거 만들자!」
				DISH_TASTE += "치즈/"
				TCVAR:111:357 = 1
			CASE 2
				PRINTFORML 「그래! 스파이시버거 만들자!」
				DISH_TASTE += "스파이스/"
				TCVAR:111:357 = 2
			CASEELSE
				;탈락
			ENDSELECT
		ENDIF
	;과자
	ELSEIF DISH_TYPE == "디저트"
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「과자 만들기?」
		CASEELSE
			PRINTFORML 「과자 만들게!?」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「나도 하고 싶어!」
		CASE 2
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「도와줄게!」
			ELSE
				PRINTFORML 「도와줄테니까, 나도 줘!」
			ENDIF
		CASEELSE
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「같이 만들자♪」
			ELSE
				PRINTFORML 「나한테도 맡겨줘!」
			ENDIF
		ENDSELECT
		;쿠키
		IF DISHNAME == "쿠키"
			PRINTW 
			PRINTFORML 「이거는 주인님이고-, 이거는 친구님이야!」
			TCVAR:111:357 = 1
			IF TALENT:111:연모 && !RAND:8
				PRINTFORML 「아! 그리고, 이건… %마스터야%……」
				TCVAR:111:357 = 2
			ENDIF
			PRINTFORML 「어때? 닮았어?」
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「밥 만드는 거야?」
		CASEELSE
			PRINTFORML 「요리하는 거야?」
		ENDSELECT
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「나도 할래-！」
		CASE 2
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「도와줄게!」
			ELSE
				PRINTFORML 「나한테 맡겨!」
			ENDIF
		CASEELSE
			IF TALENT:111:사모 || TALENT:111:연모
				PRINTFORML 「같이 만들자♪」
			ELSE
				PRINTFORML 「나도 할래!」
			ENDIF
		ENDSELECT
	ENDIF
	PRINTW 
	;야채를 뽑는다
	;야채 빼기 처리는 본체 측에 이동했습니다
	;주인이 뽑고 있든 표시되는 것은 사양이라는 것으로
	IF STRCOUNT(DISH_TASTE, "야채 빼기/")
		PRINTFORML 「윽! 이거, 야채 들어갔잖아……」
		PRINTFORML 「몰래 빼버려야지!」
		PRINTW 
	ENDIF
	;야채 넣은 야채 빼기(? )(이)라면 안 되기 때문에
	IF STRCOUNT(DISH_TASTE, "햄버거") && TCVAR:111:356
		PRINTFORML 「우와! 맛있어보여∼！」
		PRINTFORML 「빨리 먹자♪ 먹는다? 아니면 먹여줄래??」
	;금단의 요리
	ELSEIF STRCOUNT(DISH_TASTE, "로터")
		PRINTFORML 「완성했지만……, 뭐야, 이거?」
		PRINTFORML 「저, 정말로 먹는 거야……?」
	ELSE
		IF ABL:111:요리기능 >= 3
			SELECTCASE RAND:4
			CASE 1
				PRINTFORML 「완성∼♪」
				PRINTFORML 「우와! 맛있어보여∼！」
			CASE 2
				PRINTFORML 「나한테 맡기면 이정도야∼」
				PRINTFORML 「어때? 나 잘했어?」
			CASE 3
				PRINTFORMW 「해냈다! 잘했지? 잘했지!?」
				PRINTW 　　스윽스윽
				PRINTFORML 「에헷♪」
			CASEELSE
				PRINTFORML 「완성∼! 의외로 간단하네」
				PRINTFORML 「나 요리 잘하는 걸까?」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「후우-, 지쳤어……」
				PRINTFORML 「요리는 어렵구나……」
			CASE 2
				PRINTFORML 「이런 느낌?」
				PRINTFORML 「좀 더 조미료 넣어야 되는 거 아냐?」
			CASE 3
				PRINTFORML 「できた～！ もうこれでいいでしょ！」
				PRINTFORML 「食べたら変わらないって」
			CASEELSE
				PRINTFORML 「해냈다∼！이 정도면 됬지!」
				PRINTFORML 「먹으면 다 똑같다구」
			ENDSELECT
		ENDIF
	ENDIF
	WAIT
ENDIF
RETURN 1
