@M_KOJO_MESSAGE_COM_K13_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;時姦中
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORMW 「이나…그렇게 어루만지고 돌리지 마…응」
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「%UNICODE(0x2665) *1%～아거기…비쿡이라고 해버린다%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「아…거기…%UNICODE(0x2665) *1%」
			PRINTFORMW 「상냥하게 해 주어 기쁩니다…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「응응…%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「아%UNICODE(0x2665) *1%(이)나%UNICODE(0x2665) *1%아니%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「 나의 몸이 아닌 보고 싶다…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「이런 일 당해…응응%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「머릿속이 저려…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……아…아%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…(이)나다 나, 이상하게 되어버린다…%UNICODE(0x2665) *1%」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
