;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K13_100
CALL M_KOJO_MESSAGE_COM_K13_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K13_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		PRINTFORMW 「이나…통, 아！」
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「야…이상한 기분이 되어 버린다」
		PRINTFORMW 「아, 히」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 울부짖어 사과하는 대요정을 무시해, 부드러운 엉덩이를 히두드렸다.
		RETURN 1
	ENDIF
ENDIF
RETURN 1

