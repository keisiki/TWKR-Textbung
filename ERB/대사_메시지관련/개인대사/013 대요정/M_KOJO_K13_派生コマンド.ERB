;==================================================
@M_KOJO_MESSAGE_SCOM_K13_5
CALL M_KOJO_MESSAGE_SCOM_K13_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K13_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
			;時姦中
			IF FLAG:70
				PRINTFORMW 
			ELSE
				PRINTFORMW 「이렇게…입니까…짠다」
				PRINTFORMW 「………」
			ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「응………%UNICODE(0x2665) *1%」
			PRINTFORMW 「…응…나와…？」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「,…」
		ELSE
			PRINTFORMW 「레로레로……」
		ENDIF
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「………응%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「즉, 쥬박%UNICODE(0x2665) *1%」
			PRINTFORMW 「…%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「응…………」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……」
			PRINTFORMW 「입이 피로있습니다…」
		ELSE
			PRINTFORMW 「응응……」
			PRINTFORMW (이상한 맛이 나…）
		ENDIF
	ENDIF
ENDIF
