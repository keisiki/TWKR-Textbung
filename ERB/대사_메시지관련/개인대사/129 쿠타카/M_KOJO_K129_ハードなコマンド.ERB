;==================================================
;150,가슴골을 범한다
;==================================================
@M_KOJO_MESSAGE_COM_K129_150
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K129_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K129_150_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「어, 억지로 젖가슴에…!?」
		RETURN 1
	ENDIF
	;기본 세트
	;가슴 3이상
	IF ABL:가슴 > 2
		IF RAND:3 == 0
			PRINTFORMW 「젖가슴 범해져 버렸던%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW (억지로 되는 것도 뭔가 느껴 버립니다……%UNICODE(0x2665) *1%)
		ELSE
			PRINTFORMW 「응응…… 이렇게 젖가슴으로 비비어지는 것을 좋아한다 라고 터무니 없는 젖가슴쇼우인씨이시네요♪」
		ENDIF
	;그 이외
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「맥박치고 있어 뜨거워서……%CALLNAME:MASTER%씨를 느낍니다……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「너, 너무 억지로는……!」
		ELSE
			PRINTFORMW 「응응…… 조금 아픕니다……」
		ENDIF
	ENDIF
ENDIF
RETURN 1
