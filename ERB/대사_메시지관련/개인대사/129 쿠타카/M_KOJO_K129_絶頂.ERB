;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;분기 만들기에 유용할 것 같은 변수 따위의 메모
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:C절정 NOWEX:V절정 NOWEX:B절정 NOWEX:A절정 NOWEX:M절정
;각 부위의절정 강도. 우선 0~3을 할당할 수 있어 다중절정에 의해 최대 16배 된다. 즉 단부위라면 최대 3, 5겹절정에서 최대 48
;NOWEX:TotalEX
;다중절정 보정을 받기 전의 각 부위절정 강도의 합계치. 1~15. 이 텐프레에 있어서의 분기의 기준.

;TEQUIP:50
;V에 누구의 장대가 들어가 있을까. TEQUIP:50 = MASTER로 당신이 삽입중이 된다.
;TEQUIP:51
;A에 누구의 장대가 들어가 있을까. TEQUIP:51 = MASTER로 당신이 삽입중이 된다.
;NOWEX:사정
;사정량, 최대 3. 예를 들어 IF NOWEX:MASTER:사정 > 1이라고, 당신이 대량 사정 이상 했을 경우가 된다.
;SYNCED_ORGASM(TARGET)
;상대와 당신이 동시에절정 했을 경우진이 된다.절정의 부위는 묻지 않는다.

;-------------------------------------------------
;절정시의 말
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K129
#DIM TotalEX
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	IF FLAG:70
		PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
	ELSE
		PRINTFORMW 「응%UNICODE(0x2665) *1%」
		PRINTFORMW 「젖가슴 나와 버렸던%UNICODE(0x2665) *1%」
	ENDIF
ENDIF
;-------------------------------------------------
;사정했다
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
;합계절정수로 분기
;이것만으로는 어느 부위가절정 했는가는 구별되지 않기 때문에, 나누어 쓰고 싶으면 임의로 분기가 가필되었고.
IF LOCAL
	;-------------------------------------------------
	;오중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:오중절정
		IF FLAG:70
			PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
		ELSE
			PRINTFORMW 「응%UNICODE(0x2665) *1%」
			PRINTFORMW 쿠타카는 소리가 되지 않는 교성을 올리면서 터무니 없는 쾌락에 몸을 진동시키고 있다.
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;사중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:사중절정
		;부위를 지정하지 않고 간이적으로 쓰는 경우, [SKIPSTART]&[SKIPEND]의 comment out를 제외해, [SKIPSTART]곳의 코멘트동안에 써 주세요
			IF FLAG:70
				PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
			ELSEIF RAND:5 == 0
				PRINTFORMW 「(이)나 %UNICODE(0x2665) *1% 등 째%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「응응%UNICODE(0x2665) *1% 아%UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「응 아 %UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「%UNICODE(0x2665) *1% 응응%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「응%UNICODE(0x2665) *1% 안돼%UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:사중절정
			;C＆V＆A＆B절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆V＆A＆M절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆V＆B＆M절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆A＆B＆M절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆A＆B＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:삼중절정
		;부위를 지정하지 않고 간이적으로 쓰는 경우, [SKIPSTART]&[SKIPEND]의 comment out를 제외해, [SKIPSTART]곳의 코멘트동안에 써 주세요
			IF FLAG:70
				PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
			ELSEIF RAND:5 == 0
				PRINTFORMW 「응응%UNICODE(0x2665) *1% %UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「응%UNICODE(0x2665) *1% %UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「응%UNICODE(0x2665) *1% %UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「아%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1% 가 버리는 %UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「(이)나다 %UNICODE(0x2665) *1% 두 바보가 되어 버리는 %UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;C＆V＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;B＆V＆M절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆V＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;C＆B＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;A＆B＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;C＆A＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;이중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:이중절정
		;부위를 지정하지 않고 간이적으로 쓰는 경우, [SKIPSTART]&[SKIPEND]의 comment out를 제외해, [SKIPSTART]곳의 코멘트동안에 써 주세요
			IF FLAG:70
				PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
			ELSEIF RAND:5 == 0
				PRINTFORMW 「응응%UNICODE(0x2665) *1% 아%UNICODE(0x2665) *1% 좀 더 주세요 %UNICODE(0x2665) *1%」
			ELSEIF RAND:4 == 0
				PRINTFORMW 「응아%UNICODE(0x2665) *1% 아 %UNICODE(0x2665) *1%」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「좀 더 %UNICODE(0x2665) *1% 주세요 %UNICODE(0x2665) *1%」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「응%UNICODE(0x2665) *1% 아%UNICODE(0x2665) *1% 응%UNICODE(0x2665) *1%」
			ELSE
				PRINTFORMW 「(이)나%UNICODE(0x2665) *1% %UNICODE(0x2665) *1% 가 버리는 %UNICODE(0x2665) *1%」
			ENDIF
		[SKIPSTART]
		SELECTCASE NOWEX:이중절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:C절정
		;시간중
		IF FLAG:70
			PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「응아%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「아%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「아%UNICODE(0x2665) *1% 응%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「(이)나%UNICODE(0x2665) *1% 민감합니다 %UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:V절정
		IF FLAG:70
			PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「응응%UNICODE(0x2665) *1% 안돼%UNICODE(0x2665)*1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1% 아%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1% 응응%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「아%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			PRINTFORMW (가 버리는 %UNICODE(0x2665) *1%)
		ELSE
			PRINTFORMW 「%UNICODE(0x2665) *1% 아 %UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;시간중
		IF FLAG:70
			PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「아%UNICODE(0x2665) *1% 엉덩이%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「엉덩이의 구멍에서 바보가 되어 버립니다%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응응%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「좀 더 %UNICODE(0x2665) *1% 엉덩이 만지작거려 주세요 %UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;시간중
		IF FLAG:70
			PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW 「%UNICODE(0x2665) *1%」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「%UNICODE(0x2665) *1% 가슴은 민감합니다 %UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「젖가슴 안돼%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응응%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「좀 더 %UNICODE(0x2665) *1% %UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;시간중
		IF FLAG:70
			PRINTFORMW 얼어붙었을 때의 안에서 쿠타카는절정 했다.
		ELSEIF RAND:5 == 0
			PRINTFORMW (구에서 실시해 버렸던……%UNICODE(0x2665) *1%)
		ELSEIF RAND:4 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1% 입속 기분이 좋습니다%UNICODE(0x2665) *1%」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「응%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「좀 더 구까지 %CALLNAME:MASTER%씨의 것에……」
		ENDIF
	ENDIF
ENDIF

