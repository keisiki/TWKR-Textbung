@M_KOJO_MESSAGE_COM_K2_81_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:4
			CASE 1
				SELECTCASE ABL:혀
				CASE 0,1
					PRINTFORML 「쮸웁 ……후아……저기, 나, 확실할 수 있습니까?」
					PRINTFORMDL 구에서의 봉사에 익숙하지 않은 %타겟이%, 불안한 듯하게 물어 왔다.
					PRINTFORMW 「좀 더 %CALLNAME:MASTER% 님을 기분 좋게 해 드리고 싶습니다」
				CASE 2,3
					CALL K2_HPH_PRINT,@"「……?…… 에헤 HPH 이이곳을 알아 왔던 HPH」","W"
				CASE IS >= 4
					PRINTFORMDL %타겟은% 입술로 칼리를 자극하면서, 동시에 음경소대를 혀로 살짝살짝 꾸짖고 있다.
					CALL K2_HPH_PRINT,@"「…?, HPH ……쭈욱……할짝…………HPH」","W"
				ENDSELECT
			CASE 2
				;풀 발기
				IF CFLAG:MASTER:발기도 >= 1500
					CALL K2_HPH_PRINT,@"「카치카치 자지, 이대로 입 봉사로 좋습니까?  보지로 두근두근 라도 좋아요 HPH」","W"
				;삽입 가능한 발기
				ELSEIF CFLAG:MASTER:발기도 >= 1000
					IF !TEQUIP:MASTER:콘돔
						CALL K2_HPH_PRINT,@"「아앗, 음란한 국물이 새어 왔어요 HPH」","W"
					ELSE
						GOTO BLOWJOB_LOOP
					ENDIF
				;반발
				ELSEIF CFLAG:MASTER:발기도 > 500
					CALL K2_HPH_PRINT,@"「입을 사용해, 자지 좀 더 단단하게 해 드리는 HPH」","W"
				;응
				ELSE
					CALL K2_HPH_PRINT,@"「흐늘흐늘 자지, 입 속에서 코로코로 하는 것 즐겁습니다 HPH」","W"
				ENDIF
			CASE 3
				IF TEQUIP:MASTER:콘돔
					PRINTFORML 「%CALLNAME:MASTER% 님의 자지, 분명하게 직접 맛보고 싶습니다……」
					PRINTFORMDW %타겟은% 고무월 해에서의 펠라티오에 불만인 같다.
				ELSE
					PRINTFORMDL %타겟은% 불기둥을 빨면서 힘줄의 근처에 코를 꽉 눌러 끊임없이 슨슨과 냄새를 냄새 맡고 있다.
					CALL K2_HPH_PRINT,@"「하아…HPH 대단히 음란해 있고 냄새나…… 쭉 냄새 맡아 아픕니다 HPH」","W"
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「쪽…………?…………?옥 HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:사정
				PRINTL
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「자지 실룩실룩 하고 있습니다 HPH 익응이군요 HPH 부디 HPH」","L"
					IF EXP_UP(15, TARGET) == NOWEX:PLAYER:사정 * 3 && !TEQUIP:MASTER:콘돔 && ABL:정액중독 >= 5 && TCVAR:정액경험 >= 20
						CALL K2_HPH_PRINT,@"「자지 국물 너무 좋아인 메이드에게 후끈응 시켜 주세요 HPH」","W"
					ELSEIF !TEQUIP:MASTER:콘돔 && TCVAR:정액경험 >= 20 
						CALL K2_HPH_PRINT,@"「%CALLNAME:TARGET%의 입 보지에 정액-퓨웃 해 주세요 HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「내 입으로 싸주세요 HPH」","W"
					ENDIF
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%의 %K2_EXPRESSION_VARIATION("PENIS")%는 이제(벌써) 폭발 직전이다.
					IF EXP_UP(15, TARGET) == NOWEX:PLAYER:사정 * 3 && !TEQUIP:MASTER:콘돔 && ABL:정액중독 >= 5 && TCVAR:정액경험 >= 20
						CALL K2_HPH_PRINT,@"「꿀꺽꿀꺽은 가득 먹여 주세요 HPH 좋지요?  그런데? HPH 자지 국물 가지고 싶어서 견딜 수 없습니다 HPH」","W"
					ELSEIF !TEQUIP:MASTER:콘돔 && TCVAR:정액경험 >= 20 
						CALL K2_HPH_PRINT,@"「아암, , 쭈욱 , 으흑, 응 HPH , 들 있고에, 안 옛 HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「쮸웁 , , 쭙 HPH 이대로 입으로 끝까지 있네요 HPH」","W"
					ENDIF
				ENDSELECT
			ENDIF
		ELSEIF TALENT:사모 || TALENT:섹프
			SELECTCASE RAND:4
			CASE 1
				IF ABL:혀 < 2
					PRINTFORMW 「쪽……할짝……응…어렵습니다……」
				ELSE
					GOTO BLOWJOB_LOOP
				ENDIF
			CASE 2
				PRINTFORMW 「응, 쭈욱 ……질퍽………쮸욱………」
			CASE 3
				PRINTFORMW 「쮸웁 ……하앗, 어때요?  기분이 좋습니까?」
			CASEELSE
				PRINTFORMW 「흐응 , 실룩실룩 하고 있습니다. 이키 그렇게 되면(자) 말해 주세요」
			ENDSELECT
		ELSE
			IF ABL:봉사정신 >= 2
				;주인의 페니스에, 정액인가 애널인가 점액이나 피의 서로 더러워져 , 한편 콘돔 없음
				IF STAIN:MASTER:2 & 108 && !TEQUIP:MASTER:콘돔
					PRINTFORML 「……더러워지고 있어…냄새가 납니다……응, ……쪽」
					PRINTFORMDW %CALLNAME:TARGET%입고 말이야 실 말하면서도 담담하게 봉사를 계속했다.
				ELSE
					PRINTFORMW 「하암, 응……질퍽……쭙……………쪽」
				ENDIF
			ELSE
				PRINTFORMW 「, 빱니까?  ……」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

