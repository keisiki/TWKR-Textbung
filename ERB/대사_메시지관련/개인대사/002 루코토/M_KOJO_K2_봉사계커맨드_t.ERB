;봉사계 커맨드
;※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;80, 수음
;==================================================
@M_KOJO_MESSAGE_COM_K2_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_80_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && FIRSTTIME(SELECTCOM)
		PRINTFORM 「후훗, 
		SELECTCASE TALENT:MASTER:형상
		CASE 1
			PRINTFORM 귀여운 
		CASE 2
			PRINTFORM 대단히 작은 
		CASE 3
			PRINTFORM 늠름한 
		CASE 4
			PRINTFORM 눈부신 
		CASE 5
			PRINTFORM 특이한 형태의 
		CASE 6
			PRINTFORM 훌륭한 
		CASE 10
			PRINTFORM 굉장히 큰 
		CASEELSE
			PRINTFORM 음란한 
		ENDSELECT
		PRINTFORMW 자지네요」
		PRINTFORMDW %타겟은% 야한 미소를 지으면서, 한 손으로 음경을 만지작거리고 있다.
		PRINTFORMW 「그럼, 자지 봉사하겟습니다」
		RETURN 1
	ENDIF
	
	$HANDJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:6
			CASE 1
				IF ABL:기교 >= 4 && !NOWEX:MASTER:사정
					CALL K2_HPH_PRINT,@"「손으로 스윽스윽 하는거 기분 좋으신가요?  언제든 퓻퓻해도 좋아요HPH」","W"
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:손가락 >= 4
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%님은 여기를 스윽스윽 해버리면 금방 가버리네요HPH」","L"
					PRINTFORMDW %타겟은%, 교묘한 손의 움직임으로 %CALLNAME:MASTER%의 약점을 괴롭히고 있다.
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:눈가리개 && !TEQUIP:유두빨기 && !TEQUIP:계속키스 && !TEQUIP:식스나인 && ABL:마조끼 >= 3
					PRINTFORMDL 무릎을 꿇은 %타겟은% 수음을 하면서, %CALLNAME:MASTER%의 얼굴을 눈을 올려 바라보고 있다.
					PRINTFORMW 「%CALLNAME:MASTER% 님에게 업신여겨지며 이러고 있으면, 봉사하고 있다는 느낌이 드네요」
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 4
				IF ABL:손가락 >= 2 && ABL:새드끼 >= 4
					PRINTFORMDL %CALLNAME:TARGET%의 격렬한 괴롭힘에, %마스터는% 무심코 허리를 들어 버렸다.
					CALL K2_HPH_PRINT,@"「으응, 도망치면 안 돼요. 좀 더 잘해 줄게요HPH」","L"
					SIF !TEQUIP:유두빨기 && !TEQUIP:가슴주무르기 && !TEQUIP:구속 && !TEQUIP:식스나인
						PRINTFORMDL %타겟은% %CALLNAME:MASTER%의 뒤로 돌고, 도망가지 못하게 껴안고, 등뒤에서 더욱 격렬하게 움직였다.
					WAIT
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 5
				CALL K2_HPH_PRINT,@"「자지가 움찔움찔 하고 있는걸 느껴버리네요HPH」","W"
			CASEELSE
				SELECTCASE ABL:손가락
				CASE 0
					PRINTFORMW 「에 그러니까, 이런 느낌인가요?」
				CASE 1,2
					PRINTFORMW 「어떠신가요? 기분이 좋아졌나요?」
				CASE IS >= 3
					PRINTFORMDL %타겟은% 능란하게 완급을 가하면서, %마스터를% 사정으로 이끌고 있다.
					;풀 발기
					IF CFLAG:MASTER:발기도 >= 1500
						CALL K2_HPH_PRINT,@"「아핫, 엄청 딱딱하네요HPH 어떻게 할까요? 이대로 손으로 사정하실래요? 아니면 보지 할까요?HPH」","W"
					;삽입 가능한 발기
					ELSEIF CFLAG:MASTER:발기도 >= 1000
						CALL K2_HPH_PRINT,@"「앙HPH 딱딱하고 멋져요HPH 그치만, %CALLNAME:MASTER% 님은 아직 더 딱딱해질 수 있으시죠?HPH」","W"
					;반발
					ELSEIF CFLAG:MASTER:발기도 > 500
						CALL K2_HPH_PRINT,@"「영차, 영차HPH 저기요HPH 좀 더 커지지 않으면, 보지할 수 없다구요HPH」","W"
					;응
					ELSE
						CALL K2_HPH_PRINT,@"「아직도 흐늘흐늘하네요. 자아, 힘내라HPH 힘내라HPH」","W"
					ENDIF
				ENDSELECT
			ENDSELECT
			IF NOWEX:MASTER:사정 && ABL:기교 >= 4
				PRINTL
				PRINTFORMDL %CALLNAME:TARGET%의 손으로 훑어 내진 %조사처리(K2_EXPRESSION_VARIATION("PENIS"),"는")% 이제 곧 사정할 것 같다.
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「아HPH 갈 거 같으신가요?HPH 손으로 도퓻도퓻는 해 버리는건가요?HPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「앗, 벌써 낼 것 같은 거네요?  좋아요HPH 제 손에다 정액 퓻퓻 해 주세요HPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF TALENT:사모 || TALENT:섹프
			IF ABL:손가락 >= 2
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「어디가 좋은지, 점점 알아 왔습니다」
				CASEELSE
					PRINTFORML 「응……응…… 괜찮나요? 아프지 않나요?」
					PRINTFORMDW %타겟은% 질척질척 하며 소리를 내면서, 빠른 움직임으로 %CALLNAME:MASTER%에 쾌감을 주고 있다.
				ENDSELECT
			ELSE
				PRINTFORMW 「으으……팔이 지쳐버렸어요」
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「손으로 문지르면 되는거네요. 알겠습니다」
			CASEELSE
				PRINTFORML 「…………」
				PRINTFORMDW %타겟은% 말없이 그저 손을 움직이고 있다.
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;81, 펠라치오
;==================================================
@M_KOJO_MESSAGE_COM_K2_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_81_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:4
			CASE 1
				SELECTCASE ABL:혀
				CASE 0,1
					PRINTFORML 「쮸웁……후아……저기, 저, 잘 하고 있나요?」
					PRINTFORMDL 입으로 하는 봉사에 익숙하지 않은 %타겟이%, 불안한 듯이 물어 왔다.
					PRINTFORMW 「좀 더 %CALLNAME:MASTER% 님을 기분 좋게 해 드리고 싶어요」
				CASE 2,3
					CALL K2_HPH_PRINT,@"「츄팟……츄팟…… 에헤HPH 이이곳을 알아 왔던 HPH」","W"
				CASE IS >= 4
					PRINTFORMDL %타겟은% 입술로 귀두를 자극하면서, 동시에 자지의 기둥을 혀로 살짝살짝 괴롭히고 있다.
					CALL K2_HPH_PRINT,@"「구팟…쥿, 츄하HPH ……쭈욱……할짝……쥬팟……레로롯HPH」","W"
				ENDSELECT
			CASE 2
				;풀 발기
				IF CFLAG:MASTER:발기도 >= 1500
					CALL K2_HPH_PRINT,@"「딱딱 자지, 이대로 입 봉사로 해도 좋은건가요? 보지로 벌컥벌컥 퓻퓻 해도 좋아요HPH」","W"
				;삽입 가능한 발기
				ELSEIF CFLAG:MASTER:발기도 >= 1000
					IF !TEQUIP:MASTER:콘돔
						CALL K2_HPH_PRINT,@"「앗, 야한 즙이 새어 왔어요HPH」","W"
					ELSE
						GOTO BLOWJOB_LOOP
					ENDIF
				;반발
				ELSEIF CFLAG:MASTER:발기도 > 500
					CALL K2_HPH_PRINT,@"「입을 사용해서, 자지 좀 더 단단하게 해 드릴게요HPH」","W"
				;응
				ELSE
					CALL K2_HPH_PRINT,@"「흐늘흐늘 자지, 입안에서 데굴데굴 구르는거 재밌네요HPH」","W"
				ENDIF
			CASE 3
				IF TEQUIP:MASTER:콘돔
					PRINTFORML 「%CALLNAME:MASTER% 님의 자지, 생으로 직접 맛보고 싶어요……」
					PRINTFORMDW %타겟은% 콘돔을 낀 상태의 펠라티오에 불만인 같다.
				ELSE
					PRINTFORMDL %타겟은% 기둥을 핥으면서 힘줄의 근처에 코를 대고 자꾸만 킁킁 냄새를 맡고 있다.
					CALL K2_HPH_PRINT,@"「하아…HPH 엄청난 야한 냄새…… 계속 맡고 싶어요HPH」","W"
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「쪽……핥짝……츄팟……그픗……츄릇HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:사정
				PRINTL
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「자지 움찔움찔 하고 있어요HPH 가버리는거네요HPH 부디HPH」","L"
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:사정 * 3 && !TEQUIP:MASTER:콘돔 && ABL:정액중독 >= 5 && TCVAR:정음경험 >= 20
						CALL K2_HPH_PRINT,@"「정액 정말좋아하는 메이드에게 꿀꺽 마시게 해주세요HPH」","W"
					ELSEIF !TEQUIP:MASTER:콘돔 && TCVAR:정음경험 >= 20 
						CALL K2_HPH_PRINT,@"「%CALLNAME:TARGET%의 입 보지에 정액-퓨웃 해 주세요 HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「내 입으로 싸주세요HPH」","W"
					ENDIF
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%의 %조사처리(K2_EXPRESSION_VARIATION("PENIS"),"는")% 이제 폭발 직전이다.
					IF EXP_UP(15, TARGET) == NOWEX:PLAYER:사정 * 3 && !TEQUIP:MASTER:콘돔 && ABL:정액중독 >= 5 && TCVAR:정음경험 >= 20
						CALL K2_HPH_PRINT,@"「꿀꺽꿀꺽 많이 마시게 해 주세요HPH 좋지요? 네? HPH 정액 가지고 싶어서 견딜 수 없어요HPH」","W"
					ELSEIF !TEQUIP:MASTER:콘돔 && TCVAR:정음경험 >= 20 
						CALL K2_HPH_PRINT,@"「아암, 쥬팟, 쭈욱, 으흑, 응HPH 븃븃하거, 내줘어, 부타해여HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「쮸웁, 응핫, 쭙HPH 이대로 입으로 끝까지 해버릴게요HPH」","W"
					ENDIF
				ENDSELECT
			ENDIF
		ELSEIF TALENT:사모 || TALENT:섹프
			SELECTCASE RAND:4
			CASE 1
				IF ABL:혀 < 2
					PRINTFORMW 「쪽……할짝……응…어렵네요……」
				ELSE
					GOTO BLOWJOB_LOOP
				ENDIF
			CASE 2
				PRINTFORMW 「응, 쭈욱……질퍽……츄팟…쮸욱……츄팟…」
			CASE 3
				PRINTFORMW 「쮸웁……하앗, 어때요? 기분이 좋니요?」
			CASEELSE
				PRINTFORMW 「흐응, 움찔움찔 하고 있네요. 갈 것 같으면 말해 주세요」
			ENDSELECT
		ELSE
			IF ABL:봉사정신 >= 2
				;주인의 페니스에, 정액인가 애널인가 점액이나 피의 서로 더러워져 , 한편 콘돔 없음
				IF STAIN:MASTER:2 & 108 && !TEQUIP:MASTER:콘돔
					PRINTFORML 「……더러워지고 있어… 냄새나요…… 응, 하믓……쪽」
					PRINTFORMDW %타겟은% 냄새가 난다면서도 담담하게 봉사를 계속했다.
				ELSE
					PRINTFORMW 「하암, 응……질퍽……쭙……핥짝………쪽」
				ENDIF
			ELSE
				PRINTFORMW 「하, 핥는거에요? 으으……」
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

