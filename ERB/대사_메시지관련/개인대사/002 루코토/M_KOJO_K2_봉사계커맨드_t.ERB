					ENDIF
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「아앙HPH 자지로 스치고 있습니다 HPH 자지 종류종류 이이입니다 HPH」","W"
