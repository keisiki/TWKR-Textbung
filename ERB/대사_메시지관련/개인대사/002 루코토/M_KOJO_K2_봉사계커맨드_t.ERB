@M_KOJO_MESSAGE_COM_K2_88_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMD %타겟은% %K2_EXPRESSION_VARIATION("PENIS")%를 근원까지 질불공평 보고,
				SIF !TEQUIP:눈가리개 && !TEQUIP:식스나인
					PRINTFORMD 눈을 치켜 뜨고 %CALLNAME:MASTER%의 표정을 살피면서
				PRINTFORMDL 열심히 봉사하고 있다.
				CALL K2_HPH_PRINT,@"「아앙……………후우-후우-………………HPH」","W"
			CASEELSE
				PRINTFORMDL %타겟은% 입을 시들게 해 %CALLNAME:MASTER%의 %K2_EXPRESSION_VARIATION("PENIS")%전체를 빨아 올리고 있다.
				SIF !TEQUIP:식스나인
					PRINTFORMDL 뺨은 패여, 코의 구멍을 부풀게 한이면서 천한소리를 내는 모습은, 평상시의 아름다운 얼굴과의 낙차로 한층%CALLNAME:MASTER%의 정욕을 자극했다.
				CALL K2_HPH_PRINT,@"「흐응…………후아…하……응HPH……쭈욱……쪽……HPH」","W"
			ENDSELECT
		ELSEIF TALENT:사모 || TALENT:섹프
			PRINTFORMDL %타겟은% 입전체를 사용해 봉사하고 있다.
			PRINTFORMW 「아암…………………응도……쭈욱」
		ELSE
			PRINTFORMW 「쭈욱 , 쮸욱………흐응………후아……괴로왔습니다」
		ENDIF
	ENDIF
ENDIF
RETURN 1
