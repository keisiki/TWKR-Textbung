@M_KOJO_MESSAGE_COM_K2_80_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && FIRSTTIME(SELECTCOM)
		PRINTFORM 「후훗,
		SELECTCASE TALENT:MASTER:형상
		CASE 1
			PRINTFORM 사랑스럽다
		CASE 2
			PRINTFORM 대단히 작은
		CASE 3
			PRINTFORM 씩씩하다
		CASE 4
			PRINTFORM 눈부신 있고
		CASE 5
			PRINTFORM 바뀐 형태의
		CASE 6
			PRINTFORM 훌륭한
		CASE 10
			PRINTFORM 굉장히 크다
		CASEELSE
			PRINTFORM 음란한
		ENDSELECT
		PRINTFORMW 자지군요」
		PRINTFORMDW %CALLNAME:TARGET%네 야해 있고 미소를 띠면서, 한 손으로 음경도라고 놀고 있다.
		PRINTFORMW 「그럼, 자지 봉사하네요」
		RETURN 1
	ENDIF
	
	$HANDJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:6
			CASE 1
				IF ABL:기교 >= 4 && !NOWEX:MASTER:사정
					CALL K2_HPH_PRINT,@"「손이라고로 넘어 기분이 좋습니까?  언제에 해도 좋으니까HPH」","W"
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:손가락 >= 4
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%님은 여기를 주욱주욱(탁탁) 하자마자 가버린다 응이군요 HPH」","L"
					PRINTFORMDW %타겟은%, 교묘한 손의 움직임으로 %CALLNAME:MASTER%의 약점을 꾸짖고 있다.
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:눈가리개 && !TEQUIP:유두빨기 && !TEQUIP:계속키스 && !TEQUIP:식스나인 && ABL:마조끼 >= 3
					PRINTFORMDL 무릎 꿇은 %타겟은% 딸딸이를 하면서, %CALLNAME:MASTER%의 얼굴을 눈을 치켜 뜨고 응시하고 있다.
					PRINTFORMW 「%CALLNAME:MASTER% 님에게 업신여겨지고면서 이렇게 하고 있으면(자), 봉사하고 있다고 느낌이 듭니다」
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 4
				IF ABL:손가락 >= 2 && ABL:새드끼 >= 4
					PRINTFORMDL %CALLNAME:TARGET%의 격렬한 괴롭힘에, %마스터는% 무심코 허리를 당겨 버렸다.
					CALL K2_HPH_PRINT,@"「으응, 도망치면 안 돼요. 좀 더 요크 해 드려요 HPH」","L"
					SIF !TEQUIP:유두빨기 && !TEQUIP:가슴주무르기 && !TEQUIP:구속 && !TEQUIP:식스나인
						PRINTFORMDL %타겟은% %CALLNAME:MASTER%의 뒤로 돌면(자), 도망칠 수 없도록 안아 다해 등 뒤로부터 한층 더 격렬하게 훑어 냈다.
					WAIT
				ELSE
					GOTO HANDJOB_LOOP
				ENDIF
			CASE 5
				CALL K2_HPH_PRINT,@"「자지가 꿈틀 꿈틀은 맥동 해 느끼는 HPH」","W"
			CASEELSE
				SELECTCASE ABL:손가락
				CASE 0
					PRINTFORMW 「에에 와 이런 느낌입니까?」
				CASE 1,2
					PRINTFORMW 「어때요?  기분 좋고 해두었습니다인가?」
				CASE IS >= 3
					PRINTFORMDL %타겟은% 교묘하게 완급을 붙이면서, %마스터를% 사정에 이끌고 있다.
					;풀 발기
					IF CFLAG:MASTER:발기도 >= 1500
						CALL K2_HPH_PRINT,@"「하앗, 대단히 단단합니다 HPH 어떻게 합니까?  이대로 손이라고로 사정합니까?  그렇지 않으면 보지 할까요? HPH」","W"
					;삽입 가능한 발기
					ELSEIF CFLAG:MASTER:발기도 >= 1000
						CALL K2_HPH_PRINT,@"「앙 HPH 질기고 멋집니다 HPH 에서도, %CALLNAME:MASTER% 님은 아직 좀 더 단단해지는군요? HPH」","W"
					;반발
					ELSEIF CFLAG:MASTER:발기도 > 500
						CALL K2_HPH_PRINT,@"「하앗, 응실마리HPH 이봐HPH 좀 더 움켜 뜨지 않으면 보지 할 수 없어요 HPH」","W"
					;응
					ELSE
						CALL K2_HPH_PRINT,@"「 아직 흐늘흐늘이군요. 이봐, 힘내 HPH 힘내 HPH」","W"
					ENDIF
				ENDSELECT
			ENDSELECT
			IF NOWEX:MASTER:사정 && ABL:기교 >= 4
				PRINTL
				PRINTFORMDL %CALLNAME:TARGET%의 손으로 훑어 내진 %K2_EXPRESSION_VARIATION("PENIS")%는 이제 곧 사정할 것 같다.
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「아HPH 익있을 것 같습니까? HPH 손이라고로 드퓨드퓨는 해 버립니다? HPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「아앗, 이제(벌써) 낼 것 같은 응이군요?  좋아요 HPH 나의 손이라고로 국물해 주세요 HPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF TALENT:사모 || TALENT:섹프
			IF ABL:손가락 >= 2
				SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「어디가 이이인가, 점점 알아 왔습니다」
				CASEELSE
					PRINTFORML 「응……응…… 괜찮습니까?  아프지 않습니까?」
					PRINTFORMDW %타겟은% 부짝부짝 소리를 내면서, 빠른 움직임으로 %CALLNAME:MASTER%에 쾌감을 주고 있다.
				ENDSELECT
			ELSE
				PRINTFORMW 「……팔이 지치게 되었습니다」
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「손으로 비비면 괜찮겠지요. 알았습니다」
			CASEELSE
				PRINTFORML 「…………」
				PRINTFORMDW %타겟은% 말없이  단지 손을 움직이고 있다.
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

