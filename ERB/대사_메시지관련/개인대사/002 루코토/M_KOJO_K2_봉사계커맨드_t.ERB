CALL M_KOJO_MESSAGE_COM_K2_83_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_83_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$FEMORAL_SEX_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕 || TALENT:걸레
			SELECTCASE RAND:5
			CASE 1
				IF !TEQUIP:계속키스 && !( TALENT:성별 & 2 ) && TALENT:MASTER:형상 != 2
					PRINTFORMDL %타겟을% 서게 해 그 사타구니에 등뒤에서 %조사처리(K2_EXPRESSION_VARIATION("PENIS"),"를")% 비벼 댔다.
					CALL K2_HPH_PRINT,@"「후후HPH 자지가 자라난 것 같아서 재미있네요HPH」","L"
					SIF !TEQUIP:밧줄 && !TEQUIP:구속
						PRINTFORMDL %타겟은% 흥미가 없는 듯이 %CALLNAME:MASTER%의 %조사처리(K2_EXPRESSION_VARIATION("PENIS"),"를")% 만지고 있다.
					WAIT
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 2
				IF !( CFLAG:육아인원수 > 0 && !TEQUIP:MASTER:콘돔 && !TCVAR:피임약 && CFLAG:생리주기 >= 3 )
					IF CFLAG:욕구불만도 >= 500 && PALAM:윤활 >= PALAMLV:3
						PRINTFORML 「어라? 넣지않는건가요? 저의 보지는 준비만반인데…」
						PRINTFORMDW %타겟은% 왠지 원망스러운 것 같은\@ TEQUIP:눈가리개 ? 모습이다.  #얼굴로 보고있다.  \@
					ELSE
						PRINTFORMW 「정말로 넣지 않아도 좋은건가요? 보지로 질척질척 븃븃 하지 않아도 좋은건가요?」
					ENDIF
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「아앙HPH 자지로 스치고 있어요HPH 자지 스윽스윽하는거 좋아요HPH」","W"
				ELSE
					GOTO FEMORAL_SEX_LOOP
				ENDIF
			CASE 4
				CALL K2_HPH_PRINT,@"「미끈미끈 가랑이로 %CALLNAME:MASTER% 님의 자지로 철썩철썩해서HPH 아하HPH 엄청난 소리네요HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「제 허리 어떠신가요HPH 제대로 기분좋게 잘 되고 있나요?HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMDL %CALLNAME:TARGET%에 올라타서 스마타를 받기로 했다.
				PRINTFORMW 「괜찮을까요… 자, 자지를 찌부러뜨릴까봐 걱정이 되는데요…」
			CASE 2
				PRINTFORMW 「아…응…… 스, 스쳐서……응」
			CASEELSE
				PRINTFORMW 「넣지 않는건가요? 알겠습니다」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

