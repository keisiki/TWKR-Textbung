						ENDIF
					ENDIF
					RETURN 1
				ENDIF
				;2회째 이후
				IF TALENT:연인 || TALENT:연모
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER% 님HPH」","L"
					SIF !K2_EXCESSIVE_STATURE_GAP()
						PRINTFORMDL %타겟이% 팔을 잡아 몸을 접근해왔다.
