;==================================================
;상대가 덮치기을 해제
;==================================================
@M_KOJO_EVENT_K2_30(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;지쳤다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:연인 || TALENT:연모 || TALENT:애욕 || TALENT:걸레
			PRINTFORMW 「하아… 좀 더 하고 싶었지만…지쳐 버렸습니다」
		ELSE
			PRINTFORML 「하아…하아……면목…없습니다」
			PRINTFORMDW %타겟은% 지친 모습으로 %CALLNAME:MASTER%의 몸으로부터 멀어졌다.
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;만족해 해방
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연인 || TALENT:연모 || TALENT:애욕 || TALENT:걸레
			IF K2_SEXUAL_FRANKNESS() && !RAND:3
				CALL K2_HPH_PRINT,@"「하아 HPH 매우 좋았습니다HPH」","L"
			ELSEIF RAND:2
				CALL K2_HPH_PRINT,@"「하아, 하아HPH 무척이나 사랑받아버렸네요HPH」","L"
			ELSE
				CALL K2_HPH_PRINT,@"「잔뜩 사랑해 주셔서, 감사합니다HPH」","L"
			ENDIF
			PRINTFORMDW %타겟은% 만족스럽게 미소지어, 마지막에 %CALLNAME:MASTER%의 뺨에 키스를 하고 몸을 떼어 놓았다.
		ELSE
			PRINTFORML 「죄송했습니다. 나도 참 정말로…」
			PRINTFORMDW %타겟은% %TEXTR("욕망대로/열정에 사로 잡혀")% %마스터를% 덮쳐 버린 것을 사죄했다.
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;만족 하는 것을 밀어 넘어뜨려졌다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;망가질 때까지 해 준다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1




