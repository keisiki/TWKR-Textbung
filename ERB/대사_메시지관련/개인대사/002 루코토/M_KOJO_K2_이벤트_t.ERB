		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			PRINTFORML 
			RETURN 1
		ENDIF
		
		IF K2_CONSCIOUS_AND_TOLERANT() && !TALENT:무지
			IF TALENT:연인 || TALENT:연모 || TALENT:애욕 || TALENT:걸레
				SELECTCASE RAND:2
				CASE 1
					CALL K2_HPH_PRINT,@"「싫엇HPH 도망치면 안된다구요HPH 계속해서 보지에 하죠HPH」","W"
				CASEELSE
					CALL K2_HPH_PRINT,@"「안 돼요 HPH 좀더 좀더 사랑해 주세요HPH」","W"
				ENDSELECT
			ELSE
				PRINTFORML 「…죄송합니다…아직도 갖고싶어요…」
				PRINTFORMDW %타겟은% 꽉 껴안고 아무말도 하지않았다.
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
