;==================================================
;5, 수음 펠라
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_5_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$HAND_BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:6
			CASE 1
				IF ABL:기교 >= 4
					CALL K2_HPH_PRINT,@"「?옥, 흐응 HPH 우연히도 손대네요 HPH」","L"
					CALL K2_HPH_PRINT,@"「꽉이라고 되는 우연히 사랑스럽습니다 HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:혀 >= 3 && ABL:손가락 >= 2
					PRINTFORMDL %타겟은%, 귀두를 뺨의 안쪽에 꾸욱꾸욱 문지르며, 밤 쥐와 같이 뺨을 부풀게 해 있다.
					CALL K2_HPH_PRINT,@"「……후아……질퍽……흐응 HPH …응…이라도……음…………쭈욱 HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%의 입으로부터는 넘친 군침이 대량으로 늘어져, 떨어진 먼저 얼룩을 만들고 있다.
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:MASTER:콘돔
					CALL K2_HPH_PRINT,@"「끝(쪽)으로부터 국물이 흘러넘쳐 흠뻑입니다 HPH 쭈우웁 HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 4
				IF !TEQUIP:MASTER:콘돔
					CALL K2_HPH_PRINT,@"「……쪽………응~으응 HPH , 두고 히집HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 5
				PRINTFORMDL %타겟은% 열중하며 %CALLNAME:MASTER%의 자지에 달라 붙으며 있다.
				CALL K2_HPH_PRINT,@"「쭈욱 ……?……응……? HPH …HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「앙, , 응, 자지의 근원 쫄깃쫄깃, 기분 좋습니까? HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:사정 && ( TCVAR:정음경험 > 0 || TEQUIP:MASTER:콘돔 )
				PRINTL
				PRINTFORMDL 꽉과 수축한 음낭으로부터 사정이 가까운 것을 헤아린%타겟은%, 격렬하게 손을 움직여 사정으로 이끌어 간다.
				CALL K2_HPH_PRINT,@"「하앗 HPH 내 손과%CALLNAME:MASTER%님의 자지, 부짝부짝 쮸웁은 매우 엣찌한 소리 세우고 있어요  HPH」","L"
				CALL K2_HPH_PRINT,@"「에잇 HPH 에잇 HPH \@ !TEQUIP:MASTER:콘돔 && RAND:2 ? 자지 국물, 나의 혀에 내 주세요 #음란한 흰 국물이다 해 버려서 주세요 \@HPH」","W"
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				IF ABL:기교 >= 4
					PRINTFORMW 「쮸웁 , 응, 음낭도 함께, 입니까?  알았습니다. 아암」
				ELSE
					PRINTFORMW 「으흑, 하아……손과 입을 동시에 움직이는 것은 어렵습니다」
				ENDIF
			CASEELSE
				PRINTFORMW 「아암……?…질퍽……응…후아」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K2_{SELECTCOM}_1
RETURN 1

