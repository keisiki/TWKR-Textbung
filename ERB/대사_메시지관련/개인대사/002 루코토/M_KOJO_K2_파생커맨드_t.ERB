;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$PICK_G_SPOT_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕 || ( TALENT:걸레 && ABL:V감각 >= 5 )
			SELECTCASE RAND:4
			CASE 1
				;진정 걸레, V확장경험 있어, 주인에게 페니스 있어, 랜덤 6분의 1
				IF CFLAG:1001 && EXP:V확장경험 > 0 && TALENT:MASTER:성별 & 2 && !RAND:6
					PRINTFORML 「저기, 나의 보지, 가 아닙니까?」
					PRINTFORMDL 불안한 듯하게 물어 오는 %CALLNAME:TARGET%에 그렇지 않다고 말하면(자), 기쁜듯이 질을 단단히 졸라 왔다.
					CALL K2_HPH_PRINT,@"「노력해 보지 꽉 하기 때문에, 중고 보지 싫어하지 않아로 주세요 HPH」","W"
				;진정 걸레, 주인의 허리 3이상, 이번 내리는 쾌V가 4000이상
				ELSEIF CFLAG:1001 && ABL:MASTER:허리 >= 3 && CUP:쾌V >= 4000
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER% 님의 허리, 이이입니다 HPH 굉장히 능숙합니다 HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 2
				IF ABL:V감각 >= 5
					CALL K2_HPH_PRINT,@"「하아앗 아앗 HPH 어째서 , 이렇게 키모치익 HPH 히, 응HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「거기 대단한 , 대단하군요!아, 아앗, 하아아 HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「거기 동글동글 되면(자), 찌릿찌릿 하면 , 들이마시는 HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「응, 하앗, 응거기, 이상하게 , 아」
			CASEELSE
				PRINTFORMW 「으응 거기, 아, 저림, 으응」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K2_{SELECTCOM}_1
RETURN 1

;==================================================
;7, 삽입자궁입구고문
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_7
IF K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && TALENT:체형 >= -1 && TALENT:MASTER:성별 & 2 && TALENT:MASTER:형상 < 3
	DRAWLINE
	IF TEQUIP:유두빨기
		CALL MESSAGE_EQUIP_COM11
	ELSEIF TEQUIP:가슴주무르기
		CALL MESSAGE_EQUIP_COM12
	ENDIF
	PRINTFORMDL %마스터는% 열심히 허리를 털어 %CALLNAME:TARGET%의 자궁을 찌르려고 했다.
	PRINTFORMDL 그러나, %CALLNAME:MASTER%의 페니스에서는 너무 깊게 찌를 수가 없다.
	SIF !NOWEX:V절정
		PRINTFORMDL 안쪽에 닿는 감각을 거의 얻을 수 없는 %타겟은% %TEXTR("맥 빠짐 한 것 같은/불만스러운/물건 충분하고 없을 것 같은")%\@ TEQUIP:눈가리개 ? 모습 #표정 \@이다.
	PRINTW
	SIF FLAG:대사색깔
		TRYCALLFORM M_KOJO_COLOR_K{TARGET}
	IF ( TALENT:새드 && !TALENT:마조 ) || ABL:새드끼 >= 10
		IF TALENT:MASTER:형상 == 1
			LOCALS = %TEXTR("아드님/꼬맹이/미숙/유치")%
		ELSEIF TALENT:MASTER:형상 == 2
			LOCALS = %TEXTR("조말/작다/엄지/땅딸보")%
		ENDIF
	ELSE
		IF TALENT:MASTER:형상 == 1
			LOCALS = %TEXTR("작은/어린이/적게 내리는 경향")%
		ELSEIF TALENT:MASTER:형상 == 2
			LOCALS = %TEXTR("작은/단소/꼬맹이")%
		ENDIF
	ENDIF

	;이번 V절정 없음
	IF !NOWEX:V절정
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMDL 서로 크게 허리를 털고 있으면(자), %TEXTR("스폭/퐁")%과 질로부터 페니스가 빠져 버렸다.
			CALL K2_HPH_PRINT,@"「HPH %CALLNAME:MASTER% 님의 자지, 곧 누락있네요」","L"
			IF TEQUIP:체위 == 1 || TEQUIP:체위 == 2 || TEQUIP:밧줄 || TEQUIP:구속
				CALL K2_HPH_PRINT,@"「예 HPH %LOCALS%자지, 또 넣어주세요 HPH」","L"
				IF TEQUIP:밧줄 || TEQUIP:구속
					PRINTFORMD 음란하게 입을 연 꿀단지를 과시해
				ELSEIF TEQUIP:체위 == 1
					PRINTFORMD %TEXTR("크게 가랑이를 열어/비순의 안쪽의 붉은 고기를 과시해")%
				ELSE
					PRINTFORMD 엉덩이를 흔들흔들 털어
				ENDIF
				PRINTFORMDW 삽입을 %TEXTR("조르는/조른다")%%CALLNAME:TARGET%달라붙어서 놓지 않아, %K2_EXPRESSION_VARIATION("PENIS")%를 한층 더 격렬하게 꽂았다.
			ELSE
				CALL K2_HPH_PRINT,@"「영차HPH %LOCALS%자지, 또 삽무릎 HPH」","L"
				PRINTFORMDW %타겟은% 은밀한 곳을 과시하면서 %K2_EXPRESSION_VARIATION("PENIS")%를 질네 넣으면(자), 구분 수선과(뿐)만에 격렬하게 허리를 거절했다.
			ENDIF
		CASE 2
			CALL K2_HPH_PRINT,@"「응, 좀 더 깊은 곳에 갖고 싶습니다 HPH 팡팡은 찔러 주세요 HPH %LOCALS%자지 힘내 HPH」","W"
		CASEELSE
			PRINTFORML 「으응, %CALLNAME:MASTER% 님의%LOCALS%자지는 안쪽에 닿지 않아요」
			CALL K2_HPH_PRINT,@"「이봐HPH 좀 더 노력해 허리 떨구며, 보지의 안쪽까지 찌릿찌릿 시켜 주세요 HPH」","W"
		ENDSELECT
	;이번 V절정
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL K2_HPH_PRINT,@"「아앙 HPH 안쪽까지 닿지 않지만 , 이제 이키 그렇습니다 HPH」","L"
			CALL K2_HPH_PRINT,@"「아앙, 아앗, %CALLNAME:MASTER% 님의%LOCALS%자지로 이키HPH 가요 HPH」","W"
		CASE 2
			CALL K2_HPH_PRINT,@"「하앗 HPH 필사적인%CALLNAME:MASTER% 님이나 원 좋습니다 HPH」","L"
			CALL K2_HPH_PRINT,@"「음란한 %타겟은% %CALLNAME:MASTER% 님의%LOCALS%자지에서도 분명하게 가랏 로부터HPH 안심해 주세요 HPH」","W"
		CASEELSE
			CALL K2_HPH_PRINT,@"「하앙 HPH %LOCALS%자지 , 겨우, 요크 되었습니다HPH」","L"
			CALL K2_HPH_PRINT,@"「아아, 하아 HPH 오르가즘 오는 HPH 익있습니다 HPH」","W"
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_7_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$PICK_OS_UTERI_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		SELECTCASE EXP:자궁구개발경험
		CASE IS < 25
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「둔다, 구조금, 근심 쉿, 입니다 , 응」
			CASEELSE
				PRINTFORMW 「응, 응 쿠웃 쿳, 핫, 하악」
			ENDSELECT
		CASE 25 TO 50
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「조금, 익숙해져 왔던 으응」
			CASEELSE
				PRINTFORMW 「두는, 이, 이상한 , 느낌입니다 , 응」
			ENDSELECT
		CASE IS > 50
			IF TALENT:연모 || TALENT:애욕 || ( TALENT:걸레 && ABL:V감각 >= 5 )
				IF NOWEX:MASTER:사정 && ABL:질내사정중독 >= 4 && !TEQUIP:MASTER:콘돔
					SELECTCASE RAND:2
					CASE 1
						CALL K2_HPH_PRINT,@"「아앗, 자지 꿈틀 꿈틀 하고 있습니다 HPH 낼 것 같은 응이군요? HPH」","L"
						CALL K2_HPH_PRINT,@"「부디 HPH 나의 가장 안쪽--해 주세요 HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「가장 안쪽에 자지 퉁츄 오고 있습니다 HPH 주세요 HPH」","L"
						CALL K2_HPH_PRINT,@"「자지 국물 너무 좋아인 자궁에 가득 교배해 주세요 HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
					CASE 1
						IF K2_SEXUAL_FRANKNESS()
							CALL K2_HPH_PRINT,@"「아HPH 고향 똑똑은 , 기분 좋습니다 HPH」","W"
						ELSE
							GOTO PICK_OS_UTERI_LOOP
						ENDIF
					CASE 2
						CALL K2_HPH_PRINT,@"「깊은 곳 키HPH 으응, 흐응 HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「하앗, 하앗 HPH 온몸에, 영향을 주고 있습니다 HPH 응 히익 HPH」","W"
					ENDSELECT
				ENDIF
			ELSE
				SELECTCASE RAND:2
				CASE 1
