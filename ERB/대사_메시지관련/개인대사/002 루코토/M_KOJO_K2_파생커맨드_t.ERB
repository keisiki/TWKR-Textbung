CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_6_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$PICK_G_SPOT_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕 || ( TALENT:걸레 && ABL:V감각 >= 5 )
			SELECTCASE RAND:4
			CASE 1
				;진정 걸레, V확장경험 있어, 주인에게 페니스 있어, 랜덤 6분의 1
				IF CFLAG:1001 && EXP:V확장경험 > 0 && TALENT:MASTER:성별 & 2 && !RAND:6
					PRINTFORML 「저기, 나의 보지, 느슨하지 않나요?」
					PRINTFORMDL 불안한 듯하게 물어 보는 %CALLNAME:TARGET%에 그렇지 않다고 말하자, 기쁜듯이 질을 단단히 조여 왔다.
					CALL K2_HPH_PRINT,@"「노력해서 보지 꼬옥 할테니까, 중고 보지 싫어 하지 말아주세요HPH」","W"
				;진정 걸레, 주인의 허리 3이상, 이번 내리는 쾌V가 4000이상
				ELSEIF CFLAG:1001 && ABL:MASTER:허리 >= 3 && CUP:쾌V >= 4000
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER% 님의 허리, 좋아요HPH 굉장히 능숙하세요HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 2
				IF ABL:V감각 >= 5
					CALL K2_HPH_PRINT,@"「까앗HPH 어째서, 이렇게나 기분이HPH 히잇, 응HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASE 3
				IF K2_SEXUAL_FRANKNESS()
					CALL K2_HPH_PRINT,@"「거기 대단해, 대단해요! 아앗, 응앗, 하아앗HPH」","W"
				ELSE
					GOTO PICK_G_SPOT_LOOP
				ENDIF
			CASEELSE
				CALL K2_HPH_PRINT,@"「거기 동글동글 해버리면, 찌릿찌릿 해, 버려요오HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「응, 하앗, 응  거기, 이상하게, 아」
			CASEELSE
				PRINTFORMW 「으응 거기, 아앗, 저렷, 응앗」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K2_{SELECTCOM}_1
RETURN 1

;==================================================
;7, 삽입자궁구고문
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_7
IF K2_BASIC_CONDITION_IN_MATING() && CFLAG:1001 && TALENT:체형 >= -1 && TALENT:MASTER:성별 & 2 && TALENT:MASTER:형상 < 3
	DRAWLINE
	IF TEQUIP:유두빨기
		CALL MESSAGE_EQUIP_COM11
	ELSEIF TEQUIP:가슴주무르기
		CALL MESSAGE_EQUIP_COM12
	ENDIF
	PRINTFORMDL %마스터는% 열심히 허리를 흔들어 %CALLNAME:TARGET%의 자궁을 찌르려고 했다.
	PRINTFORMDL 그러나, %CALLNAME:MASTER%의 페니스로는 너무 깊게 찌르지 못한다.
	SIF !NOWEX:V절정
		PRINTFORMDL 깊이 와닿는 감각을 거의 얻지 못한 %타겟은% %TEXTR("맥빠진 듯한/불만스러운/허전해 보이는")% \@ TEQUIP:눈가리개 ?모습 #표정 \@이다.
	PRINTW
	SIF FLAG:구상색
		TRYCALLFORM M_KOJO_COLOR_K{TARGET}
	IF ( TALENT:새드 && !TALENT:마조 ) || ABL:새드끼 >= 10
		IF TALENT:MASTER:형상 == 1
			LOCALS = %TEXTR("어린이/꼬맹이/미숙/유치")%
		ELSEIF TALENT:MASTER:형상 == 2
			LOCALS = %TEXTR("변변치 않는/작다/엄지/땅딸보")%
		ENDIF
	ELSE
		IF TALENT:MASTER:형상 == 1
			LOCALS = %TEXTR("작은/어린이/자그마한")%
		ELSEIF TALENT:MASTER:형상 == 2
			LOCALS = %TEXTR("작은/단소/꼬맹이")%
		ENDIF
	ENDIF

	;이번 V절정 없음
	IF !NOWEX:V절정
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMDL 서로 크게 허리를 흔들다보니, %TEXTR("스폭/퐁")%하고 질에서 페니스가 빠져 버렸다.
			CALL K2_HPH_PRINT,@"「HPH %CALLNAME:MASTER% 님의 자지, 빠져버렸네요」","L"
			IF TEQUIP:체위 == 1 || TEQUIP:체위 == 2 || TEQUIP:밧줄 || TEQUIP:구속
				CALL K2_HPH_PRINT,@"「네HPH %LOCALS% 자지, 또 넣어주세요HPH」","L"
				IF TEQUIP:밧줄 || TEQUIP:구속
					PRINTFORMD 음란하게 입을 연 보지를 과시해 
				ELSEIF TEQUIP:체위 == 1
					PRINTFORMD %TEXTR("크게 가랑이를 열어/비순의 안쪽의 붉은 고기를 과시해")% 
				ELSE
					PRINTFORMD 엉덩이를 흔들흔들 흔들고 
				ENDIF
				PRINTFORMDW 삽입을 %TEXTR("조르며/조르는")% %CALLNAME:TARGET%맹렬하게 달라붙어, %K2_EXPRESSION_VARIATION("PENIS")%를 한층 더 격렬하게 찔렀다.
			ELSE
				CALL K2_HPH_PRINT,@"「영차HPH %LOCALS% 자지, 또 넣어 버리네요HPH」","L"
				PRINTFORMDW %타겟은% 은밀한 곳을 과시하면서 %K2_EXPRESSION_VARIATION("PENIS")%를 질네 넣으면(자), 구분 수선과(뿐)만에 격렬하게 허리를 거절했다.
			ENDIF
		CASE 2
			CALL K2_HPH_PRINT,@"「응, 더 깊은 곳에 갖고 싶어요HPH 팡팡하고 찔러 주세요HPH %LOCALS%자지 힘내라HPH」","W"
		CASEELSE
			PRINTFORML 「으응, %CALLNAME:MASTER% 님의%LOCALS%자지로는 안쪽에 닿지 않아요」
			CALL K2_HPH_PRINT,@"「자아HPH 더 열심히 허리 흔들고, 보지의 안쪽까지 찌릿찌릿하게 해 주세요HPH」","W"
		ENDSELECT
	;이번 V절정
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL K2_HPH_PRINT,@"「아앙HPH 끝까지 닿지 않았지만, 슬슬 갈 거 같아요HPH」","L"
			CALL K2_HPH_PRINT,@"「아앙, 아앗, %CALLNAME:MASTER% 님의 %LOCALS% 자지로 가버려요HPH 가버려어HPH」","W"
		CASE 2
			CALL K2_HPH_PRINT,@"「아핫HPH 필사적인%CALLNAME:MASTER% 님 귀엽네요HPH」","L"
			CALL K2_HPH_PRINT,@"「음란한 %타겟은% %CALLNAME:MASTER% 님의 %LOCALS% 자지에서도 잘 가버릴 수 있으니까요HPH 안심해 주세요HPH」","W"
		CASEELSE
			CALL K2_HPH_PRINT,@"「하앙 HPH %LOCALS% 자지 , 겨우, 좋아지기 시작했어요HPH」","L"
			CALL K2_HPH_PRINT,@"「아아, 하아 HPH 절정할꺼같아요HPH 가버려요HPH」","W"
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_7_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$PICK_OS_UTERI_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		SELECTCASE EXP:자궁구개발경험
		CASE IS < 25
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「오옷, 큿, 조금, 괴로워, 요, 응」
			CASEELSE
				PRINTFORMW 「응, 응크으읏 큿, 핫, 하악」
			ENDSELECT
		CASE 25 TO 50
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「조금, 익숙해진거같아요 으응」
			CASEELSE
				PRINTFORMW 「오옷, 이, 이상한, 느낌이예요, 응」
			ENDSELECT
		CASE IS > 50
			IF TALENT:연모 || TALENT:애욕 || ( TALENT:걸레 && ABL:V감각 >= 5 )
				IF NOWEX:MASTER:사정 && ABL:질내사정중독 >= 4 && !TEQUIP:MASTER:콘돔
					SELECTCASE RAND:2
					CASE 1
						CALL K2_HPH_PRINT,@"「앗, 자지 움찔움찔 하고 있네요HPH 내버릴것 같으세요?HPH」","L"
						CALL K2_HPH_PRINT,@"「부디HPH 나의 가장 안쪽에 븃-븃-해 주세요HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「가장 안쪽에 자지 츗츗 하고있어요HPH 그대로 퓻퓻 해주세요HPH」","L"
						CALL K2_HPH_PRINT,@"「정액 정말 좋아해버린 자궁에 가득 교배해 주세요HPH」","W"
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
					CASE 1
						IF K2_SEXUAL_FRANKNESS()
							CALL K2_HPH_PRINT,@"「아HPH 안쪽에 똑똑하는거, 기분 좋아요HPH」","W"
						ELSE
							GOTO PICK_OS_UTERI_LOOP
						ENDIF
					CASE 2
						CALL K2_HPH_PRINT,@"「깊은 곳에 왔어요HPH 으응, 흐으읏HPH」","W"
					CASEELSE
						CALL K2_HPH_PRINT,@"「후앗, 하앗HPH 전신이, 울리고있어요HPH 응히잇HPH」","W"
					ENDSELECT
				ENDIF
			ELSE
