;==================================================
;5, 수음 펠라
;==================================================
@M_KOJO_MESSAGE_SCOM_K2_5
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2_5_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	$HAND_BLOWJOB_LOOP
	IF K2_BASIC_CONDITION_IN_MATING()
		IF TALENT:연모 || TALENT:애욕
			SELECTCASE RAND:6
			CASE 1
				IF ABL:기교 >= 4
					CALL K2_HPH_PRINT,@"「츄릇, 흐응HPH 가끔 만지네요HPH」","L"
					CALL K2_HPH_PRINT,@"「꼬옥해서 가끔 사랑스럽네요HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 2
				IF ABL:혀 >= 3 && ABL:손가락 >= 2
					PRINTFORMDL %타겟은%, 귀두를 볼 안쪽에 꾹꾹 문지르고, 다람쥐처럼 볼을 볼록하게 내밀고 있다.
					CALL K2_HPH_PRINT,@"「하믓……후아……질퍽……흐응HPH …응…므긋……응아……아긋……쥬릇HPH」","L"
					PRINTFORMDW %CALLNAME:TARGET%의 입에서는 넘친 침이 대량으로 흘러, 떨어진 자리에 얼룩을 만들고 있다.
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 3
				IF !TEQUIP:MASTER:콘돔
					CALL K2_HPH_PRINT,@"「아까부터 정액이 흘러서 흠뻑 젖어버렸어요HPH 츄르릇HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 4
				IF !TEQUIP:MASTER:콘돔
					CALL K2_HPH_PRINT,@"「하믓……쪽………응후후HPH 쟈지이, 마히허여HPH」","W"
				ELSE
					GOTO HAND_BLOWJOB_LOOP
				ENDIF
			CASE 5
				PRINTFORMDL %타겟은% 열중하며 %CALLNAME:MASTER%의 자지에 달라 붙어 있었다.
				CALL K2_HPH_PRINT,@"「쥬릇……츄후……응……쥬르릇HPH 쥬팟…쥬팟HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「아믓, 쥬팟, 응, 자지의 뿌리 스윽스윽, 기분 좋으신가요?HPH」","W"
			ENDSELECT
			IF NOWEX:MASTER:사정 && ( TCVAR:정음경험 > 0 || TEQUIP:MASTER:콘돔 )
				PRINTL
				PRINTFORMDL 꽉하고 수축한 음낭으로부터 사정이 가까운 것을 알아챈 %타겟은%, 격렬하게 손을 움직여 사정으로 이끌어 갔다.
				CALL K2_HPH_PRINT,@"「아핫HPH 제 손으로 %CALLNAME:MASTER%님의 자지, 질척질척하고 엄청 야한 소리 내고 있어요HPH」","L"
				CALL K2_HPH_PRINT,@"「에잇HPH 에잇HPH \@ !TEQUIP:MASTER:콘돔 && RAND:2 ? 정액을, 제 혀에 내 주세요 #음란한 흰 액체 주세요 \@HPH」","W"
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 1
				IF ABL:기교 >= 4
					PRINTFORMW 「쮸웁 , 응, 음낭도 같이, 말이죠?  알겠습니다. 아암」
				ELSE
					PRINTFORMW 「으흑, 하아……손과 입을 동시에 움직이는 것은 어렵네요」
				ENDIF
			CASEELSE
				PRINTFORMW 「아암……츄팟…질퍽……응…후아」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K2_{SELECTCOM}_1
RETURN 1

