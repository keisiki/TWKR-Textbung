@M_KOJO_MESSAGE_COM_K2_300_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:빡침
		PRINTFORMDW %TEXTR("화난/험상궂은 표정의")%%타겟은% 거의 대답을 해 주지 않았다.
	ELSEIF TCVAR:만취
		PRINTFORMDL %타겟은% 붉그스럼한 얼굴로 멍하니 있다. 말귀를 잘 못 알아듣는것 같다.
		PRINTFORMW 「네? 뭐가요∼？」
	ELSEIF K2_CONSCIOUS_AND_TOLERANT()
		;하쿠레이 신사가 거점 맵으로 그 부엌에서 업무중의 경우
		IF GET_MAPNAME(MAIN_MAP) == "하쿠레이 신사" && 업무중체크(TARGET) && CFLAG:MASTER:현재위치 == P8부엌
			IF TALENT:연모 || TALENT:애욕
				SELECTCASE ABL:요리기능
				CASE IS >= 4
					CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%님을 위해서 맛있는 요리 만들어 드릴게요HPH」","W"
				CASE 2,3
					CALL K2_HPH_PRINT,@"「맛보기 하시지 않습니까? 애정이 잔뜩 들어갔다구요HPH」","W"
				CASE IS <= 1
					PRINTFORMW 「%CALLNAME:MASTER% 님의 위해서 맛있는 요리 만들 수 있게 되고 싶어요」
				ENDSELECT
			ELSE
				SELECTCASE ABL:요리기능
				CASE IS >= 4
					PRINTFORML 「그렇네요. 그리고 말이죠∼」
					PRINTFORMDW %타겟은% 재치 있게 요리를 만들면서 대화하고 있다.
				CASE 2,3
					PRINTFORMW 「맛보시겠어요?」
				CASE IS <= 1
					PRINTFORML 「어 그러니까 된장은 어디있더라∼？」
					PRINTFORMDW %타겟은% 요리하느라 바쁜 것 같다.
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE TFLAG:193
			CASE 1
				IF TALENT:연모 || TALENT:애욕
					SELECTCASE RAND:2
					CASE 1
						PRINTFORMDL %타겟은% %CALLNAME:MASTER%에 몸을 기대왔다.
						PRINTFORML 「일은 매일 즐겁지만, 자주 %CALLNAME:MASTER% 님과 외출할 수 없는 게 유감이네요」
						CALL K2_HPH_PRINT,@"「그러니까 이런 두 사람의 시간, 소중히 하죠HPH」","W"
					CASEELSE
						;거점이 하쿠레이 신사
						IF GET_MAPNAME(MAIN_MAP) == "하쿠레이 신사"
							PRINTFORML 「언제나 가사를 도와 주셔서, 정말로 고마워요」
							PRINTFORMW 「%CALLNAME:MASTER% 님과 함께 일을 할수있어서, 매우 행복해요」
						ELSE
							PRINTFORML 「일이 재미있어, 라고요? 네. 매우 재미있어요」
							PRINTFORMW 「거기다, 언제%CALLNAME:MASTER% 님이 오셔도 좋도록, 깨끗이 해두지 않으면」
						ENDIF
					ENDSELECT
				ELSE
					PRINTFORMW 「과연∼. %CALLNAME:MASTER% 님은 박식하시군요」
				ENDIF
			CASE 0
				SELECTCASE RAND:7
				CASE 1
					IF FLAG:역극 != 1 && FLAG:역극 != 9
						PRINTFORML 「그렇지요. 그 때 유메미 교수와 레이무님이--」
						PRINTFORMDW %타겟은% 옛날의 이야기를 해 주었다.
					ELSE
						PRINTFORML 「그런 일도 있었네요∼」
						PRINTFORMDW 옛날의 이야기로 %CALLNAME:TARGET%과 달아올랐다.
					ENDIF
				CASE 2
					PRINTFORMW 「내버려 두면 금세 방이 더러워져서 큰일이네요」
				CASE 3
					SELECTCASE TIME:5
					CASE 0,1
						PRINTFORML 「날씨가 좋네요」
						PRINTFORMW 「빨래하기 좋은 날이어서 기뻐요♪」
					CASE 2,3
						PRINTFORML 「흐린 하늘이네요」
						PRINTFORMW 「빨래가 잘 마를까나?」
					CASE IS >= 4
						PRINTFORML 「으응~, 날씨가 안좋네요」
						PRINTFORMW 「빨래를 할 수 없어서 아쉽네요……」
					ENDSELECT
				CASE 4
					PRINTFORML 「청소하는 원반 같은 게 있나요?」
					PRINTFORMW 「으응~, 나도 지지 않도록 노력해야지」
				CASE 5
					IF FLAG:역극 == 9
						PRINTFORMW 「저는 밥을 먹지 않아도 괜찮은 것 같은데요, 동력원은 어떻게 되어 있을까요?  」
					ELSE
						PRINTFORML 「유메미 교수가 저를 만들어 주셨습니다」
						PRINTFORMW 「네? 동력원이요? 글쎄요, 어떻게 되었겠죠?」
					ENDIF
				CASE 6
					;청소기능과 요리기능의 합계가 4 미만
					IF ABL:청소기능 + ABL:요리기능 < 4
						PRINTFORML 「으∼. 어째서 저는 일하는게 돌팔이일까요」
						PRINTFORML 「이러다간 곳간에 버려지는 건 아닌지……」
						PRINTFORMDW %타겟은% 상상하고는 침울해 있다.
					;청소기능과 요리기능의 합계가 4이상
					ELSE
						PRINTFORML 「일도 조금씩 할 수 있게 되었네요」
						PRINTFORMW 「%CALLNAME:MASTER% 님의 덕분이네요♪」
					ENDIF
				CASEELSE
					IF TALENT:연모 || TALENT:애욕
						;거점이 하쿠레이 신사
						IF GET_MAPNAME(MAIN_MAP) == "하쿠레이 신사"
							;오후 6시 이전
							IF TIME < 1080
								PRINTFORMW 「오늘의 저녁밥은 무엇이 좋을까요?  %CALLNAME:MASTER% 님의 좋아하는 것으로 하죠」
							;오후 6시 이후
							ELSE
								PRINTFORMW 「내일의 메뉴는 어떻게 하지. %CALLNAME:MASTER% 님의 좋아하는 것으로 할까요」
							ENDIF
						ELSE
							;오후 6시 이전
							IF TIME < 1080
								PRINTFORMW 「오늘의 저녁밥은 무엇이 괜찮을까∼♪」
							;오후 6시 이후
							ELSE
								PRINTFORMW 「내일의 메뉴는 어떻게 하지∼♪」
							ENDIF
						ENDIF
					ELSE
						;오후 6시 이전
						IF TIME < 1080
							PRINTFORMW 「오늘의 저녁밥은 어떻게 해야 할까요」
						;오후 6시 이후
						ELSE
							PRINTFORMW 「내일의 메뉴는 어떻게 해야 할까요」
						ENDIF
					ENDIF
				ENDSELECT
			CASE -1
				PRINTFORMW 「으응~. 그 이야기는, 저에게는 어려워요……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
