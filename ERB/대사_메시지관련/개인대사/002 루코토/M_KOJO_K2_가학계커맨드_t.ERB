;==================================================
;100, 스팽킹
;==================================================
@M_KOJO_MESSAGE_COM_K2_100
CALL M_KOJO_MESSAGE_COM_K2_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2_100_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF

	$SPANKING_LOOP
	IF ( TALENT:연모 || TALENT:애욕 || TALENT:사모 || TALENT:섹프 ) && K2_BASIC_CONDITION_IN_MATING()
		IF ( ABL:마조끼 >= 4 || ( TALENT:마조 && ABL:마조끼 >= 2 ) ) && ( !TALENT:새드 || ABL:욕망 >= 20 )
			SELECTCASE RAND:4
			CASE 1
				IF CFLAG:1001 && !RAND:3
					IF TALENT:연모 || TALENT:연인
						CALL K2_HPH_PRINT,@"「%CALLNAME:MASTER%님에게 처녀를 바치지 못해서 죄송해요HPH」","L"
						CALL K2_HPH_PRINT,@"「여러 사람의 자지를 맛본 중고 로봇를 용서해 주세요HPH」","W"
					ELSEIF ABL:청소기능 + ABL:요리기능 < 4
						CALL K2_HPH_PRINT,@"「일을 생각하지도 않고, 보지에 하는 것만 생각하는 파렴치한 메이드에게 벌을 내려주세요HPH」","W"
					ELSE
						CALL K2_HPH_PRINT,@"「응, 하앙HPH 누구와도 보지에 해버리는 음란 메이드라서 죄송해요HPH」","W"
					ENDIF
				ELSE
					GOTO SPANKING_LOOP
				ENDIF
			CASE 2
				CALL K2_HPH_PRINT,@"「아하HPH 못된 메이드에게 좀더 벌해 주세요HPH」","W"
			CASE 3
				CALL K2_HPH_PRINT,@"「응후읏HPH 하아HPH 아픈거가 굉장하네요HPH」","W"
			CASEELSE
				CALL K2_HPH_PRINT,@"「아히HPH 하악, 응HPH 하앙, 하아… 벌써…끝인건가요?HPH」","W"
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「우우…… 엉덩이 팡팡하는거 아파요」
			CASEELSE
				PRINTFORMW 「으응…… 엉덩이 찡찡하네요」
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「쿠웃…… 아파요, 그만해주세요」
		CASEELSE
			PRINTFORMW 「아얏, 응, 하아, 하아…… 그, 그만해, 주세요……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

