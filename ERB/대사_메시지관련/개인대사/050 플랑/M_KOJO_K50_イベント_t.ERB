;==================================================
;아침에 일어 났을 때(안녕 말)
;==================================================
@M_KOJO_EVENT_K50_2(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;비동거(同棲) 한편 상대가 먼저 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET 사실에 MASTER가 들어가 있다(연모시 동침중취침시만?
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER 사실에 TARGET가 들어 오고 있다(연모 없음에서도 들어 올 가능성 있어
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 자고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTER가 일어나고 있어, 상대가 일어났다
	;동침 때 TARGET 기상은 이쪽(TARGET 쇠약 or 동침중TARGET 피곤함 패배 후 그대로 MASTER가 계속 일어난 다음 상대가 기상했을 경우 한정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;비동거(同棲)
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「안녕 오라버니」
			PRINTFORMW 「오라버니. 밤은 안녕이야」
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;동거(同棲) 안
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「안녕 오라버니」
			PRINTFORMW 「오라버니. 밤은 안녕이야」
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

