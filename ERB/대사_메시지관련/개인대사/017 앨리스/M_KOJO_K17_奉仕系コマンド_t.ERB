	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_부위C)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K17_GRT(TARGET) >= K17C_연모 && CFLAG:K17C_CF_허덕여타입 == K17C_접수_애원계 && FIRSTTIME(@"{SELECTCOM}말", 1)
		PRINTFORMDL %타겟은% %CALLNAME:MASTER%의 무릎 위에서 마주 보듯이 앉았다
		IF  FIRSTTIME(@"311무릎 싣고-연모", 1)
			;일상 커맨드 무릎 이상 경험이 끝난 상태
			PRINTFORML 「이거, 아앙 %K17_AP_DEC()%　항상, 하는 거지만……좋은거야?」
			PRINTFORMDL %타겟은% %마스터가% 평소처럼 껴안을 때와 같이 쏙 무릎 위로 들어와 앉더니, 허리를 천천히 움직여서 서로의 고간을 자극했다
			
		ELSE
			;미경험
			PRINTFORML 「이거, 아앙 %K17_AP_DEC()%　뭔가, 진짜로 하고 있는……거처럼 보여」
			PRINTFORMDL %타겟은% %CALLNAME:MASTER%의 무릎 위에서 조심스럽게 허리를 흔들어 서로의 고간을 맞댄 채 비볐다
		ENDIF
		CALLF FIRSTTIME(@"{SELECTCOM}말")
		PRINTFORML 「%K17_AP_NUM(2) %%K17_CALL_P(K17C_부위C)%좋아 %K17_AP_NUM(3,1)%」
		PRINTFORML (이거, %K17_CNAME()%을 사용해서 자위, 하는거 같아……)
		PRINTW
		PRINTFORMDL %CALLNAME:MASTER%은 조금 지쳐서 허리를 멈췄지만, %타겟은% 스스로 허리를 흔들며 쾌락을 탐내고 있었다……
		PRINTFORMDL 그 모습을 눈치챈 %CALLNAME:MASTER%은 %CALLNAME:TARGET%를 귓속말로 희롱했다
		PRINTFORML 「%K17_AP_NUM(2)%……후엣?　, 달라, %K17_CNAME()%이 좋아할 거……같아서 한건데…… 너무해, 진짜」
		PRINTFORML 「둘이서 함께……하자?　둘이서 함께하는 엣찌, 니까……」
		PRINTW
		;범용
		CALL K17_MESSAGE
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
		PRINTFORML 「%K17_AUTO_PVOICE()%」
	ELSE
		;2회째 이후
		IF !(CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐)
			;졸라 계
			PRINTFORMDL %타겟은% %CALLNAME:MASTER%의 무릎 위에서 마주 보듯이 앉았다
			PRINTFORML 「%K17_RS("둘이서 함께 엣찌/함께 엣찌") %%K17_RS("하는거 좋아")%%K17_AP_DEC()%　%K17_AP_NUM(2,1)%」
			;범용
			CALL K17_MESSAGE
			CALL K17_CLL_COUNTER_REACTIONMESSAGE
			PRINTFORML 「%K17_AUTO_PVOICE()%」
		ELSE
			;덮쳐 계
			PRINTFORMDL %CALLNAME:MASTER%은 %CALLNAME:TARGET%를 무릎 위에서 마주 보듯이 앉게 했다
			PRINTFORML 「%K17_RS("문지르지 마/부비부비 하지 마")%%K17_RS("시럿/안돼") %%K17_AP_DEC()%　%K17_AP_NUM(2,1)%」
			PRINTFORMDL 허리를 비틀며, 떨어지려고 하는 %CALLNAME:TARGET%를 억누른 채, %CALLNAME:MASTER%은 몇 번이고 허리를 흔들어 괴롭혔다
			;추가
			SIF TCVAR:카운터행동의파생
				PRINTFORMDL 그리고 %CALLNAME:TARGET%의 두 다리를 잡아, 활짝 벌려서 국부를 드러냈다
			TCVAR:카운터행동의파생 = 0
			PRINTFORML 「%K17_AUTO_PVOICE()%」
		ENDIF
	ENDIF
	
ENDIF
RETURN 1

