	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_성기술_없음)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(@"{SELECTCOM}말", 1) && K17_GRT() >= K17C_연모 && CFLAG:K17C_CF_허덕여타입 == K17C_접수_애원계
		PRINTFORML 「아, 다리로……?　비벼줬으면 좋겠다니……그러니까 이렇게 하면 되는걸까」
	ENDIF
	
	;니소 있을까 보고 푼다
	IF EQUIP:양말== 2 || CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐
		IF EQUIP:양말== 2
			PRINTFORMDL %CALLNAME:MASTER%은 %CALLNAME:TARGET%의 다리를 잡고서, 니삭스에 %TINKO(PLAYER)%를 쳐박고 허리를 흔들었다
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%은 %CALLNAME:TARGET%의 다리를 잡고서, 닫혀진 다리의 틈새에 %TINKO(PLAYER)%를 쳐박고 허리를 흔들었다
		ENDIF
		IF !(CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐)
			;졸라 계
			PRINTFORML 「에엣, 어……%K17_RS("거기에 넣어보고 싶었던 거/그게 기분 좋은거")%야……?」
			PRINTFORMDL %타겟은% 부끄러운 듯이 얼굴을 붉힌 채 %CALLNAME:MASTER%을 올려다보고 있었다
		ELSE
			;덮쳐 계
			PRINTFORML 「%K17_RS("변태/바보/엣찌")%……읏 %K17_AP_DEC(1)%」
			PRINTFORMDL %타겟은% 부끄러운 듯이 %K17_RS("고간/얼굴")%을 가렸다
		ENDIF
	ELSEIF !(CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐)
		;졸라 계
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMDL 다리를 벌린 채 치켜 올리니, %타겟은% 허리를 살랑살랑 흔들었다
				PRINTFORMDL 마치 뭔가를 더 조르는 듯이 듯이 보인다
				PRINTFORML 「다리로 말고……이 쪽으로……해줘?　%K17_RS("갖고 싶어/넣어줘")%……」
			CASE 1
				PRINTFORMDL %CALLNAME:MASTER%는 부끄러워서 다리를 닫으려는 %CALLNAME:TARGET%의 두 다리를 붙잡고서, 억지로 벌렸다
				PRINTFORML 「싫어……이거, %K17_RS("엣찌/부끄러워")%……」
				PRINTFORML ( 내 %K17_CALL_G(TARGET, 1)%……엄청나게 보고 있어……이게 %K17_RS("재료/자위용으")%로 쓰인다던 그거야……? )
		ENDSELECT
		;범용
		CALL K17_MESSAGE
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
	ENDIF
	PRINTFORML 「%K17_AUTO_PVOICE()%」
ENDIF
RETURN 1
