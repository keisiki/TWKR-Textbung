@M_KOJO_MESSAGE_COM_K17_82_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_성기술_가슴)
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && K17_GRT(TARGET) >= K17C_연모 && CFLAG:K17C_CF_허덕여타입 == K17C_접수_애원계 && FIRSTTIME(@"{SELECTCOM}말", 1)
		CALLF FIRSTTIME(@"{SELECTCOM}말")
		;성별 판정
		IF TALENT:MASTER:성별 == 2
			PRINTFORML 「가슴 사이에 끼우면 되는거지……?　알고 있어, 남자들은 이거 엄청 좋아하잖아?」
		ELSE
			;양성의 여자
			PRINTFORML 「가슴 사이에 끼우면 되는거지……?」
		ENDIF
	ENDIF
	;2회째 이후
	IF !(CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐)
		;?␀ 계
		;범용성 기술 판정
		CALL K17_MESSAGE
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
		
		;파이즈리 펠라때 허덕여 개소를 바꾼다
		IF TCVAR:카운터행동의파생 == 4
			CALL K17_SET_AUTO_PVOICE(-1, K17C_부위M_펠라)
		ELSE
			SELECTCASE ABL:TARGET:가슴
				CASE IS < 2
					;PRINTL 너무 기분 좋지 껜蔑?
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"이거 %K17_RS("어려울/힘들")%지도 …\\%K17_AP_NUM(2) %\\%K17_AP_NUM(1)%","\\") %%K17_AP_NUM(1,1)%」
				CASE IS  > 2
					;PRINTFORML %타겟은% 소화된 봉사로%CALLNAME:PLAYER%를 기쁨…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"이거 %K17_RS("기분 좋/능숙해졌")%지?　…\\%K17_AP_NUM(2) %\\%K17_AP_NUM(1)%","\\") %%K17_AP_NUM(1,1)%」
				CASEELSE
					;PRINTFORML %CALLNAME:TARGET%의 봉사는%CALLNAME:PLAYER%를 기분 잘 시키고 있다…
					PRINTFORML 「%K17_SPLIT_SHUFFLE(@"이거 %K17_RS("익숙해졌/능숙해졌")%을지도 …\\%K17_AP_NUM(2) %\\%K17_AP_NUM(1)%","\\") %%K17_AP_NUM(1,1)%」
			ENDSELECT
		ENDIF
	ELSE
		;덮쳐 계
		PRINTFORMDL %CALLNAME:MASTER%은 %CALLNAME:TARGET%의 가슴 사이에 %TINKO(PLAYER)%를 갖다 댄 채로 허리를 흔들었다
		;카운터 추가
		IF TCVAR:카운터행동의파생
			PRINTFORMDL 하는 김에 %CALLNAME:TARGET%의 입까지 돌진시켰다
			CALL K17_SET_AUTO_PVOICE(-1, K17C_성기술_혀)
			TCVAR:카운터행동의파생 = 0
		ENDIF
	ENDIF
	
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTW
	;범용 감각 판정
	PRINTFORMDL %CALLNAME:MASTER%의 %TINKO(PLAYER)%에 %OPPAI_DESCRIPTION(TARGET)%가 %K17_RS("자극 되어서/스쳐서")%, 봉사하는 %CALLNAME:TARGET%도 기분 좋은 듯하다
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_부위B)
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	
ENDIF
RETURN 1

