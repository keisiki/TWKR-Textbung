@M_KOJO_MESSAGE_COM_K17_60
IF FLAG:시간정지 || CFLAG:TARGET:수면
	CALL TRAIN_MESSAGE
	CALL K17_처녀상실(TARGET)
	SIF !RESULT
		CALL M_KOJO_MESSAGE_COM_K17_수면애무
ELSE
	CALL K17_SET_KOJO_COLOR
	CALL M_KOJO_MESSAGE_COM_K17_60_1
	PRINTW
ENDIF
TCVAR:카운터행동의파생 = 0
RETURN 1

@M_KOJO_MESSAGE_COM_K17_60_1
#DIMS	cStyle		;몸의자세별 불러
#DIM	iTarget_Num	;TARGET의 인원수
VARSET cStyle
iTarget_Num = GET_TARGETNUM()
CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_부위V)
;-------------------------------------------------
;60, 정상위
;처녀
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && TCVAR:파과 == 1
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	CALL K17_처녀상실(TARGET)
	RETURN 1
ENDIF

;-------------------------------------------------
;몸의자세 분기
;정상위 립 정도
;-------------------------------------------------
IF CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐
	;덮쳐 계
	;구속계 본다
	IF TEQUIP:구속
		cStyle = 구속 당한 채로
	ELSEIF TEQUIP:밧줄
		cStyle = 묶인 채로
	ELSE
		cStyle = 억지로
	ENDIF
ELSE
	;졸라 계
	IF TFLAG:SELECTCOM분기
		cStyle = 마주 보면서
	ELSE
		cStyle = 정면에서
	ENDIF
ENDIF
;-------------------------------------------------
;60, 정상위, 립 정도
;비처녀 접수_애원 계
;연모 한사람 삽입 순간만(몸의자세 바꾸고×
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
{
IF LOCAL && CFLAG:K17C_CF_허덕여타입 == K17C_접수_애원계 && K17_GRT(TARGET) >= K17C_연모 && 
	TCVAR:V삽입관리플래그 == -1 && iTarget_Num == 1
}
	;삽입
	CALL K17_MESSAGE
	IF FIRSTTIME(@"{SELECTCOM}말-{K17C_접수_애원계}")
		;첫회
		PRINTFORML 「히야앙%K17_HEART()%　%cStyle% 들어왔다%K17_HEART()%」
		PRINTFORML (몸으로 받아들인단 게 이런, 뜻이었구나…… %K17_CNAME()%을 몸으로 받아들여서…… 내 몸이 멋대로 기뻐하고 있어)
		PRINTW
		PRINTFORMDL %마스터는% 자신의 %TINKO(MASTER)%가 괜찮냐고 말하는 듯이 %TINKO(MASTER)%로 마구 찔렀다
		PRINTFORML 「하앙 %K17_AP_DEC()%　그렇겟, 하지 않아도 %K17_AP_DEC()% 나의 거기……%K17_CNAME()%의 %K17_CALL_G(MASTER, 1)% 형태로, 딱……이야」
		IF TALENT:MASTER:성별 >= 2
			PRINTFORML (%K17_CALL_G(MASTER)%의 크기가 신경 쓰이는 걸까…?　발기해서……딱딱한 것만으로도 최고인데……)
			PRINTW
			PRINTFORML 「있잖아……나는 그……%K17_CNAME()%의 %K17_CALL_G(MASTER)% 딱딱해서, 굉장히, 좋아……하니까」
		ENDIF
		PRINTFORML 「아아앙 %K17_AP_DEC()%……갑자기 흥분하지 말라니까아」
	ELSE
		;2회째 이후
		PRINTFORML 「%K17_RS("히야아앙/히우우읏/아아앙/아아읏") %%K17_HEART()%　%cStyle% 들어 왔다%K17_AP_DEC(1)%」
		PRINTDATAL
			DATAFORM 「어서 오세요, %K17_CNAME() %%K17_AP_DEC(1)%」
			DATAFORM 「역시 이 자세로 %K17_RS(@"%K17_CALL_P(K17C_부위M_키스)% 받는/손 잡는/꾸욱하는")% 거……%K17_RS("좋아/조앗") %%K17_AP_DEC(1)%」
			DATAFORM 「괜찮으니까……%K17_RS(@"잔뜩 %K17_CALL_G(MASTER)% 기분 좋아져도/언제든지 %K17_RS("싸도/해도/가버려도")% 괜찮으니까") %%K17_AP_DEC(1)%」
		ENDDATA
	ENDIF
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL %타겟은% 양팔을 %CALLNAME:MASTER%의 등 뒤로 감싸 안더니, 꼭 껴안았다
	PRINTFORML (%K17_RS("잔뜩/열심히/정신 없이/기분 좋은 듯이")% 허리 흔들어대서…… 뭔가 %K17_RS("귀여워/행복해/기뻐")%……좀 더 나를 갖고 싶어해줘, %K17_CNAME(MASTER, 1)%)
	RETURN 1
ENDIF
;-------------------------------------------------
;60, 정상위, 립 정도
;비처녀 접수_합의덮쳐계
;연모 한사람 삽입 순간만(몸의자세 바꾸고×
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
{
IF LOCAL && CFLAG:K17C_CF_허덕여타입 == K17C_접수_합의덮쳐계 && K17_GRT(TARGET) >= K17C_연모 && 
	TCVAR:V삽입관리플래그 == -1 && iTarget_Num == 1
}
	;-----------------------
	;끝만
	;-----------------------
	IF FIRSTTIME(@"{SELECTCOM}말-{K17C_접수_합의덮쳐계}")
		;합의 덮쳐 첫회
		PRINTFORMDL %마스터가% %CALLNAME:TARGET%의 고간에 %TINKO(MASTER)%를 갖다대자, %CALLNAME:TARGET%는 허리를 비틀며 도망치려고 했다
		PRINTFORML 「넣으면 안 댓 %K17_AP_DEC()% 이렇게 억지로, 하는 거…… 안 댄, 다니까%K17_AP_DEC(1)%」
		PRINTFORMDL %마스터는% 끝쪽만 비빌 테니까, 라고 하면서, 삽입은 안 하겠단 듯이 %CALLNAME:TARGET%의 가랑이에 %TINKO(MASTER)%를 상하로 비벼대고 있었다
		PRINTFORML 「정말로……끝쪽……만이야? %K17_AP_DEC(1)%」
		PRINTW
		PRINTFORMDL %마스터가% 귀두 끝으로 질구를 쓸어올리고 있으면 %타겟이% 자기 스스로 허리를 꾸욱 밀어넣어서, %TINKO(MASTER)%가 질 안쪽까지 삽입 되어 버렸다……
		PRINTFORML 「아앙 %K17_AP_DEC()% 안엣, 멋대로 안까지, 넣엇 %K17_AP_DEC()% 억지로 %K17_AP_DEC(1)%」
		PRINTFORML 「빼줘어 %K17_AP_DEC()% 끝만 비빌 거라고, 말했잖앗 %K17_AP_DEC()% 안쪽까지 닿앗 %K17_AP_DEC(1)%」
		PRINTFORMDL %CALLNAME:MASTER%이 약속을 어기고 허리를 흔들기 시작하니까, %타겟은% 도망치려는 듯한 자세로 허리를 움직이며 쾌락을 탐하고 있었다
		PRINTW
		PRINTFORML 「범해졋 %K17_AP_DEC() % %K17_CNAME()%에게 %cStyle%, 범해져 버렷%K17_AP_DEC(1)%」
		PRINTFORMDL 범했다니 대체 무슨……
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
		PRINTFORML 「%K17_AUTO_PVOICE()%」
		;몸의자세별
		IF TFLAG:SELECTCOM분기
			PRINTFORMDL %마스터는% 도망치지 못하게끔 신체를 구속하듯이 %타겟을% 꼭 껴안았다
		ELSE
			PRINTFORMDL %마스터는% %CALLNAME:TARGET%의 몸을 감싸안듯이, 몸으로 짓눌러서 도망치지 못하게끔 신체를 구속했다
		ENDIF
		RETURN 1
		
	ELSE
		;2회째 이후
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMD %마스터가% %TINKO(MASTER)%를 삽입하려고 하자, %타겟은% 
				PRINTDATADL
					DATAFORM 양손으로 %K17_RS("고간을/얼굴을")% 가리고 싫어하고 있었다
					DATAFORM 두 다리를 꽉 오므려서 사타구니를 가리고 싫어하고 있었다
				ENDDATA
			CASE 1
				PRINTFORMDL %마스터는% %K17_RS("싫어하는/날뛰는/도망치려고 하는")% %CALLNAME:TARGET%의 두 %K17_RS("손목을/다리를")% 붙잡아서는 %K17_RS("억지로/억지로")% 벌렸다
			CASE 2
				;몸의자세별
				IF TFLAG:SELECTCOM분기
					;립 정도
					PRINTFORMDL %마스터는% %K17_RS("싫어하는/날뛰는/도망치려고 하는")% %CALLNAME:TARGET%의 %K17_RS("한쪽 발을 붙잡아서 들어 올렸다/한쪽 발을 잡아서 벽에 붙였다")%
				ELSE
					;정상위
					PRINTFORMDL %마스터는% %K17_RS("싫어하는/날뛰는/도망치려고 하는")% %CALLNAME:TARGET%의 %K17_RS("두 다리를 붙잡아서는 강제로 끌어당겼다/두 다리를 붙잡아서는 강제로 넘어트렸다")%
				ENDIF
		ENDSELECT
		;대사
		PRINTDATAL
			DATAFORM 「%K17_RS("넣으면/집어 넣는거") % %K17_RS("시럿/안댓") % %K17_AP_DEC()% 이렇게 %K17_RS("억지로/강제로")%, 하는 거……%K17_RS("시럿/안댓") % %K17_AP_DEC(1)%」
			DATAFORM 「%K17_RS("끝쪽/비비기") % %K17_AP_DEC()%……%K17_RS("끝쪽으/비비기")% 로만 %K17_RS("용서해줘/참아줘") %%K17_AP_DEC(1)%」
			DATAFORM 「부탁햇 %K17_AP_DEC()%……적어도, %K17_RS("난폭하게 하지마앗/아프게만 하지마앗") %%K17_AP_DEC(1)%」
		ENDDATA
		PRINTW
	ENDIF
ENDIF

;-------------------------------------------------
;60, 정상위, 립 정도
;비처녀 접수_억지로계
;연모 한사람 구속 억지로 플래그 쪽도 있다
;삽입 순간만(몸의자세 바꾸고×
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
{
IF LOCAL && CFLAG:K17C_CF_허덕여타입 == K17C_접수_억지로계 && K17_GRT(TARGET) >= K17C_연모 && 
	TCVAR:V삽입관리플래그 == -1 && iTarget_Num == 1
}
	IF FIRSTTIME(@"{SELECTCOM}말-{K17C_접수_억지로계}")
		;억지로 첫회
		PRINTFORMDL %마스터는% 도망치려고 날뛰는 %CALLNAME:TARGET%의 허리를 꽉 붙잡아서, %TINKO(MASTER)%를 %K17_RS("은밀한 곳/은밀한 틈")%에 갖다대었다
		
		IF TEQUIP:구속
			PRINTFORML 「이런 식으로, 여자아이를 구속해서는 억지로 하려고 들다니……비겁해!」
		ELSE
			PRINTFORML 「이런 식으로, 여자아이를 힘으로 제압해서는 억지로 하려고 들다니…… %K17_CNAME()%은 쓰레기야!」
		ENDIF
		
	ELSE
		;2회째 이후
		;몸의자세별
		IF TFLAG:SELECTCOM분기
			;립 정도
			PRINTFORMDL %마스터는% %K17_RS("날뛰는/도망치려고 하는")% %CALLNAME:TARGET%의 %K17_RS("허리를 붙잡아서/몸을 껴안고서/양손을 잡아서 벽에 붙인 채/한쪽 발을 잡고 들어 올려서")%, %TINKO(MASTER)%를 %K17_RS("은밀한 곳/은밀한 틈")%에 %K17_RS("갖다대었다/문질렀다")%
		ELSE
			;정상위
			PRINTDATADL
				DATAFORM %마스터는% %K17_RS("날뛰는/도망치려고 하는")% %CALLNAME:TARGET%의 %K17_RS("허리를/몸을/양손을/두 다리를")% 붙잡아서 %TINKO(MASTER)%를 %K17_RS("은밀한 곳/은밀한 틈")%에 %K17_RS("갖다대었다/비벼댔다")%
				
			ENDDATA
		ENDIF
		;대사
		SELECTCASE RAND:3
			CASE 0
				IF TEQUIP:구속
					PRINTFORML 「이런 식으로 %K17_RS("나를/여자아이를") %%K17_RS("속박/구속/인형 취급")% 하려 드는 거, 부끄럽지도 않아? 읏 %K17_RS(@"이 쓰레기/변태\@TALENT:MASTER:성별==2? 자식#\@/최저/강간마")%!」
				ELSE
					PRINTFORML 「이런 식으로 %K17_RS("나를/여자아이를") %%K17_RS("억지로/힘으로 제압해서/강제로")% 하려 드는 거, 부끄럽지도 않아? 읏 %K17_RS(@"이 쓰레기/변태\@TALENT:MASTER:성별==2? 자식#\@/최저/강간마")%!」
				ENDIF
			CASE 1
				;구무렵
				PRINTFORML 「큭……%K17_RS("믿고 있었는데/맘대로 하던가/절대 용서 안 할거야/그냥 죽이세요")%!」
			CASE 2
				PRINTFORML 「지금이라면 아직 %K17_RS("용서 받을 수 있어/돌아갈 수 있어/돌이킬 수 있어")%……크읏……부탁해 %K17_RS("그만둬/그만 두세요/멈춰")%!」
		ENDSELECT
	ENDIF
	;피임
	SIF !(TEQUIP:MASTER:콘돔) && !(TCVAR:TARGET:피임약)
		PRINTFORML 「%K17_RS("피임/고무써줘어")%!　적어도 %K17_RS("안에는 싸지마/밖에다 싸줘")%!」
	PRINTW
	;삽입
	CALL K17_MESSAGE
	;삽입 반응
	PRINTFORML 「%K17_RS("시러엇/안대앳/그만햇") % %K17_AP_DEC()%……이 %K17_RS(@"쓰레기/변태 %K17_AP_DEC()%변태")%%K17_AP_DEC(1)%」
	;억지로 가치라면 리액션 합의 덮쳐라면 보통으로 낸다
	SIF TCVAR:TARGET:억지로
		CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	;몸의자세별
	IF TFLAG:SELECTCOM분기
		PRINTFORMDL %마스터는% 절대 도망치지 못하게끔 신체를 구속하듯이 %타겟을% 꽉 껴안았다 
	ELSE
		PRINTFORMDL %마스터는% %CALLNAME:TARGET%의 몸을 덮어 씌우듯이, 몸으로 짓눌러서 절대 도망치지 못하게끔 신체를 구속했다
	ENDIF
	RETURN 1
ENDIF

;-------------------------------------------------
;60, 립 정도
;비처녀 접수_도움 계
;연모립 정도 일인외
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
{
IF LOCAL && CFLAG:K17C_CF_허덕여타입 == K17C_접수_도움계 && K17_GRT(TARGET) >= K17C_연모 && 
	TCVAR:V삽입관리플래그 == -1 && TFLAG:SELECTCOM분기 && iTarget_Num == 1 && !(INROOM(CFLAG:현재위치))
}
	;범용
	CALL K17_MESSAGE
	IF FIRSTTIME(@"{SELECTCOM}말-{K17C_접수_도움계}")
		;첫회
		PRINTFORML 「흐으윽……%K17_AP_DEC(1)%」
		PRINTFORMDL %타겟은% 입을 눌러서, 신음 소리를 참고 있었다
		PRINTFORML (밖인데 ,%cStyle% 넣어 버리다니……진짜로……하는 거구나)
		PRINTW
	ELSE
		;2회째 이후
		PRINTFORML 「%K17_RS("크으읏/히으응/흐으윽") % %K17_AP_DEC()% 밖인데 이렇게, %cStyle% %K17_RS("넣어 버리다/삽입해 버리다")%니 %K17_AP_DEC()%」
		PRINTDATAL
			DATAFORM 「다른 사람 %K17_RS("한테 들키기/오기")% 전에, 빨리 끝내엣……」
			DATAFORM 「소리 나와버리니까, %K17_RS("천천히/조용하게")% 해……」
			DATAFORM 「%K17_RS("비밀이/특별히 해준 거")%니까 ……다음엔 이런 짓 하면 안 돼……」
		ENDDATA
		PRINTW
	ENDIF
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL %타겟은% %CALLNAME:MASTER%의 목을 양팔로 감싸안아서 %K17_RS("몸을 밀착시켰다/얽매여왔다")%
	PRINTFORML (이런 모습…… 다른 사람에게 들키면, 어디서든 %K17_RS("욕정하는/엣찌하고 다니는") % %K17_RS("음란한 아가씨/엣찌한 아가씨")%라고, %K17_RS("들켜버/알려져버")%렷 )
	RETURN 1
ENDIF

;-------------------------------------------------
;60, 정상위
;비처녀 졸라 계 범용
