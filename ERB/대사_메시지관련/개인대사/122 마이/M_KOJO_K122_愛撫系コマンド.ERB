;==================================================
;0,愛撫
;==================================================
@M_KOJO_MESSAGE_COM_K122_0
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K122_0_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K122_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:광기
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「그래, 그런 느낌으로……후훗, %CALLNAME:MASTER%도 잘하네―%UNICODE(0x2764)%」
			PRINTFORMW 마이는 %CALLNAME:MASTER%의 애무에 황홀한 미소를 짓고 있다
		CASE 1
			PRINTFORML 「네 마음대로 해도 되는데? 자%UNICODE(0x2764)%」
			PRINTFORMW 마이는 몸을 조금 움직여 %마스터가% 만지기 쉽도록 해 주었다
		ENDSELECT
	;恋慕
	ELSEIF TALENT:연모
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「%마스터가% 만지면, 굉장히 두근거려……%UNICODE(0x2764)%」
			PRINTFORMW 마이는 가쁜 숨을 몰아쉬며 %조사처리(CALLNAME:MASTER,"로")%부터 주는 쾌락을 누리고 있다
		CASE 1
			PRINTFORML 「읏, 으읏%UNICODE(0x2764)% %CALLNAME:MASTER%, 기분 좋아…%UNICODE(0x2764)%」
			PRINTFORMW 마이는 %CALLNAME:MASTER%의 애무에 황홀한 미소를 짓고 있다
		ENDSELECT
	RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「응……후후, 나쁘지는 않네…%UNICODE(0x2764)%」
			PRINTFORMW 마이는 %CALLNAME:MASTER%의 애무에 만족스러운 미소를 지었다
		CASE 1
			PRINTFORML 「좀 더, 해도 좋다고?%UNICODE(0x2764)%」
			PRINTFORMW 마이는 %마스터를% 유인하듯이 신체를 밀착시켰다
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
