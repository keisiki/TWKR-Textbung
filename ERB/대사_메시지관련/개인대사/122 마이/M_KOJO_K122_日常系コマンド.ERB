;==================================================
@M_KOJO_SUCCESS_COM_K122_303
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K122_303
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K122_303_1
