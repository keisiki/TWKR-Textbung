@M_KOJO_MESSAGE_COM_K122_303_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE TFLAG:193 
