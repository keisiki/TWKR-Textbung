	RETURN
SIF CFLAG:ARG:1001
	RETURN
PRINTFORML %CALLNAME:ARG%は望まぬ洗脳セックスを強制されているという噂があります
CALL ASK_YN(@"そうだね （%CALLNAME:ARG%が大切な何かを失います）","うそだね （素質の変更をしません）")
IF !RESULT
	CFLAG:ARG:処女喪失記念日 = -1
	TALENT:ARG:キス未経験 = 0
	CFLAG:ARG:ファーストキス喪失記念日 = -1
	CSTR:ARG:処女喪失履歴 = 望まぬ洗脳セックスで失った
	CSTR:ARG:アナル処女喪失履歴 = 望まぬ洗脳セックスで失った
	CSTR:ARG:ファーストキス喪失履歴 = 望まぬ洗脳セックスで失った
	EXP:ARG:キス経験 = 20 + RAND:5
	EXP:ARG:Ｖ経験 = 50 + RAND:20
	EXP:ARG:Ｖ性交経験 = 20 + RAND:10
	EXP:ARG:Ａ経験 = 15 + RAND:5
	EXP:ARG:Ａ性交経験 = 10 + RAND:5
	EXP:ARG:精液経験 = 30 + RAND:10
	EXP:ARG:膣射経験 = 10 + RAND:5
	EXP:ARG:肛射経験 = 5 + RAND:5
	CALL COLORMESSAGE(@"%CALLNAME:ARG%は【処女】を喪失し、心の平衡を失ってしまった……",C_YELLOW,1)
	CFLAG:ARG:1001 = -1
	IF TALENT:ARG:処女 == 1
		TALENT:ARG:処女 = 0
	ENDIF
	TALENT:ARG:狂気 = 1
ELSE
