;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 라이코의 구강을 범하기 시작했다.
			ELSE
				PRINTFORMW 「…쮸웁…응…」
				PRINTFORMW 「아…가득 내…♥」
			ENDIF
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 얼어붙은 시간 속에서 라이코의 손에 음경을 잡게 한이면서 구강을 범하기 시작했다.
	;연모
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「응…쪽…」
			PRINTFORMW 「……내자…?」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…응…」
			PRINTFORMW 「…쭈욱…응…」
		ELSE
			PRINTFORMW 「구…쑤욱…」
			PRINTFORMW 「…응…조임과는 주는…♥」
		ENDIF
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「아……」
			PRINTFORMW 「…응…쪽…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「쑤욱…응…」
			PRINTFORMW 「응…근심…」
		ELSE
			PRINTFORMW 「…분명하게 씻고 있어?」
			PRINTFORMW 「깨끗이 해 주어요…」
		ENDIF
	;그 이외
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…쭈욱…」
			PRINTFORMW 「응…이것…」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……」
			PRINTFORMW 「…응…쭈욱…」
		ELSE
			PRINTFORMW 「…앙…」
			PRINTFORMW 「…응…」
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K96_{SELECTCOM}_1
