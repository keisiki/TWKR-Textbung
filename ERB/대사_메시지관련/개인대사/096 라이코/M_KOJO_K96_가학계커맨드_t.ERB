;==================================================
@M_KOJO_MESSAGE_COM_K96_100
CALL M_KOJO_MESSAGE_COM_K96_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K96_100_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 라이코의 격려 하기 시작했다.
				RETURN 1
			ELSE
				PRINTFORMW 「…응!」
				PRINTFORMW 「…조금!…아앙 ♥」
				RETURN 1
			ENDIF
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 얼어붙은 시간 속에서 라이코의 격려 하기 시작했다.
		RETURN 1
	;V삽입 스팬킹
	ELSEIF LOCAL:1 && TEQUIP:50 == PLAYER
		IF RAND:3 == 0
			PRINTFORMW 「응…♥」
			PRINTFORMW 「…이나다…이것…♥」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「아♥…으응 ♥」
			PRINTFORMW 「…삽면서…♥」
			RETURN 1
		ELSE
			PRINTFORMW 「아앙 ♥…아앗 ♥」
			PRINTFORMW 「응…이제(벌써)…♥」
			RETURN 1
		ENDIF
	;V삽입 스팬킹
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		IF RAND:3 == 0
			PRINTFORMW 「응♥……」
			PRINTFORMW 「…엉덩이…♥」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「아♥…응…♥」
			PRINTFORMW 「…이것…기분이 좋은…♥」
			RETURN 1
		ELSE
			PRINTFORMW 「아♥…응 ♥」
			PRINTFORMW 「이제(벌써)…♥…바보…♥」
			RETURN 1
		ENDIF
	;연모
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「응…♥ 아…♥」
			PRINTFORMW 「…엉덩이…안되게 되어버리는…♥」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「아…♥ 응…♥」
			PRINTFORMW 「…이것…♥」
			RETURN 1
		ELSE
			PRINTFORMW 「아앗…♥ 이런 건…♥」
			PRINTFORMW 「엉덩이 바보같게 되어버리는…♥」
			RETURN 1
		ENDIF
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「응♥」
			PRINTFORMW 「…아♥…엉덩이 ♥」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「아♥…응…♥」
			PRINTFORMW 「…이것…너무…응 ♥」
			RETURN 1
		ELSE
			PRINTFORMW 「…아앙 ♥…히♥」
			RETURN 1
		ENDIF
	;그 이외
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「응…이제(벌써)…」
			PRINTFORMW 「…나의 엉덩이는 두드리는 것이 아니지만…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「너무…두드리지 말아 …」
			PRINTFORMW 「…아프다 고요…이래도」
			RETURN 1
		ELSE
			PRINTFORMW 「…응…」
			PRINTFORMW 「아프다…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

