@M_KOJO_MESSAGE_PALAMCNG_A_K96_0(ARG)
;-------------------------------------------------
;콘돔
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;시간중
	IF FLAG:70
		PRINTFORMW 얼어붙은 시간 속에서 %마스터는% 라이코의 구강에 정액을 토해냈다.
	ELSEIF TALENT:MASTER:농후정액
		IF RAND:3 == 0
			PRINTFORMW 「응…가득 모았었던 것…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…오나금이라도 했어?」
			RETURN 1
		ELSE
			PRINTFORMW 「응…충분히…♥」
			RETURN 1
		ENDIF
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「응…이제(벌써) 내 버렸어?」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…임신해버릴 것 같구나」
			RETURN 1
		ELSE
			PRINTFORMW 「…맛있을 것 같구나…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

