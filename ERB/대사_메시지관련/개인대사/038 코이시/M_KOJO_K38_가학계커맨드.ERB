;==================================================
;100,스팽킹
;==================================================
@M_KOJO_MESSAGE_COM_K38_100
CALL M_KOJO_MESSAGE_COM_K38_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K38_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 멈춘 시간 속에서 코이시의 엉덩이를 하염없이 두들겼다
		PRINTFORMW 나중에 몰아쳐올 고통에 울부짖는 코이시의 모습을 상상하며 더욱더 세게 엉덩이를 때려나갔다
		RETURN 1
	ELSE
		;PRINTFORMW 「」 
		IF FLAG:시간정지
			RETURN 1
		ENDIF
		IF ABL:마조끼 <= 5
			IF MARK:반발각인 == 3
				IF TALENT:연인
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMW 「윽...뭐..뭐하는거야아...」
						CASE 1
							PRINTFORMW 「아...파아...! 그만해줘어...」 
						CASE 2
							PRINTFORMW 「으으읏...싫어어...」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「읏...아아악...」 
						CASE 1
							PRINTFORMW 「잠...하지마아아...!」 
						CASE 2
							PRINTFORMW 「으으읏...싫어어...」
					ENDSELECT
				ENDIF
			ELSEIF TALENT:연인
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「으읏...아..아파아...용서해줘어...」 
					CASE 1
						PRINTFORMW 「히이잇...그..그만해에...」 
					CASE 2
						PRINTFORMW 「으으읏...싫어어...」
				ENDSELECT
			ELSEIF TALENT:연모||TALENT:사모
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「아흣...아파...그..그만...」
					CASE 1
						PRINTFORMW 「으윽...왜...왜 그래에...」
					CASE 2
						PRINTFORMW 「으으읏...싫어어...」
				ENDSELECT
			ELSEIF TALENT:섹프||TALENT:애욕
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「으아아....아파아...전혀 기분 안좋으니까...그만해...」
					CASE 1
						PRINTFORMW 「히끅...아파...왜 그러는거야...」 
					CASE 2
						PRINTFORMW 「으으읏...싫어어...」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「으...아파...! 그만해줘어...」 
					CASE 1
						PRINTFORMW 「흐읏...그...그만해...아프단말야...」
					CASE 2
						PRINTFORMW 「으으읏...싫어어...」 
				ENDSELECT
			ENDIF
			SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 코이시는 고통에 울먹이고 있다...
				CASE 1
					PRINTFORMW 갑작스런 고통에 코이시는 몸을 덜덜 떨었다
				CASE 2
					PRINTFORMW 코이시는 울먹이며 용서를 빌어왔다
				CASE 3
					PRINTFORMW 고통에 몸부림치며 어떻게든 벗어나려 하고있다...
			ENDSELECT
		ELSE
			IF MARK:반발각인 == 3&&!TALENT:연인
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「흐으읏...아...아픈데도오...」
					CASE 1
						PRINTFORMW 「히극...내 몸...어째서 기분 좋은거야아...」
					CASE 2
						PRINTFORM 「
						CALL KOISHI_CLIMAX(RAND:3)
						PRINTFORM ...%UNICODE(0x2764)%
						PRINTFORML 」
				ENDSELECT
			ELSEIF TALENT:연인
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「흐으읏...찌릿찌릿한거어...기분좋아아...」
					CASE 1
						PRINTFORMW 「아...마리가 멍해...」
					CASE 2
						PRINTFORM 「
						CALL KOISHI_CLIMAX(RAND:3)
						PRINTFORM ...%UNICODE(0x2764)%
						PRINTFORML 」
				ENDSELECT
			ELSEIF TALENT:섹프
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「...찌릿찌릿해에...머리속이 하얘져...」
					CASE 1
						PRINTFORMW 「아픈데에...기분 좋아...」
					CASE 2
						PRINTFORM 「
						CALL KOISHI_CLIMAX(RAND:3)
						PRINTFORM ...%UNICODE(0x2764)%
						PRINTFORML 」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「...기분 좋아...찌릿찌릿...따끔따끔...」
					CASE 1
						PRINTFORMW 「흐으윽...아파...좀더 쓰다듬어줘어엇...읏...쓰라려...」
					CASE 2
						PRINTFORM 「
						CALL KOISHI_CLIMAX(RAND:3)
						PRINTFORM ...%UNICODE(0x2764)%
						PRINTFORML 」
				ENDSELECT
			ENDIF
			SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 고통과 함께 찾아오는 쾌감에 코이시는 몸을 배배 꼬고 있었다
				CASE 1
					PRINTFORMW 코이시는 고통스러워 하는 한편 쾌감에 몸을 바르르 떨고 있다
				CASE 2
					PRINTFORMW 분명 고통스러운 행위인데도 코이시는 쾌감을 느끼며 부들부들 떨었다
				CASE 3
					PRINTFORMW 고통으로 느끼고 있는 코이시는 맞으면서도 작게 신음소리를 냈다
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1



;==================================================
;104,아이마스크
;==================================================
@M_KOJO_MESSAGE_COM_K38_104
CALL M_KOJO_MESSAGE_COM_K38_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K38_104_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ENDIF
		PRINTFORMW 「뭐...뭐야 이거...안보여...거기 있는거야...?」
		PRINTFORMW 앞이 보이지 않는다는 두려움에 코이시는 필사적으로 %마스터를% 찾았다
		RETURN 1
	ENDIF
	;基本セット
	;장착
	IF TEQUIP:눈가리개
		;時姦中
		IF FLAG:70
			RETURN 1
		ELSE
			IF ABL:마조끼 <= 5
				IF MARK:반발각인 == 3
					IF TALENT:연인
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「...거..거기 있는거 맞지..? 버리고 간거 아니지...?」
								PRINTFORMW 코이시는 필사적으로 %마스터를% 찾아 나섰지만 허사였다
							CASE 1
								PRINTFORMW 「...%CALLNAME:MASTER%...? 어디 가지 마...? 버리면 안돼...?」
								PRINTFORMW %마스터가% 자신을 버릴까 노심초사 하고있는것 같다 
						ENDSELECT
					ELSE
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「...안보여...어디 간거야...?」 
								PRINTFORMW 코이시는 고개를 갸웃거렸다
							CASE 1
								PRINTFORMW 「...갔어...? 아무도 없는거야?」
								PRINTFORMW 「...아직 있는거야...?」 
						ENDSELECT
					ENDIF
				ELSEIF TALENT:연인
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「에...어..어디간거야...?」
							PRINTFORMW 코이시는 무서운지 파르르 떨었다 
						CASE 1
							PRINTFORMW 「잠...안보여~...나 버리고 가는거 아니지...?」
							PRINTFORMW 코이시는 불안한듯 주위를 살펴보았지만 역시나 허사였다 
					ENDSELECT
				ELSEIF TALENT:연모||TALENT:사모
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「에...이..이제 뭐하려고...?」
							PRINTFORMW 「...저기~? 나 버리고 간거야...?」
							PRINTFORMW %마스터가% 대답을 않자 코이시는 무척 불안해 했다... 
						CASE 1
							PRINTFORMW 「히이...자...잠깐...버리고 가는거 아니지? 응?」
							PRINTFORMW 코이시는 무서운지 몸을 덜덜 떨고 있다
					ENDSELECT
				ELSEIF TALENT:섹프||TALENT:애욕
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「이..이제 뭐하려는거야...?」
							PRINTFORML 미지에 대한 공포심으로 덜덜 떠는 코이시였지만
							PRINTFORMW 한편으론 %마스터가% 또 어떤 쾌락을 보여줄지 기대하고 있는것 같다
						CASE 1
							PRINTFORMW 「...안 보여...이거 정말 기분 좋아지는거 맞아...?」
							PRINTFORMW 코이시는 입술을 삐죽였다
							PRINTFORMW 그러나 그 끝이 바들바들 떨리는것을 누구나 알아볼수 있었다...
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「이게 뭐가 좋은거야아...?」
						CASE 1
							PRINTFORMW 「...이거 싫어...빨리 벗겨줘...」
					ENDSELECT
					PRINTFORMW 코이시는 눈가리개가 마음에 안 드는것 같다
				ENDIF
			ELSE
				;마조끼 높아
				IF MARK:반발각인 == 3&&!TALENT:연인
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「...하아...안보여...이제 뭐하려고...?」
							PRINTFORMW 「심한짓은 하지마...」
						CASE 1
							PRINTFORMW 「으...이런게 뭐가 좋은지 참...」
							PRINTFORMW 그렇게 말하면서도 기대감에 얼굴이 새빨게 지고 있었다
					ENDSELECT
				ELSEIF TALENT:연인
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「하아아...이제 뭐할거야...?」
							PRINTFORMW 시야가 차단되는 상황에 코이시는 무척 기대하고 있다
						CASE 1
							PRINTFORMW 「흐으으...빨리이~ 빨리 뭔가 해줘어~...♥」
					ENDSELECT
				ELSEIF TALENT:섹프
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「이제 뭐할거야? 응?」
							PRINTFORMW 눈가리개를 씌우자마자 바로 다음 행위를 조르고 있다
						CASE 1
							PRINTFORMW 「에헤헤...다음은 뭐야?」
							PRINTFORMW 「온 몸이 민간해져서...뭘 해줘도 기쁘지만♥」
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「눈 가리는것도 재밌으니까 좋아...」
							PRINTFORMW 「그러고보니 난 눈 세 개를 다 닫아버렸네? 후후...」
						CASE 1
							PRINTFORMW 「이제 뭐 할려구?」
							PRINTFORMW 「뭐어...재밌으니 뭐든 좋지만~」
					ENDSELECT
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
	;해제
	ELSE
		IF ABL:마조끼 <= 5
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「흐으으...무서웠어...」
					PRINTFORMW 코이시는 안도의 한숨을 쉬었다
				CASE 1
					PRINTFORMW 「이젠 이런거 하지마...」
				CASE 2
					PRINTFORMW 「...나 이거 싫어...」
			ENDSELECT
		ELSE
			IF MARK:반발각인 == 3&&!TALENT:연인
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「...어라, 벌써 끝...?」
					CASE 1
						PRINTFORMW 「아읏...눈 부셔...」
				ENDSELECT
			ELSEIF TALENT:연인
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「에...? 끝이야...?」
						PRINTFORMW 「므...어쩔수 없지..」
					CASE 1
						PRINTFORMW 「읏...눈 부셔어...! 갑자기 벗기지 말아줘...」
					CASE 2
						PRINTFORMW 「...앗, %CALLNAME:MASTER%~♥」
						PRINTFORMW 코이시는 싱글벙글 웃으며 %마스터를% 바라보았다
				ENDSELECT
			ELSEIF TALENT:섹프
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「히잇...눈 부셔라...」
					CASE 1
						PRINTFORML 「...응? 끝이야...?」
						PRINTFORML 코이시는 무척 아쉬워했다
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「으으...눈부셔...! 갑자기 벗기면 어떡해...」
					CASE 1
						PRINTFORMW 「어라, 이제 안대는 끝이야...?」
				ENDSELECT
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;105,밧줄
;미실장
;==================================================
@M_KOJO_MESSAGE_COM_K38_105
CALL M_KOJO_MESSAGE_COM_K38_105_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K38_105_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;장착
	IF TEQUIP:밧줄
		;時姦中
		IF FLAG:70
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;해제
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1





