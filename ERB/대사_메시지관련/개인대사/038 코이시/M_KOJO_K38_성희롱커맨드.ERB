@M_KOJO_MESSAGE_COM_K38_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;PRINTFORMW 「」
	IF FLAG:70
		PRINTFORMW 멈춘 시간 속에서도 코이시의 엉덩이는 그 말랑함을 유지하고 있었다
	ELSEIF CFLAG:수면
		PRINTFORML 잠들어 있는 코이시의 엉덩이는 매우 말캉말캉했다
		PRINTFORML 깨지 않도록 최대한 조심하며 그 감촉을 즐겼다
	ELSE
		IF CFLAG:우후후&&TFLAG:193 != 1
			;야스중, 커맨드 실패 안뜸
			IF MARK:반발각인 == 3
				IF TALENT:연인
					SELECTCASE RAND:3
						CASE 0
							PRINTFORMW 「..부끄럽게에...」
							PRINTFORMW 「...싫은건 아니지만...」
						CASE 1
							PRINTFORMW 「에...간지러워...히히...」
							PRINTFORMW 「으응, 하지마아~」
						CASE 2
							PRINTFORMW 「으읏...%UNICODE(0x2764)% 변태...」
							PRINTFORMW 코이시는 싫은소리를 냈지만 내심 기뻐하는것 같은 표정이다
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「...그게 마음에 들어...?」
							PRINTFORMW 「으응...맘대로 해...닳는것도 아닌데...」
						CASE 1
							PRINTFORMW 「흐으...기분이 묘해...」
							PRINTFORMW 「...그렇게 까지 싫진 않지만」
					ENDSELECT
				ENDIF
			ELSEIF TALENT:연인
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「흐에에...%UNICODE(0x2764)% 그렇게 만지지마아아...%UNICODE(0x2764)%」
						PRINTFORMW 부끄러워하는 말투지만 실상 스스로 엉덩이를 흔들며 더 만져주길 바라고 있었다
					CASE 1
						PRINTFORMW 「말랑해? 에헤헤...부끄러워라...좋을대로 만져도 좋아-%UNICODE(0x2764)%」
						PRINTFORMW 코이시는 엉덩이를 쭉 내밀어 만지기 쉽게 해주었다
				ENDSELECT
			ELSEIF TALENT:연모||TALENT:사모||TALENT:애욕
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「헤에...엉덩이 만지는게 좋아?」
						PRINTFORMW 「%마스터라%면 맘껏 만져도 괜찮아...%UNICODE(0x2764)%」
					CASE 1
						PRINTFORMW 「으응~%UNICODE(0x2764)% 어쩐지 이상한 느낌...마음에 들어...%UNICODE(0x2764)%」
						PRINTFORMW 「좀더 잔뜩 만져줘...%UNICODE(0x2764)%」
				ENDSELECT
			ELSEIF TALENT:섹프
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「으응...쓰다듬지만 말구우...%UNICODE(0x2764)%」
						PRINTFORMW 「좀 더 안쪽으로...%UNICODE(0x2764)%」
						PRINTFORMW 코이시는 엉덩이를 흔들며 보채왔다
					CASE 1
						PRINTFORMW 「후에에...기분좋다...%UNICODE(0x2764)%」
						PRINTFORMW 엉덩이를 만져지며 무척이나 기분좋아보이는 표정을 짓고있다
						PRINTFORMW 조금 세게 만져주니 움찔움찔거리며 촉촉한 눈으로 %마스터를% 바라봤다 
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「내 엉덩이 어때? 말랑해?」
						PRINTFORMW 「후후...좀 더 잔뜩 만져도 괜찮아~ 닳는것도 아닌데」
						PRINTFORMW 코이시는 엉덩이를 쭉 내밀어 만지기 쉽게 해주었다
					CASE 1
						PRINTFORMW 「에헤헤...부끄러워라...」
						PRINTFORMW 「그래도 싫지만은 않은걸...%UNICODE(0x2764)%」
				ENDSELECT
			ENDIF
		ELSEIF TFLAG:193 == 1
			;커맨드 실패
			SELECTCASE RAND:2
				CASE 0
					CALL KOISHI_REFUSE
					PRINTFORMW 코이시는 무척이나 불만스러워 보이는 표정이다...
				CASE 1
					IF MARK:반발각인 == 3&&TALENT:연인 == 0
						PRINTFORMW 「...어딜 만지는거야...」
						PRINTFORMW 코이시는 무척이나 화난것 같은 표정으로 %마스터를% 째려봤다
					ELSE
						PRINTFORMW 「...이런거 안되는거 알면서...」
						PRINTFORMW 코이시는 %마스터를% 째려봤다
					ENDIF
			ENDSELECT
		ELSE
			;일상생활중, 커맨드 성공
			IF MARK:반발각인 == 3
				IF TALENT:연인
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「하아...뭐하는거야...」
							PRINTFORMW 코이시는 %마스터를% 바라보았다
							PRINTFORMW 「...싫다곤 안했어...그치만 이런건...부끄럽잖아...」
							PRINTFORMW 좀 더 과감히 엉덩이를 만져주자 코이시는 얼굴을 붉히며 %마스터를% 노려보았다
						CASE 1
							PRINTFORMW 「으응...자..잠깐...」
							PRINTFORMW 코이시는 당황한듯한 표정으로 %마스터를% 바라봤다
							PRINTFORMW 「힛, 하지마아...부끄럽게에...」
							PRINTFORMW 무척 부끄러웠는지 얼굴이 새빨게진채 엉덩이를 마구 주물러지고 있다
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「으응...잠깐...너무 만지잖아...」
							PRINTFORMW 「으...부끄러워...」
							PRINTFORMW 코이시는 새빨게진 얼굴을 양 손으로 가린채 얌전히 엉덩이를 만져졌다
						CASE 1
							PRINTFORMW 「하아앗...안대...」
							PRINTFORMW 부끄러워 하는 코이시의 엉덩이를 억지로 마구 주물러주었다
							PRINTFORMW 코이시는 아무 말도 못한채 새빨게진 얼굴로 발만 동동 굴렀다
					ENDSELECT
				ENDIF
			ELSEIF TALENT:연인
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「으응...부끄러워...」
						PRINTFORMW 「그치만 기분 좋아...」
						PRINTFORMW 코이시는 %마스터의% 손에 몸을 맡긴채 엉덩이를 마구마구 주물러졌다
					CASE 1
						PRINTFORMW 「하아아...」
						PRINTFORMW 코이시는 %마스터의% 몸에 기댄채 엉덩이를 주물러졌다
						PRINTFORMW 간간히 세게 만지면 움찔움찔 떨며 %마스터를% 촉촉한 눈빛으로 바라보았다  
				ENDSELECT
			ELSEIF TALENT:연모||TALENT:사모||TALENT:애욕
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「으응? 엉덩이 만지는게 좋아?」
						PRINTFORMW 「맘껏 만져줘~ 에헤헤...%UNICODE(0x2764)%」
						PRINTFORMW 코이시는 기분좋은듯 몸을 맡겨왔다
					CASE 1
						PRINTFORMW 「엉덩이만 만져도 괜찮아?」
						PRINTFORMW 「원한다면 다른데 만져도 괜찮긴 한데에」
						PRINTFORMW 코이시는 몸을 기대며 말했다
				ENDSELECT
			ELSEIF TALENT:섹프
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「으응%UNICODE(0x2764)% 정마알...호색한이라니까...%UNICODE(0x2764)%」
						PRINTFORMW 「내친김에 지금 할래...?」
						PRINTFORMW 코이시는 히죽히죽 웃고있다
					CASE 1
						PRINTFORMW 「엉덩이만으로 괜찮아? 다른데도 많잖아~」
						PRINTFORMW 「편식은 나쁜거라구~ 응?」
						PRINTFORMW 코이시는 몸을 기댄채 보채왔다
				ENDSELECT
				BASE:무드 += 200
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「으응? 이런게 좋아...?」
						PRINTFORMW 「뭐, 그럼 맘껏 만져」
						PRINTFORMW 코이시는 순순히 엉덩이를 내 주었다
						PRINTFORMW 별다른 반응없는 코이시의 엉덩이를 마음껏 만질수 있었다
					CASE 1
						PRINTFORMW 「에? 엉덩이 만지려구?」
						PRINTFORMW 「음...조금만이다?」
						PRINTFORMW 코이시는 %마스터의% 몸에 착 달라붙었다
						PRINTFORMW 코이시를 껴안으며 두 손 가득 말랑말랑한 엉덩이를 마구마구 만질수 있었다
				ENDSELECT
			ENDIF
		ENDIF
		;---
	ENDIF
ENDIF
RETURN 1
