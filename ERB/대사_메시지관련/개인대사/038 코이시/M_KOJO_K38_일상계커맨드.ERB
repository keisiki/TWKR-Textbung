@M_KOJO_MESSAGE_COM_K38_333_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
;PRINTFORMW 「」
	IF CFLAG:수면
		IF CFLAG:1400 == 0
			CFLAG:1400 = 1
		ENDIF
		RETURN 1
	ENDIF
	IF TFLAG:193 >= 5
		;취함 성공
			IF MARK:반발각인 == 3
				IF TALENT:연인
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「흐으으...후련해에...」
							PRINTFORMW 코이시는 할 말을 해서 그런지 조금이나마 기분이 나아진듯 하다...
						CASE 1
							PRINTFORMW 「아아..후련해에...히히...」
							PRINTFORMW 할말을 해서 조금 편안해진듯하다...
					ENDSELECT
				ELSE
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「...그허니까아아...조옴...좀만 더 상냐햐게에...해다라..고오....」
							PRINTFORMW 코이시는 꼬부라진 혀로 투덜투덜 불만을 털어놓았다
							PRINTFORMW 불만을 털어놓아 평소의 울분이 조금이라도 풀렸던것 같다...
						CASE 1
							PRINTFORMW 「...그런거야아아...으흑...」
							PRINTFORMW 한참동안 푸념을 털어 놓은 코이시는 갑자기 엉엉 울며 %마스터를% 껴안았다...
							PRINTFORMW 취해서인지 감정이 불안해진것 같다...
					ENDSELECT
				ENDIF
			ELSEIF TALENT:연인
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「...에헤헤...그래도 역시 내 말 잘 들어준다니까아~」
						PRINTFORMW 코이시는 기분이 나아졌는지 히죽히죽 웃으며 달라붙었다
					CASE 1
						PRINTFORMW 「...으응~ 조금 기분 좋아져써어~」
						PRINTFORMW 코이시는 생글생글 웃으며 %마스터를% 바라봤다
				ENDSELECT
			ELSEIF TALENT:연모||TALENT:사모||TALENT:애욕
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「...하아아~...후련하다아...」
						PRINTFORMW 코이시는 한참동안 푸념을 하곤 후련하다는듯이 기지개를 폈다
					CASE 1
						PRINTFORMW 「히끅...그래도, 내 말 끝까지 들어줬네, 고마워...」
						PRINTFORMW 「...우에에에엥...」
						PRINTFORMW 술때문에 감정이 격해졌는지 또다시 훌쩍훌쩍 울기 시작했다
						PRINTFORMW %마스터의% 가슴팍에 달려들어선 엉엉 울며 놔줄 생각을 안했다... 
				ENDSELECT
			ELSE
				PRINTFORMW 「...하아아아...이젠 좀 후련하네에에...」
				PRINTFORMW 코이시는 기분이 좀 나아진듯 하다...
			ENDIF
	ELSEIF TFLAG:193 == 4
