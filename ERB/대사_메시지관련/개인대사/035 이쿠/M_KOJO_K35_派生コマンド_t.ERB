;==================================================
;6, 삽입 G스포 고문
;==================================================
@M_KOJO_MESSAGE_SCOM_k35_6
CALL M_KOJO_MESSAGE_SCOM_k35_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_k35_6_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「후와아%UNICODE(0x2764)%! 응…… 뭐죠…… 방금 건……」
		PRINTFORML  당신은 이쿠의 약점을 발견하고선 그 곳을 집중적으로 파고들었다
		PRINTFORML 「거기잇%UNICODE(0x2764)% 좋아앗%UNICODE(0x2764)% 안 돼 머리가 새하얗게%UNICODE(0x2764)%%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回사정した
		IF TCVAR:104
			PRINTFORML 「방금%UNICODE(0x2764)% 사정했으면서%UNICODE(0x2764)%」
			PRINTFORML 「거기 좋아%UNICODE(0x2764)% 안 돼%UNICODE(0x2764)% 게롭히면 안대에%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「햐읏%UNICODE(0x2764)% 거기 조아%UNICODE(0x2764)% 더 게롭혀져어%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「으응%UNICODE(0x2764)% 거긴 약한 곳이라%UNICODE(0x2764)% 안 돼%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「기분 좋은 것만 잔뜨윽%UNICODE(0x2764)% 이상해져버려%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_k35_{SELECTCOM}_1
RETURN 1

