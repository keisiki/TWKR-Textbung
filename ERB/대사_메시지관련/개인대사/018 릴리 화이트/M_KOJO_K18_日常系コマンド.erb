;-------------------------------------------------
;記入チェック（=0, 非表示,1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:시간정지
	ELSEIF CFLAG:318 == 1
		PRINTFORML 
	ELSEIF DAY:2 == 1  || TALENT:연모
		;春
		IF TALENT:연모 
			SELECTCASE RAND:2
				;CASE 1
					;PRINTFORML 
					;PRINTFORMW 
				CASE 0
					PRINTFORML 「보옴… Zzzz」
					PRINTFORMW 졸음이 쏟아지는지 릴리는 꾸벅꾸벅 졸았다.
				CASEELSE
					PRINTFORML 「꼬옥~」
					PRINTFORML 릴리는 %조사처리(CALLNAME:MASTER, "을")% 안는 것처럼 머리를 감싸 줬다.
					PRINTFORMW 「봄으로 감싸 줄게요~」
			ENDSELECT
		ELSEIF TALENT:사모
			SELECTCASE RAND:2
				;CASE 1
					;PRINTFORML 
					;PRINTFORMW 
				CASE 0
					PRINTFORML 「봄~ 봄~ 봄~ 이~ 에~ 요~♪」
					PRINTFORMW 릴리 자장가를 들으며 잠시 낮잠에 빠졌다.
				CASEELSE
					PRINTFORML 「~♪」
					PRINTFORMW 릴리의 포근한 향과 부드러운 무릎 감촉을 즐겼다.
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
				;CASE 1
					;PRINTFORML 
					;PRINTFORMW 
				CASE 0
					PRINTFORML 「어, 나비~」
					PRINTFORMW 릴리는 %CALLNAME:MASTER% 머리를 치우고 나비를 쫓아갔다.
				CASEELSE
					PRINTFORML 「으…」
					PRINTFORMW 릴리는 보채듯이 무릎을 들썩였다.
			ENDSELECT
		ENDIF
	ELSE
		SELECTCASE DAY:2
			CASE 2
			;夏
				IF TALENT:사모
					PRINTFORML 「………」
					PRINTFORMW 릴리는 오싹한 눈빛으로 %조사처리(CALLNAME:MASTER, "을")% 내려다봤다.
				ELSE
					PRINTFORML 「뜨거운 거예요………」
					PRINTFORMW 릴리는 무릎베개한 지 2초 만에 %조사처리(CALLNAME:MASTER, "을")% 밀쳤다.
				ENDIF
			CASE 3
			;秋
				IF TALENT:사모
					PRINTFORML 「봄이 어딨을까요…」
					PRINTFORMW 릴리는 %CALLNAME:MASTER% 머리에 몸을 맡기고 멍하니 있었다..
				ELSE
					PRINTFORML 「우울한 거예요…」
					PRINTFORMW 릴리는 %CALLNAME:MASTER% 머리를 만지작거리며 봄을 그리워했다.
				ENDIF
			CASE 4
			;冬
				IF TALENT:사모
					PRINTFORML 「부비부비…」
					PRINTFORML 「하아… 쓱쓱」
					PRINTFORMW 릴리는 %CALLNAME:MASTER% 뺨을 문지르며 몸을 녹이려 했다.
				ELSE
					PRINTFORML 「더 따뜻해지지 않을래요~? 덜덜덜」
					PRINTFORMW %CALLNAME:MASTER% 머리도 릴리 따라 덜덜 진동했다.
				ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
