CALL M_KOJO_MESSAGE_COM_K18_311_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K18_311_1
;-------------------------------------------------
;記入チェック（=0, 非表示,1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CHK_DATENOW(CFLAG:데이트중)
	ELSEIF CFLAG:수면
	ELSEIF CFLAG:우후후 == 2
		SELECTCASE TCVAR:20
			CASE 32
			;クンニ強制
				PRINTFORML 
			CASE 60
			;スカート内見せつけ
				PRINTFORML 
			CASE 73
			;素股
				PRINTFORML 
			CASE 85
			;V挿入する
				PRINTFORML 
			CASE 88
			;腰振りV
				PRINTFORML 
			CASE 86
			;だいしゅきホールド
				PRINTFORML 
			CASEELSE
				PRINTFORML 
		ENDSELECT
	ELSEIF TALENT:연모
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%한테서 봄 향기가 나요~♪」
				PRINTFORMW 릴리는 %CALLNAME:MASTER% 가슴에 뺨을 댔다.
			CASE 0
				PRINTFORML 「으응…」
				PRINTFORML 릴리는 %CALLNAME:MASTER% 품 속에서 몸을 꼼지락거렸다.
				PRINTFORMW 「…어, 어… 그게, 아무것도 아닌 거예요~!」
			CASEELSE
				PRINTFORML 「더 꼭 안아 줘도… 환영이에요~♪」
				
		ENDSELECT
	ELSEIF TALENT:사모
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「포옹이에요~」
				PRINTFORMW %조사처리(CALLNAME:MASTER, "이")% 안아 주자 릴리도 그에 응했다.
			CASE 0
				PRINTFORML %조사처리(CALLNAME:MASTER, "은")% 작은 릴리를 거뜬히 안아 들었다.
				PRINTFORMW 「멀리 보여요~! %조사처리(CALLNAME:MASTER, "은")% 힘이 장사네요~!」
			CASEELSE
				PRINTFORML 「응흐~ 봄 향기가 나요~」
				PRINTFORMW 릴리에겐 벚꽃 향기가 났다.
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「으응~!」
				PRINTFORMW 릴리는 버둥거렸다.
			CASE 0
				PRINTFORML 「으으~ 움직이기 힘든 거예요~」
				PRINTFORMW 「얼른 놓으세요~!」
			CASEELSE
				PRINTFORML 「우왓~!」
				PRINTFORML 「…왜 그러세요~?」
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
