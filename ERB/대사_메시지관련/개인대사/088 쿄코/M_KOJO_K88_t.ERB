;=================================================
; 노예의 절정시의 말
;---------------------------------------
; NOWEX:0~3 CVAB 절정
; NOWEX:이중절정 이중절정
; NOWEX:삼중절정 삼중절정
; NOWEX:사중절정 사중절정
; NOWEX:10 분유
; NOWEX:11 사정
; NOWEX:12 방뇨
; 적당하게 조합해 사용해 주세요
;=================================================
@M_KOJO_MESSAGE_PALAMCNG_B_K88
;분유
LOCAL = 0
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF

;사정
LOCAL = 0
IF LOCAL && NOWEX:11
	PRINTFORMW 
ENDIF

;방뇨
LOCAL = 0
IF LOCAL && NOWEX:12
	PRINTFORMW 
ENDIF

;절정
LOCAL = 0
IF LOCAL
	;사중절정
	LOCAL:1 = 0
	IF NOWEX:사중절정
		PRINTFORMW 
		RETURN 1
	ENDIF
	;삼중절정
	LOCAL:1 = 0
	IF NOWEX:삼중절정
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
		ENDSELECT
	ENDIF
	;이중절정
	LOCAL:1 = 0
	IF NOWEX:이중절정
		SELECTCASE NOWEX:이중절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
		ENDSELECT
	ENDIF
	IF NOWEX:0
		;강절정 C
		IF NOWEX:0 == 2
			PRINTFORMW 
		;절정 C
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:1
		;강절정 V
		IF NOWEX:1 == 2
			PRINTFORMW 
		;절정 V
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:2
		;강절정 A
		IF NOWEX:2 == 2
			PRINTFORMW 
		;절정 A
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:3
		;강절정 B
		IF NOWEX:3 == 2
			PRINTFORMW 
		;절정 B
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


