;===========================================================
; "EVENT"계 이벤트
;===========================================================
; 방에서 만남
;=================================================
@M_KOJO_EVENT_K88_1(ARG, ARG:1)
;말 주인은 TARGET
LOCAL = 1
IF LOCAL
	; 호환 처리 호출
	SIF !TCVAR:351
		CALL K88_KYOKO_UPDATE
	; 나눠쓰기가 힘들어서 어느 쪽이 들어온 경우에도 뭉쳐둠
	; 내부에서는 ARG로 분기할 가능성도 있다
	LOCAL:1 = 1
	IF LOCAL:1 && ARG
		; 업무중(아직 가득)
		IF CFLAG:행동 == 5 && BASE:일량 > MAXBASE:일량 / 2
			PRINTFORML 쿄코는 리듬 좋게 불경을 외우면서 청소 중이다
			PRINTFORMW 일이 끝나려면 좀 더 시간이 필요한 것처럼 보인다
			TCVAR:351++
		; 업무중(이제 곧 끝난다)
		ELSEIF CFLAG:행동 == 5 && BASE:일량 > 0
			PRINTFORML 쿄코는 콧노래를 부르면서 청소 중이다
			PRINTFORMW 일은 조금 더 하면 끝날 것 같다
			TCVAR:351++
		; 그 이외
		ELSE
			;아직 아침 인사하지 않았음
			IF TIME:2 <= 2 && TCVAR:오늘만났는지 == 1
				IF TALENT:연모 && RAND:2
					PRINTFORML 「안녕하세요―
					PRINTFORMW  안녕하세요―!」
					PRINTFORML 기운찬 인사와 함께 강아지가 재롱 부리는 것처럼 쿄코가 달려들었다
					PRINTFORMW 안녕 멍멍이라고나 이름을 붙일까? 마음속으로
					TCVAR:오늘만났는지 = 2
				ELSE
					PRINTFORML 「안녕하세요―!」
					PRINTFORML 오오, 안녕 안녕
					PRINTFORMW 오늘도 아침부터 기운이 팔팔한 모양이다
					TCVAR:오늘만났는지 = 2
				ENDIF
			; 아직 낮 인사 안 했음
			ELSEIF TIME:2 > 2 && TIME:2 <= 4 && TCVAR:오늘만났는지 <= 2
				IF TALENT:연모 && RAND:2
					PRINTFORMW 「안녕합쇼―!」
					PRINTFORML ……그 인사는 좀 아니다 싶어서 충고했더니
					PRINTFORMW 참배하러 왔던 마을 사람이 그렇게 말하길래 흉내를 낸 거라고 한다
					PRINTFORMW 네 이놈 순진한 애한테 뭔 인삿말을 불어넣은 거야
					IF TCVAR:오늘만났는지 == 2
						TCVAR:오늘만났는지 = 4
					ELSE
						TCVAR:오늘만났는지 = 3
					ENDIF
				ELSE
					PRINTFORML 「안녕하세요!!」
					PRINTFORMW 오늘도 절에 모두의 웃음을 부르는 듯한 인사가 울린다
					IF TCVAR:오늘만났는지 == 2
						TCVAR:오늘만났는지 = 4
					ELSE
						TCVAR:오늘만났는지 = 3
					ENDIF
				ENDIF
			; 아직 밤 인사 안 했음
			ELSEIF TIME:2 > 4 && TCVAR:오늘만났는지 <= 4
				IF TALENT:연모 && RAND:2
					PRINTFORML 「안녕하세요!」
					SIF TCVAR:오늘만났는지 == 4
						PRINTFORML 「오늘은 아침 인사부터 저녁 인사까지 컴플리트했네!」
					PRINTFORMW 생글생글 웃는 쿄코의 모습에 그만 넋 잃으며 바라보고 말았다.
					TCVAR:오늘만났는지 = 5
				ELSE
					PRINTFORML 「안녕하세아우울―!」
					PRINTFORML 인사 도중에 들린 울음소리를 의리 있게 반사해주는 건 좋은데
					PRINTFORML 메아리와 인사는 안 섞는 편이 낫다고 본다
					PRINTFORMW 물론 야마비코의 정체성을 빼앗을 수는 없으니 말로 할 수는 없다
					TCVAR:오늘만났는지 = 5
				ENDIF
			; 이미 인사가 끝난 상태
			ELSE
				IF TCVAR:351 > 8
					PRINTFORML 「왠지 오늘은 자주 보네」
					IF ARG == 2
						PRINTFORML 쿄코는 갸우뚱거리고 있지만, 그 원인은 쿄코가 돌아다니고 있기 때문이 않을까?
					ELSE
						PRINTFORML (혹시 따라다닌 걸까?)
						PRINTFORML 왠지 쿄코는 \@ TALENT:연모 ? 기쁜 듯한 # 복잡한\@ 표정을 짓고 있다
					ENDIF
				ELSEIF TCVAR:351 > 4
					; <공사중> 뭔가 떠오르면 나중에 다시 쓸게
					PRINTFORML ＼Google!/
				ELSEIF TCVAR:351 > 2
					; <공사중> 뭔가 떠오르면 나중에 다시 쓸게
					PRINTFORML ＼Excite!/
				ELSE
					; <공사중> 뭔가 떠오르면 나중에 다시 쓸게
					PRINTFORML ＼YAHOO!/
				ENDIF
			ENDIF
			TCVAR:351++
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;=================================================
; 아침에 일어 났을 때(안녕 말)
;---------------------------------------
; 말 주인은 TARGET
;=================================================
@M_KOJO_EVENT_K88_2(ARG, ARG:1)
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	; 호환 처리 호출
	SIF !TCVAR:351
		CALL K88_KYOKO_UPDATE
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;비동거(同棲) 한편 상대가 먼저 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET 방에 MASTER가 들어가 있다(연모시 동침중취침시만?
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER 방에 TARGET가 들어 오고 있다(연모 없음에서도 들어 올 가능성 있어
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 자고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTER가 일어나고 있어, 상대가 일어났다
	;동침 때 TARGET 기상은 이쪽(TARGET 쇠약 or 동침중TARGET 피곤함 패배 후 그대로 MASTER가 계속 일어난 다음 상대가 기상했을 경우 한정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;비동거(同棲)
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORML 「하움……, 안녕하세여―……」
			PRINTFORML 쿄코는 졸린 듯이 작은 하품을 손으로 가리며 인사했다
			PRINTFORMW 잠이 덜 깬 것일까? %마스터를% 잘 인식하지 못한 눈치다
			SETBIT TCVAR:350, (TIME:2 - 1)
			TCVAR:351++
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;동거(同棲) 안
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORML 「하움……, 안녕하세여―……」
			PRINTFORML 아직 졸린 듯이 작게 하품하면서 인사하는 쿄코의 머리를 벅벅 쓰다듬어주었다
			PRINTFORML 「꺅, 무슨 짓이야!」
			PRINTFORMW 부루퉁한 표정과 맞바꿔서 쿄코는 눈을 또렷하게 뜬 모양이다
			SETBIT TCVAR:350, (TIME:2 - 1)
			TCVAR:351++
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

;=================================================
; 밤에 잘 때(휴업(휴가) 말)
;---------------------------------------
; 말 주인은 TARGET
;=================================================
@M_KOJO_EVENT_K88_3(ARG, ARG:1)
LOCAL = 1
IF LOCAL
	;자기 방에 돌아간다(정시가 되어 잔다)
	LOCAL:1 = 1
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「안녕히 주무세요,%MASTERNAME:88%」
		PRINTFORMW 휴업(휴가)를 주고 받아\@ FLAG:더부살이캐릭터 == 8 ?  방으로 돌아가는 # 산에 돌아가는 \@쿄코를 전송했다
		RETURN 1
	ENDIF
	
	;졸린 듯이 하고 있다
	LOCAL:1 = 1
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「저기 벗긴데 있고의―……」
		PRINTFORML 졸릴것인가 라고 하는 질문에 꾸벅꾸벅배를 저으면서 음률의 이상한 말로 대답된다
		PRINTFORMW 누가 어떻게 봐도 졸린 것을 무리하고 교제하고 있는 감이 배어 나오고 있었다
		RETURN 1
	ENDIF
	
	;수면중(일어나지 않기 때문에 회화 라기 보다는 잠꼬대? )
	LOCAL:1 = 0
	IF LOCAL:1 && ARG == 3
		PRINTFORML 
		RETURN 1
	ENDIF
	
	;장난중
	LOCAL:1 = 0
	IF LOCAL:1 && ARG == 4
		PRINTFORML 
		RETURN 1
	ENDIF
	
	;쇠약중
	LOCAL:1 = 1
	IF LOCAL:1 && ARG == 5
		IF TALENT:연모 && FLAG:더부살이캐릭터 == 8
			PRINTFORML 「이제 안 돼-누군가 옮겨 주지 않을까―？」
			PRINTFORMW 쿄코는 거치거나 붐비어 %마스터를% 힐끔힐끔 보고 있다
			PRINTFORMW 알기 쉬운 태도에 쓴웃음하면서도 가벼운 쿄코를 안아 방까지 옮겨 넣었다
		ELSE
			PRINTFORML 「이제 안 돼……무리……」
			PRINTFORMW 흔들흔들\@ FLAG:더부살이캐릭터 == 8 ?  방 # 산 \@에 날아 가는 쿄코를 봐, 정말로 괜찮은 것인가 하고 걱정으로 되었다
		ENDIF
		RETURN 1
	ENDIF
	
	;MASTER가 다운(방까지 옮겨진다)
	LOCAL:1 = 1
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「응%MASTERNAME:88%,……는 자 버린다 것」
		PRINTFORML 대답이 없는 것에 깨닫으면(자)%마스터는% 깊이 잠들어 버리고 있었다
		PRINTFORMW 「, 어쩔 수 없네요」
		PRINTFORML 과연은 약해도 요괴라고 말해야할 것인가,
		PRINTFORMW 쿄코는\@ K88_KYOKO_DIFF_BODYSIZE(TARGET, MASTER) < 0 ?  체격차이를 물건이라고도 하지 않고 # \@%마스터를% 껴안으면(자) 방까지 날아 옮겨 갔다
		RETURN 1
	ENDIF
	
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

;=================================================
; 이동시 엇갈림
;---------------------------------------
; 말 주인은 TARGET
;=================================================
@M_KOJO_EVENT_K88_4(ARG, ARG:1)
LOCAL = 1
IF LOCAL
	;멈춰 섰다
	LOCAL:1 = 0
	IF LOCAL:1 && ARG == 1
		PRINTFORML 
		RETURN 1
	ENDIF
	
	;가볍게 인사 해 통과했다
	LOCAL:1 = 1
	IF LOCAL:1 && ARG == 2
		; 연모
		IF TALENT:연모
			; 업무중이라면 대사 추가
			PRINTFORML 「\@ CFLAG:행동 == 5 ?  업무중이니까 # \@또 다음에―！」
			PRINTFORMW 서두르고 있기 (위해)때문에 회화하고 있을 틈은 없었지만 건강하고 최상이다
		; 타
		ELSE
			PRINTFORML 「수고 하셨습니다!」
			PRINTFORMW 콧노래를 노래하고 있는 쿄코에 인사 해 통과하면(자), 건강의 양격려의 말을 걸 수 있었다
		ENDIF
		RETURN 1
	ENDIF
	
	;무시해 통과했다
	LOCAL:1 = 1
	IF LOCAL:1 && ARG == 3
		; 연인이라고 다음에 해명 하는 이벤트를 삽입한다
		IF TALENT:연인
			PRINTFORML 서두르고 있었으므로 그만 쿄코를 무시해 버렸다
			PRINTFORML 물끄러미 뒤를 되돌아 보면(자) 눈물로 귀를 늘어뜨려 낙담하고 있는 모습이 보였다
			PRINTFORMW ……다음에 시간이 생기면(자) 해명 해 둘까
			SETBIT TCVAR:352, 0
		ENDIF
		RETURN 1
	ENDIF
	
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

