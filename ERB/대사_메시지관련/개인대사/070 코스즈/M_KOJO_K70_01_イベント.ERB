;******************************************************************************
;■専用関数
;鈴奈庵判定
;1=鈴奈庵,2=自分部屋(引っ越した後も考慮),3=部屋追い出し
@M_KOJO_K70_SUZUNAAN(ARG)
#FUNCTION
IF MAIN_MAP == 2
	IF ARG == 213
		RETURNF 1
	ELSEIF ARG == 226
		;連れ出す関数ができてました＆特定条件で連れ出せるので対応しました
		IF COM_351_CONDITION(70)
			RETURNF 2
		;不法侵入,연모でも사모でもなく阿求と一緒でもなく連れ出すこともできない,要するに時止めて侵入した(一応阿求と一緒に入って阿求が出て行くパターンもある)
		ELSEIF !TALENT:70:연모 && !TALENT:70:사모 && CFLAG:MASTER:현재위치 != CFLAG:80:현재위치 && COM_351_CONDITION(70) == 0
			RETURNF 3
		ELSE
			RETURNF 2
		ENDIF
	ENDIF
ELSE
	IF ARG == 220
		RETURNF 1
	ELSEIF CFLAG:70:현재위치 == OMANEKIBEYA() || CFLAG:70:현재위치 == CFLAG:70:초기위치
		RETURNF 2
	ENDIF
ENDIF

;危ない所判定
@M_KOJO_K70_ABUNAAI(ARG)
#FUNCTION
SIF AT_HOME(70) && GROUPMATCH(MAIN_MAP, 3, 4, 5, 6, 7, 8, 9, 10)
	RETURNF 1

;分岐用のゲストキャラ判定
@M_KOJO_K70_GUEST(ARG)
#DIM CHARA
;ARG	0=会話, 1=お茶
VARSET RESULT
RESULTS = 
;阿求(80)は最優先
IF CFLAG:80:현재위치 == CFLAG:70:현재위치 && !TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(80, ARG)) && M_KOJO_K70_GUEST_ABLE(80)
	RESULTS = %CALLNAME:80%
	RESULT:1 = 80
	RETURN M_KOJO_K70_BALANCE(70, 80)
ENDIF
FOR LOCAL, 1, 10
	;霊夢(1),魔理沙(11),紫(26),文(29),마미조(69),阿求(80)
	CHARA = TARGET:LOCAL
	IF GROUPMATCH(CHARA, 1, 11, 26, 29, 69, 80) && !TCVAR:70:(M_KOJO_K70_GUEST_TCVAR(CHARA, ARG)) && M_KOJO_K70_GUEST_ABLE(CHARA)
		RESULTS = %CALLNAME:CHARA%
		RESULT:1 = CHARA
		;台詞を先に考えてたらくわからん分岐になってしまった
		;연모の小鈴と동행中(デート含む)
		IF CFLAG:70:동행 && TALENT:70:연모
			RETURN 4
		;鈴奈庵でゲストと동행中
		ELSEIF CFLAG:CHARA:동행 && M_KOJO_K70_SUZUNAAN(CFLAG:70:현재위치) == 1
			RETURN 3
		;ゲストとデート中
		ELSEIF CFLAG:CHARA:동행 && FLAG:데이트상대 == CHARA
			RETURN 2
		;연모のゲストと동행中
		ELSEIF CFLAG:CHARA:동행 && TALENT:CHARA:연모
			RETURN 2
		;小鈴が연모
		ELSEIF TALENT:70:연모
			RETURN 4
		ELSE
			RETURN 1
		ENDIF
	ENDIF
NEXT

;ゲスト可否
@M_KOJO_K70_GUEST_ABLE(ARG)
#FUNCTION
;수면中
SIF CFLAG:ARG:수면
	RETURNF 0
SIF CFLAG:70:수면
	RETURNF 0
;동침중
SIF CFLAG:ARG:동침중
	RETURNF 0
SIF CFLAG:70:동침중
	RETURNF 0
;浴場
SIF BATHCHECK(CFLAG:MASTER:현재위치)
	RETURNF 0
RETURNF 1

;ゲストキャラ変換用
@M_KOJO_K70_GUEST_TCVAR(ARG, ARG:1)
#FUNCTION
VARSET LOCAL
SELECTCASE ARG
;霊夢
CASE 1
	LOCAL = 361
;魔理沙
CASE 11
	LOCAL = 362
;紫
CASE 26
	LOCAL = 363
;文
CASE 29
	LOCAL = 364
;마미조
CASE 69
	LOCAL = 365
;阿求
CASE 80
	LOCAL = 366
ENDSELECT
SIF ARG:1
	LOCAL += 10
RETURNF LOCAL

;関係性比較関数,小鈴と阿求にしか使ってないけど一応汎用できるうに
@M_KOJO_K70_BALANCE(ARG:1, ARG:2)
#FUNCTION
;ARG:1	主対象
;ARG:2	比較対象
;独占
IF TALENT:(ARG:1):연인
	RETURNF 8
;悲恋
ELSEIF TALENT:(ARG:2):연인 && TALENT:(ARG:1):연모
	RETURNF 7
;揶揄
ELSEIF TALENT:(ARG:2):연인
	RETURNF 6
;信頼
ELSEIF TALENT:(ARG:1):연모 && (TALENT:(ARG:2):연모 || TALENT:(ARG:2):사모) && CFLAG:(ARG:1):신뢰도 > 1000
	RETURNF 5
;敵対
ELSEIF TALENT:(ARG:1):연모 && (TALENT:(ARG:2):연모 || TALENT:(ARG:2):사모)
	RETURNF 4
;연모
ELSEIF TALENT:(ARG:1):연모
	RETURNF 3
;応援
ELSEIF TALENT:(ARG:2):연모
	RETURNF 2
;平穏
ELSE
	RETURNF 1
ENDIF

