;ゲストキャラ変換用
@M_KOJO_K70_GUEST_TCVAR(ARG, ARG:1)
#FUNCTION
VARSET LOCAL
SELECTCASE ARG
;霊夢
CASE 1
	LOCAL = 361
;魔理沙
CASE 11
	LOCAL = 362
;紫
CASE 26
	LOCAL = 363
;文
CASE 29
	LOCAL = 364
;マミゾウ
