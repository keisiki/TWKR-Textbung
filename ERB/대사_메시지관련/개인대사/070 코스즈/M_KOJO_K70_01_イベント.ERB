		CALL M_KOJO_EVENT_K70_1_END
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック(=0, 非表示,1, 表示)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6 && !GETBIT(TCVAR:70:360, 3)
