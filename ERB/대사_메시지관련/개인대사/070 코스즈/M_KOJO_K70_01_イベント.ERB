;■専用関数
;鈴奈庵判定
;1=鈴奈庵,2=自分部屋（引っ越した後も考慮）,3=部屋追い出し
@M_KOJO_K70_SUZUNAAN(ARG)
#FUNCTION
IF MAIN_MAP == 2
	IF ARG == 213
		RETURNF 1
	ELSEIF ARG == 226
		;連れ出す関数ができてました＆特定条件で連れ出せるので対応しました
		IF COM_351_CONDITION(70)
			RETURNF 2
		;不法侵入,恋慕でも思慕でもなく阿求と一緒でもなく連れ出すこともできない,要するに時止めて侵入した（一応阿求と一緒に入って阿求が出て行くパターンもある）
		ELSEIF !TALENT:70:연모 && !TALENT:70:사모 && CFLAG:MASTER:현재위치 != CFLAG:80:현재위치 && COM_351_CONDITION(70) == 0
			RETURNF 3
		ELSE
			RETURNF 2
		ENDIF
	ENDIF
ELSE
	IF ARG == 220
		RETURNF 1
	ELSEIF CFLAG:70:현재위치 == OMANEKIBEYA() || CFLAG:70:현재위치 == CFLAG:70:초기위치
		RETURNF 2
	ENDIF
ENDIF

;危ない所判定
@M_KOJO_K70_ABUNAAI(ARG)
#FUNCTION
SIF AT_HOME(70) && GROUPMATCH(MAIN_MAP, 3, 4, 5, 6, 7, 8, 9, 10)
