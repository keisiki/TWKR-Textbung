;関係性比較関数,小鈴と阿求にしか使ってないけど一応汎用できるように
@M_KOJO_K70_BALANCE(ARG:1, ARG:2)
#FUNCTION
;ARG:1	主対象
;ARG:2	比較対象
;独占
IF TALENT:(ARG:1):연인
	RETURNF 8
;悲恋
ELSEIF TALENT:(ARG:2):연인 && TALENT:(ARG:1):연모
	RETURNF 7
;揶揄
ELSEIF TALENT:(ARG:2):연인
	RETURNF 6
;信頼
ELSEIF TALENT:(ARG:1):연모 && (TALENT:(ARG:2):연모 || TALENT:(ARG:2):사모) && CFLAG:(ARG:1):신뢰도 > 1000
	RETURNF 5
;敵対
ELSEIF TALENT:(ARG:1):연모 && (TALENT:(ARG:2):연모 || TALENT:(ARG:2):사모)
	RETURNF 4
;恋慕
ELSEIF TALENT:(ARG:1):연모
	RETURNF 3
;応援
ELSEIF TALENT:(ARG:2):연모
	RETURNF 2
;平穏
ELSE
	RETURNF 1
ENDIF

