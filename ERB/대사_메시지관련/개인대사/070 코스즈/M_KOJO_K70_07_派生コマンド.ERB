@M_KOJO_MESSAGE_SCOM_K70_9_1
#DIM CHARA
CHARA = MASTER_POSE(4,1,1)
;-------------------------------------------------
;記入チェック（=0, 非表示,1, 表示）
LOCAL = CFLAG:70:1006
;-------------------------------------------------
IF LOCAL
	IF CHARA == 80
		SELECTCASE M_KOJO_K70_BALANCE(70, 80)
		CASE 8, 5
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「아큐,같이 %CALLNAME:MASTER%씨에게 봉사하자구%UNICODE(0x2764)%」
			CASEELSE
				PRINTFORMW 「후후%UNICODE(0x2764)% 아큐와 반씩이네%UNICODE(0x2764)%」
			ENDSELECT
		CASE 7, 6
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「ㅅ,승부야 아큐！ 어느 쪽이 %CALLNAME:MASTER%씨를 기분 좋게 만드는지…！」
			CASEELSE
				PRINTFORMW 「ㅇ,으응……아큐만 치사하게……！ 나도……！」
			ENDSELECT
		CASE 4, 3
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「아큐보다 기분 좋게 해드릴게요！」
			CASEELSE
				PRINTFORMW 「아큐에게 넘겨주지는 않을테니까…읏！ 아음%UNICODE(0x2764)%」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 1
			PRINTFORMW 「응,으으……%CALLNAME:CHARA%씨만 치사해요……！ 저도……！」
		CASEELSE
			PRINTFORMW 「으ー읏！ %CALLNAME:CHARA%씨,제게도 주세요……읏！」
		ENDSELECT
	ENDIF
	;サディスティックになる
	IF NOWEX:MASTER:사정
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「응긋……구읏……어허시까요？ 조하요,잔뜩 내주세요%UNICODE(0x2764)%」
			PRINTFORMW 「응%UNICODE(0x2764)% 꿀꺽%UNICODE(0x2764)% 응,읏%UNICODE(0x2764)% 응,읏%UNICODE(0x2764)% 응,읏%UNICODE(0x2764)% 응%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「%CALLNAME:MASTER%씨, 이렇게 부풀려서……%UNICODE(0x2764)% 제가 편하게 해드릴게요%UNICODE(0x2764)%」
			PRINTFORMW 「츄읏%UNICODE(0x2764)% 츄픗%UNICODE(0x2764)% 응구읏%UNICODE(0x2764)% 꿀꺽%UNICODE(0x2764)% 에로에로…%UNICODE(0x2764)% 구폿%UNICODE(0x2764)% 쮸읍%UNICODE(0x2764)%」
		CASEELSE
			PRINTFORMW 「츄픗%UNICODE(0x2764)% 응구읏%UNICODE(0x2764)% 응,읏%UNICODE(0x2764)% 기분 조흐신가효？ 푸하…%UNICODE(0x2764)% 알겠어요…%UNICODE(0x2764)%」
			PRINTFORMW 「%ONEDARI_MASTER_TINKO()%우유, 잔뜩 주세요%UNICODE(0x2764)% 응풋%UNICODE(0x2764)% 꿀꺽%UNICODE(0x2764)% 꿀꺽%UNICODE(0x2764)% 」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「츄읏%UNICODE(0x2764)% 핥짜악…%UNICODE(0x2764)% 아음%UNICODE(0x2764)% 레로%UNICODE(0x2764)%」
		CASE 2
			PRINTFORMW 「하음…%UNICODE(0x2764)% 꿀꺽%UNICODE(0x2764)% 쮸읍%UNICODE(0x2764)% はぁ%UNICODE(0x2764)% んぷっ%UNICODE(0x2764)%」
		CASEELSE
			PRINTFORMW 「어허신가요～？이분 조흐신가요～？ 응,프웃%UNICODE(0x2764)% 츄읍%UNICODE(0x2764)% 꿀꺽%UNICODE(0x2764)%」
		ENDSELECT
	ENDIF
