@M_KOJO_MESSAGE_PALAMCNG_A_K26_22(ARG)
CALL PALAM_MESSAGE_A
;-------------------------------------------------
;シックスナイン
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;同時射精
	IF NOWEX:사정 && NOWEX:MASTER:사정
		PRINTFORMW
		RETURN 1
	;TARGETだけ射精
	ELSEIF NOWEX:사정
		;PLAYERについてる
		IF GETBIT(TALENT:PLAYER:성별,1)
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	;PLAYERだけ射精
	ELSE
		;TARGETについてる
		IF GETBIT(TALENT:성별,1)
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

