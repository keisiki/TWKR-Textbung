;==================================================
@M_KOJO_SUCCESS_SCOM_K26_63
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_SCOM_K26_63
CALL M_KOJO_MESSAGE_SCOM_K26_63_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K26_63_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	;恋慕
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「응…%UNICODE(0x2665) *1% 더 해줘어…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응…%UNICODE(0x2665) *1%… 조금, 더…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…으응%UNICODE(0x2665) *1% 정말, 너무 격렬해…%UNICODE(0x2665) *1%」
		ENDIF
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「…좀 더…%UNICODE(0x2665) *1%%CALLNAME:MASTER%의 입술, 줘어…%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응…%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…%UNICODE(0x2665) *1%…으응…%UNICODE(0x2665) *1%」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「으읏%UNICODE(0x2665) *1%」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응～%UNICODE(0x2665) *1%」
		ELSE
			PRINTFORMW 「…%UNICODE(0x2665) *1%응…%UNICODE(0x2665) *1%」
		ENDIF
	ENDIF
ENDIF
RETURN 1
