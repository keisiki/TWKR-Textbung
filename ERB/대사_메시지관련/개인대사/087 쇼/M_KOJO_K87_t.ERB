	IF NAME:MASTER != "당신"
		LOCALS = %NAME:MASTER%
	ELSE
		LOCALS = %CALLNAME:MASTER%
	ENDIF
	PRINTBUTTON @"[이름(%LOCALS%)을 자칭하는]", @"%LOCALS%"
	PRINTL 　
	;이름을 입력하는 루프
	$INPUT_LOOP_ONAMAE
	INPUTS
	;/훌륭한 쿄코 말의 환파크라고 감탄하지만 어디도 이상하지는 않다
	; 줄머리행 꼬리의 공백 문자 삭제
	LOCALS = %RESULTS%
	REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
	;10문자 이상은 받아들이지 않는다
	IF STRLENS(LOCALS) > 10
		CLEARLINE 1
		REUSELASTLINE 「…조금 너무 길네요. 통칭이라든지 닉네임은 가지고 있습니까?」
		GOTO INPUT_LOOP_ONAMAE
	;공백은 받아들이지 않는다
	ELSEIF RESULTS == ""
		PRINTL 이름을 입력해 주세요
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		; 동성 동명
		IF FLAG:역극 != 87 && STRCOUNT(LOCALS, "\(토라마루\)\*\\s\*쇼")
			CLEARLINE 1
			REUSELASTLINE 「…조롱하고 있지요」
			GOTO INPUT_LOOP_ONAMAE
		; 뱌쿠렌이 주인은 아니고, 뱌쿠렌이라고 하는 프레이즈가 포함되어 있다. 혹은
		; 쿄코가 주인은 아니고, 카소다니 혹은 쿄코라고 하는 프레이즈가 포함되어 있다
		ELSEIF (FLAG:역극 != 55 && STRCOUNT(LOCALS, "히지리\*\\s\*뱌쿠렌")) || (FLAG:역극 != 88 && STRCOUNT(LOCALS, "\(카소다니\)\*\\s\*쿄코\$"))
			CLEARLINE 1
			REUSELASTLINE 「…있잖아요 네」
			GOTO INPUT_LOOP_ONAMAE
		; 묘렌사세는 곧
		; 응 그렇네요 타타라 승고로라든지 가르치는 사람도 있을지도 모르는 걸
		ELSEIF (FLAG:역극 != 39 && LOCALS == "나즈린") || (FLAG:역극 != 40 && STRCOUNT(LOCALS, "\(타타라\)\*\\s\*코가사")) || (FLAG:역극 != 85 && STRCOUNT(LOCALS, "\(쿠모이\)\*\\s\*이치린")) || (FLAG:역극 != 86 && STRCOUNT(LOCALS, "\(\(무라사\)*\\s\*미나미츠\|무라사\)"))
