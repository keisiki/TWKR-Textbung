			IF STRCOUNT(LOCALS, "\(토라마루\)\*\\s\*쇼")
				CLEARLINE 1
				REUSELASTLINE 「…조롱하고 있지요」
				GOTO INPUT_LOOP_ONAMAE
			; 뱌쿠렌이 주인은 아니고, 뱌쿠렌이라고 하는 프레이즈가 포함되어 있다. 혹은
			; 쿄코가 주인은 아니고, 카소다니 혹은 쿄코라고 하는 프레이즈가 포함되어 있다
			ELSEIF (NO:MASTER != 5 && STRCOUNT(LOCALS, "히지리\*\\s\*뱌쿠렌")) || (NO:MASTER != 4 && STRCOUNT(LOCALS, "\(카소다니\)\*\\s\*쿄코\$"))
				CLEARLINE 1
				REUSELASTLINE 「…있잖아요 네」
				GOTO INPUT_LOOP_ONAMAE
			; 명련사세는 곧
			; 응 그렇네요 타타라 승고로라든지 가르치는 사람도 있을지도 모르는 걸
			ELSEIF (NO:MASTER != 1 && LOCALS == "나즈린") || (NO:MASTER != 2 && STRCOUNT(LOCALS, "\(타타라\)\*\\s\*코가사")) || (NO:MASTER != 3 && STRCOUNT(LOCALS, "\(쿠모이\)\*\\s\*이치린")) || (NO:MASTER != 4 && STRCOUNT(LOCALS, "\(\(무라사\)*\\s\*미나미츠\|무라사\)"))
				CLEARLINE 1
				REUSELASTLINE 「…우리 사람과 혼동하기 쉽기 때문에, 할 수 있으면 다른 것에」
				GOTO INPUT_LOOP_ONAMAE
			ELSEIF (NO:MASTER != 7 && STRCOUNT(LOCALS, "\(호쥬\)\*\\s\*누에")) || (NO:MASTER != 9 && STRCOUNT(LOCALS, "\(후타츠이와\)\*\\s\*마미조"))
				CLEARLINE 1
				REUSELASTLINE 「네…어쩌면 장난입니까?」
				GOTO INPUT_LOOP_ONAMAE
			; 도교 세력은 곧
			ELSEIF STRCOUNT(LOCALS, "\(미야코\)\*\\s\*요시카") || STRCOUNT(LOCALS, "\^곽\*\\s\*청아") || STRCOUNT(LOCALS, "\(소가\)\*\\s\*토지코") || STRCOUNT(LOCALS, "\(모노노베\)\*\\s\*후토") || STRCOUNT(LOCALS, "\(\(토요사토미미\)\*\\s\*미코\|성덕태자\)")
				CLEARLINE 1
				REUSELASTLINE 
				GOTO INPUT_LOOP_ONAMAE
			; 하쿠레이 레이무라고 하는 프레이즈가 포함되어 있다. 혹은
			; 코치야 사나에라고 하는 프레이즈가 포함되어 있다
			ELSEIF STRCOUNT(LOCALS, "\(하쿠레이\)\*\\s\*레이무") || STRCOUNT(LOCALS, "\(코치야\)\*\\s\*사나에")
				PRINTFORML 「당신은 본 느껴 그녀만큼은 선이…그것으로 좋습니까?」
			; 키리사메 마리사라고 하는 프레이즈가 포함되어 있다.
			ELSEIF STRCOUNT(LOCALS, "\(키리사메\)\*\\s\*마리사")
				PRINTFORML 「곧바로보다 구부러지는 레이저의 것이 강합니다!…그것으로 좋습니까?」
			; 직접 입력이라면 안된다고 생각했는지? 연주하군!
			ELSEIF ABL:친밀 < 4 && STRCOUNT(LOCALS, "\(\(\(형(오빠)\|에게 있고\)\|\(언니(누나)\|응\)\)\(씨\|\)\)\$")
				CLEARLINE 1
				REUSELASTLINE 「과연 그건 좀…」
				GOTO INPUT_LOOP_ONAMAE
			ELSEIF ABL:친밀 < 7 && STRCOUNT(LOCALS, "\(\(\(형(오빠)\|에게 있고\)\|\(언니(누나)\|응\)\)\(모양\|님\)\)\$")
				CLEARLINE 1
				REUSELASTLINE 「과연 그건 좀…」
				GOTO INPUT_LOOP_ONAMAE
			ELSEIF (ABL:순종 < 7 || ABL:봉사정신 < 7) && STRCOUNT(LOCALS, "\(\(\|\)\(주인\|주인\)\(모양\|님\)\)$")
				CLEARLINE 1
				REUSELASTLINE 「과연 그건 좀…」
				GOTO INPUT_LOOP_ONAMAE
			ELSEIF (!TALENT:연인 || !TALENT:연모) && STRCOUNT(LOCALS, "\(\(남편\|남편\)\(모양\|님\)\)\$")
				CLEARLINE 1
				REUSELASTLINE 「과연 그건 좀…」
				GOTO INPUT_LOOP_ONAMAE
			ELSE
				PRINTFORML 「%RESULTS%…군요」
			ENDIF
			PRINTL [0] 네
			PRINTL [1] 아니오
			;입력한 이름을 확정하는 루프
			$INPUT_LOOP_KAKUTEI
			INPUT
			IF RESULT == 0
				;당신의 이름을%MASTERNAME:87%에 격납
				MASTERNAME:87 = %RESULTS%
				PRINTFORML 
			ELSEIF RESULT == 1
				PRINTL 이름을 입력해 주세요
				GOTO INPUT_LOOP_ONAMAE
			ELSE
				PRINTL 이름을 입력해 주세요
				GOTO INPUT_LOOP_ONAMAE
			ENDIF
		ENDIF
	PRINTFORMW 「다시 한번, %MASTERNAME:87%씨」
	PRINTFORMW 「지금부터, 잘 부탁드려요」

RETURN 1

