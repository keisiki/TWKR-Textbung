;-------------------------------------------------
;立位
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && TFLAG:193
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		;処女
		IF TCVAR:파과 == 1 && !FLAG:70
			IF ALCOHOL_FACE(TARGET) == "주정꾼" || ALCOHOL_FACE(TARGET) == "곤드레만드레"
				;酔ってる
				PRINTFORML 「이…큿………머, 머애…? 아파…」
				PRINTFORMW 「아……아지만…좀 둥실둥실 해…?」
				PRINTFORMDL
				PRINTFORMDL 처녀를 잃었음을 나타내는 피가 %CALLNAME:TARGET%의 다리를 따라 흘러간다
				PRINTFORMDW %타겟은% 상황을 잘 모르는 것인지, 아픔을 달래려는 듯 허리를 움직이고 있다
				PRINTFORMDW 꽉 누르듯 깊숙히 삽입하니, 미지의 감각에 %CALLNAME:TARGET%의 허리가 살짝 들뜬다
				PRINTFORMDL
				PRINTFORMW 「앗………아앗…아으…」
				PRINTFORMDL
				PRINTFORMDW 심박수가 올라가고 더 취했는지, %CALLNAME:TARGET%의 얼굴이 유독 빨개진다
				PRINTFORMDW 탐하듯이 허리를 흔들자, %타겟은% 조금씩 교성을 지르기 시작했다…
			ELSE
				PRINTFORML
				PRINTFORML 「후읏…크읏………꽤 아프구나……」
				PRINTFORMDL
				PRINTFORMDL 파과의 피가 두사람의 결합부를 적셔간다…
				PRINTFORMDW %타겟은% 아랫입술을 조금 깨물어 통증을 견디고 있는 것 같다
				PRINTFORMDW %마스터는%, 부드럽게, 깊숙히 나아가고 있다
				PRINTFORML
				PRINTFORML 「히…끄윽……깊숙한 고옷……움찔하고………」
				PRINTFORML
				PRINTFORMDW 이제 익숙해졌는지, %타겟은% 무의식적인지, 안쪽을 문질문질하고 돌리듯이 허리를 흔들기 시작했다
				PRINTFORMDL 필요이상으로 아파하지 않아서 안심한 것과 동시에, 탐욕스러운 몸을 주체 못하는 %CALLNAME:TARGET%에 약간 어이없어하면서
				PRINTFORMDW %마스터는% 탐하듯이 허리를 흔들기 시작했다…
				PRINTFORML
			ENDIF
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;挿入継続
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;前回射精した
		IF TCVAR:102
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
