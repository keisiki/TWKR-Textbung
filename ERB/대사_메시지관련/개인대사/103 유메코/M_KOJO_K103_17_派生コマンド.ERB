;==================================================
;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K103_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K103_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K103_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
	;	PRINTFORMW 
	;	RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
			LOCAL:2 = M_KOJO_K103_KANKEI()
			SELECTCASE LOCAL:2
			CASE 10, 11, 12, 13, 21, 22, 23, 24
				;特別な感情は抱いていない時
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「으읏……거기, 문질문질 하면…」
				CASE 2
					PRINTFORMW 「뱃속에서 밀어올려지는 것 같아…읏」
				CASE 3
					PRINTFORMW 「…%M_KOJO_K103_AEGI()%…」
				CASEELSE
					PRINTFORMW 「안쪽 문질문질 하고……목소리 나와버려……읏」
				ENDSELECT
				PRINTFORML
			CASE 31,32,33,34,61,62,63,64
				;思慕ORセフレ
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「하아…읏…으읏………」
				CASE 2
					PRINTFORMW 「아앗, 아읏, 안돼앳…안돼앳…!」
				CASE 3
					PRINTFORMW 「앗…아앗…허리 멋대로 움직여버려……읏」
				CASEELSE
					PRINTFORMW 「뱃속에서 그렇게 날뛰지 말고……!」
				ENDSELECT
				PRINTFORML
			CASE 41,42,43,44,51,52,53,54,71,72,73,74
				;恋慕OR愛欲
				SELECTCASE M_KOJO_K103_RANDNR(4,6)
				CASE 1
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
				CASE 2
					PRINTFORMW 「으으으응읏…깊숙한 곳에 오고 있어……읏…%M_KOJO_K103_HEART()%」
				CASE 3
					PRINTFORMW 「…아앗, 거기, 거기이……!%M_KOJO_K103_HEART()%」
				CASEELSE
					PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()% 문질문질 하고…허, 허리 멈춰버려……읏…!%M_KOJO_K103_HEART()%」
				ENDSELECT
				PRINTFORML
			ENDSELECT
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K103_{SELECTCOM}_1
RETURN 1

