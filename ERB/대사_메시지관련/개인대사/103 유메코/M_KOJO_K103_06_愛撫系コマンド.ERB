	ENDIF
	;-------------------------------------------------
	;絶頂した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		PRINTFORMW 「아아앗, %M_KOJO_K103_AEGI()% 안돼, 안돼안돼안돼…%M_KOJO_K103_HEART()%」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모 || TALENT:애욕
		SELECTCASE M_KOJO_K103_RANDNR(8, 912);
		CASE 1
			PRINTFORMW 「앗, ……으읏…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「어때…맛있어……?%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「핥짝핥짝…기분좋아…%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「앗, 거기……으응, 크리토리스 있는데…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「더……좀 더……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「앗, 앗 크리토리스 빨면 안돼…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗……아앗…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「하아…%M_KOJO_K103_AEGI()%…아…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「거기, 거기를, 좀 더 만져줘……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「아으읏, ………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗, 앗, 안돼, 크리토리스를 그렇게……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「앗……안으로 혀가 들어왓…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗…안잇…안이이………%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「으읏 %M_KOJO_K103_AEGI()% 그런 필사적으로…읏%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:괘씸각인 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(8, 912);
		CASE 1
			PRINTFORMW 「앗, ……으읏…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「그런 곳…맛 없다고……?%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「손가락이랑…읏…다른 느낌으로…%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「앗, 크리토리스 방금…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗…앗……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「앗, 앗 크리토리스 빨면 안돼…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗……아앗…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「하아…%M_KOJO_K103_AEGI()%…아…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「거기, 거기를, 좀 더 만져줘……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「あ으읏, ………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗, 앗, 안돼, 크리토리스를 그렇게……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「앗……안으로 혀가 들어왓…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗…으으응………%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「으읏 %M_KOJO_K103_AEGI()% 그런 필사적으로…읏%M_KOJO_K103_HEART()%」
