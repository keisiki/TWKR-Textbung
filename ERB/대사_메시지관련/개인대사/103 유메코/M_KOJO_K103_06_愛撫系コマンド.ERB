@M_KOJO_MESSAGE_COM_K103_0_1
;-------------------------------------------------
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;1行入れたい
	PRINTFORML 
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
		PRINTFORML 「앗, 잠깐, 그만, 간지러워……읏…」
		PRINTFORML 
		PRINTFORMDW %CALLNAME:MASTER%의 애무가 간지러운지 유메코는 몸을 비틀고 있다
		PRINTFORMDW 그러나 시간이 지날수록, 점점 유메코의 숨이 가빠져간다…
		PRINTFORMDW 촉촉히 습기를 머금은 음부를 %M_KOJO_K103_ZURASHI()% 손가락으로 살짝 문지르자, 작은 울음소리를 흘렸다…
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML
			CALL M_KOJO_K103_CALL_ZAMEN()
			RETURN 1
		ELSE
			PRINTFORMW 「내 %M_KOJO_K103_GET("ヴァギナ")%, 질척질척 소리가 나……」
			;RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORML
			CALL M_KOJO_K103_CALL_ZAMEN()
			RETURN 1
		ELSE
			PRINTFORMW 「%M_KOJO_K103_GET("アヌス")%…, 아゛으읏……」
			;RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모 || TALENT:애욕
		SELECTCASE M_KOJO_K103_RANDNR(8, 911)
		CASE 1
			PRINTFORMW 「앗, ……으읏…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「더……좀 더 만져줘……%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「하아……으읏…귀가 약해…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「%M_KOJO_K103_AEGI()%……%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「앗, 거기 기분 좋아…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「더……좀 더……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「아아으읏……만지는 게 야해…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗…하지만………좋아, 기분이 좋아…아앗…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「하아……아…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「여기, 여기를, 더 만져줘……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「아으읏, ………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗, 앗, 안돼, 크리토리스를 그렇게……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「으으으읏…가슴, 가슴 좀 더 만져줘…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗…지분좋아……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「좀 더 여러 곳을 만져줘…%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗…그래……아아으읏……%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	;屈服3
	;ELSEIF MARK:괘씸각인 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		SELECTCASE M_KOJO_K103_RANDNR(8, 911)
		CASE 1
			PRINTFORMW 「앗, ……으읏…」
			PRINTFORMW 「아, 거기 기분좋아……%M_KOJO_K103_HEART()%」
		CASE 2
			PRINTFORMW 「하아……으읏…귀…」
			PRINTFORMW 「%M_KOJO_K103_AEGI()%……%M_KOJO_K103_HEART()%」
		CASE 3
			PRINTFORMW 「앗, 거기, 거깃…」
			PRINTFORMW 「앗…앗……%M_KOJO_K103_HEART()%」
		CASE 4
			PRINTFORMW 「아아으읏……만지는 게 야해…」
			PRINTFORMW 「앗…하지만………좋아, 기분이 좋아…아앗…%M_KOJO_K103_HEART()%」
		CASE 5
			PRINTFORMW 「하아……아…」
			PRINTFORMW 「여기, 이쪽, 이쪽이 기분 좋아……%M_KOJO_K103_HEART()%」
		CASE 6
			PRINTFORMW 「아으읏, ………%M_KOJO_K103_HEART()%」
			PRINTFORMW 「앗, 안돼, 크리토리스를 그렇게……%M_KOJO_K103_HEART()%」
		CASE 7
			PRINTFORMW 「으으읏…가슴 느껴져…」
			PRINTFORMW 「앗…기분좋아……%M_KOJO_K103_HEART()%」
		CASEELSE
			PRINTFORMW 「온몸을 만지작거려…으으읏」
			PRINTFORMW 「앗…안돼……아아으읏……%M_KOJO_K103_HEART()%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1
