RETURN RESULT

@M_KOJO_MESSAGE_COM_K103_3_1
;-------------------------------------------------
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:70
			PRINTFORML
			PRINTFORMW 「……뭐랄까, 뱃속에 손가락이 들어와서, 이상한 느낌……」
			PRINTFORML
			PRINTFORMDL %타겟은% 어깨에 조금 힘을 주자, 처음 손가락의 감각을 확인하듯이, 
			PRINTFORMDW 질을 조여 온다
			PRINTFORML
			PRINTFORMW 「……후아앗…으응…처음인데……!」
			PRINTFORML
			PRINTFORMDL 조금 손가락을 질 벽에 갖다대고 문지르자, 애액이 바로 안쪽에서 흘러나온 것을 
			PRINTFORMDW 느꼈다
			PRINTFORMDL 오히려 스스로 무의식적으로 손가락을 기분 좋은곳으로 유도하도록 움직이는 그녀
			PRINTFORMDW 를 한 팔로 꽉 껴안으면서, 다시 손가락을 움직여 간다…
			PRINTFORML
		RETURN 1
	ENDIF
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	;ELSEIF TALENT:恋慕
	;	PRINTFORMW 
	;	RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	PRINTFORMW 
	;	RETURN 1
	;それ以外
	ELSE
		IF TCVAR:파과 == 1
			;指で処女奪っちゃった時
			PRINTFORMDW 손가락을 안쪽 깊숙히 넣자, 툭, 하는 느낌이 손끝에 와 닿았다
			PRINTFORMDW 잠시 음부를 바라보니, 파과의 증거인 선혈이 손가락에서 손에 흐르고 있었다
			PRINTFORML
			PRINTFORMW 「……에…아팟………엣…?」
			PRINTFORML
			PRINTFORMDW %타겟은% 무슨 일이 일어났는지 바로 깨닫지 못하고 
			PRINTFORMDW 당황한 표정을 지은 다음 뭐라 말할 수 없고 억울한 듯한, 그러다가 
			PRINTFORMDW 쓸쓸한 표정을 지은 다음 포옹을 요구하듯이 손을 내밀었다
			PRINTFORML
			PRINTFORMW 「……저기…아파………」
			PRINTFORML
			PRINTFORMDW %마스터는% 일단 그녀를 달래기로 했다
			PRINTFORML
			RETURN 1
		ENDIF
		
		LOCAL:1 = M_KOJO_K103_KANKEI()
		SELECTCASE LOCAL:1
		CASE 10, 11, 12, 13, 21, 22, 23, 24
			;特別な感情は抱いていない時
			SELECTCASE M_KOJO_K103_RANDNR(4,3)
			CASE 1
				PRINTFORMW 「속잇……으……아으……」
			CASE 2
				PRINTFORMW 「기다려…줘, 다, 다리, 가 부들부들 떨려 버려……」
			CASE 3
				PRINTFORMW 「질척질척 소리가 나네…안 들려줘도 되니까, 그렇게 격렬하게엣……!」
			CASEELSE
				PRINTFORMW 「앗……히으…읏! 따끔따끔해…읏!」
			ENDSELECT
			PRINTFORML
		CASE 31,32,33,34,61,62,63,64
			;思慕ORセフレ
			SELECTCASE M_KOJO_K103_RANDNR(4,3)
			CASE 1
				PRINTFORMW 「…아앙……안돼, 휘젓지 마…읏……」
			CASE 2
				PRINTFORMW 「안은…안돼애…기분 좋아앗…!」
			CASE 3
				PRINTFORMW 「으응……아앙……읏……」
			CASEELSE
				PRINTFORMW 「으…으응……하앗…핫………휘적휘적…하지 말아줘어…!」
			ENDSELECT
			PRINTFORML
		CASE 41,42,43,44,51,52,53,54,71,72,73,74
			;恋慕OR愛欲
			SELECTCASE M_KOJO_K103_RANDNR(4,3)
			CASE 1
				PRINTFORMW 「안돼앳…기분좋아서 머리가 망가져버려……아, 거기…아아앙…%M_KOJO_K103_HEART()%」
			CASE 2
				PRINTFORMW 「속을 휘젓고 다니면, 의식 날라버릴거 같아……하앗…아아……%M_KOJO_K103_HEART()%」
			CASE 3
				PRINTFORMW 「…%M_KOJO_K103_AEGI()%…안돼, 안돼애애…%M_KOJO_K103_AEGI()%…%M_KOJO_K103_HEART()%」
			CASEELSE
				PRINTFORMW 「질척질척 소리 나게 하지 마… 이, 이제 알겠으니까아아……%M_KOJO_K103_HEART()%」
			ENDSELECT
			PRINTFORML
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

