;==================================================
;아침에 일어 났을 때(안녕 말)
;==================================================
@M_KOJO_EVENT_K86_2(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	SIF TCVAR:353 == 1
		TCVAR:353 = 0
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;비동거(同棲) 한편 상대가 먼저 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET 방에 MASTER가 들어가 있다(연모시 동침중취침시만의 괄
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER 방에 TARGET가 들어 오고 있다(연모 없음에서도 들어 올 가능성 있어
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;동거(同棲) 한편 상대가 자고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTER가 일어나고 있어, 상대가 일어났다
	;동침 때 TARGET 기상은 이쪽(TARGET 쇠약 or 동침중TARGET 피곤함 패배 후 그대로 MASTER가 계속 일어난 다음 상대가 기상했을 경우 한정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;비동거(同棲)
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			IF CFLAG:MASTER:동침중
				PRINTFORML 「…응…… 후아…………」
				PRINTFORMW 「아…… 안녕 %CALLNAME:MASTER%, 후훗」
				;무라사사실 이외의 방을 사실로 설정해 있다(조건적으로 있을 수 있지 않고
				;SIF CFLAG:MASTER:311 != 14
					;PRINTFORMW 「%마스터가% 왔었던 것이라면… 좀 더 일찍 일어나고 있으면 괜찮았을까」
				PRINTL
				PRINTFORMW %마스터와% %타겟은% 따스함을 아끼면서 이불을 빠져 나갔다…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;동거(同棲) 안
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF CFLAG:MASTER:동침중
				PRINTFORML 「…응…… 후아…………」
				PRINTFORMW 「아…… 안녕%CALLNAME:MASTER%, 후훗」
				;무라사사실 이외의방을사실로 설정해 있다(조건적으로 있을 수 있지 않고
				;SIF CFLAG:MASTER:311 != 14
					PRINTFORMW 「%마스터가% 왔었던 것이라면… 좀 더 일찍 일어나고 있으면 괜찮았을까」
				PRINTL
				PRINTFORMW %마스터와% %타겟은% 따스함을 아끼면서 이불을 빠져 나갔다…
			ENDIF
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
