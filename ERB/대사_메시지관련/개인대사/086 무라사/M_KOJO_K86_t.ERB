;			PRINTFORMW 
;			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입 계속
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:100 == PLAYER
		;전회 사정했다
		IF TCVAR:102
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	PRINTFORMW 「… 하… … 」
