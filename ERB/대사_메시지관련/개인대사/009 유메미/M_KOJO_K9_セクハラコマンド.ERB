@M_KOJO_MESSAGE_COM_K9_330_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		;시간정지시
		PRINTFORML 시간정지 중, 유메미의%SHOW_BOTTOM(TARGET, 1)%에 손을 댄다.
		PRINTFORML 물론이지만 저항은 없고, 용이하게 그 내용을 볼 수가 있었다.
		PRINTFORML 그러나 한편, 시간이 멈추어 있지만이기 때문에 반응도 없고, 조금 허무하기도 한다.
	ELSE
		SELECTCASE TFLAG:193
			CASE 1
				;넘길 수 있습니다만 유메미짱, 입고 있습니다?
				IF CFLAG:291
					;입지 않아
					IF ABL:14 >=  8
						;노출벽 Lv8 이상의 유메미짱은 노 팬츠로 외출도 생기게 되는 엣치한 대학교수입니다
						IF PALAM:9 >=  5000
							;질척질척이 아닌가오이
							PRINTFORML 「봐 %CALLNAME:MASTER%, 나의 저기, 지금 이렇게 되어 버리고 있는 것……%UNICODE(0x2764)%」
							PRINTFORML 기쁜듯이 유메미는 스스로%SHOW_BOTTOM(TARGET, 1)%를 걷어올려, 음모가 무성한 흠뻑 젖음의 보지를 과시해 왔다.
							PRINTFORML 의복으로부터 풀어진 암컷의 냄새가 %CALLNAME:MASTER%의 비강을 자극해 마지않다
							PRINTFORML 「저기…… 보고 있을 뿐이 아니고…… 말야?」
							PRINTFORML 음미로운 미소를 띄운 유메미가 %CALLNAME:MASTER%의 귓전으로 속삭여 온다
							PRINTFORML 「……시·이라고%UNICODE(0x2764)%」
						ELSE
							PRINTFORML 「우후후…… 응 봐 %CALLNAME:MASTER%, 나오늘, 입지 않은 것……%UNICODE(0x2764)%」
							PRINTFORML 추잡한 미소를 띄운 유메미가 스스로%SHOW_BOTTOM(TARGET, 1)%를 걷어올려, 음모가 무성한 비부[秘部]를 쬐었다
							PRINTFORML 눈앞에 퍼지는 절경에 무심코 숨을 죽이는 %CALLNAME:MASTER%
							PRINTFORML 「아앗%UNICODE(0x2764)%, %CALLNAME:MASTER%로 보여지면…… 젖어 와 버린%UNICODE(0x2764)%」
							PRINTFORML 유메미가 말하는 대로, 흠칫흠칫 꿈틀거리는 질구[膣口]로부터는 이미 애액이 스며나오고 있어 음미로운 열기를 띠고 있다
							PALAM:9 += 500
						ENDIF
					ELSEIF ABL:14 < 8 && ABL:14 >=  6
						;노출벽 Lv6 이상, 어디에서라도 걸어 준다
						IF PALAM:9 >=  5000
							;질척질척이 아닌가
							PRINTFORML 「후후…… 나의 철벅철벅 젖고 있는 호색가 보지, 보고 싶은거야……? %UNICODE(0x2764)%」
							PRINTFORML 「좋아요, 보여 주는……%UNICODE(0x2764)%, 어떨까……?」
							PRINTFORML 유메미가 유혹하는 것 같은 눈초리로%SHOW_BOTTOM(TARGET, 1)%를 걷어올려, 음모가 무성한 흠뻑 젖어 보지를 드러냈다
							PRINTFORML 의복으로부터 풀어진 암컷의 냄새가 %CALLNAME:MASTER%의 비강을 자극해 온다……
						ELSE
							PRINTFORML 「조금…… %CALLNAME:MASTER%, 오늘, 입지 않았는데……%UNICODE(0x2764)%」
							PRINTFORML 그렇게 말하면서도 유메미는 스스로%SHOW_BOTTOM(TARGET, 1)%를 걷어올려, 음모가 무성한 비부[秘部]를 쬐었다
							PRINTFORML 「정말 %CALLNAME:MASTER%도 참, 호기심이지요……%UNICODE(0x2764)%」
						ENDIF
					ELSEIF ABL:14 < 6 && ABL:14 >=  3
						;노출벽 Lv3 이상 6 미만
						IF GET_TARGETNUM() >=  2
							;그 밖에 누군가 있다
							PRINTFORML 「조금 %CALLNAME:MASTER%, 안 돼요 여기다……」
							PRINTFORML 「사람이…… 없는 곳으로라면, 그……」
							PRINTFORML 「보여, 주는…… 으로부터……?」
						ELSE
							PRINTFORML 「이제(벌써), 그렇게 보고 싶은거야? %CALLNAME:MASTER%도 호기심이지요」
							PRINTFORML 「이봐요…… 오늘은…… 그……」
							PRINTFORML 「노 팬츠…… 야…… 는, 그렇게 초롱초롱 보지 말아요……\/\/\/\/」
						ENDIF
					ELSE
						;노출벽 Lv3 미만
						PRINTFORML 「, 그만두고 그만두엇…… 지, 지금은 그……!」
						PRINTFORML (입지 않으니까!! \/\/\/\/\/)
					ENDIF
				;입고 있다
				ELSE 
					IF ABL:14 >=  6
						;노출벽 Lv6 이상, 어디에서라도 걸어 준다
						PRINTFORML 「후후…… 보고 싶어……? 좋아요……%UNICODE(0x2764)%」
						PRINTFORML 그렇게 말하면 유메미는 주저 없고%SHOW_BOTTOM(TARGET, 1)%를 걷어올려, 속옷을 드러냈다
						PRINTFORML 「오늘은 %PANTSNAME(EQUIP:TARGET:하반신속옷2, TARGET)%이지만, 어때%UNICODE(0x2764)%」
						PRINTFORML 「저기, 보고 있는 것만으로 좋은거야……?」
					ELSEIF ABL:14 < 3
						;노출벽 Lv3 미만
						PRINTFORML 「안 돼요, 안된단 말야! 그렇게 하면 팬티 보여 버린다……\/\/\/」
					ELSE
						;노출벽 Lv3 이상 6 미만
						IF GET_TARGETNUM() >=  2
							;사람이 있다
							PRINTFORML 「좀, 여기는 과연 안 돼요…… 사람이 보고 있으면 어떻게 하는거야……\/\/\/」
							PRINTFORML 「사람이 없는 곳이라면, 얼마든지 봐도 좋으…… 니까?」
							PRINTFORML 「앗, , 그러니까, 보여 버리기 때문에 안된다고 말하고 있겠죠……\/\/\/」
						ELSE
							;사람이 없다
							PRINTFORML 「이제(벌써), 그렇게 보고 싶은거야? %CALLNAME:MASTER%도 호기심이지요」
							PRINTFORML 유메미는 부끄러워하면서도%SHOW_BOTTOM(TARGET, 1)%를 걷어올려, 속옷을 드러내 주었다
							PRINTFORML 「이봐요…… 오늘은……%PANTSNAME(EQUIP:TARGET:하반신속옷2, TARGET)%로 해 보았지만, 어떻게…… 일까? \/\/\/」
						ENDIF
					ENDIF
				ENDIF
			CASE 0
				;(이)나다도―(희 무성하고)
				PRINTFORML 「이제(벌써), %CALLNAME:MASTER%도 참, 그만두세요……\/\/\/\/」
			CASE -1
				;(이)나다도―(싫어)
				PRINTFORML 「좀, 바보 같은 흉내는 그만두세요」
			CASE -3
				;꼬집겠어 치한 치녀들
				PRINTFORML 「갑자기 어디에 손 늘리고 있어 변태」
				PRINTFORML 있고다다다다! 그만두어 주세요 가죽이 조각조각 흩어진다!
				PRINTFORML 「하? 후려쳐지지 않았던 것 뿐 좋다고 생각하세요」
			CASE -5
				;최대의 분노와 혐오를 담은 보칵! 보칵!
				PRINTFORML 「잇!? 갑자기 뭐 해 변태!!」
				PRINTFORML 보칵! 보칵!
				PRINTFORML 귀싸대기에 그치지 않고, 유메미에 굿으로 마음껏 머리를 후려쳐졌다! 혹으로부터 김 내자!
				PRINTFORML 「그 정도로 끝난 것이니까 고맙다고 생각하세요」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
