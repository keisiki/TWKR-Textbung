@M_KOJO_SPEVENT_K9_3(ARG, ARG:1)
;말주는 TARGET
;지문 표시
CALL SPEVENT_MESSAGE_3(ARG, ARG:1)
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
