	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;挿入中
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	;V挿入
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A挿入
	ELSEIF LOCAL:1 && TEQUIP:51 == PLAYER
		;射精した
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	ELSEIF TALENT:광기
		IF TALENT:연인
			PRINTFORML 「하아……%UNICODE(0x2764)% 우후후,%CALLNAME:MASTER%, 기분좋아…%UNICODE(0x2764)%」
			PRINTFORMW 사토노는 황홀한 표정을 지으며 %CALLNAME:MASTER%의 손을 받아 들이고 있다
		ELSE
			PRINTFORML 「으……읏………」
			PRINTFORMW 몸은 쾌락에 반응하는 것 같지만, 사토노의 눈은 허공을 응시하고 있다…
		ENDIF
	;恋慕
	ELSEIF TALENT:연모
		PRINTFORML 「하아……%UNICODE(0x2764)% 우후후,%CALLNAME:MASTER%, 기분좋아…%UNICODE(0x2764)%」
		PRINTFORMW 사토노는 황홀한 표정을 지으며 %CALLNAME:MASTER%의 손을 받아 들이고 있다
		RETURN 1
	;それ以外
	ELSE
		PRINTFORML 「후훗, 많이 만져줘♪」
		PRINTFORMW 사토노는 싱글벙글 사랑스러운 미소를 짓고 있다
		RETURN 1
	ENDIF
ENDIF
RETURN 1
