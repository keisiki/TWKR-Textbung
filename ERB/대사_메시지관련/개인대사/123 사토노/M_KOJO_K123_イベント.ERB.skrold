;==================================================
;デート帰り
;==================================================
@M_KOJO_SPEVENT_K123_3(ARG,ARG:1)
;口上主はTARGET
;地の文表示
CALL SPEVENT_MESSAGE_3(ARG,ARG:1)
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常帰宅
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:恋慕
			PRINTFORML 「今日はこれでお終い？ そっか…」
			PRINTFORMW 「ちょっと寂しいけど、仕方ないわね。それじゃあ、バイバイ%UNICODE(0x2764)%」
		ELSE
			PRINTFORML 「今日はお疲れ様。じゃあねー」
			PRINTFORMW 里乃は%CALLNAME:MASTER%に小さく手を振った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML （%CALLNAME:MASTER%、思ったよりも反省してたみたいね）
		PRINTFORML （じゃないとあんなに気遣ってくれるはずないし…）
		PRINTFORMW 里乃は%CALLNAME:MASTER%のことを完全に許したようだ。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「…あっ！」
		PRINTFORML 「言い忘れてたけど、まだ貴方のこと許したわけじゃないから♪」
		PRINTFORMW 里乃はにっこりと人懐こく微笑んだ。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「あ、言い忘れてた」
		PRINTFORML 「ご機嫌取り、ご苦労様でした♪」
		PRINTFORMW 里乃はにっこりと微笑んでいる…が、その目は笑っていない……。
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K123_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:狂気
			PRINTFORML 「……あ、%CALLNAME:MASTER%。元気？」
			PRINTFORMW 虚空を見つめていた里乃の目に光が戻った。
		ELSEIF TALENT:恋慕
			PRINTFORML 「あ、%CALLNAME:MASTER%。ごきげんよう～♪」
			PRINTFORMW 里乃は%CALLNAME:MASTER%に手を振ると、にこにこ笑いながら近寄ってきた。
		ELSE
			PRINTFORML 「あ、%CALLNAME:MASTER%がいる～」
			PRINTFORMW 里乃は%CALLNAME:MASTER%を見つけると、挨拶代わりに手に持っていた茗荷を軽く振った。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕
			PRINTFORML 「やっほ～、%CALLNAME:MASTER%～♪」
			PRINTFORML 背後から誰かにいきなり声をかけられた。
			PRINTFORMW 「うふふ、驚かせちゃった？ ごきげんよう、里乃だよー%UNICODE(0x2764)%」
		ELSE
			PRINTFORML 「お邪魔しまーす%UNICODE(0x2764)%」
			PRINTFORMW 背中の扉からいきなり里乃が現れた……。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:狂気
			PRINTFORML 「あっ」
			PRINTFORML 風呂に入ってきた里乃は、慌てて手に持っていたタオルで自身の裸体を隠す。
			PRINTFORML 「見てない、よね？」
			PRINTFORMW %CALLNAME:MASTER%が頷くと、里乃は少し安心したような表情を浮かべた。
		ELSEIF TALENT:恋慕
			PRINTFORML 「誰が入っているかと思えば…%CALLNAME:MASTER%だったのねー」
			PRINTFORML 里乃は%CALLNAME:MASTER%の視線を気にして、さっと自身の裸体を隠した。
			PRINTFORMW 「もう、そんなに見ないでよー。恥ずかしいから…」
		ELSE
			PRINTFORML 「%CALLNAME:MASTER%が入っていたのね。お邪魔しまーす」
			PRINTFORML 里乃はほっとした表情を浮かべると、特に恥じらう様子もなく湯に入ってきた。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:狂気
			PRINTFORML 「ひっ…」
			PRINTFORML 里乃の裸体には、あちこちに鬱血した痕や何かの"汚れ"が見て取れた。
			PRINTFORMW 彼女は顔を真っ青にすると、急いで風呂から出ていってしまった…。
		ELSEIF TALENT:恋慕
			PRINTFORML 「%CALLNAME:MASTER%が入っていたのね……」
			PRINTFORML 里乃は恥ずかしそうに頬を染め、自身の裸体をタオルで隠した。
			PRINTFORMW 「……後で入ろうかな。だって%CALLNAME:MASTER%に見られるの、恥ずかしいし…」
		ELSE
			PRINTFORML 「あら、先客がいたのねー」
			PRINTFORML 里乃は「出直すわー」と呟き、風呂から出ていってしまった。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:狂気
			PRINTFORML 「ひっ…」
			PRINTFORML 里乃の裸体には、あちこちに鬱血した痕や何かの"汚れ"が見て取れた。
			PRINTFORML 「み、見ないで…お願い……あっち行って……」
			PRINTFORMW %CALLNAME:MASTER%は大人しく風呂から出ることにした。
		ELSEIF TALENT:恋慕
			PRINTFORML 「あ、%CALLNAME:MASTER%が入っていたのね」
			PRINTFORML 「……えっと、今日は、一緒に入るのはちょっと困るんだけれど……」
			PRINTFORMW %CALLNAME:MASTER%は大人しく風呂から出ることにした。
		ELSE
			PRINTFORML 「あ、先客。なんだー、ゆっくり入れると思ったのにぃ」
			PRINTFORML 里乃にそう言われ、%CALLNAME:MASTER%は少し居心地が悪くなってしまった。
			PRINTFORMW ほどなくして、風呂を出ることにした…。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 「あれ、こんなところで奇遇だねー」
		PRINTFORMW 里乃は%CALLNAME:MASTER%に会釈した。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他キャラとデート中に遭遇した
	;ARG:1にデート相手
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:恋慕
			PRINTFORML 「へー。女の子とデートだなんて、%CALLNAME:MASTER%ってば暇なのねー」
			PRINTFORMW 里乃は少し不機嫌そうな顔で、棘のある言葉を投げかけた。
		ELSE
			PRINTFORML 「あららー、お熱いねー」
			PRINTFORMW 里乃はにやにやしながらこちらを見ている。
		ENDIF
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K123_2(ARG,ARG:1)
