		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K123_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時になって寝る）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:恋慕
			PRINTFORML 「ふあぁ……そろそろ寝ようかな……」
			PRINTFORML 「%CALLNAME:MASTER%、おやすみなさい。また今度♪」
			PRINTFORMW 里乃はにこりと微笑むと、%CALLNAME:MASTER%の背中の扉を使って帰っていった。
		ELSE
			PRINTFORML 「ふあぁ……さすがにもうおねむだわ。おやすみー」
			PRINTFORMW 里乃は勝手に%CALLNAME:MASTER%の背中の扉を開け、帰っていった。
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「うーん……」
		PRINTFORMW 里乃は眠たげに舟をこいでいる。
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:狂気
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「や……さわらないで……」
			CASE 1
				PRINTFORMW 「ひっ……きもちわるい………」
			CASE 2
				PRINTFORMW 「やめて……まいを…つれていかないで……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「すぅ……むにゃ……」
			CASE 1
