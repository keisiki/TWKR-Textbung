;==================================================
;5, 수음 펠라
;==================================================
@M_KOJO_MESSAGE_SCOM_K25_5
CALL M_KOJO_MESSAGE_SCOM_K25_5_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K25_5_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;연모
	IF TALENT:연모 || MARK:괘씸각인 == 3
		;지금 사정했다
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「…………응? 후후…………♪」
				PRINTFORML 눈을 치켜뜨고 시선만으로 말하는 란의 입에, 퓻퓻 마음껏 정액을 토해냈다
			CASE 1
				PRINTFORML 손가락, 입술, 혀의 3개의 감촉이 동시에 페니스를 괴롭힌다
				PRINTFORML 정액을 빨아들이는 흡입력까지 더해져, 더이상 참을 여유가 사라져버렸다
			CASE 2
				PRINTFORML 페니스를 사랑스럽게 어루만지는 손가락과 혀의 자극에 한계에 이르렀다
				PRINTFORML 사정의 여운이 사라져버릴 때까지, 란은 정성스럽게 봉사를 계속했다
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 입으로 귀두를 빨아들이며, 한 손으로는 뿌리까지 훑으며, 다른 한 손으로 불알을 상냥하게 애무한다
				PRINTFORML 란의 봉사는 매우 교묘하고 정중해서, 그 풍부한 경험을 엿볼 수 있었다
			CASE 1
				PRINTFORML 축축한 입속에서, 양쪽 불알 전부가 혀 위를 구른다
				PRINTFORML 빈 자지는 쿠퍼액으로 미끈미끈해진 손으로 훑어낸다는, 빈틈이 없는 봉사다
			CASE 2
				PRINTFORML 「응, 쮸웁…, 음, 츄릅……」
				PRINTFORML 수음과 동시에 혀를 움직이는 란의 입으로부터 추잡한 소리가 새어나왔다
			ENDSELECT
			RETURN 1
		ENDIF
	;그 이외
	ELSE
		PRINTFORML 가늘고 긴 손가락이 고리를 만들어, 음경의 뿌리를 부드럽게 훑는다
		PRINTFORML 집요하게 귀두를 핥는 혀가 사정을 재촉한다
		RETURN 1
	ENDIF
	PRINTW
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K25_{SELECTCOM}_1
RETURN 1

;==================================================
;6, 삽입 G스포 고문
;==================================================
@M_KOJO_MESSAGE_SCOM_K25_6
CALL M_KOJO_MESSAGE_SCOM_K25_6_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K25_6_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;시간중
	IF FLAG:70
		;지금 사정했다
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 미세한 떨림과 강한 압박감에, 란의 질육이 눈물 대신 애액으로 페니스에 용서를 빈다
				PRINTFORML 쾌락 고문의 마지막까지 가르쳐주기 위해, %마스터는% 가장 민감한 곳에 퓻퓻 정자를 토해냈다
			CASE 1
				PRINTFORML 하복부가 솟아오를 정도로 쑤셔박은 페니스가 퓻, 하고 사정한다
				PRINTFORML 정자를 짜내던 보지가 만족스럽다는 듯 전율하자, 란이라는 여자를 정복했다는 달성감이 들었다
			CASE 2
				PRINTFORML 다 토해낸 정액을 아직 부족한 듯 벌렁거리는 질벽에 박아넣는다
				PRINTFORML 잔뜩 사용해 사용감이 좋아진 암컷 구멍의 안에, 천천히 페니스를 왕복시키며 여운을 즐겼다
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML G스팟이라 불리는 범위를 푹푹 격렬하게 찔러간다
				PRINTFORML 시간이 멈춘 란의 몸은, 너무 강한 자극에 몸을 비비 꼬는 일조차 할 수 없다
			CASE 1
				PRINTFORML 질의 위쪽, 여성의 약점을 노려 계속 찌르자 그 순간 분비되는 애액이 증가했다
				PRINTFORML 질 입구도 훨씬 강하게 압박하기 시작해, 삽입한 페니스를 절대 놓치지 않으려 했다
			CASE 2
				PRINTFORML 란의 성감대를 용서 없이 집요하게 괴롭혀주었다
				PRINTFORML 잠깐 쉬려고 허리를 멈춰도, 가혹하게 쑤셔진 보지는 부들부들 계속 경련하고 있다
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:연모 || MARK:괘씸각인 == 3
		;지금 사정했다
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML G스팟에 강하게 귀두를 비비며 사정했다
				PRINTFORML 사정의 맥박에 맞추어 질구가 페니스를 꽉 조인다
			CASE 1
				PRINTFORML 「하앗, 흐으응……♡」
				PRINTFORML 민감한 질육과 거기에 삽입된 귀두 사이로 퓻, 하고 정액을 내뿜었다
			CASE 2
				PRINTFORML 퓨웃, 퓻, 퓨릇……
				PRINTFORML 정액이 뿜어져나올때의 마찰과 진동이 란의 성감대를 떨었다
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「윽, 읏, 웃, 후우……」
				PRINTFORML G스팟을 노리고 보지를 쑤실 때마다 란이 야한 신음을 흘렸다
			CASE 1
				PRINTFORML 「아, 잠, 기다, 려엇……♡, 」
				PRINTFORML 꽤나 드문 란의 나약한 소리를 무시하고, 거슬거슬한 질벽을 조금씩 밀어 올렸다
			CASE 2
				PRINTFORML 페니스를 반 정도 꽂아넣고, 약점을 찾아 입구 주변에서 빙글빙글 돌린다
				PRINTFORML 란이 입을 크게 벌리고 몸을 뒤로 젖혀, 온몸에 식은땀을 흘렸다
			ENDSELECT
			RETURN 1
		ENDIF
	ELSE
		PRINTFORML 페니스를 반정도 삽입하고 천장을 긁어내며, 여자의 약점을 자극했다
		PRINTFORML 란의 몸이 쾌감으로 힘이 빠지는 것을 느꼈다
		RETURN 1
	ENDIF
	PRINTW 
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K25_{SELECTCOM}_1
RETURN 1

