;==================================================
;44,바이브
;==================================================
@M_KOJO_MESSAGE_COM_K30_44
CALL M_KOJO_MESSAGE_COM_K30_44_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_44_1
;-------------------------------------------------
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	;時姦中・수면中
	IF CFLAG:MASTER:장난 == 2 || FLAG:70
		RETURN 1
	ENDIF
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF CFLAG:1801 == 1
			IF TCVAR:파과 != 0
				PRINTFORMW 「뭔가요, 그 도구는……아앗, 설마」
				PRINTFORMW 「안 돼요! 절대 안 돼! 그런 도구로 하라니 장난치는거지!?」
				PRINTFORMW 「……진심, 이야? …잠깐만……싫어, 제발…싫어엇!」
				PRINTFORMW 「그만해앳! …아아아아앗!!」
				TCVAR:30:기다리지않다 = 1
				TCVAR:30:억지로 = 1
				;플래그に残す
				SIF TCVAR:파과 == 1
					SETBIT CFLAG:1800, 2
				RETURN 1
			ELSE
				PRINTFORMW 「뭔가요, 그 도구는……히익!」
				PRINTFORMW 「싫어…안쪽이……떨리고 있어……」
				RETURN 1
			ENDIF
		ELSE
			IF TCVAR:파과 != 0
				PRINTFORML 「뭔가요, 그 도구는……아앗, 설마」
				PRINTFORMW 「잠깐, 잠깐 기다려주세요!……히익!」
				PRINTFORMW 「저는… 도구로 처음을………아뇨, 아무것도 아니에요……」
				RETURN 1
			ELSE
				PRINTFORMW 「뭔가요, 그 도구는……히익!」
				PRINTFORMW 「싫어…안쪽이……떨리고 있어……」
				RETURN 1
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;マグロスイッチオン
	IF CFLAG:1501 || TCVAR:30:억지로 || TCVAR:30:만취
		CALL K30_GASPING_MAGURO
		RETURN 1
	ENDIF
	;基本セット
	;바이브解除時
	IF !TEQUIP:바이브
		IF EXP:V경험 > 70
			IF RAND:2 == 1
				PRINTFORML 「아앗, 뽑는거야?…… 그럼, 다음은…」
				PRINTFORMW 「%K30_C_NAME(MASTER)%에게, 사랑받고 싶어」
			ELSE
				PRINTFORML 「……뽑지 마」
				PRINTFORMW 「좀 더 당신에게, 괴롭힘 받고싶어…♥」
			ENDIF
		;V경험40以上
		ELSEIF EXP:V경험 > 40
			PRINTFORML 「하아, 하아……」
			PRINTFORMW 「앗…… 끝, 이야?」
		;V경험20以上
		ELSEIF EXP:V경험 > 20
			PRINTFORMW 「으으읏……역시, 익숙해지지 않아…」
		;それ以外
		ELSE
			PRINTFORMW 「싫어, 또……거기에, 뭔가 꽂혀있는 느낌이…」
		ENDIF
	;%TINKO%引き抜いてからすぐ入れた
	ELSEIF TCVAR:100 == MASTER
			IF EXP:V경험 > 100
				IF RAND:2 == 1
					PRINTFORML 「하으……으읏♥　이상한 곳에, 닿았어엇!」
					PRINTFORMDW 바이브로 몸 속에서 휘저어지며, 에이키의 표정도 녹아내리기 시작했다…
				ELSE
					PRINTFORML 「아하아아앗♥　굉장해앳! 이쪽도, 굉장해앳♥」
					PRINTFORMDL %마스터가% 바이브에서 손을 떼자, 에이키의 신체와 마찬가지로 바이브가 씰룩씰룩 움직이고 있다
					PRINTFORMDW 에이키의 질이 바이브를 삼키기 위해 우물거리고 있는 것처럼 보인다…
				ENDIF
			ELSEIF EXP:V경험 > 20
				PRINTFORMW 「아으…♥　도구로 가다니, 그런……」
			ELSE
				PRINTFORMW 「싫, 어…도구가 아니라, 당신의 것이… 좋은데…」
			ENDIF
	ELSE
		IF ABL:V감각 > 2
			IF RAND:2 == 1
				PRINTFORML 「들어왔어…… 안쪽까지, 도착했어…♥」
				PRINTFORMW 「아앗♥　안대, 안대앳! 이거… 안대애♥」
				PRINTFORMDW 바이브를 질 속까지 삼키고, 에이키는 비명과도 같은 기쁨의 목소리를 내고 있다…
			ELSE
				PRINTFORML 「부탁이니까, …빨리 넣어주세요……♥」
				PRINTFORMDW 익숙하게 만드는 것을 애태우는 것으로 착각해서, 에이키가 조르고 있다…
				PRINTFORMDL %마스터는% 요구에 응하기 위해 에이키의 젖은 고기구멍에 바이브를 꽂아넣었다
				PRINTFORMW 「후아아아아아앗♥　안쪽, 기분 조아아아앗…♥」
			ENDIF
		ELSEIF PALAM:윤활 <= PALAMLV:1
			PRINTFORML 「잠깐만…… 기다려! 무리하게, 하지마…」
			PRINTFORMDW 아직 젖지 않은 에이키의 갈라진 틈의 바이브가 무척 뻑뻑해 보인다…
		ELSEIF EXP:V경험 > 20
			IF RAND:2 == 1
				PRINTFORML 「아……아…, 점점…안쪽까지……하아아아앙앗♥」
				PRINTFORMDW 바이브를 전부 받아들이고 에이키는 크게 한숨을 내쉬었다
			ELSE
				PRINTFORML 「아아아앗♥　시러, 이런거… 들어와버리면……」
				PRINTFORMDW 불안한 듯한 말투와는 달리, 에이키의 질은 바이브를 순순히 받아들였다…
			ENDIF
		ELSE
			IF RAND:2 == 1
				PRINTFORMW 「그 도구들 조금 무서워……아프게 하면 안 돼?」
			ELSE
				PRINTFORML 「움직이기만 하는 건 괜찮지만, 그… 떨리는 건, 그다지…」
				PRINTFORMDW 에이키는 바이브의 미묘한 진동에 아직 익숙지 않은 것 같다…
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;45,애널바이브
;ナズ구상構文参考にして二本差し分岐も추가
;==================================================
@M_KOJO_MESSAGE_COM_K30_45
IF FLAG:70 || CFLAG:1501 || TCVAR:30:억지로 || TCVAR:30:만취
	CALL TRAIN_MESSAGE
ELSE
	CALL K30_TRAIN_MESSAGE_45
ENDIF
CALL M_KOJO_MESSAGE_COM_K30_45_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_45_1
;-------------------------------------------------
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	IF FLAG:70
		RETURN 1
	ENDIF
	;マグロスイッチオン
	IF CFLAG:1501 || TCVAR:30:억지로 || TCVAR:30:만취
		CALL K30_GASPING_MAGURO
		RETURN 1
	ENDIF
	;基本セット
	;解除
	IF !TEQUIP:애널바이브
		SELECTCASE ABL:A감각
			CASE IS > 4
				IF NOWEX:A절정 && !NOWEX:이중절정
					CALL HPH_PRINT("「앗HPH　앗, 아아앗HPH　빠져버려! 빠져버려어HPH」", "W")
				ELSE
					CALL HPH_PRINT("「앗HPH　앗、아아앗HPH　……하아…하아」", "W")
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「……엉덩이, 빼면, 시러요」
							PRINTFORMW 「저기……좀 더……♥」
						CASE 1
							PRINTFORML 「지금, 빼버리면, 조금… 애절해서」
							PRINTFORMW 「이번에는……당신이, 갖고 싶어…♥」
						CASE 2
							PRINTFORML 「하아, 하아……굉장, 했어요……♥」
					ENDSELECT
				ENDIF
			CASE 2, 3, 4
				CALL HPH_PRINT("「응읏HPH　빠지고…있어……아앙앗HPH」", "W")
				IF RAND:2 == 1
					PRINTFORMW 「으으읏… 끝, 이야? ……그렇구나…」
				ELSE
					PRINTFORMW 「…히익, 벌어진 채라니, 부끄러운 말은 하지마!」
				ENDIF
			CASE 1
				PRINTFORML 「안 돼애! 안 돼앳! 보지마앗! 나와버려! …앗、아앗♥」
				PRINTFORMW 「…정말, 부끄러우니까, 보지마… 부탁이니까…」
			CASE 0
				PRINTFORMW 「으, 그으읏…으으으…아앗!!」
				PRINTFORML 「하아, 하아, 하아………」
				PRINTFORMDW 에이키는 가쁘게 숨을 몰아쉬며, 잠시 눈을 마주쳐 주지 않았다…
		ENDSELECT
	;装着
	ELSE
		;二本差し
		IF TEQUIP:50 == PLAYER || TEQUIP:바이브
			SELECTCASE ABL:A감각
				CASE IS > 4
					SELECTCASE RAND:3
						CASE 0
							PRINTFORML 「아아…\@ TEQUIP:바이브 ? 앞 # 당신 \@의 것만 해도 \@ ABL:V감각 > 2 ? 굉장했는데 # 꽉 찼는데 \@… 엉덩이, 까지…♥」
							CALL HPH_PRINT("「앗…앗、아아앗HPH　들어오고있어HPH」", "W")
						CASE 1
							CALL HPH_PRINT(@"「아아아앗HPH　싫어, 싫어HPH　동시에, 움직이면, 안대애HPH」", "L")
							IF TEQUIP:50 == PLAYER
								PRINTFORMDW 애널바이브를 삼키자, 에이키의 질 안은 더욱 더 뜨거움과 꿈틀거림을 더해 갔다…
							ELSE
								PRINTFORMDW 양 구멍의 바이브가 가져다주는 무기질적인 쾌감에, 에이키는 농락당하며 비명을 질러댄다
							ENDIF
						CASE 2
							CALL HPH_PRINT("「앗、아앗HPH　안대앳, 안대애HPH　이런거엇, 안대애HPH」", "L")
							PRINTFORMD %마스터는% 무엇이 안 되는 것이냐고 일부러 짓궃게 속삭이며 
							IF TEQUIP:바이브
								PRINTFORMDW 양쪽의 바이브를 빼고 꽂았다…
							ELSE
								PRINTFORMDW 허리를 \@ TEQUIP:체위 < 3 ? 천천히 앞뒤로 움직였다 # 가볍게 밀어올렸다 \@…
							ENDIF
							CALL HPH_PRINT("「시러어HPH　안쪽, 닿아서HPH　스치고 있어HPH　안대애HPH」", "W")
					ENDSELECT
				CASE 2, 3, 4
					IF RAND:2 == 1
						CALL HPH_PRINT("「앗… 양쪽, 들어왔어…응, 으으읏HPH」", "W")
						CALL HPH_PRINT("「이거…안돼앳, 버릇이 되버려… 안대앳HPH」", "W")
					ELSE
						PRINTFORML 「이렇게, 쉽게 들어가서, 기분…좋아지고 있어…」
						CALL HPH_PRINT(@"앗HPH　안대앳… 지금 \@ TEQUIP:바이브 ? 움직 # 당신이 움직 \@이면…HPH」", "W")
					ENDIF
				CASE 1
					PRINTFORML 「아, 아으으, 힘들, 지만……으으으」
					PRINTFORMW 「앗、잠깐… 움직이지, 마, 이대로…」
					PRINTFORMW 「앗……♥」
				CASE 0
					PRINTFORMW 「으으읏… 괴로워…서… 역시, 무리야…」
			ENDSELECT
		ELSE
			;애널바이브のみ
			SELECTCASE ABL:A감각
				CASE IS > 4
					SELECTCASE RAND:3
						CASE 0
							CALL HPH_PRINT("「앗HPH　굉장해, 엉덩이 안쪽까지…으읏HPH　으응읏HPH」", "W")
							CALL HPH_PRINT("「안쪽HPH　안쪽이, 안쪽까지HPH　떨리고이써엇HPH」", "W")
						CASE 1
							PRINTFORMW 「…빨리, 넣어…주세요… 참을 수 없어요…빨리…♥」
							PRINTFORMDL 엉덩이를 애절하게 흔들며 삽입을 조르는 에이키에게는, 말할 수 없을 정도의 미색이 감돌고 있다
							PRINTFORMDW %마스터는% 움츠러든 곳에 바이브를 에이키의 깊숙히까지 꽂아넣었다…
							CALL HPH_PRINT("「―――읏! 좋아앗HPHHPH　이거, 대단해애애HPHHPH」", "W")
						CASE 2
							PRINTFORMW 「준비, 다 됐어요… 빨리, 그걸…휘저어주세요…♥」
							PRINTFORMDL 바이브를 밀어넣자, 에이키의 항문은 유연하게 입을 벌리며 부드럽게 바이브를 집어삼키고 있다…
							CALL HPH_PRINT("「앗HPH　앗HPH　하아아HPH　엉덩이이, 들어왔어HPH」", "W")
					ENDSELECT
				CASE 2, 3, 4
					IF RAND:2 == 1
						PRINTFORML 「이런 부끄러운 일…부정한 구멍에 물건을 넣다니…」
						PRINTFORMDW 원망스러운 듯이 중얼거리는 에이키였지만, 꼭 맞는 바이브의 진동에는 씰룩씰룩 반응하고 있다…
						PRINTFORMDL %마스터는% 움츠러든 곳에 바이브를 천천히 꽂아넣었다…
						CALL HPH_PRINT("「으…아…아…앗, 앗HPH　배가, 눌리고 있어…HPH」", "W")
					ELSE
						PRINTFORML 「………우…ㅇ, 으으읏…아아아앗♥」
						PRINTFORMDW 애널바이브를 꽂힌 에이키는 어떻게든 쾌감을 참으려 하고 있었다
