;62,正常位アナル
;ナズ구상の構文構成参考にしてます
;==================================================
@M_KOJO_MESSAGE_COM_K30_62
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K30_62_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K30_62_1
TCVAR:30:397 = 1
;-------------------------------------------------
;立位
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && TFLAG:193
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;マグロスイッチオン
	ELSEIF CFLAG:1501 || TCVAR:30:억지로 || TCVAR:30:만취
		CALL K30_GASPING_MAGURO
		RETURN 1
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	ELSEIF LOCAL:1 && FIRSTTIME(SELECTCOM)
	ENDIF
	;-------------------------------------------------
	;挿入継続
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回사정した
		IF TCVAR:104 == 1 && RAND:3 != 1
			IF ABL:A감각 < 3
				PRINTFORML 「아아앙앗♥　기다려, 잠깐…멈춰, 멈춰엇……」
				PRINTFORMDL 정액으로 미끌미끌해진 후장을 천천히 밀어올리자, 에이키는 %CALLNAME:MASTER%에게 매달리면서 부탁해온다
				PRINTFORMW 「안대앳, 엉덩이, 안대앳… 찌걱찌걱, 하는거, 시러어…♥」
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT, @"「후후훗… 아직, 부족해? %K30_C_NAME(MASTER)%의 속이 시원해질 때까지…부디HPH」", "L"
						PRINTFORMDL 항문 사정의 여운에 잠긴 에이키는 황홀한 표정으로 %CALLNAME:MASTER%의 목을 끌어안고 있다
						PRINTFORMDW 그 표정도 후장에 완만하게 파고들며, 금새 희열로 바뀌어 갔다
					CASE 1
						CALL HPH_PRINT, @"「소리, 커서…부끄러워…아앗HPH　그렇게, 찌르면, 앗HPH」", "L"
						PRINTFORMDL 에이키의 항문에서는 %마스터가% 허리를 흔들 때마다 공기와 떨어지는 정액으로 아주 천박한 소리를 내고 있다…
						CALL HPH_PRINT, @"「부끄러워엇… 이런 거, 싫어엇HPH　소리, 내면HPH　시러어HPH」", "W"
					CASE 2
						PRINTFORMDL %마스터는% 에이키의 수치를 부채질하듯이 한 발을 번쩍 들어 껴안았다
						PRINTFORMDL 그런 자세로 천천히 큰 피스톤 운동을 계속하자, 페니스를 집어삼킨 항문에서 거품이 이는 정액이 흘러나왔다…
						CALL HPH_PRINT, @"「엉덩이, 잔뜩, 인데엣HPH　안댓, 새버려어, 새버려어어HPH」", "W"
				ENDSELECT
			ENDIF
		ELSE
			IF ABL:A감각 < 3
				PRINTFORML 「으으읏, 시러…응으으읏♥　이런 모습, 무리잖아…」
				PRINTFORMDW 한쪽 다리로 서면 저절로 힘이 들어가 뒷구멍이 닫힌다… 그것을 페니스로 도려내는 기묘한 감각에 에이키는 부들부들 떨고 있다
			;立位連続で駅弁に移行…という脳内設定
			ELSEIF TEQUIP:체위2 == 1 && RAND:2 == 1
				PRINTFORMDW 같은 체위만 하면 싫증나지 않을까… 문득, 그런 생각이 들어 허리의 움직임을 멈춘다
				PRINTDATADL
					DATAFORM %마스터는% 에이키를 에키벤의 체위로 고쳐, 다시 한 번 \@ ABL:애널 < 3 ? 단단히 죄어 오는 # 절묘하게 감싸오는 \@ 애널에 단숨에 삽입했다
					DATALIST
						DATAFORM 에이키에게 꼭 붙잡으라 속삭이고 엉덩이로 손을 돌려 들어 올린다
						DATAFORM 흘러내리는 에이키의 엉덩이를 받치듯이 %조사처리(TINKO(),"를")% 후장에 삼키게 했다
					ENDLIST
				ENDDATA
				PRINTFORMW 
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT, @"「하으읏! 안쪽, 닿아버려어HPH　굉장해, 굉장해애HPH」", "L"
						PRINTFORMDW 체중이 걸리는 만큼, 에이키의 애널은 %CALLNAME:MASTER%의 페니스를 더욱 깊이 삼키고 있다
					CASE 1
						CALL HPH_PRINT, @"「시러어엇HPH　이거엇, 무서워어, 너무 기분 좋아서, 무서워엇HPH」", "L"
						CALL HPH_PRINT, @"「%K30_C_NAME(MASTER)%, %K30_C_NAME(MASTER)%HPH　무서워, 무서워엇HPH」", "W"
						PRINTFORMDL 끝없는 쾌락과 불안정한 자세에 에이키는 겁에 질려 %CALLNAME:MASTER%에게 필사적으로 매달린다
						PRINTFORMDW 가학심을 부풀어오른 %마스터는% 굉장히 흥분해서 에이키를 격렬하게 상하로 흔들어댔다…
					CASE 2
						CALL HPH_PRINT, @"「시러ㅡㅡㅡㅡㅡ엇HPH　………앗, 아앗HPH　……커헉……」", "L"
						PRINTFORMDW 항내 깊숙히 페니스를 박혀, 에이키는 의식을 잃을 듯이 헐떡이고 있다…
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 0
						CALL HPH_PRINT, @"「%K30_C_NAME(MASTER)%, %K30_C_NAME(MASTER)%HPH　……응읏ーーHPH　으으읏ーHPHHPH」", "L"
						PRINTFORMDL 에이키의 게걸스럽게 입술을 탐해오며…입맞춤과 동시에 \@ ABL:애널 < 3 ? 애널을 꽉 조여 # 직장이 페니스를 감싸쥐어 \@왔다
						PRINTFORMDW %마스터는% 몸을 그대로 기대오는 에이키를 확실히 받쳐, 욕망대로 \@ ABL:애널 < 3 ? 꽉 조이는 # 개발된 \@ 애널을 탐했다
					CASE 1
						PRINTFORML 「굉장한 꼴로, 만드는구나………으읏… 아아앙앗♥　시러어, 엉덩이잇, 벌려져버려어♥」
						PRINTFORMDW 체위를 바꾸어 꽂는 깊이는 얕아졌지만, 그만큼 귀두가 에이키의 후장을 확장하며 수치와 성감을 높여간다
						PRINTFORMDL 항문에 얕게 박혔다가 빼낸다… 이 움직임이 반복되자 에이키는 참지 못하고 교성을 올렸다
						CALL HPH_PRINT, @"「아아아앗HPH　그런 거엇, 안대앳HPH　엉덩이, 문질문질하는거, 안대애HPH」", "W"
					CASE 2
						CALL HPH_PRINT, @"「응, 응으읏HPH　당신이, 아까보다, 커졌어…HPH　앗, 응아아앗HPH」", "L"
						PRINTFORMDW 한쪽 다리로 선 채로 몸에 힘이 들어가, 여느 때보다 항문은 페니스를 죄어온다…
						PRINTFORMDL 빈 손으로 몸을 만지작거리며 익숙해지도록 피스톤질을 계속하자, 에이키는 더욱 좋은 반응을 해왔다
						CALL HPH_PRINT, @"「앗HPH　대단, 엉덩이, 대단해앳HPH　앗HPH　아아아앗HPH」", "W"
				ENDSELECT
			ENDIF
		ENDIF
		;절정に応じて一言
		SIF ABL:A감각 > 2 && (NOWEX:MASTER:11 || (PALAM:욕정 > PALAMLV:4 && NOWEX:A절정))
			CALL K30_PRE_ORG_MES_A(0)
		RETURN 1
	ENDIF
	;基本セット
	IF ABL:A감각 > 4
		IF ABL:노출벽 > 4 && RAND:4 == 0
			PRINTFORML 「선 채로, 엉덩이로라니…… 당신은 정말 이상해. 변태 씨네요…♥」
			PRINTFORMDW …자신의 뒷구멍과 %CALLNAME:MASTER%의 페니스를 만지작거리며, 에이키가 무엇인가 말하고 있다
			CALL HPH_PRINT, @"「그런 변태 씨에게, 딱 맞는 구멍…후후훗、부디…HPH」", "L"
			PRINTFORMDW 준비완료인 애널에 페니스가 삼켜지자, 에이키의 표정은 금세 희열로 녹기 시작했다…
		ELSE
			SELECTCASE RAND:3
				CASE 0
					CALL HPH_PRINT, @"「아앗아앙HPH　엉덩이잇, 좀더엇HPH　움직여줘어, 좀 더어HPH」", "L"
					PRINTFORMDW …넣고 가볍게 움직였을 뿐인데, 에이키는 이미 쾌락의 소용돌이에 빠져 버렸다
					PRINTFORMDL 다리를 후들후들 떨면서 %마스터가% 지탱하지 않으면 서 있는 것조차 불안해 보인다
					CALL HPH_PRINT, @"「하아ー앗HPH　하-앗HPH　……앗, 앗HPH　앗HPH　아아앗HPHHPH」", "W"
				CASE 1
					CALL HPH_PRINT, @"「후으읏HPH　후으읏HPH　……응읏………후으읏HPH」", "L"
					PRINTFORMDW 에이키는 %CALLNAME:MASTER%에게 볼을 잡아 끌어당기며, 바싹 껴안아 왔다
					PRINTFORMDL 좀 움직이긴 힘들지만 여리여리한 에이키의 몸을 껴안자 사랑스러움이 넘쳐흐른다
					PRINTFORMDL %마스터는% 허리의 움직임을 피스톤에서 그라인드로 바꾸어 느긋한 애널 섹스를 즐겼다…
					CALL HPH_PRINT, @"「으읏! 앗HPH　굉장햇HPH　……엉덩이잇, 찔꺽찔꺽, 하고 이써어…HPH」", "W"
				CASE 2
					CALL HPH_PRINT, @"「%K30_C_NAME(MASTER)%, 키스, 해줘HPH　쪼ㅡ옥, 해줘HPH」", "L"
					CALL HPH_PRINT, @"「응츄우HPH　츄웃, 츄우웃……쥬르릅, 츄르릅HPH　쥬으읍HPH」", "W"
					PRINTFORMDL 선 채로 뒷구멍을 범해져, 타액을 %마스터와% 교환하며 에이키는 욕정으로 얼굴이 녹아있다
					PRINTFORMDW 염마님을 이렇게까지 타락한 것에 만족하면서 %마스터는% 본능에 따라 피스톤을 계속했다
			ENDSELECT
		ENDIF
	ELSEIF ABL:A감각 > 2
		IF RAND:2 == 1
			PRINTFORML 「저기…넣기 힘들면, 다른 자세로……으읏♥　……좋을지도……아앙앗♥」
			PRINTFORMDW 정면으로 부둥켜안아 사타구니에 페니스를 문지를 때마다, 에이키는 민감한 반응을 돌려준다…
			PRINTFORMDL 조금 애태워 주고 나서, 한쪽 발을 들어 천천히 페니스를 항문에 미끄러뜨린다
			CALL HPH_PRINT, @"「아아으HPH　들어와버려어, 엉덩이, 들어와버려어어HPH」", "W"
		ELSE
			PRINTFORML 「응, 아으읏♥　당신의 것이, 평소보다……커」
			PRINTFORMDW 한 다리로 서있는 것은 몸이 힘이 들어가는 것인지, 에이키의 애널은 여느 때 이상으로 죄어 온다
			PRINTFORMDL …그러나 침입을 거부하는 것이 아니라 오히려 에이키를 보다 민감하게 하는 결과가 되었다
			CALL HPH_PRINT, @"「시럿HPH　앗…엉덩이, 구멍이HPH　질척질척, 안대애HPH」", "W"
		ENDIF
	ELSE
