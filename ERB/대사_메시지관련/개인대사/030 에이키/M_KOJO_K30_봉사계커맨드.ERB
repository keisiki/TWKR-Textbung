			IF TCVAR:MASTER:갈것같음
				CALL HPH_PRINT("「응후웃…HPH　굉장히, 딱딱해…HPH」", "L")
				CALL HPH_PRINT("「여기에, 싸면…무척, 기분 좋을거에요HPHHPH」", "L")
