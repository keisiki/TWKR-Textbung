;임신소망カウンター
;抱きつくばっかじゃなくてちゃんと妊活してよ、という雑なテコ入れ
;固定化されてもつまらないので基本確率はあんまり上げない
;本体側が優秀なので適当に数字いれても問題無し
@K30_COUNTER_WISH_PREGNANCY
#DIM WISH_VAL
;-------------------------------------------------
WISH_VAL = 10
;素股플래그時
SIF TCVAR:30:362 == 1
	WISH_VAL += 999
;妊活に励む日
SIF ESTRUS_CYCLE(30) >= 70
	WISH_VAL += 15
;溜め込んでたらそりゃ狙われる
SIF TALENT:MASTER:농후정액
	WISH_VAL += 15
SIF WISH_VAL < RAND:100
	RETURN 1
;もうしてたら腰振りorだいしゅきホールド
IF TEQUIP:50 == PLAYER
	TCVAR:차회카운터 = 88
	SIF BASE:MASTER:사정 > MAXBASE:MASTER:사정
		TCVAR:차회카운터 = 86
;좋아하는체위で孕みたいとかえっちい感じがする
ELSEIF CFLAG:좋아하는체위 && RAND:2 == 1
	SELECTCASE CFLAG:좋아하는체위
		CASE 1, 4, 6, 9
			TCVAR:차회카운터 = 41
		CASE 2, 5, 7, 10 
			TCVAR:차회카운터 = 43
		CASE 3, 8
			TCVAR:차회카운터 = 85
	ENDSELECT
;やる気まんまんの時はズブリといっちゃう
ELSEIF WISH_VAL + RAND:50 > 50
	TCVAR:차회카운터 = 85
;ここからはもう適当
ELSE
	SELECTCASE RAND:3
		CASE 0
			TCVAR:차회카운터 = 85
		CASE 1
			IF RAND:2 == 1
				TCVAR:차회카운터 = 41
			ELSE
				TCVAR:차회카운터 = 43
			ENDIF
		CASE 2
			IF RAND:2 == 1
				TCVAR:차회카운터 = 51
			ELSE
				TCVAR:차회카운터 = 44
			ENDIF
	ENDSELECT
ENDIF
RETURN 1

