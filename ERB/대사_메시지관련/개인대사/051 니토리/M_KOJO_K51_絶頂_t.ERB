;-------------------------------------------------
;노예의 절정시의 말
;NOWEX:C절정~3 CVAB 절정
;NOWEX:M절정 이중절정
;NOWEX:이중절정 사중절정
;NOWEX:분유분유
;NOWEX:사정 사정
;NOWEX:방뇨 방뇨
;적당하게 조합해 사용해 주세요
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K51
;-------------------------------------------------
;분유
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;사정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;오중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:오중절정
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;사중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:사중절정
		;부위를 지정하지 않고 간이적으로 쓰는 경우,[SKIPSTART]&[SKIPEND]의 코멘트 아웃을 제외해,[SKIPSTART]곳의 코멘트의 사이에 써 주세요
		;[SKIPSTART]
		SELECTCASE NOWEX:사중절정
			;C＆V＆A＆B절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆V＆A＆M절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆V＆B＆M절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆A＆B＆M절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆A＆B＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정
		;부위를 지정하지 않고 간이적으로 쓰는 경우,[SKIPSTART]&[SKIPEND]의 코멘트 아웃을 제외해,[SKIPSTART]곳의 코멘트의 사이에 써 주세요
		;[SKIPSTART]
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;C＆B＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆V＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;C＆V＆M절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;B＆V＆M절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆V＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;C＆B＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;A＆B＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;C＆A＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;이중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
		;부위를 지정하지 않고 간이적으로 쓰는 경우,[SKIPSTART]&[SKIPEND]의 코멘트 아웃을 제외해,[SKIPSTART]곳의 코멘트의 사이에 써 주세요
		;[SKIPSTART]
		SELECTCASE NOWEX:이중절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:C절정
		;초강 절정 C
		IF NOWEX:C절정 == 3
			PRINTFORMW 
		;강절정 C
		ELSEIF NOWEX:C절정 == 2
			PRINTFORMW 
		;절정 C
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;초강 절정 V
		IF NOWEX:V절정 == 3
			PRINTFORMW 
		;강절정 V
		ELSEIF NOWEX:V절정 == 2
			PRINTFORMW 
		;절정 V
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;초강 절정 A
		IF NOWEX:A절정 == 3
			PRINTFORMW 
		;강절정 A
		ELSEIF NOWEX:A절정 == 2
			PRINTFORMW 
		;절정 A
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;강절정 B
		IF NOWEX:B절정 == 3
			PRINTFORMW 
		;강절정 B
		ELSEIF NOWEX:B절정 == 2
			PRINTFORMW 
		;절정 B
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;초강 절정 M
		IF NOWEX:M절정 == 3
			PRINTFORMW 
		;강절정 M
		ELSEIF NOWEX:M절정 == 2
			PRINTFORMW 
		;절정 M
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


