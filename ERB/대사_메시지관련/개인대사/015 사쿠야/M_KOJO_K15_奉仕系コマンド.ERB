;80, 수음
;==================================================
@M_KOJO_MESSAGE_COM_K15_80
CALL M_KOJO_MESSAGE_COM_K15_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_80_1
;-------------------------------------------------
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;시 간중
	IF FLAG:70
		PRINTFORMW 시간이 멈춘 사쿠야의, 섬세하고 가느다란 하얀 손에 페니스를 문지른다
		PRINTFORMW 부드럽고 가녀린 손을 범하고 있는 것에 흥분한 %마스터는%,
		PRINTFORMW 좀 더 기분 좋아지기 위해, 사쿠야의 손가락을 움직여 더욱 페니스에 착 달라붙도록 조정했다……
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「손으로 하는 거야?」
		PRINTFORMW 사쿠야는 그렇게 말하고, 양손에 타액을 발라 한 손으로는 장대를 쥐고 다른 한쪽으로는 불알을 주물러 애무한다
		PRINTFORMW 거기에 열린 입으로는 귀두를 핥아대고 있다……
		PRINTFORMW 「읏……언제라도 내도 좋아요?」
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		CALL HPH_PRINT("「손애무군요, 알겠습니다 HPH」", "L")
		PRINTFORMW 사쿠야는 그렇게 말하고, 장대 위에서 입을 벌리고 군침을 늘어뜨려, 양손으로 펴바르면서 절묘한 힘 조절로 훑어냈다
		PRINTFORMW 게다가 그것으로 만족하지 않고, 열린 입에 귀두를 머금고, 칼리나 요도구멍을 혀로 애무 했다……
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 「손으로, 입니까……알았습니다」
		PRINTFORMW 사쿠야는 그렇게 말하고, 손에 타액을 떨어뜨려 양손으로 펴발라,
		PRINTFORMW 한 손으로 장대를, 다른 한쪽의 손으로 불알을 주물러 애무했다
		PRINTFORMW 「……」
		PRINTFORMW 사쿠야는 담담하게 %CALLNAME:MASTER%의 성처리를 서포트하고 있다……
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;81, 펠라치오
;==================================================
@M_KOJO_MESSAGE_COM_K15_81
CALL M_KOJO_MESSAGE_COM_K15_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K15_81_1
;-------------------------------------------------
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;시 간중
	IF FLAG:70
		PRINTFORMW %마스터는% 사쿠야의 입을 오나홀로 사용하기로 했다
		PRINTFORMW 멈춰버린 시간속에서 사쿠야의 턱을 잡아, 억지로 입을 열게 했다
		PRINTFORMW %마스터가% 그대로 나중 일도 배려하지 않고 거리낌없이 페니스를 찔러 넣으면, 무방비인 혀가 페니스를 맞이해 주었다
		PRINTFORMW %CALLNAME:MASTER%의 페니스는, 부드럽게 미끄러지고, 거기다 약간의 조임과 흡입력이 있는 사쿠야의 입오나홀이 마음에 든 것 같다……
		PRINTFORMW 그리고 그대로 %마스터는%, 사쿠야의 얼굴에 허리를 쳐박듯이 움직이며 격분한 페니스로 사쿠야의 목 안까지를 범했다
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		IF PREVCOM == 81
			SELECTCASE RAND:2
				CASE 0
				CALL HPH_PRINT("「쥬릅HPH 쥬룻 HPH 할짝HPH」", "L")
				PRINTFORMW 사쿠야는 일심불란으로 페니스를 가득 삼키고 있다……
				CASEELSE
				CALL HPH_PRINT("「으응…읏, 쿠흣, 읏……HPH」", "L")
				PRINTFORMW 사쿠야는 스스로 목 안을 사용해 장대를 삼키고, 혀로 뿌리를, 목 전체로 귀두를 구석구석까지 애무했다
				ENDSELECT
		ELSE
		CALL HPH_PRINT("「실례합니다……HPH」", "L")
		PRINTFORML %마스터가% 다리를 열면, 사쿠야는 말하지 않아도 스스로 봉사를 시작했다
		CALL HPH_PRINT("「응……푸핫, 후우～～웃……HPH」", "L")
		CALL HPH_PRINT("（굉장히 진한 냄새……HPH 나를 한마리의 암컷으로 만드는, “남자”의 냄새……HPHHPH）", "L")
		PRINTFORML 입으로 바지의 지퍼를 내리고, 얼굴을 내민 페니스의 냄새를 가슴속에 들이 마셔, 넋을 잃은 표정을 보였다
		CALL HPH_PRINT("「그럼……잘먹겠습니닷HPH 하븝HPH 응, 므우우우……HPH」", "L") 
		PRINTFORML 그리고 그대로 귀두를 입에 물어, 칼리뒤를 혀로 청소하면서 장대를 뿌리까지 삼켰다……
		RETURN 1
		ENDIF
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 「응, 날름……어때? 기분 좋아? 응츗」
		PRINTFORMW %CALLNAME:MASTER%의 가랑이에 얼굴을 묻어 봉사하는 사쿠야에 기분 좋다고 전하면,
		PRINTFORMW 사쿠야는 페니스를 가득 머금으면서 기쁜듯이 미소지었다
		RETURN 1
	;그 이외
	ELSE
		SELECTCASE RAND:2
			CASE 0
			PRINTFORMW 사쿠야에 페니스를 빨아 줄 것을 부탁했다
			PRINTFORMW 「읏……피챠……날름……」
			PRINTFORMW 분명히 사쿠야의 페라는 기분이 좋은 것이지만, 마음이 들어가 있지 않다
			PRINTFORMW 사쿠야는 그대로 기계적으로 봉사를 계속했다……
			CASEELSE
			PRINTFORMW 「우……제대로 씻고 있습니까, 이거?」
			PRINTFORMW 사쿠야는 페니스에 얼굴을 가까이 하고, 남성기 특유를 비릿한 냄새에 얼굴을 찡그렸다
			PRINTFORMW 「……알고 있어요. ……응, 날름……우에……츗……」
			PRINTFORMW 페니스를 응시한 채로 잠깐 굳어지고 있던 사쿠야였지만, %CALLNAME:MASTER%에게 재촉받아 봉사를 시작했다……
			ENDSELECT
	RETURN 1
	ENDIF
ENDIF
RETURN 1

