;==================================================
;6,삽입Ｇ스팟 고문
;==================================================
@M_KOJO_MESSAGE_SCOM_K105_6
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K105_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K105_6_1
;-------------------------------------------------
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;특수상황
	IF FLAG:70 ;시간정지
		PRINTFORML 
		RETURN 1
	ELSEIF CFLAG:MASTER:장난 == 2
		PRINTFORML 
		RETURN 1
	ELSEIF TCVAR:만취 == 1
		PRINTFORML 
		RETURN 1
	ELSE
		IF TALENT:105:연인 || TALENT:105:연모
			PRINTDATAW
				DATAFORM 「읏, 거기, 안, 됀……다구！　잠깐, 히잇, %UNICODE(0x2665) *1%」
				DATAFORM 「그래, 거기……얏, 우아아앗！　히얏, 읏, 좋아%UNICODE(0x2665) *1%」
			ENDDATA
		ELSE
			PRINTFORMW 「！?　잠까, 거긴……아%UNICODE(0x2665) *1%」
			SIF TALENT:사모
				PRINTFORMW （정말, 좋은곳, 들켜버려……크읏%UNICODE(0x2665) *1%）
		ENDIF
	ENDIF
ENDIF
;미기입
;CALLFORM M_KOJO_MESSAGE_COM_K105_{SELECTCOM}_1
RETURN 1

