@M_KOJO_EVENT_K58_14(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;가지고 있던 음식을 먹을 수 있었다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == -1
		;업무중
		IF CFLAG:행동 == 5
			IF TALENT:연인 || TALENT:연모
				PRINTFORML 「아앗, %CALLNAME:MASTER%」
