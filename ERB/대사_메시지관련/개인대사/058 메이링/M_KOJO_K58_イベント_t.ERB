@M_KOJO_EVENT_KX_27(ARG, ARG:1)
;말 주인은 LOCAL
;ARG:1은 목적지
;연모 전제
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;권해 불만
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「있잖아요 %CALLNAME:MASTER%, 괜찮다면 외출하지 않겠어요?」
				PRINTFORML 「에에, 데이트 신청이에요♪ 어때요?」
			CASE 1
				PRINTFORML 「왠지 오늘은 %마스터와% 외출하고 싶은 기분인데요……」
				PRINTFORML 「같이 와주실래요?」
			CASEELSE
				PRINTFORML 「%CALLNAME:MASTER%, 데이트 해요, 데이트!」
				PRINTFORML 「……안 되나요?」
			ENDSELECT
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;받았다
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「앗싸! 그럼 빨리 출발해요!」
				PRINTFORML 「～♪ 데이트～ 데이트～♪」
			CASE 1
				PRINTFORML 「좋았어! 외출이에요!」
				PRINTFORML 「후후, 이 환상향의 멋진 문지기가 모은 데이트 장소의 정보를 바탕으로요……」
				PRINTFORML 「아, 기다려요, 두고 가지마요～」
			CASE 2
				PRINTFORML 「다행이다. 그럼, 어울려 주세요」
			CASEELSE
				PRINTFORML 「고마워요. 그럼 갈까요♪」
				PRINTFORML 「자자, 데이트라면 손을 잡아야죠」
			ENDSELECT
			PRINTW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;끊었다
		;기입 체크(=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:4
			CASE 0
				IF TALENT:연인
					PRINTFORML 「으～, 연인의 권유를 거절하다니 어찌된 일인가요」
					PRINTFORML 「이유를 말해줘요 이유를, 이얍 이얍～」
					PRINTFORML 농담 삼아 관절기를 걸어오는 메이링은, 어딘가 쓸쓸해 보인다……
				ELSE
					PRINTFORML 「누그앗, 정말인가요…… 그럼, 다음에 또……」
						;ぬぐぁっ、マジですか……。では、また今度……
				ENDIF
			CASE 1
				PRINTFORML 「……어라, 무슨 볼일이라도?」
				PRINTFORML 「뭐 기회가 된다면 다음에, 라고 하는 걸로」
			CASE 2
				PRINTFORML 「에～!? 왜요～……」
				PRINTFORML 「가요～…………데이트……」
			CASEELSE
				PRINTFORML 「그런가요……으―응, 쓸쓸하지만, 어쩔 수 없네요」
				PRINTFORML 「형편 좋을 것 같은 때 또 권유할게요」
			ENDSELECT
			PRINTW 
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

