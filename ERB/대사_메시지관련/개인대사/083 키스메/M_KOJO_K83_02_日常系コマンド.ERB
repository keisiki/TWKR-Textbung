;621,釣りをする
;TFLAG:193(0=釣れない 1～4=魚 11～15=うまい魚 610=木材 131=守矢の守札 21=ローター 609=氷の鱗 606=龍の爪)
;TFLAG:194(1000以上ならTARGETが釣った)
;==================================================
@M_KOJO_MESSAGE_COM_K83_621
;CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_621_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	SELECTCASE TFLAG:193
	CASE 0
		IF TALENT:연모
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「……빈둥거릴 수 있어, 좋아해」
			CASE 2
				PRINTFORMW 「원, 나의 통은 낚시해 용무가 아니야！」
			CASEELSE
				PRINTFORMW 「천천히 기다리고 있으면 반드시……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「, 통에 넣지 말아줘……」
			CASEELSE
				PRINTFORMW 「잡히지 않는다……」
			ENDSELECT
		ENDIF
		RETURN 1
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「아！」
			PRINTL 
			PRINTFORMDL %CALLNAME:TARGET%하지만 목편을 낚시했다…
			PRINTW
			PRINTFORMW 「…………쓸쓸히」
		ELSE
			PRINTFORMDL 목편이 잡혔다……
			PRINTW
			PRINTFORMW 「어머……」
		ENDIF
		RETURN 1
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORML 「아！」
		ELSE
			PRINTFORMDL 종잇조각이 잡혔다
		ENDIF
		PRINTDW 
		PRINTDL ┌─────────────┐
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　모리야부적　　　　　　｜
		PRINTDL ｜　   　　　　　　　　　　　｜
		PRINTDL ｜    　5장 모으면                　｜
		PRINTDL ｜　　   뽑기 1회 가능합니다　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜이익은 보증하기 어렵습니다 ｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　　　　　　모리야신사　｜
		PRINTDL └─────────────┘
		PRINTW 
		PRINTFORMW 「이것, 무엇？」
		RETURN 1
	;KR판 오리지날 수정. 원래 구버전 낚시 메시지 기반인 것을 최신판에 맞춤
	CASE 1 TO 20, 22 TO 60
		LOCALS = %FISH_NAME(TFLAG:193)%
	CASE 21
		LOCALS = 로터
	CASEELSE
		LOCALS = %ITEMNAME:(TFLAG:193)%
	ENDSELECT

	IF TFLAG:194 > 1000
		PRINTFORMW 「아！」
		PRINTL 
		PRINTFORMDL %타겟이% %조사처리(LOCALS,"를")% 낚았다！
		PRINTW 
		SELECTCASE TFLAG:193
		;가물치
		CASE 15
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：強")% , 무겁다！ %CALLNAME:MASTER%, 살려……！」
		;ローター
		CASE 21
			PRINTFORMW 「이것은……\/\/\/」
		;カエル
		CASE 700
			PRINTFORMW 「뭐야 개구리인가……」
		;お宝
		CASE 609, 606
			PRINTFORMW 「원, 기려……！」
		CASEELSE
			PRINTFORMW 「개, 잡혔다……！」
		ENDSELECT
	ELSE
		PRINTFORMDL %조사처리(LOCALS,"가")% 잡혔다！
		PRINTW 
		SELECTCASE TFLAG:193
		;ローター
		CASE 21
			IF TALENT:연모
				PRINTFORMW 「%CALLNAME:MASTER%, 어떻게 했어……？」
			ELSE
				PRINTFORMW 「개, 이것은……\/\/\/」
			ENDIF
		;カエル
		CASE 700
			PRINTFORMW 「개구리나 원 좋다……」
		;お宝
		CASE 609, 606
			PRINTFORMW 「깨끗하다……！」
		CASEELSE
			PRINTFORMW 「, 대단해……！」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
