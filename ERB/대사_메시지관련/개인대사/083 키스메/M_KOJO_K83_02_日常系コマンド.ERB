;410,掃除
;FLAG:70(1=時間停止中
;TFLAG:193(0=見てるだけ 1=一緒に掃除)
;TFLAG:194(3=汚れ高 2=汚れ中 1=汚れ低 0=汚れ最小
;OUTROOF(CFLAG:현재위치)(野外掃除（冬は雪かき
;==================================================
@M_KOJO_MESSAGE_COM_K83_410
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_410_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_410_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	IF FLAG:시간정지 || CFLAG:수면
	;うふふ中
	ELSEIF CFLAG:우후후
		PRINTFORMW 「？ , 뭐 해……？」
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE(83)
		CALL M_KOJO_K83_TALKSET("주정꾼")
	;不機嫌
	ELSEIF M_KOJO_K83_FUKIGEN(83)
		PRINTFORML 「청소……」
		;手伝う（だいたいこっち）
		SIF TFLAG:193
			PRINTFORML 「 나도 한다……」
		WAIT
	;自分の部屋
	ELSEIF CFLAG:현재위치 == OMANEKIBEYA() || CFLAG:현재위치 == CFLAG:초기위치
		PRINTFORML 「네……나의 방은 이것으로 좋지만……」
		IF TALENT:연모
			PRINTFORMW 「%CALLNAME:MASTER%(을)를 하고 싶으면……」
		ELSE
			PRINTFORMW 「, 그것은 쓰레기가 아니다！」
		ENDIF
	ELSE
		;雪かき
		IF OUTROOF(CFLAG:MASTER:현재위치) && DAY:2 == 4
			PRINTFORML 「눈, 쌓였군요……」
		;掃除
		ELSE
			IF TALENT:연모
				PRINTFORML 「청소해？」
			ELSEIF TALENT:사모
				PRINTFORML 「청소를 좋아하다……」
			ELSE
				PRINTFORML 「청소는……의미 있어？」
			ENDIF
		ENDIF
		;手伝う（だいたいこっち）
		IF TFLAG:193
			IF TALENT:연모
				PRINTFORMW 「있고, 함께 한다……」
			ELSEIF TALENT:사모
				PRINTFORMW 「이라고, 도와 준다……」
			ELSE
				PRINTFORMW 「……나도 한다」
			ENDIF
		ELSE
			WAIT
		ENDIF
	ENDIF
ENDIF
RETURN 1
