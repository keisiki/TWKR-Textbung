		PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")%」
	ENDSELECT
ENDIF
RETURN 1
