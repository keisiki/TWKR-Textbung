@M_KOJO_MESSAGE_COM_K83_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい
	CALL M_KOJO_COLOR_K83
	;酔ってる
	IF M_KOJO_K83_YOIDORE([[키스메]])
		CALL M_KOJO_K83_TALKSET("주정꾼")
	;회화불능
	ELSEIF TCVAR:[[키스메]]:회화불능
		IF TALENT:[[키스메]]:연모
			PRINTFORMW 「그, 그렇구나……」
		ELSEIF TALENT:[[키스메]]:사모
			PRINTFORMW 「무, 뭐……?」
		ELSE
			PRINTFORMW 「…………」
		ENDIF
	;不기분
	ELSEIF M_KOJO_K83_FUKIGEN([[키스메]])
		IF TALENT:[[키스메]]:연모
			PRINTFORMW 「응…… 응……」
		ELSE
			PRINTFORMW 「응…………」
		ENDIF
;	;デート会話,未定
;	ELSEIF TALENT:[[키스메]]:연모 && FLAG:デート相手 == [[키스메]] && DATENAME_SPOT() != "道中" && !デート会話:(CFLAG:[[키스메]]:현재위치 / 10)
	;くしゃみ
	ELSEIF !RAND:200
		PRINTFORMW 「에취! ……끄흠」
	;通常
	ELSE
		;失敗
		IF TFLAG:SELECTCOM분기 == -1
			IF TALENT:[[키스메]]:연모
				PRINTFORMW 「그, 그런 건……」
			ELSE
				PRINTFORMW 「아, 그래……」
			ENDIF
		;연모
		ELSEIF TALENT:[[키스메]]:연모
			SELECTCASE RAND:14
			CASE 1
				PRINTFORMW 「후후……%CALLNAME:MASTER%하고 얘기하는 거, 재밌어……」
			CASE 2
				PRINTFORMW 「뼈, 뼈를 모으는 게…… 취미……」
			CASE 3
				PRINTFORML 「지상은 자꾸 이변이 일어나는데…… 지저는 항상 변함 없어……」
				PRINTFORMW 「그게 맘에 들어……」
			CASE 4
				PRINTFORMW 「%마스터는% 인간이면서 강하네……」
			CASE 5
				PRINTFORMW 「바깥 세계에선 우물이 사라져 가면서, 여기엔 우물이 많아……」
			CASE 6
				PRINTFORML 「요즘엔 물통이 유행하더라……」
				PRINTFORMW 「나, 난 좀……」
			CASE 7
				PRINTFORMW 「바람 불면 통장수가 돈을 번다……? 통장수……??」
			CASE 8
				PRINTFORMW 「좁은 곳에선 침착해져서 좋아……」
			CASE 9
				PRINTFORMW 「도깨비불이란 건…… 그…… 불……」
			CASE 10
				PRINTFORML 「가끔 우물을 통해서 바깥 세계랑 이어지는……」
				PRINTFORMW 「비, 비밀로 해 줘……」
			CASE 11
				PRINTFORMW 「어, 어떻게 하면 강해질 수 있을까……?」
			CASE 12
				CALL M_KOJO_K83_TALKSET("汎用")
			CASE 13
				CALL M_KOJO_K83_TALKSET("思慕")
			CASEELSE
				CALL M_KOJO_K83_TALKSET("キャラ")
			ENDSELECT
		;사모
		ELSEIF TALENT:[[키스메]]:사모
			IF !RAND:6
				CALL M_KOJO_K83_TALKSET("汎用")
			ELSE
				CALL M_KOJO_K83_TALKSET("思慕")
			ENDIF
		;それ以外
		ELSE
			;地底住まい
			IF MAIN_MAP == 9
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「밖에서 왔구나…… 음……」
				CASE 2
					PRINTFORMW 「별로 얘기하는 건…… 자신 없어……」
				CASE 3
					PRINTFORMW 「지, 지저는…… 어때?」
				CASE 4
					PRINTFORMW 「여, 여러가지 일이 있었구나……」
				CASE 5
					PRINTFORMW 「인간이 여기에 있으면 위험할 것 같아……」
				CASEELSE
					CALL M_KOJO_K83_TALKSET("汎用")
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「밖에서 왔구나…… 음……」
				CASE 2
					PRINTFORMW 「별로 얘기하는 건…… 자신 없어……」
				CASE 3
					PRINTFORMW 「나, 난 지저에서 살지……」
				CASE 4
					IF TIME >= 1200 || TIME < 360
						PRINTFORMW 「이, 이런 시간에 왜 돌아다니는 거야……?」
					ELSEIF !AT_HOME(MASTER)
						PRINTFORMW 「무, 무슨 일로 왔어……?」
					ELSE
						PRINTFORMW 「여, 여기서 지내는구나……」
					ENDIF
				CASE 5
					IF !AT_HOME(MASTER)
						PRINTFORMW 「인간이 여기에 있으면 위험할 것 같아……」
					ELSE
						PRINTFORMW 「어, 어쩌다 왔을 뿐이야……」
					ENDIF
				CASEELSE
					CALL M_KOJO_K83_TALKSET("汎用")
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
