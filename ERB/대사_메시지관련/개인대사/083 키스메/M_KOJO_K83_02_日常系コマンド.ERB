@M_KOJO_SUCCESS_COM_K83_300
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K83_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～話をした。」とかが消えます。また、削除した場合は自動で口上色が付きません。
CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_MESSAGE_COM_K83_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;ここに口上書いて下さい
	CALL M_KOJO_COLOR_K83
	;酔ってる
	IF M_KOJO_K83_YOIDORE(83)
		CALL M_KOJO_K83_TALKSET("주정꾼")
	;会話不能
	ELSEIF TCVAR:회화불능
		IF TALENT:연모
			PRINTFORMW 「, 그렇다……」
		ELSEIF TALENT:사모
			PRINTFORMW 「, 뭐……？」
		ELSE
			PRINTFORMW 「…………」
		ENDIF
	;不機嫌
	ELSEIF M_KOJO_K83_FUKIGEN(83)
		IF TALENT:연모
			PRINTFORMW 「응……응……」
		ELSE
			PRINTFORMW 「응…………」
		ENDIF
;	;デート会話,未定
;	ELSEIF TALENT:연모 && FLAG:데이트상대 == 83 && DATENAME_SPOT() != "도중" && !デート会話:(CFLAG:현재위치 / 10)
	;くしゃみ
	ELSEIF !RAND:200
		PRINTFORMW 「응！ ……염주」
	;通常
	ELSE
		;失敗
		IF TFLAG:SELECTCOM분기 == -1
			IF TALENT:연모
				PRINTFORMW 「, 그러한 것은……」
			ELSE
				PRINTFORMW 「아, 그래……」
			ENDIF
		;恋慕
		ELSEIF TALENT:연모
			SELECTCASE RAND:14
			CASE 1
				PRINTFORMW 「후후……%CALLNAME:MASTER%(이)라고 이야기하는 것, 즐겁다……」
			CASE 2
				PRINTFORMW 「, 뼈를 모으는 것이……취미……」
			CASE 3
				PRINTFORML 「지상은 자주(잘) 이변이 일어나고 있는데……지저는 언제나 변함없다……」
				PRINTFORMW 「거기를 좋아해……」
			CASE 4
				PRINTFORMW 「%마스터는% 인간인데 강하다……」
			CASE 5
				PRINTFORMW 「밖의 세계에서 우물이 없어져 가기 때문에, 여기에는 우물이 많이 있어……」
			CASE 6
				PRINTFORML 「최근에는 물통이 유행하고 있다……」
				PRINTFORMW 「원, 나에게는 조금……」
			CASE 7
				PRINTFORMW 「바람이 불면 통장수가 득을 본다……？ 통장수……？？」
			CASE 8
				PRINTFORMW 「좁은 곳은 침착하기 때문에 좋아……」
			CASE 9
				PRINTFORMW 「도깨비불이라고 하는 것은……그……화……」
			CASE 10
				PRINTFORML 「이따금 우물을 통해서 밖의 세계와 연결되어……」
				PRINTFORMW 「히, 비밀로 해……」
			CASE 11
				PRINTFORMW 「, 어떻게 하면 강하게 될 수 있을까……？」
			CASE 12
				CALL M_KOJO_K83_TALKSET("汎用")
			CASE 13
				CALL M_KOJO_K83_TALKSET("思慕")
			CASEELSE
				CALL M_KOJO_K83_TALKSET("キャラ")
			ENDSELECT
		;思慕
		ELSEIF TALENT:사모
			IF !RAND:6
				CALL M_KOJO_K83_TALKSET("汎用")
			ELSE
				CALL M_KOJO_K83_TALKSET("思慕")
			ENDIF
		;それ以外
		ELSE
			;地底住まい
			IF MAIN_MAP == 9
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「밖으로부터 왔다……-응……」
				CASE 2
					PRINTFORMW 「너무 이야기하는 것은……골칫거리……」
				CASE 3
					PRINTFORMW 「, 지저는……어때？」
				CASE 4
					PRINTFORMW 「있고, 여러가지 있었군요……」
				CASE 5
					PRINTFORMW 「인간이 여기에 있는 것은 위험하다고 생각한다……」
				CASEELSE
					CALL M_KOJO_K83_TALKSET("汎用")
				ENDSELECT
			;それ以外
			ELSE
				SELECTCASE RAND:6
				CASE 1
					PRINTFORMW 「밖으로부터 왔다……-응……」
				CASE 2
					PRINTFORMW 「너무 이야기하는 것은……골칫거리……」
				CASE 3
					PRINTFORMW 「원, 나는 지저에 살고 있다……」
				CASE 4
					IF TIME >= 1200 || TIME < 360
						PRINTFORMW 「개, 이런 시간에 어째서 돌아 다녀……？」
					ELSEIF !AT_HOME(MASTER)
						PRINTFORMW 「, 무엇을 하러 왔어……？」
					ELSE
						PRINTFORMW 「개, 여기에 살고 있어……」
					ENDIF
				CASE 5
					IF !AT_HOME(MASTER)
						PRINTFORMW 「인간이 여기에 있는 것은 위험하다고 생각한다……」
					ELSE
						PRINTFORMW 「, 어딘지 모르게 왔을 뿐……」
					ENDIF
				CASEELSE
					CALL M_KOJO_K83_TALKSET("汎用")
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1
;==================================================
;会話セット
;==================================================
@M_KOJO_K83_TALKSET(ARGS)
SELECTCASE ARGS
CASE "汎用"
	IF TALENT:연모
		SELECTCASE RAND:5
		CASE 1
			PRINTFORMW 「 나도 옛날은……」
		CASE 2
			PRINTFORMW 「어와……아마……」
		CASE 3
			PRINTFORMW 「응……그렇다」
		CASE 4
			PRINTFORMW 「어제는 이런 일이……」
		CASEELSE
			PRINTFORMW 「저기요,%CALLNAME:MASTER%……」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORMW 「어와……그……」
		CASE 2
			PRINTFORMW 「…응……」
		CASE 3
			PRINTFORMW 「그……그러니까……」
		CASEELSE
			PRINTFORMW 「%CALLNAME:MASTER%하……」
		ENDSELECT
	ENDIF
CASE "キャラ"
	SELECTCASE RAND:7
	CASE 1
		PRINTFORMW 「야마메 는……내가 태어났을 때로부터 함께 있어 주고 있다……소중한 사람」
	CASE 2
		PRINTFORMW 「파르스는……뿌리는 상냥한 사람. 나에게도 상냥하게 해 준다……」
	CASE 3
		PRINTFORML 「용의씨는……누구라도 동일 취급 해 준다……」
		PRINTFORMW 「 그렇지만, 나는 약있고로부터……별로……」
	CASE 4
		PRINTFORMW 「득도함씨는……밖에 나오지 않기 때문에, 별로 보지 않는다……」
	CASE 5
		PRINTFORMW 「린는……가끔 상관해 준다……」
		PRINTFORMW 「 그렇지만, 그 흙, 모래를 운반하는 일륜차에는 넣게 해 주지 않다……」
	CASE 6
		PRINTFORMW 「하늘 는……조금 무엇 생각할까 알지 않는다……」
	CASEELSE
		PRINTFORMW 「개, 진하고 는……아, 별로 본 적 없다……」
	ENDSELECT
CASE "思慕"
	SELECTCASE RAND:5
	CASE 1
		PRINTFORMW 「, 밖의 세계의 일……(듣)묻고 싶다……」
	CASE 2
		PRINTFORMW 「이 통은……나의 일부, 같은……」
	CASE 3
		IF MAIN_MAP == 9
			PRINTFORMW 「, 지저의 생활은 어때……？」
		ELSE
			PRINTFORMW 「, 지저는 즐거워……아마……」
		ENDIF
	CASE 4
		PRINTFORMW 「%마스터는% 인간인데……변」
	CASEELSE
		PRINTFORMW 「두레박 흘림은……도깨비불을 내거나 한다……」
	ENDSELECT
CASE "주정꾼"
	SELECTCASE RAND:4
	CASE 1
		PRINTFORML 「에에에……후후」
	CASE 2
		PRINTFORML 「네에에에에……」
	CASE 3
		PRINTFORML 「에에……히구！」
	CASEELSE
		PRINTFORML 「후후, 아하하！」
	ENDSELECT
	PRINTFORMDW 키스메는 취하고 있다…….
ENDSELECT
