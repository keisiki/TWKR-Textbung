@M_KOJO_MESSAGE_COM_K83_621_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;地の文をカットしてるのでMESSAGE処理を内蔵する
IF LOCAL
	CALL M_KOJO_COLOR_K83
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
		; PRINTFORMDW 何も釣れなかった…… ; 낚시 메시지 수정
		IF TALENT:[[키스메]]:연모
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「……느긋해서, 좋아」
			CASE 2
				PRINTFORMW 「내, 내 통은 낚시에 쓰는 게 아냐!」
			CASEELSE
				PRINTFORMW 「천천히 기다리면 분명……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「토, 통에 넣지 마……」
			CASEELSE
				PRINTFORMW 「잡히지 않네……」
			ENDSELECT
		ENDIF
		RETURN 1
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTFORMW 「앗!」
			PRINTL 
			PRINTFORMDL %조사처리(CALLNAME:TARGET, "이")% 나무 토막을 낚았다…
			PRINTW
			PRINTFORMW 「…………시무룩」
		ELSE
			PRINTFORMDL 나무 토막을 낚았다……
			PRINTW
			PRINTFORMW 「어라……」
		ENDIF
		RETURN 1
	;紙切れ
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORML 「앗!」
		ELSE
			PRINTFORMDL 종이가 낚였다.
		ENDIF
		PRINTDW 
		PRINTDL ┌─────────────┐
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　모리야부적　　　　　　｜
		PRINTDL ｜　   　　　　　　　　　　　｜
		PRINTDL ｜    　5장 모으면                　｜
		PRINTDL ｜　　   뽑기 1회 가능합니다　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜이익은 보증하기 어렵습니다 ｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　　　　　　모리야신사　｜
		PRINTDL └─────────────┘
		PRINTW 
		PRINTFORMW 「이게, 뭐야?」
		RETURN 1
	;KR판 오리지날 수정. 원래 구버전 낚시 메시지 기반인 것을 최신판에 맞춤
	CASE 1 TO 20, 22 TO 60
		LOCALS = %FISH_NAME(TFLAG:193)%
	CASE 21
		LOCALS = 로터
	CASEELSE
		LOCALS = %ITEMNAME:(TFLAG:193)%
	ENDSELECT

	IF TFLAG:194 > 1000
		PRINTFORMW 「앗!」
		PRINTL 
		PRINTFORMDL %타겟이% %조사처리(LOCALS,"를")% 낚았다！
		PRINTW 
		SELECTCASE TFLAG:193
		;가물치
		CASE 15
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：強")% 무, 무거워! %CALLNAME:MASTER%, 도와 줘……!」
		;ローター
		CASE 21
			PRINTFORMW 「이건……\/\/\/」
		;カエル
		CASE 700
			PRINTFORMW 「왜 개구리가……」
		;お宝
		CASE 609, 606
			PRINTFORMW 「와, 예뻐……!」
		CASEELSE
			PRINTFORMW 「나, 낚았어……!」
		ENDSELECT
	ELSE
		PRINTFORMDL %조사처리(LOCALS,"가")% 잡혔다！
		PRINTW 
		SELECTCASE TFLAG:193
		;ローター
		CASE 21
			IF TALENT:[[키스메]]:연모
				PRINTFORMW 「%CALLNAME:MASTER%, 뭐 해……?」
			ELSE
				PRINTFORMW 「이, 이건……\/\/\/」
			ENDIF
		;カエル
		CASE 700
			PRINTFORMW 「개구리 귀여워……」
		;お宝
		CASE 609, 606
			PRINTFORMW 「예쁘네……!」
		CASEELSE
			PRINTFORMW 「대, 대단해……!」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
