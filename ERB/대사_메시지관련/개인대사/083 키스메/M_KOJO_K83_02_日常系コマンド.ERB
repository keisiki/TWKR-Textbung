		PRINTFORMW 「……여기가 좋거든……」
	;アナル舐め
	CASE 56
		PRINTFORMW 「괜찮아……더럽지 않아……」
	;乳首舐め
	CASE 59
		PRINTFORMW 「……날름날름……」
	;ディープキス
	CASE 60
		CALL HPH_PRINT(@"「もっとしよ……HPH んむ……くちゅっ、くちゅくちゅ……」", "W")
	;クンニ強制,アナル舐め強制,強制アソコ愛撫
	CASE 70, 73, 76
		PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")%」
	;Ｖ挿入,Ａ挿入
	CASE 85, 87
		CALL HPH_PRINT(@"「んあっ！ %CALLNAME:MASTER%のおちんちん……きたぁHPH」", "W")
	;だいしゅきホールド
	CASE 86
		CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")%%M_KOJO_K83_AUTO("二連刺激")%あぁ～～っHPH」", "W")
