;TCVAR:껴안고첫회　1=普通に合意ゲット　2=先にキスぐらいしろよ　3=媚薬or発情には勝てなかったよお…　4=悔しい…刻印さえなければﾋﾞｸﾋﾞｸﾝ
;5=強引に迫られると断れぬ(素質臆病持ち専用) 6=まんざらでもない　7=駄目　8=論外 98=口上内特殊判定による失敗 99=口上内特殊判定による成功
;口上内特殊判定は該当するCFLAGが0だと無効
;==================================================
@M_KOJO_MESSAGE_COM_K83_350
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_350_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_350_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	;押し倒され値	ENDURE(83) >= RAND:300 + 300で真,しかしRANDの値は変化するのでここでは400を超えたら押し倒されることにする
	IF FLAG:시간정지
	;野外失敗
	ELSEIF TFLAG:194 == 2
		IF ENDURE(83) > 400
			PRINTFORMW 「어, 여기에서는 조금……안돼……」
		ELSE
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")% (이)나, 그만두어……！」
		ENDIF
	;人目失敗
	ELSEIF TFLAG:194 == 1
		IF ENDURE(83) > 400
			PRINTFORMW 「이다, 안돼……사람이 있어……」
		ELSE
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")% (이)나, 그만두어……！」
		ENDIF
	;乱交失敗
	ELSEIF TFLAG:194 == 3
		PRINTFORMW 「예！？ , 그러한 것은……(이)나다……」
	;ムード失敗
	ELSEIF TFLAG:194 == 4
		PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")% , 미안……」
	;泥酔失敗
	ELSEIF TFLAG:194 == 5
		CALL M_KOJO_K83_TALKSET("주정꾼")
		PRINTL 
		PRINTFORMDW 맹숭맹숭한 얼굴의 목격자가 있는데 취 범하는 것 가지 않는다
	;泥酔
	ELSEIF TCVAR:만취 == 1
		PRINTFORMW 「……, 로 해 의……？」
	;乱交
	ELSEIF GET_TARGETNUM() > 1
		IF CFLAG:현재위치 == CFLAG:84:현재위치
			PRINTFORMW 「야, 야마메 도……？ %CALLNAME:MASTER%(을)를 하고 싶으면……아무튼……」
		ELSE
			PRINTFORMW 「네……보고, 모두는……%CALLNAME:MASTER%(을)를 하고 싶으면……아무튼……」
		ENDIF
	;それ以外
	ELSEIF !TCVAR:351
		IF M_KOJO_K83_UFUFU(83)
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")%」
		ELSE
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")%」
		ENDIF
	ENDIF

	;先に押し倒された
	IF TCVAR:351
		IF M_KOJO_K83_UFUFU(83)
			PRINTL 
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% , 미안해요……상냥하게 해……」
		ENDIF
	;通常押し倒し
	ELSEIF !FLAG:시간정지 && !TFLAG:194
		PRINTL 
		SELECTCASE TCVAR:껴안고첫회
		;普通の初回,キスがまだ
		CASE 1, 2
			PRINTFORMW 「%CALLNAME:MASTER%……이것은, 그……」
			SIF M_KOJO_K83_UFUFU(83)
				PRINTFORMW 「……(이)나, 상냥하게 해……그런데」
		;媚薬or発情
		CASE 3
			PRINTFORMW 「아……좋아,%CALLNAME:MASTER%……마음대로 해……\/\/\/」
		;時姦
		CASE 4
			PRINTFORMW 「어, 나다……어째서……어째서……」
		;臆病
		CASE 5
			PRINTFORMW 「, 그런…………」
		;もうちょっと
		CASE 6
			PRINTFORMW 「%CALLNAME:MASTER%……나……아직……」
		;ダメ
		CASE 7
			PRINTFORMW 「어와……개, 이런 것은……」
		;早すぎ
		CASE 8
			PRINTFORMW 「이나 , 나다～！！」
		;初回以降
		CASEELSE
			;酔ってる
			IF M_KOJO_K83_YOIDORE(83)
				PRINTFORMW 「～,%CALLNAME:MASTER%～～！」
			;不機嫌
			ELSEIF M_KOJO_K83_FUKIGEN(83)
				PRINTFORMW 「……」
			;屋外
			ELSEIF OUTROOF(CFLAG:MASTER:현재위치)
				IF M_KOJO_K83_UFUFU(83)
					PRINTFORMW 「개, 이런 곳에서……있고, 좋아……\/\/\/」
				ELSE
					PRINTFORMW 「네……개, 여기서 해……？」
				ENDIF
			ELSE
				IF M_KOJO_K83_UFUFU(83)
					SELECTCASE RAND:6
					CASE 1
						PRINTFORMW 「후후, 좋아……가득 기분 좋고……」
					CASE 2
						PRINTFORMW 「아……%CALLNAME:MASTER%……」
					CASE 3
						PRINTFORMW 「응……, 마음대로 해……」
					CASE 4
						PRINTFORML 
						PRINTFORMW 「……%CALLNAME:MASTER%……좋아」
					CASE 5
						PRINTFORMW 「%CALLNAME:MASTER%……」
					CASEELSE
						PRINTFORMW 「이나, 상냥하게 해……그런데」
					ENDSELECT
				ELSE
					PRINTFORMW 「……(이)나, 상냥하게 해……그런데」
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
