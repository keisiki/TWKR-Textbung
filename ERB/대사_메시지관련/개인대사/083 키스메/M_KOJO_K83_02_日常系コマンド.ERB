;会話セット
;==================================================
@M_KOJO_K83_TALKSET(ARGS)
SELECTCASE ARGS
CASE "汎用"
	IF TALENT:[[키스메]]:연모
		SELECTCASE RAND:5
		CASE 1
			PRINTFORMW 「나도 예전엔……」
		CASE 2
			PRINTFORMW 「어…… 아마……」
		CASE 3
			PRINTFORMW 「응…… 그래」
		CASE 4
			PRINTFORMW 「어젠 이런 일이……」
		CASEELSE
			PRINTFORMW 「있잖아, %CALLNAME:MASTER%……」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
		CASE 1
			PRINTFORMW 「어…… 그게……」
		CASE 2
			PRINTFORMW 「으… 응……」
		CASE 3
			PRINTFORMW 「저기…… 그러니까……」
		CASEELSE
			PRINTFORMW 「%마스터는%……」
		ENDSELECT
	ENDIF
CASE "キャラ"
	SELECTCASE RAND:7
	CASE 1
		PRINTFORMW 「야마메는…… 내가 태어났을 때부터 함께 있어 줬어…… 소중한 사람이야」
	CASE 2
		PRINTFORMW 「파르시는…… 속은 착한 사람이야. 나한테도 잘 대해 줘……」
	CASE 3
		PRINTFORML 「유기 씨는…… 누구든지 평등하게 대해……」
		PRINTFORMW 「근데, 난 약해서…… 별로……」
	CASE 4
		PRINTFORMW 「사토리 씨는…… 밖에 안 나와서, 별로 못 봤어……」
	CASE 5
		PRINTFORMW 「린은…… 가끔 만나……」
		PRINTFORMW 「근데, 그 화차엔 들어가지 못 하게 하더라……」
	CASE 6
		PRINTFORMW 「우츠호는…… 좀 무슨 생각을 하는지 모르겠어……」
	CASEELSE
		PRINTFORMW 「코, 코이시는…… 벼, 별로 본 적 없어……」
	ENDSELECT
CASE "思慕"
	SELECTCASE RAND:5
	CASE 1
		PRINTFORMW 「바, 바깥 세계에 대해서…… 듣고 싶어……」
	CASE 2
		PRINTFORMW 「이 통은…… 내 일부, 같은 거야……」
	CASE 3
		IF MAIN_MAP == 9
			PRINTFORMW 「지, 지저 생활은 어때……?」
		ELSE
			PRINTFORMW 「지, 지저는 즐거워…… 아마……」
		ENDIF
	CASE 4
		PRINTFORMW 「%마스터는% 인간인데…… 사랑하게 됐어」
	CASEELSE
		PRINTFORMW 「츠루베오토시는…… 도깨비불을 만들어내기도 해……」
	ENDSELECT
CASE "주정꾼"
	SELECTCASE RAND:4
	CASE 1
		PRINTFORML 「헤헤헤…… 흐흐」
	CASE 2
		PRINTFORML 「헤헤헤헤헤……」
	CASE 3
		PRINTFORML 「흐헤헤…… 딸꾹!」
	CASEELSE
		PRINTFORML 「흐흐, 아하하!」
	ENDSELECT
	PRINTFORMDW 키스메는 취해 있다……
ENDSELECT
