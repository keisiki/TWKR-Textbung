@M_KOJO_MESSAGE_PALAMCNG_A_K83_6(ARG)
;-------------------------------------------------
;素股
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	SELECTCASE RAND:5
	CASE 1
		CALL HPH_PRINT(@"「후후, 질척해졌어……HPH」", "W")
	CASE 2
		CALL HPH_PRINT(@"「휴…… 기, 기분 좋았어……HPH」", "W")
	CASE 3
		CALL HPH_PRINT(@"「흐에에…… 지, 지치네…… %CALLNAME:MASTER%, 뽀뽀해 줘…HPH」", "W")
	CASE 4
		PRINTFORMW 「하아, 하아…… %CALLNAME:MASTER%, 기분 좋아 보여……」
	CASEELSE
		IF TALENT:[[키스메]]:처녀 == 1 || TALENT:[[키스메]]:처녀 == -1
			PRINTFORMW 「하아, 하아…… 지, 진짜로 한 것 같아……\/\/\/」
		ELSE
			CALL HPH_PRINT(@"「하아, 하아…… 다, 다음엔 안에다 해 줘……HPH」", "W")
		ENDIF
	ENDSELECT
	RETURN 1
ENDIF
RETURN 1

