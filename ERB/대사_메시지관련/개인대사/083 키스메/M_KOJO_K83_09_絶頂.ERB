@M_KOJO_MESSAGE_PALAMCNG_A_K83_20(ARG)
;-------------------------------------------------
;手淫フェラ
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	;精飲
	IF EXP_UP(15) == NOWEX:PLAYER:사정 * 3
		SELECTCASE RAND:3
		CASE 1
			PRINTFORML 「으응! 응끄, 응끄…… 꿀꺽」
			CALL HPH_PRINT(@"「푸아…… 다 마셨어……HPH」", "W")
		CASE 2
			PRINTFORML 「응~! 응끄으…… 쮸르릅……」
			CALL HPH_PRINT(@"「푸아…… 맛있었어HPH」", "W")
		CASEELSE
			PRINTFORML 「응!  응…… 응끄…… 꿀꺽!」
			CALL HPH_PRINT(@"「푸아…… 잘 먹었어HPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 1
			CALL HPH_PRINT(@"「응끄…! 푸아…… 많이 쌌네……HPH」", "W")
		CASE 2
			PRINTFORMW 「으응…! 푸아……%CALLNAME:MASTER%, 기분 좋아 보여……」
		CASEELSE
			PRINTFORMW 「응…! 푸아…… 좀 써……」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
RETURN 1
