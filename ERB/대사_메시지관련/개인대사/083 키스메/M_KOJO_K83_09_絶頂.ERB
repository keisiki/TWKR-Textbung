;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:Ｃ絶頂 NOWEX:Ｖ絶頂 NOWEX:Ｂ絶頂 NOWEX:Ａ絶頂 NOWEX:Ｍ絶頂
;各部位の絶頂強度。まず0~3が割り当てられ、多重絶頂により最大16倍される。つまり単部位なら最大3、5重絶頂で最大48
;NOWEX:TotalEX
;多重絶頂補正を受ける前の各部位絶頂強度の合計値。1~15。このテンプレにおける分岐の基準。

;TEQUIP:50
;Ｖに誰の竿が入ってるか。TEQUIP:50 = MASTERであなたが挿入中となる。
;TEQUIP:51
;Ａに誰の竿が入ってるか。TEQUIP:51 = MASTERであなたが挿入中となる。
;NOWEX:사정
;사정量、最大3。たとえばIF NOWEX:MASTER:사정 > 1だと、あなたが大量사정以上した場合となる。
;SYNCED_ORGASM(TARGET)
;相手とあなたが同時に絶頂した場合真となる。絶頂の部位は問わない。

;-------------------------------------------------
;絶頂時の口上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K83
#DIM TotalEX
SIF FLAG:시간정지
	RETURN 1
;-------------------------------------------------
;분유
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 「시, 싫어……\/\/\/ 가슴이…… %M_KOJO_K83_AUTO("単刺激…！")%」
ENDIF
;;-------------------------------------------------
;;사정した
;;記入チェック（=0, 非表示、1, 表示）
;LOCAL = 0
;;-------------------------------------------------
;IF LOCAL && NOWEX:사정
;	PRINTFORMW 
;ENDIF
;-------------------------------------------------
;방뇨
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 「흐에에…… 오줌 나와…… 읏!」
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため、書き分けたければ任意で分岐を加筆されたし。
IF LOCAL
	IF M_KOJO_K83_UFUFU([[키스메]])
		SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")% 안 돼, %M_KOJO_K83_AUTO("単刺激")%! 간다간다간다앗HPH 아아아아앗HPHHPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")% 와 버렷, %M_KOJO_K83_AUTO("単刺激")%! 응아아앗HPH 가 버려엇~~HPHHPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("二連刺激")% 햐응, %M_KOJO_K83_AUTO("単刺激")%! 가 버려엇~HPH 미안해엣~~HPHHPH」", "W")
			ENDSELECT
		;中
		CASE 2 TO 4
			PRINTFORM 「%M_KOJO_K83_AUTO("二連刺激")%%M_KOJO_K83_AUTO("単刺激")% 
			SELECTCASE RAND:3
			CASE 1 
				CALL HPH_PRINT(@"가 버려엇! 간다앗HPH」", "W")
			CASE 2
				CALL HPH_PRINT(@"아, 앙대엣! 갸 버려엇HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"간다앗! 응아앗!HPH」", "W")
			ENDSELECT
		;弱
		CASE 1
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 가 버렷…!」
			CASE 2
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 아, 안 돼…!」
			CASEELSE
				PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 가, 가…!」
			ENDSELECT
		ENDSELECT
	ELSE
		SELECTCASE NOWEX:TotalEX
		;強
		CASE 5 TO 15
			PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")% %M_KOJO_K83_AUTO("単刺激")% 간다앗!」
		;中
		CASE 2 TO 4
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 간다앗!」
		;弱
		CASE 1
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激…！")% 가앗…!」
		ENDSELECT
	ENDIF
ENDIF
