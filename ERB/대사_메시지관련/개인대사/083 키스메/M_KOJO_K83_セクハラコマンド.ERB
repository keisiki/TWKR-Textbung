@M_KOJO_MESSAGE_COM_K83_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	;時間停止中
	IF FLAG:시간정지
	;睡眠中
	ELSEIF CFLAG:수면
		PRINTFORMW 「…………」
	;押し倒され中
	ELSEIF CFLAG:우후후 == 2
		IF FIRSTTIME("ファーストキス")
			PRINTFORML 「！」
			PRINTFORMW 「네에에……키스 했어……처음\/\/\/」
		ELSE
			SIF !M_KOJO_K83_UFUFU(83)
				RETURN 1
			SELECTCASE TCVAR:카운터행동
			;前から抱きつく
			CASE 32
				PRINTFORML 「……」
				PRINTFORMW 「응……(이)다 넘으면서의 키스……기쁘다……\/\/\/」
			;手淫
			CASE 50
				PRINTFORML 「………」
				PRINTFORMW 「어때…？ 기분 좋다？」
			;ディープキス
			CASE 60
				PRINTFORML 「………………」
				PRINTFORMW 「후~, 후~……도, 좀 더……」
			;Ｖ挿入,Ａ挿入,Ｖ腰振り,Ａ腰振り
			CASE 85, 87, 88, 89
				CALL HPH_PRINT(@"「き、キスしながら……イこHPH」", "L")
				PRINTFORMW 「…, 응, 응응응！」
			;だいしゅきホールド
			CASE 86
				PRINTFORML 「, 아, 들 째……좀 더！」
				CALL HPH_PRINT(@"「このままイって……HPH」", "W")
			;それ以外
			CASEELSE
				PRINTFORMW 「……. ……도, 이제(벌써) 있고 야…………」
			ENDSELECT
		ENDIF
	;添い寝
	ELSEIF CFLAG:동침중
		IF FIRSTTIME("ファーストキス")
			PRINTFORMW 「은 원！！」
			PRINTFORMW 「원……나의 처음……\/\/\/」
		ELSE
			PRINTFORMW 「응……///」
		ENDIF
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE(83)
		CALL M_KOJO_K83_TALKSET("酔いどれ")
	;拒絶
	ELSEIF TFLAG:SELECTCOM분기 == 99 || TFLAG:192 == -2
		PRINTFORMW 「이나, 나다……！」
	;ファーストキス
	ELSEIF FIRSTTIME("ファーストキス")
		IF TALENT:연모
			PRINTFORMW 「……！！」
			PRINTFORMW 「원원원……%CALLNAME:MASTER%(와)과……키스, 해 버렸다……\/\/\/」
		ELSEIF TALENT:사모
			PRINTFORMW 「응응……！！」
			PRINTFORMW 「원, 나의……처음\/\/\/」
		ELSE
			PRINTFORMW 「응……！！」
			PRINTFORMW 「원, 나……처음……」
		ENDIF
	;通常
	ELSE
		IF TALENT:연모
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「……. ……」
			CASE 2
				PRINTFORMW 「…….……네에에\/\/\/」
			CASE 3
				PRINTFORMW 「～……」
			CASE 4
				PRINTFORMW 「응……. -로서 온다……」
			CASEELSE
				PRINTFORMW 「―……. 응, 좀 더……」
			ENDSELECT
		ELSEIF TALENT:사모
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「응……///」
			CASE 2
				PRINTFORMW 「……」
			CASEELSE
				PRINTFORMW 「……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「응응！？ …………」
			CASEELSE
				PRINTFORMW 「……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
