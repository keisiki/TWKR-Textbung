TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K83_310
CALL M_KOJO_MESSAGE_COM_K83_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	;시간정지中
	IF FLAG:시간정지
	;수면中
	ELSEIF CFLAG:수면
	;押し倒され中
	ELSEIF CFLAG:[[키스메]]:우후후 == 2
		SIF !M_KOJO_K83_UFUFU([[키스메]])
			RETURN 1
		PRINTFORMW 「후후…… 기다려……」
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE([[키스메]])
		CALL M_KOJO_K83_TALKSET("주정꾼")
	;不기분
	ELSEIF M_KOJO_K83_FUKIGEN([[키스메]])
		IF TALENT:[[키스메]]:연모
			PRINTFORMW 「%CALLNAME:MASTER%, 심술쟁이……」
		ELSE
			PRINTFORMW 「으에에…… 너, 너무해……」
		ENDIF
	;通常
	ELSE
		IF TALENT:[[키스메]]:연모
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")%」
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「야…… 야해!」
			CASE 2
				PRINTFORMW 「……바보」
			CASE 3
				PRINTFORMW 「가, 갑자기 그러지 마……」
			CASE 4
				PRINTFORMW 「노, 놀랐잖아……」
			CASEELSE
				PRINTFORMW 「어휴……」
			ENDSELECT
		ELSEIF TALENT:[[키스메]]:사모
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：強")%」
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「오, 왜……?」
			CASE 2
				PRINTFORMW 「어, 어째서……?」
			CASEELSE
				PRINTFORMW 「시, 싫어……」
			ENDSELECT
		ELSE
			PRINTFORMW 「꺄아아악!!」
			PRINTFORMW 「그, 그만해…………」
		ENDIF
	ENDIF
ENDIF
RETURN 1
