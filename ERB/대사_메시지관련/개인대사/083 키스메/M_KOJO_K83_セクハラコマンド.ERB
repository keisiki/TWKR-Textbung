			CASE 85, 87, 88, 89
				CALL HPH_PRINT(@"「키, 키스하면서…… 가HPH」", "L")
				PRINTFORMW 「응, 쪽… 하읍, 응, 으으응!」
			;だいしゅきホールド
			CASE 86
				PRINTFORML 「쯉, 앗, 안 댓…… 좀 더!」
				CALL HPH_PRINT(@"「이대로 가……HPH」", "W")
			;それ以外
			CASEELSE
				PRINTFORMW 「쮸…… 푸아…… 또, 또 해…… 쮸으으……」
			ENDSELECT
		ENDIF
	;添い寝
	ELSEIF CFLAG:[[키스메]]:동침중
		IF FIRSTTIME("ファーストキス")
			PRINTFORMW 「흐왓!!」
			PRINTFORMW 「내…… 내 처음……\/\/\/」
		ELSE
			PRINTFORMW 「응……\/\/\/」
		ENDIF
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE([[키스메]])
		CALL M_KOJO_K83_TALKSET("주정꾼")
	;拒絶
	ELSEIF TFLAG:SELECTCOM분기 == 99 || TFLAG:192 == -2
		PRINTFORMW 「시, 싫어……!」
	;ファーストキス
	ELSEIF FIRSTTIME("ファーストキス")
		IF TALENT:[[키스메]]:연모
			PRINTFORMW 「쪽……!!」
			PRINTFORMW 「으와아…… %CALLNAME:MASTER%하고…… 키스, 했어……\/\/\/」
		ELSEIF TALENT:[[키스메]]:사모
			PRINTFORMW 「으응……!!」
			PRINTFORMW 「내, 내…… 처음\/\/\/」
		ELSE
			PRINTFORMW 「응……!!」
			PRINTFORMW 「나, 나…… 처음……」
		ENDIF
	;通常
	ELSE
		IF TALENT:[[키스메]]:연모
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「응, 쪽…… 푸아……」
			CASE 2
				PRINTFORMW 「쪽…… 에헤헤……\/\/\/」
			CASE 3
				PRINTFORMW 「흐아……」
			CASE 4
				PRINTFORMW 「응아…… 후끈해져……」
			CASEELSE
				PRINTFORMW 「쮸…… 응, 좀 더……」
			ENDSELECT
		ELSEIF TALENT:[[키스메]]:사모
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「응……\/\/\/」
			CASE 2
				PRINTFORMW 「흐아……」
			CASEELSE
				PRINTFORMW 「쪽……」
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「으응!?  …………」
			CASEELSE
				PRINTFORMW 「아읍……」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
