		SELECTCASE TCVAR:카운터행동
		;スカートの中を見せる
		CASE 35
			PRINTFORMW 「아 아！ 좋다！ 기분 좋다……！」
		;クンニ強制
		CASE 70
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 좋아……좀 더 만져……！」
		;それ以外
		CASEELSE
			PRINTFORMW 「%M_KOJO_K83_AUTO("二連刺激")%응응, 좀 더……！」
		ENDSELECT
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE(83)
		CALL M_KOJO_K83_TALKSET("酔いどれ")
	;不機嫌
	ELSEIF M_KOJO_K83_FUKIGEN(83)
		IF TALENT:연모
			PRINTFORMW 「이나……그만두어……」
		ELSE
			PRINTFORMW 「～……」
		ENDIF
	;通常
	ELSE
		IF TALENT:연모
			PRINTFORM 「%M_KOJO_K83_AUTO("単刺激")% 
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 실마리……서고는……」
			CASE 2
				PRINTFORMW -…-！」
			CASE 3
				PRINTFORMW 후~, 후~……！」
			CASE 4
				PRINTFORMW 에, 거치지 않아 싶다……！」
			CASEELSE
				PRINTFORMW , 어째서……」
			ENDSELECT
		ELSEIF TALENT:사모
			PRINTFORM 「%M_KOJO_K83_AUTO("単刺激")%
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW (이)다, 안돼……」
			CASE 2
				PRINTFORMW 있고, 아니……」
			CASEELSE
				PRINTFORMW (이)나, 나다……」
			ENDSELECT
		ELSE
			PRINTFORML 「%M_KOJO_K83_AUTO("悲鳴：強")%」
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「이나다～！」
			CASEELSE
				PRINTFORMW 「그만두어……！！」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
