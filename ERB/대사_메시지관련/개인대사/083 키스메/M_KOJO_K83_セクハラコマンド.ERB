;==================================================
@M_KOJO_SUCCESS_COM_K83_310
;成否判定
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K83_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K83_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_310_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	CALL M_KOJO_COLOR_K83
	;時間停止中
	IF FLAG:시간정지
	;睡眠中
	ELSEIF CFLAG:수면
	;押し倒され中
	ELSEIF CFLAG:우후후 == 2
		SIF !M_KOJO_K83_UFUFU(83)
			RETURN 1
		PRINTFORMW 「후후……기다리고 있어……」
	;酔ってる
	ELSEIF M_KOJO_K83_YOIDORE(83)
		CALL M_KOJO_K83_TALKSET("酔いどれ")
	;不機嫌
	ELSEIF M_KOJO_K83_FUKIGEN(83)
		IF TALENT:연모
			PRINTFORMW 「%CALLNAME:MASTER%의 심술쟁이……」
		ELSE
			PRINTFORMW 「……히, 심하다……」
		ENDIF
	;通常
	ELSE
		IF TALENT:연모
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：弱")%」
			SELECTCASE RAND:5
			CASE 1
				PRINTFORMW 「네……！」
			CASE 2
				PRINTFORMW 「……바보」
			CASE 3
				PRINTFORMW 「, 갑자기 하지 말아줘……」
			CASE 4
				PRINTFORMW 「, 놀랐다……」
			CASEELSE
				PRINTFORMW 「 이제(벌써)……」
			ENDSELECT
		ELSEIF TALENT:사모
			PRINTFORMW 「%M_KOJO_K83_AUTO("悲鳴：強")%」
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「, 뭐……？」
			CASE 2
				PRINTFORMW 「, 어째서……？」
			CASEELSE
				PRINTFORMW 「이나, 나다……」
			ENDSELECT
		ELSE
			PRINTFORMW 「아！！」
			PRINTFORMW 「이나, 그만두어…………」
		ENDIF
	ENDIF
ENDIF
RETURN 1
