	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:연모 || TALENT:연인
			PRINTFORMW 「개, 미안……아직……무섭다……」
		ELSE
			PRINTFORMW 「어……, 그것은……뭐, 아직……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;断った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연모 || TALENT:연인
			PRINTFORMW 「미안해요……미안……」
		ELSE
			PRINTFORMW 「 , 미안……고마워요……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;断ったのに粘られて切れた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:연모 || TALENT:연인
			PRINTFORMW 「이나, 나다……부탁…………」
		ELSE
			PRINTFORMW 「안돼……！ 무리…………」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;危険日以外許可
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:연모 || TALENT:연인
			PRINTFORMW 「, 거기까지 말한다면……」
		ELSE
			PRINTFORMW 「원, 알았다……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;危険日許可
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:연모 || TALENT:연인
			PRINTFORMW 「원, 알았다……%CALLNAME:MASTER%(을)를 하고 싶으면……좋아」
		ELSE
			PRINTFORMW 「……원, 알았다…………」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
