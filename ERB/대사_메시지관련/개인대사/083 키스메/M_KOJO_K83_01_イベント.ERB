;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K83_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU(83)
;-------------
IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「후~, 후~……개, 지쳤다……」
		PRINTFORMW 「 , 미안해요……%CALLNAME:MASTER%……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「후~, 후~……도, 이제(벌써), 충분히……」
		PRINTFORMW 「%CALLNAME:MASTER%……아, 고마워요……」
		TCVAR:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORMW 「！！」
		IF TALENT:연인
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「ん……おしおきして……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「ん……かわりばんこね……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「……, 미안해요……！」
			CASEELSE
				PRINTFORMW 「……, 용서해……！」
			ENDSELECT
		ENDIF
		TCVAR:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		IF TALENT:연인
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「%CALLNAME:MASTER%……好きなんだね……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「じゃあ、いっぱい遊んであげる……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「後悔しても遅いよ……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「ふふ……それじゃあ、遠慮しないよ……HPH」", "W")
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

