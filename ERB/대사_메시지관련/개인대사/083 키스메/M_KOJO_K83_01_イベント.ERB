		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「응,%CALLNAME:MASTER%……, 집에 오지 않는다……？」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;部屋に誘われたのを受けた
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「정말？ 그러면, 자 조속히……！」
			TALENT:기분 = 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;部屋に誘われたのを断った
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			PRINTFORMW 「그래……」
			TALENT:기분 = MIN(TALENT:기분, 0)
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

