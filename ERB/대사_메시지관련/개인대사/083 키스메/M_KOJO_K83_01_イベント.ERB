				ELSEIF M_KOJO_K83_UFUFU(83)
					CALL HPH_PRINT(@"「はぁはぁ……き、きもちかった……HPH」", "W")
				ELSEIF TALENT:사모
					CALL HPH_PRINT(@"「……HPH」", "W")
				ELSE
					;あるのか？？？
					PRINTFORMW 「…………\/\/\/」
				ENDIF
				RETURN 1
			ENDIF
		CASE 6
			;-------------------------------------------------
			;事後お泊り
			;記入チェック（=0, 非表示、1, 表示）
			LOCAL:1 = 1
			;-------------------------------------------------
			IF LOCAL:1
				PRINTL 
				IF TALENT:연인
					PRINTFORMW 「과 묵어 간다……？ 정말？ 했다……！」
					PRINTFORMW 「오늘은,%CALLNAME:MASTER%하지만 와 주면(자) 되고 생각했어……」
					PRINTFORMW 「그랬더니 정말로 와 주어, 나…………」
					CALL HPH_PRINT(@"「ねぇ、%CALLNAME:MASTER%……もっかいしよHPH」", "W")
				ELSEIF M_KOJO_K83_UFUFU(83)
					PRINTFORMW 「과 묵어……？ 있고, 좋아……」
					PRINTFORMW 「 실은 조금 전까지,%CALLNAME:MASTER%하지만 와 주면(자) 되고 생각했어……」
					PRINTFORMW 「그랬더니 정말로 와 주어, 나…………」
					PRINTFORMW 「응,%CALLNAME:MASTER%……그런데 말이야――」
				;死に分岐な気がして手抜き。
				ELSEIF TALENT:사모
					CALL HPH_PRINT(@"「……HPH」", "W")
				ELSE
					PRINTFORMW 「…………\/\/\/」
				ENDIF
				RETURN 1
			ENDIF
	ENDSELECT
ENDIF

