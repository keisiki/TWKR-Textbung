;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K83_80
CALL M_KOJO_MESSAGE_COM_K83_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		;未成熟,粗チン
		IF TALENT:MASTER:형상 == 1 || TALENT:MASTER:형상 == 2
			PRINTFORMW 「잡고 문질러……?」
		ELSE
			PRINTFORMW 「구, 굵어…… 잡고 문질러……?」
		ENDIF
		PRINTFORMW 「이, 이렇게……? 어때……?」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		SELECTCASE RAND:2
		CASE 1
			PRINTFORML 「엄청 부풀어 올랐어…… 갈 것 같나 보네……」
			CALL HPH_PRINT(@"「좋아…… 잔뜩 퓻퓻 싸 줘……HPH」", "W")
		CASEELSE
			PRINTFORML 「갈 것 같아? 그럼 더 빨리 해야지……」
			CALL HPH_PRINT(@"「좋아…… 잔뜩 퓻퓻 싸 줘……HPH」", "W")
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「어, 어때……?」
			PRINTFORMW 「기분 많이 좋나 보네……」
		CASE 2
			PRINTFORML 「귀여워해 줄게……」
			PRINTFORMW 「후후…… 깜찍해……」
		CASE 3
			PRINTFORML 「잘 되고 있어? 정말?」
			PRINTFORMW 「다행이네…… 더 힘내야지……」
		CASE 4
			PRINTFORML 「쓱싹쓱싹, 기분 좋아……?」
			PRINTFORMW 「그래, 더 기분 좋아져……」
		CASEELSE
			;未成熟,粗チン
			IF TALENT:MASTER:형상 == 1 || TALENT:MASTER:형상 == 2
				PRINTFORML 「%CALLNAME:MASTER% 자지…… 작고 귀여워……」
				PRINTFORMW 「괜찮아, 내 몸에 딱 맞으니까……」
			;発光,イボ付き
			ELSEIF TALENT:MASTER:형상 == 4 || TALENT:MASTER:형상 == 5
				PRINTFORML 「%CALLNAME:MASTER% 자지…… 그게……」
				PRINTFORMW 「트, 특이한 것 같아…… 역시……」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER% 자지…… 굵어……」
				PRINTFORMW 「이렇게 굵은 게…… 내 안에…… 꿀꺽」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

;==================================================
;81,フェラチオ
;==================================================
@M_KOJO_MESSAGE_COM_K83_81
CALL M_KOJO_MESSAGE_COM_K83_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		PRINTFORML 「구, 굵어…… 이거 안 들어갈지도 몰라……」
		PRINTFORMW 「아니, 최, 최선을 다해 보자……!」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「응풋, 응……」
		CALL HPH_PRINT(@"「갈 것 같아? 응, 그럼 이대로, 입 안에다 줘……HPH」", "W")
	ELSE
		SELECTCASE RAND:5
		CASE 1
			PRINTFORML 「으응, 응끄…… 푸하아」
			PRINTFORMW 「미, 미안, 역시 커서……」
		CASE 2
			PRINTFORML 「응끄… 쯉…… 쮸파」
			PRINTFORMW 「하아… 하아…… 할짝」
		CASE 3
			PRINTFORML 「이, 입으로 해 줄게…… 아음… 응끄으……」
			PRINTFORMW 「기, 기분 좋아……？」
		CASE 4
			PRINTFORML 「할짝… 할짝…… 아끄… 끄쯉……」
			PRINTFORMW 「아, 하히, 아이써……」
		CASEELSE
			PRINTFORML 「쯉… 쯉…… 푸아」
			PRINTFORMW 「기, 기분 좋았어? 에헤헤……」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

