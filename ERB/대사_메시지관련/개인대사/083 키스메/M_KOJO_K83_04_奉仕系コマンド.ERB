;82,パイズリ
;==================================================
@M_KOJO_MESSAGE_COM_K83_82
CALL M_KOJO_MESSAGE_COM_K83_82_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_82_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM) && !FLAG:시간정지
		PRINTFORML 「가, 가슴……」
		PRINTFORMW 「%CALLNAME:MASTER%…… 놀리는 거야?」
		RETURN 1
	ENDIF
	IF FLAG:시간정지
	;イきそう
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「후후, %CALLNAME:MASTER%, 갔나 보네……」
		CALL HPH_PRINT(@"「되게 기분 좋아 보여서…… 다행이야……HPH」", "W")
	ELSE
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「으으…… 어, 어려워……」
		CASE 2
			PRINTFORMW 「하아… 하아…… 뜨거워……」
		CASEELSE
			IF TALENT:[[키스메]]:가슴사이즈 < 0
				PRINTFORMW 「……더 컸으면 좋을 텐데…… 미안해……」
			ELSE
				PRINTFORMW 「어때…… 나, 잘 했어?」
			ENDIF
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

