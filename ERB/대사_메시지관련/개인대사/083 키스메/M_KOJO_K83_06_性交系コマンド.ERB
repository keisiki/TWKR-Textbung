LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	IF FLAG:시간정지
	ELSEIF TCVAR:[[키스메]]:파과 == 1
		PRINTFORMW 「너, 넣는구나…… 괜찮아, 무섭지 않아」
		PRINTFORMW 「%CALLNAME:MASTER%, 잘 봐……」
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「%M_KOJO_K83_AUTO("単刺激")% %M_KOJO_K83_AUTO("二連刺激")% %M_KOJO_K83_AUTO("単刺激")%」
		CALL M_KOJO_K83_NAKADASHI
	ELSE
		SELECTCASE RAND:5
		CASE 1
			CALL HPH_PRINT(@"「후후…… 범해 줄게……HPH」", "L")
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激…！")% %M_KOJO_K83_AUTO("二連刺激")% 아, 안 돼, 머, 멈출 수 없어!」
		CASE 2
			CALL HPH_PRINT(@"「좋아…… 기분 좋게 해 줄게……HPH」", "L")
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激…！")% %M_KOJO_K83_AUTO("二連刺激")% 아핫HPH 내 보지, 기분 좋아?」", "W")
		CASE 3
			CALL HPH_PRINT(@"「じゃあ……い、いただきます……HPH」", "L")
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激…！")% %M_KOJO_K83_AUTO("二連刺激")% 좋앗! %CALLNAME:MASTER% 자지, 좋앗!」
		CASE 4
			CALL HPH_PRINT(@"「내가 %CALLNAME:MASTER% 위에……HPH」", "L")
			PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激…！")% %M_KOJO_K83_AUTO("二連刺激")% 하앗, 하앗, 조, 좀 더……!」
		CASEELSE
			CALL HPH_PRINT(@"「%CALLNAME:MASTER%…… 히, 힘 내……HPH」", "L")
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激…！")% %M_KOJO_K83_AUTO("二連刺激")% 흐아앗! %CALLNAME:MASTER%…! %CALLNAME:MASTER%……HPH」", "W")
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

