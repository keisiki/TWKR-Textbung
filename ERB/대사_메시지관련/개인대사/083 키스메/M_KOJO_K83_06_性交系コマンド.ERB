;==================================================
@M_KOJO_K83_NAKADASHI
;一応暴発防止
SIF !M_KOJO_K83_UFUFU([[키스메]])
	RETURN 
SIF !NOWEX:MASTER:사정
	RETURN
SIF FLAG:시간정지
	RETURN

SELECTCASE RAND:7
CASE 1
	CALL HPH_PRINT(@"「싸, 쌀 거야? 좋아, 안에……! 앗HPH」", "W")
CASE 2
	CALL HPH_PRINT(@"「줘… %CALLNAME:MASTER% 정자…… 많이 줘HPH」", "W")
CASE 3
	CALL HPH_PRINT(@"「나와…… 정자 잔뜩… 내 안에HPH」", "W")
CASE 4
	CALL HPH_PRINT(@"「꼭 안아 줘……! %CALLNAME:MASTER%… 좋아……HPH」", "W")
CASE 5
	CALL HPH_PRINT(@"「좋아…! 안에…… 듬뿍 싸 줘HPH」", "W")
CASE 6
	CALL HPH_PRINT(@"「하앗, 하앗, 나와… 안에다 싸HPH」", "W")
CASEELSE
	CALL HPH_PRINT(@"「앗, 아읏, 그, 그대로 싸……! 제일 안쪽에HPH」", "W")
ENDSELECT

;==================================================
;파과
;==================================================
@M_KOJO_K83_VIRGIN
;一応
SIF TCVAR:[[키스메]]:파과 != 1
	RETURN 
SIF !M_KOJO_K83_UFUFU([[키스메]])
	RETURN 
SIF FLAG:시간정지
	RETURN
PRINTFORMW 「너, 넣을 거야…? 좋아……」
PRINTFORMW 「%조사처리(CALLNAME:MASTER, "이")% 원하는 대로 해……」

