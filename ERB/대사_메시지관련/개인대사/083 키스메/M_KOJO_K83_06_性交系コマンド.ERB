;==================================================
;72,挿入Ｇスポット責め
;==================================================
@M_KOJO_MESSAGE_COM_K83_72
CALL M_KOJO_MESSAGE_COM_K83_72_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_72_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	IF FLAG:시간정지
	ELSEIF NOWEX:MASTER:사정
		PRINTFORML 「%M_KOJO_K83_AUTO("単刺激")% %M_KOJO_K83_AUTO("二連刺激")% %M_KOJO_K83_AUTO("単刺激")% %M_KOJO_K83_AUTO("単刺激…！")%」
		CALL M_KOJO_K83_NAKADASHI
	ELSE
		PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% %M_KOJO_K83_AUTO("二連刺激")% %M_KOJO_K83_AUTO("単刺激")% %M_KOJO_K83_AUTO("単刺激…！")%」
	ENDIF
ENDIF
RETURN 1

;==================================================
;73,挿入子宮口責め
;==================================================
@M_KOJO_MESSAGE_COM_K83_73
CALL M_KOJO_MESSAGE_COM_K83_73_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K83_73_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------------------------------------------
IF LOCAL
	IF FLAG:시간정지
	;種付けプレス
	ELSEIF NOWEX:MASTER:사정
		SELECTCASE RAND:5
		CASE 1
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 온닷, 정액 드러왓! 아기 생기겠어엇HPH」", "W")
		CASE 2
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% %CALLNAME:MASTER%, 죻아HPH 싸 줘HPH 내 가장 안쪽에 싸 줘HPH」", "W")
		CASE 3
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% %CALLNAME:MASTER%! %CALLNAME:MASTER%~HPH 갸 버려엇HPH」", "W")
		CASE 4
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 와 줘…! %CALLNAME:MASTER%, 몽땅 줘엇HPH」", "W")
		CASEELSE
			CALL HPH_PRINT(@"「%M_KOJO_K83_AUTO("単刺激")% 좋아앗HPH 자궁에 잔뜩 퓻퓻해 줘엇HPH」", "W")
		ENDSELECT
	ELSE
		PRINTFORMW 「%M_KOJO_K83_AUTO("単刺激")% 괴, 괴로워엇…! %M_KOJO_K83_AUTO("単刺激")%」
	ENDIF
ENDIF
RETURN 1
