				CASE 0
					PRINTFORMW 「응, 우음... 후우....」
					PRINTFORMW 「하아... %CALLNAME:MASTER%의 침, 엄청 달아요...」
					RETURN 1
				CASE 1
					PRINTFORMW 「정말, 사랑해요.」
				CASE 2
					PRINTFORMW 「하아, 하아... 응, 혀를 넣다니...」
					PRINTFORMW 「우음... 음...」
					RETURN 1				
			ENDSELECT
		RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「하아, 하아.... 조금만, 더....」
					RETURN 1
				CASE 1
					PRINTFORMW 「우움... 후우...」
					PRINTFORMW 「하아... 하아...」
					PRINTFORMW 「조금만 더, 해요...」
					RETURN 1				
			ENDSELECT
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1


