;==================================================
@M_KOJO_EVENT_K43_17(ARG, ARG:1)
;말 주인은 TARGET
;배타와 명기되어 있지 않은 물건은 상황에 따라서는 연속으로 표시되므로 주의
;ARG에 사정 상황
;ARG:1에 사정 상태
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;ARG=1~3은 배타
	;수수께끼의 쾌감
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;팬티 분실
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각입안 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;정액중독저정음
		IF ARG:1 == 1
		;정액중독저토출
		ELSEIF ARG:1 == 2
		;정액중독저손으로 가린다
		ELSEIF ARG:1 == 3
		;정액중독고정음
		ELSEIF ARG:1 == 4
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각안면 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		;정액경험 있어 닦아 잡기
		IF ARG:1 == 1
		;정액경험 이루어 닦아 잡기
		ELSEIF ARG:1 == 2
		;얼굴에 사정하기 정액 빨고 새
		ELSEIF ARG:1 == 3
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각수음 사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		;닦아 잡기
		IF ARG:1 == 1
		;빨고 새
		ELSEIF ARG:1 == 2
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각애널사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;A감각 0
		IF ARG:1 == 1
		;그것 이외
		ELSEIF ARG:1 == 2
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각처녀상실】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;【무자각질내사정】
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;비처녀 질내사정 피임약 없음
		IF ARG:1 == 1
		;비처녀 질내사정 피임약 있음
		ELSEIF ARG:1 == 2
		;처녀 질내사정
		ELSEIF ARG:1 == 3
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
