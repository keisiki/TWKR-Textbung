@M_KOJO_MESSAGE_PALAMCNG_A_K4_22(ARG)
;-------------------------------------------------
;식스나인
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF TALENT:연모
		IF TCVAR:PLAYER:콘돔 == 1
			;동시 사정
			IF NOWEX:사정 && NOWEX:MASTER:사정
				PRINTFORMW 
				RETURN 1
			;TARGET만 사정
			ELSEIF NOWEX:사정
				;PLAYER에 대하고 있다
				IF GETBIT(TALENT:PLAYER:성별, 1)
					PRINTFORMW 
					RETURN 1
				;붙지 않다
				ELSE
					PRINTFORMW 
					RETURN 1
				ENDIF
			;PLAYER만 사정
			ELSE
				;TARGET에 대하고 있다
				IF GETBIT(TALENT:성별, 1)
					PRINTFORMW 
					RETURN 1
				;붙지 않다
				ELSE
					IF RAND:5 == 0
						PRINTFORML 「이얍, 나왔어♪」
						PRINTFORML %마스터는% 미마의 엉덩이에 매달리면서, 허리를 띄워 사정을 했다
						PRINTFORMW 「인간은 약벼…%UNICODE(0x2665)%　악령을 빨면(자) 안 돼?」
					ELSEIF RAND:4 == 0
						PRINTFORML 사정에 의해 힘이 빠진%마스터는%, 쿠니를 멈추어 버렸다
						PRINTFORML 「할짝…할짝…, 조금%CALLNAME:MASTER%, 게으름 피우지 않고 늦고」
						PRINTFORMW 미마에 주의받아 쿠니를 재개하지만, 귀두를 빨 수 있어 다시 힘이 빠져 버렸다
					ELSEIF RAND:3 == 0
						PRINTFORML 「…♪　내 쪽이 한 수 위임손이었다 라는 것일까?」
						PRINTFORML 허무하게 웃는 미마에 대해서 %마스터는% 항복의 손을 들었다
						PRINTFORMW 「후후…♪」
					ELSEIF RAND:2 == 0
						PRINTFORML 미마는 정액이 들어온 콘돔을 내걸면서 되돌아 보았다
						PRINTFORML 「내 승리구나 %UNICODE(0x2665)%」
						PRINTFORMW 수수께끼의 패배감을 맛본%마스터는%, 다음이야말로는과 비렬에 혀를 붙였다
					ELSE
						PRINTFORML 「앙……?…」
						PRINTFORML 미마는 입을 떼어 놓아 콘돔에 모여 가는 정액을 바라보고 있다
						PRINTFORMW %마스터는% 쿠니를 단념해 사정의 쾌감에 잠겼다
					ENDIF
					RETURN 1
				ENDIF
			ENDIF
		ELSE
			;동시 사정
			IF NOWEX:사정 && NOWEX:MASTER:사정
				PRINTFORMW 
				RETURN 1
			;TARGET만 사정
			ELSEIF NOWEX:사정
				;PLAYER에 대하고 있다
				IF GETBIT(TALENT:PLAYER:성별, 1)
					PRINTFORMW 
					RETURN 1
				;붙지 않다
				ELSE
					PRINTFORMW 
					RETURN 1
				ENDIF
			;PLAYER만 사정
			ELSE
				;TARGET에 대하고 있다
				IF GETBIT(TALENT:성별, 1)
					PRINTFORMW 
					RETURN 1
				;붙지 않다
				ELSE
					IF RAND:5 == 0
						PRINTFORML 「이얍, 나왔어♪」
						PRINTFORML %마스터는% 미마의 엉덩이에 매달리면서, 허리를 띄워 사정을 했다
						PRINTFORMW 「인간은 약벼…%UNICODE(0x2665)%　악령을 빨면(자) 안 돼?」
					ELSEIF RAND:4 == 0
						PRINTFORML 사정에 의해 힘이 빠진%마스터는%, 쿠니를 멈추어 버렸다
						PRINTFORML 「쪽―…쪽―…, 조금%CALLNAME:MASTER%, 게으름 피우지 않고 늦고」
						PRINTFORMW 미마에 주의받아 쿠니를 재개하지만, 정액을 빨아내져 다시 힘이 빠져 버렸다
					ELSEIF RAND:3 == 0
						PRINTFORML 「…♪　내 쪽이 한 수 위임손이었다 라는 것일까?」
						PRINTFORML 허무하게 웃는 미마에 대해서 %마스터는% 항복의 손을 들었다
						PRINTFORMW 「후후…♪」
					ELSEIF RAND:2 == 0
						PRINTFORML 미마는 입꼬리를 뒤따른 정액을 손가락으로 닦으면서 되돌아 보았다
						PRINTFORML 「내 승리구나 %UNICODE(0x2665)%」
						PRINTFORMW 수수께끼의 패배감을 맛본%마스터는%, 다음이야말로는과 비렬에 혀를 붙였다
