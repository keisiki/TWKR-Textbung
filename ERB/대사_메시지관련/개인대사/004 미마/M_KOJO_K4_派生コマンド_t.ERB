;==================================================
;6, 삽입 G스포 고문
;==================================================
@M_KOJO_MESSAGE_SCOM_K4_6
CALL M_KOJO_MESSAGE_SCOM_K4_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K4_6_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF TALENT:연모
			PRINTFORMW 「히잇!　아아앙!」
			PRINTFORMW 미마는 G 스폿을 괴롭힘 당하는 교성을 올리고 있다
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	PRINTFORML 
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		;대면입위
		IF PREVCOM == 60 && TFLAG:193
			IF RAND:5 == 0
				PRINTFORML %마스터는% 직립 한 채로 미마에 삽입하고 있다
				PRINTFORML 미마도 다리를 펴%CALLNAME:MASTER%에 들러붙어, 질로 페니스를 자극하고 있다
				PRINTFORMW 서로 상하로 몸을 움직이면서 쾌감을 높여 갔다
			ELSEIF RAND:4 == 0
				PRINTFORML 「%UNICODE(0x2665)%　%UNICODE(0x2665)%　윽 %UNICODE(0x2665)%」
				PRINTFORML 미마는 조금 몸을 띄워 %CALLNAME:MASTER%에 달라붙고 있다
				PRINTFORMW %마스터는% 미마를 껴안으면(자) 필사적으로 성기끼리를 조정
			ELSEIF RAND:3 == 0
				PRINTFORML 페니스가 질리를 격렬하게 켠다
				PRINTFORML 「아, 좋다!　저의 있어라 %UNICODE(0x2665)%」
				PRINTFORMW %마스터는% 무릎을 진동시키면서 허리를 계속 거절했다
			ELSEIF RAND:2 == 0
				PRINTFORML 「%CALLNAME:MASTER%%UNICODE(0x2665)%　기분 좋은…%UNICODE(0x2665)%　기분 좋아…%UNICODE(0x2665)%」
				PRINTFORML 미마는 %CALLNAME:MASTER%의 등에 팔을 돌려, 헛말과 같이 이름을 부르고 있다
				PRINTFORMW 두 명은 선 채로 행해지는 결합을 마음껏 즐겼다
			ELSE
				PRINTFORML 「아%UNICODE(0x2665)%　아%UNICODE(0x2665)%　굉장해 %UNICODE(0x2665)%」
				PRINTFORML 귀두가 미마의 자궁구를 꾸준히 하고 두드리고 있다
				PRINTFORMW %마스터는% 미마의 태 넙적다리를 움켜 쥔 채로 몇 번이나 페니스를 밀어 올렸다
			ENDIF
		;정상위
		ELSEIF PREVCOM == 60
			IF RAND:5 == 0
				PRINTFORML %마스터는% 넣었다 뺐다 운동을 하면서 미마의 손을 잡았다
				PRINTFORML 미마도 손가락을 걸어 잡아 돌려주어 온다
				PRINTFORMW 연인 이음을 한 채로, 두 명은 말없이  성기를 서로 켰다
			ELSEIF RAND:4 == 0
				PRINTFORML 페니스를 부딪히는 소리가 울리고 있다
				PRINTFORML 「이나 %UNICODE(0x2665)%　아%UNICODE(0x2665)%　아앙 %UNICODE(0x2665)%　아앗 %UNICODE(0x2665)%」
				PRINTFORMW 성기로부터 보내져 오는 쾌락에 미마와 %마스터는% 땀을 흘렸다
			ELSEIF RAND:3 == 0
				PRINTFORML 「쪽…%UNICODE(0x2665)%　…%UNICODE(0x2665)%　…%UNICODE(0x2665)%」
				PRINTFORML %마스터와% 미마는 키스를 하면서 정상위를 실시했다
				PRINTFORMW 상과아래의 입을 서로 위로하면서 , 두 명은 흥분을 높여 간다
			ELSEIF RAND:2 == 0
				PRINTFORML 「%CALLNAME:MASTER%%UNICODE(0x2665)%　좋아 %UNICODE(0x2665)%　좋아해 %UNICODE(0x2665)%」
				PRINTFORML 정담을 주고 받는 미마의 질이 꽉과 꼭 죄인다
				PRINTFORMW 페니스를 압박받은 %마스터는% 허리와 진동시켰다
			ELSE
				PRINTFORML 「아%UNICODE(0x2665)%　좀 더 %UNICODE(0x2665)%　좀 더 %UNICODE(0x2665)%」
				PRINTFORML 미마는 최안쪽을 켜지고 흐트러지고 있다
				PRINTFORMW %CALLNAME:MASTER%도 사정까지 겨우 도착하지 않아와 열심히 허리를 털었다
			ENDIF
		;배면입위
		ELSEIF PREVCOM == 61 && TFLAG:193
			IF RAND:5 == 0
				PRINTFORML 「…%UNICODE(0x2665)%　%UNICODE(0x2665)%　%UNICODE(0x2665)%」
				PRINTFORML 미마는 쾌락에 신음하면서 질을 찔리고 있다
				PRINTFORMW 끝 없게 부풀어 올라 가는 페니스를 %마스터는% 기분이 내킬 때까지 전후 시켰다
			ELSEIF RAND:4 == 0
				PRINTFORML 미마의 허리에 팔을 돌려, 등에 얼굴을 붙이면서 %마스터는% 허리를 털고 있다
				PRINTFORML 「하앗…%UNICODE(0x2665)%　응…%UNICODE(0x2665)%　응…%UNICODE(0x2665)%」
				PRINTFORMW 미마는 %CALLNAME:MASTER%의 손등을 애처로운 듯이 어루만졌다
			ELSEIF RAND:3 == 0
				PRINTFORML %마스터는% 등 뒤로부터 미마의 가슴유하기 만
				PRINTFORML 「아!　이거 참!」
				PRINTFORMW 손바닥으로부터 흘러넘칠 정도의 거유를 잡으면서, %마스터는% 배면입위를 즐겼다
			ELSEIF RAND:2 == 0
				PRINTFORML 「야,  %UNICODE(0x2665)%　잘게 썲에 %UNICODE(0x2665)%」
				PRINTFORML %마스터는% 허리를 작게 왕복시켜 미마의 질리를 괴롭히고
				PRINTFORMW 집중적인 애무에 미마는 소리를 진동시키고 있다
			ELSE
				PRINTFORML 「아앙 %UNICODE(0x2665)%　흐응 %UNICODE(0x2665)%　으응 %UNICODE(0x2665)%」
				PRINTFORML 미마는 뒤로부터 찔려 교성을 올리고 있다
				PRINTFORMW %마스터가% 페니스를 찌를 때에 미마는 숨을 흘렸다
			ENDIF
		;후배위
		ELSEIF PREVCOM == 61
			IF RAND:5 == 0
				PRINTFORML 「%CALLNAME:MASTER%, 두드리지 마…」
				PRINTFORML 미마의 말을 들은체 만체 하면서, %마스터는% 재차 엉덩이를 경구 털었다
				PRINTFORMW 「히%UNICODE(0x2665)%　(이)나다 , 그만두어…%UNICODE(0x2665)%」
			ELSEIF RAND:4 == 0
				PRINTFORML 「휴우…%UNICODE(0x2665)%　기분, 좋아 …%UNICODE(0x2665)%」
				PRINTFORML 전신을 진동시키면서 짜내는 듯이 미마가 말했다
				PRINTFORMW %CALLNAME:MASTER%도 단 질의 감촉에 황홀로 한 채로 악령과의 섹스를 계속했다
			ELSEIF RAND:3 == 0
				PRINTFORML 미마의 등에 지워져, 흔들리듯 늘어지고 있는 유방을 들어 올렸다
				PRINTFORML 「아%UNICODE(0x2665)%　가슴은, 은 켤레야 %UNICODE(0x2665)%」
				PRINTFORMW %마스터는% 양손의 히등으로 미마의 가슴을 즐기면서, 짐승처럼 허리를 털었다
			ELSEIF RAND:2 == 0
				PRINTFORML %마스터는% 미마의 엉덩이를 잡은 채로 허리를 전후 시키고 있다
				PRINTFORML 「안돼 …%UNICODE(0x2665)%　엉덩이에 손대지 말고…%UNICODE(0x2665)%」
				PRINTFORMW 미마의 소원을 무시하며, %마스터는% 엉덩이를 어루만지면서 후배위를 즐겼다
			ELSE
				PRINTFORML 「히%UNICODE(0x2665)%　%UNICODE(0x2665)%　벗길 수 있는 쉿 %UNICODE(0x2665)%　아%UNICODE(0x2665)%」
				PRINTFORML 엎드린 자세로 된 미마에 %마스터는% 페니스를 삽입하고 있다
				PRINTFORMW 일발 찌르기 할 때에 미마의 거유 흔들리는 와 흔들렸다
			ENDIF
		;기승위
		ELSEIF PREVCOM == 65
