	LOCAL:1 = 1
	;-------------------------------------------------
	;V삽입
	IF LOCAL:1 && TEQUIP:50 == PLAYER
		;사정했다
		IF NOWEX:PLAYER:11
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	;A삽입
