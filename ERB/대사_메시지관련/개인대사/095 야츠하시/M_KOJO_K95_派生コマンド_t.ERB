@M_KOJO_MESSAGE_SCOM_K95_1_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;내기 직전
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「응 ♥ 응 ♥ ♥ ?옥 ♥ 날름 ♥ 으흑 ♥ ♥」
			PRINTFORMW 「정자 가득…♥…내…♥」
