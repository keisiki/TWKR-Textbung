;방뇨
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;절정
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;사중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
		PRINTFORMW 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;삼중절정
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정
		SELECTCASE NOWEX:삼중절정
			;C＆V＆A절정
			CASE 1
				PRINTFORMW 
				RETURN 0
			;B＆V＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 0
			;C＆B＆A절정
