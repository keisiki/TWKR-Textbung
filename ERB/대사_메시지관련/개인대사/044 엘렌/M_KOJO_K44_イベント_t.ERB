;방에서 만남
;==================================================
@M_KOJO_EVENT_K44_1(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL && TCVAR:362 == 0
	;조우한 회수를＋1한다
	TCVAR:351 += 1
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;MASTER가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		$SOUGUU
		;딸딸이들킴
		IF CFLAG:딸딸이들킴 == 2 && TALENT:연모
			PRINTFORMW 「……%CALLNAME:MASTER%의 엣찌//」
		ELSEIF CFLAG:딸딸이들킴
			IF TALENT:연인 && TALENT:연모
				IF CFLAG:딸딸이들킴 == 3
					PRINTFORMW 「이제(벌써)//　일전에는 정말로 부끄러웠어요//」
					SIF FIRSTTIME(1021,1)
					PRINTFORMW 「엣찌도 아직인데 저런 전신 거울는……//」
					SIF ABL:욕망 >= 10 && ABL:노출벽 >= 3
					PRINTFORMW (에서도, 그것은 그것으로 기분 좋았지//)
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%도 참//　열쇠가 열리고 있으니는 마음대로 들어가지 말아//」
				ENDIF
			ELSEIF TALENT:연모 || TALENT:연인
				PRINTFORMW 「아, 하하하하//」
			ELSEIF TALENT:사모
				PRINTFORMW 「그러니까……//」
			ELSE
				PRINTFORMW (저런 전신 거울라고 어떤 얼굴로 이야기하면 좋은거야!//)
			ENDIF
		;신년, 아침이 되어서
		ELSEIF DAY:2 == 1 && DAY:3 == 1 && TCVAR:358 == 0 && DAY:0 != 1
			CALL M_KOJO_MESSAGE_EVENT_K44_EX_TALK
		;신사 더부살이 시한정
		ELSEIF FLAG:더부살이캐릭터 == 44 && CFLAG:현재위치 == 16 && CFLAG:1016 == 0 && !FIRSTTIME(1003,1) && RAND:3 == 2 && TCVAR:358 == 0
			PRINTFORMW 「그럼, 레이무의 허가는 받았고 고지도 끝마쳤다」
			PRINTFORMW 「가게의 청소도 상품의 진열도 끝났기 때문에 이것으로 준비완…, 는 %마스터가% 아닌 %UNICODE(0x2665)%」
			PRINTFORMW 「무엇을 하고 있는지라는?　가게의 준비야 %UNICODE(0x2665)%」
			PRINTFORMW 「이봐, 매일 신사로부터 가게까지 다니는 것은 큰 일이겠지. 그러니까 또 신사에서 가게를 열기로 한 것 %UNICODE(0x2665)%」
			PRINTFORMW 「그래서, 무슨용무일까 %UNICODE(0x2665)%」
			TCVAR:358 = 1
			CFLAG:1016 = 1
			RETURN 1
		;2번째 이후
		ELSEIF TCVAR:351 >= 2
			IF TALENT:연인 && TALENT:연모
				PRINTFORMW 「아, %마스터가% 아닌 %UNICODE(0x2665)%」
			ELSEIF TALENT:연인 || TALENT:연모 || CFLAG:1014 == 1
				PRINTFORMW 「다시 만난 원이군요%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「아, 또 만난 원이군요\@ TALENT:사모 ?  %UNICODE(0x2665)% #  \@」
			ENDIF
		;조
		ELSEIF TIME:2 == 1 || TIME:2 == 2
			IF TALENT:연인
				PRINTFORMW 「안녕 %CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「아 %CALLNAME:MASTER%, 안녕―\@ TALENT:사모 || TALENT:연모 ?  %UNICODE(0x2665)% #  \@」
			ENDIF
		;주
		ELSEIF TIME:2 == 3 || TIME:2 == 4
			IF TALENT:연인
				PRINTFORMW 「안녕하세요, %CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「아 %CALLNAME:MASTER%, 안녕하세요―\@ TALENT:사모 || TALENT:연모 ?  %UNICODE(0x2665)% #  \@」
			ENDIF
		;야
		ELSEIF TIME:2 >= 5 && TIME:2 <= 7
			IF TALENT:연인
				PRINTFORMW 「안녕하세요%UNICODE(0x2665)% %CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
				PRINTFORMW 「아 %CALLNAME:MASTER%, 안녕하세요―\@ TALENT:사모 || TALENT:연모 ?  %UNICODE(0x2665)% #  \@」
			ENDIF
		ENDIF
		SIF TALENT:기분 != -1 && TALENT:연인 && TALENT:연모 && CFLAG:350 != 43 && CFLAG:350 != 41
		PRINTFORMDW %마스터를% 찾아낸 엘렌은 기쁜듯이 접근해 왔다
		SIF TCVAR:351 == 1 && !TALENT:MASTER:연인 && TALENT:연모
		PRINTFORMW (원―%UNICODE(0x2665)%　%마스터를% 만날 수 있었다―%UNICODE(0x2665)%)
		;특별한 날이라면 추가
		IF TCVAR:358 == 0
			CALL M_KOJO_MESSAGE_EVENT_K44_EX_TALK
		;같은 날에 몇회인가 만나고 있으면 추가
		ELSEIF TCVAR:351 >= 5 && TCVAR:352 == 0 && (TALENT:사모 || TALENT:연모)
			PRINTFORMW 「문득 생각했지만 , 오늘은 자주(잘)%마스터를% 보여요」
			IF TALENT:연인
				PRINTFORMW 「뭐, 연인이니까 잘 얼굴을 맞대는 것이 당연해요%UNICODE(0x2665)%」
			ELSEIF TALENT:연모 || CFLAG:1014 == 1
				PRINTFORMW 「왠지 기뻐요%UNICODE(0x2665)%」
			ENDIF
			TCVAR:352 = 1
		ENDIF
		;문제 회피용
		SIF TCVAR:358 == 2
		TCVAR:358 = 1
		;주먹밥용
		SIF TCVAR:353 == 1
		CALL M_KOJO_MESSAGE_EVENT_K44_RICE
		;임신용
		SIF TALENT:임신 && CFLAG:무자각임신 == 0 && CFLAG:1015 == 0 && GET_TARGETNUM() == 1 && TALENT:연인 && TALENT:연모
		CALL M_KOJO_MESSAGE_EVENT_K44_NINSIN
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;신년, 아침이 되어서
		IF DAY:2 == 1 && DAY:3 == 1 && TCVAR:358 == 0 && DAY:0 != 1
			CALL M_KOJO_MESSAGE_EVENT_K44_EX_TALK
		;2번째 이후
		ELSEIF TCVAR:351 >= 2
			IF TALENT:연인 && TALENT:연모
				PRINTFORMW 「아, 여기에 있던 것 %UNICODE(0x2665)% 찾았어요」
			ELSEIF TALENT:연인 || TALENT:연모 || CFLAG:1014 == 1
				PRINTFORMW 「다시 만난 원이군요%UNICODE(0x2665)%」
			ELSEIF TALENT:사모
				PRINTFORMW 「아아, 여기에 있었어」
			ELSE
				PRINTFORM 「아, 또 만난 원이군요」
			ENDIF
		;조
		ELSEIF TIME:2 == 1 || TIME:2 == 2
			IF TALENT:연인
				PRINTFORMW 「안녕 %CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
				PRINTFORM 「안녕―%UNICODE(0x2665)% %CALLNAME:MASTER%
				SIF TALENT:사모 || TALENT:연모
				PRINTFORM %UNICODE(0x2665)%
				PRINTW 」
			ENDIF
		;주
		ELSEIF TIME:2 == 3 || TIME:2 == 4
			IF TALENT:연인
				PRINTFORMW 「안녕하세요, %CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
				PRINTFORM 「안녕하세요―%UNICODE(0x2665)% %CALLNAME:MASTER%
				SIF TALENT:사모 || TALENT:연모
				PRINTFORM %UNICODE(0x2665)%
				PRINTW 」
			ENDIF
		;야
		ELSEIF TIME:2 >= 5 && TIME:2 <= 7
			IF TALENT:연인
				PRINTFORMW 「안녕하세요%UNICODE(0x2665)% %CALLNAME:MASTER%%UNICODE(0x2665)%」
			ELSE
				PRINTFORM 「안녕하세요―%UNICODE(0x2665)% 
				SIF TALENT:사모 || TALENT:연모
				PRINTFORM %UNICODE(0x2665)%
				PRINTW 」
			ENDIF
		ENDIF
		SIF TALENT:기분 != -1 && TALENT:연인 && TALENT:연모 && CFLAG:350 != 43 && CFLAG:350 != 41
		PRINTFORMDW %마스터를% 찾아낸 엘렌은 기쁜듯이 접근해 왔다
		SIF TCVAR:351 == 1 && !TALENT:MASTER:연인 && TALENT:연모
		PRINTFORMW (원―%UNICODE(0x2665)%　%마스터를% 만날 수 있었다―%UNICODE(0x2665)%)
		;특별한 날이라면 추가
		SIF TCVAR:358 == 0
		CALL M_KOJO_MESSAGE_EVENT_K44_EX_TALK
		;문제 회피용
		SIF TCVAR:358 == 2
		TCVAR:358 = 1
		;주먹밥용
		SIF TCVAR:353 == 1
		CALL M_KOJO_MESSAGE_EVENT_K44_RICE
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 함께 들어간다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:연인 && TALENT:연모
			PRINTFORMW 「아, %CALLNAME:MASTER%도 욕실에 왔었던 것%UNICODE(0x2665)%」
			PRINTFORMW 「모처럼이고 함께 들어갑시다 %UNICODE(0x2665)%」
		ELSEIF TALENT:연모 || CFLAG:1014 == 1 || TALENT:연인
			PRINTFORMW 「무늬//　%CALLNAME:MASTER%도 욕실에 왔어//」
			PRINTFORMW 「……함께 들어가도 괜찮을까//」
		ELSEIF TALENT:사모
			PRINTFORMW 「아, %마스터가% 아니다」
			PRINTFORMW 「조금 방해 시켜 주어요%UNICODE(0x2665)%」
		ELSE
			PRINTFORMW 「무늬, 먼저 온 손님이 있었어」
			PRINTFORMW 「뭐 좋은가, 실례하겠습니다~ %UNICODE(0x2665)%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 나와 갔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:연모 || TALENT:연인
			PRINTFORMW 「그러니까……　부디 천천히―//」
		ELSEIF TALENT:사모
			PRINTFORMW 「그럼 천천히 %UNICODE(0x2665)%」
		ELSE
			PRINTFORMW 「아야야, 먼저 온 손님이 있었어」
			PRINTFORMW 「어쩔 수 없기 때문에 돌아옵시다」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 내쫓아졌다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출처에서 조우했다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		GOTO SOUGUU
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;타캐릭터와 데이트중에 조우했다
	;ARG:1에 데이트상대
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:연인 && TALENT:연모
			PRINTFORML 「아, 둘이서 외출?」
			IF CFLAG:MASTER:초기위치 == CFLAG:초기위치
				PRINTFORMW 「그다지 늦게 안 되는 동안에 돌아가―」
				PRINTFORMW 「……그래그래, 바람기는 절대로 안되니까」
			ELSE
				PRINTFORMW 「……뭐, 바람기가 아니면 좋아요」
			ENDIF
		ELSEIF TALENT:연모
			PRINTFORMW 「둘이서 데이트군요―, 부러워요」
			PRINTFORMW (이번%마스터를% 데이트 신청해 볼까//)
		ELSEIF TALENT:연인
			PRINTFORML 「당연하지만, 바람기는 안되니까」
			PRINTFORMDW 엘렌에 다짐을 받아졌다
		ELSEIF TALENT:사모
			PRINTFORMW 「두 사람 모두 즐겨 와 %UNICODE(0x2665)%」
		ELSE
			PRINTFORMW 「아, 어쩌면 둘이서 데이트?」
			PRINTFORMW 「좋은데―, 언젠가 나도 연인과 데이트 해 보고 싶어요」
		ENDIF
		RETURN 1
	ENDIF
ELSE
	TCVAR:362 = 0
ENDIF
RETURN 1

;주먹밥용 함수, 일부러 함수로 하지 않아도 괜찮을지도…
@M_KOJO_MESSAGE_EVENT_K44_RICE
PRINTFORMW 「오늘은 수고했어 같구나」
PRINTFORMW 「그런 당신에게, 옛 %UNICODE(0x2665)%　내 손 만들기 주먹밥이야 %UNICODE(0x2665)%」
PRINTFORMW 「이것을 먹어 건강하게 되어 %UNICODE(0x2665)%」
PRINTFORMDW %마스터는% 엘렌으로부터 받은 주먹밥을 먹었다
PRINTFORMDL ……
PRINTFORMDW …
IF ABL:요리기능 < 4
	PRINTFORMDW 특별한 것은 들어가 있지 않은 단순한 소금 쥠이지만 이것이 꽤 맛좋다, 엘렌의 애정이 깃들이고 있으니일까
ELSE
	PRINTFORMDW !
	PRINTFORMDW %CALLNAME:MASTER%에 기꺼이 받고 싶다고 하는 엘렌의 구상의 덕분인가, 단순한 소금 쥠이라고 하는데 매우 맛있다!
ENDIF
PRINTL  
PRINTFORMDW %마스터는% 주먹밥을 할짝 평정했다
PRINTFORMW 「이제(벌써) 다 먹었어!　빠르네요―」
PRINTFORMW 「그래서, 나의 주먹밥은 맛있었어? //」
PRINTFORMW 「…에헤헤//　%CALLNAME:MASTER%에 기뻐하실 수 있어 기뻐요%UNICODE(0x2665)%」
PRINTFORMDW ……그러나 어디에서 소금을 조달한 것일까?
;A 외로부터 가져왔다는 일로
TCVAR:353 = 0
TIME += 5
EXP:요리경험 += 2
EXP:애정경험 += 3
BASE:MASTER:체력 = MIN(BASE:MASTER:체력 + 100 + (ABL:요리기능 * 80) , MAXBASE:MASTER:체력)
BASE:MASTER:기력 = MIN(BASE:MASTER:기력 + 100 + (ABL:요리기능 * 60) , MAXBASE:MASTER:기력)
TCVAR:PLAYER:312 = 1440 * DAY + TIME + 180
RETURN 1

;특별한 날의 인사
@M_KOJO_MESSAGE_EVENT_K44_EX_TALK
;신년, 아침이 되어서
IF FESTIVAL() == "설날" && DAY:0 != 1
	PRINTFORMW 「새해 복많이 받으세요, %CALLNAME:MASTER%%UNICODE(0x2665)%」
	IF TALENT:연인
		PRINTFORMW 「올해도 가득 함께 보냅시다 %UNICODE(0x2665)%」
	;ELSEIF TALENT:연모 || CFLAG:1014 == 1
	;	PRINTFORMW 「올해도 %마스터와% 여러가지 하고 싶어요%UNICODE(0x2665)%//」
	ELSE
		PRINTFORMW 「올해도 잘 부탁해 %UNICODE(0x2665)%」
	ENDIF
	TCVAR:358 = 2
;셋트바르엔틴
ELSEIF FESTIVAL() == "발렌타인 데이"
	IF TALENT:연인 || TALENT:연모
		PRINTFORMW 「오늘은 발렌타인 데이%UNICODE(0x2665)%」
	ELSEIF TALENT:연모 || CFLAG:1014 == 1 || TALENT:연인
		PRINTFORMW 「오늘은 발렌타인//」
	ELSEIF CFLAG:호감도 > 5000
		PRINTFORMW 「오늘은 발렌타인이야」
	ENDIF
	SELECTCASE CFLAG:호감도
	CASE 1000 TO 3000
		PRINTFORMW 「모처럼이니까 이것을 줄게요」
	CASE 3001 TO 5000
		PRINTFORMW 「%CALLNAME:MASTER%에는 여러 가지 시중이 되었고 이 초콜릿을 주는 %UNICODE(0x2665)%」
	CASE IS > 5000
		IF TALENT:연인 && TALENT:연모
			PRINTFORM 「그러니까, 정말 좋아하는 %마스터를% 위해서(때문에)
			IF EXP:애정경험 < 100
				PRINT 분발해
			ELSE
				PRINT 깜짝 놀랄만한 맛있는 초콜릿을 만들어 나무
			ENDIF
			PRINTFORMW 원%UNICODE(0x2665)% 옛 %UNICODE(0x2665)%%UNICODE(0x2665)%」
		ELSEIF TALENT:연모 || CFLAG:1014 == 1 || TALENT:연인
			PRINTFORMW 「그러니까, %마스터를% 위해서(때문에) 맛있는 초콜릿을 준비해 왔어요//　받아 %UNICODE(0x2665)%//」
		ELSE
			PRINTFORMW 「그러니까, 언제나 시중이 되어 있는 %마스터를% 위해서(때문에) 특별한 초콜릿을 준비해 왔어요%UNICODE(0x2665)%」
		ENDIF
	ENDSELECT
	TCVAR:오늘만났는지 = 1
	RESETCOLOR
	CALL SEASONAL(44)
	TCVAR:358 = 2
;크리스마스
ELSEIF FESTIVAL() == "크리스마스" && (TALENT:사모 || TALENT:연모)
	PRINTFORMW 「오늘은 크리스마스!　세상이 마법으로 가득 차는 하루야-%UNICODE(0x2665)%」
	PRINTFORMW 「…이라고 하는 것으로, 가득 즐깁시다 %UNICODE(0x2665)%」
	TCVAR:358 = 2
ELSE
	TCVAR:358 = 1
ENDIF
;이벤트가 있는 날은 기분이 안좋게 안 된다
SIF TCVAR:358 == 2 && TALENT:기분 == -1
TALENT:기분 = 0
;기념용
IF TALENT:연인 && TALENT:연모 && DAY:2 == CFLAG:1001 && DAY:3 == CFLAG:1002 && DAY:0 > 124 && DAY:0 < 249
	;반드시 매우 기분이 좋게 된다
	TALENT:기분 = 1
