;180, 로션
;-------------------------------------------------
@M_KOJO_MESSAGE_COM_K44_180
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K44_180_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K44_180_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 0
;-------------------------------------------------
IF LOCAL

	;CFLAG:1500 == 십진수
	;0. 이벤트가 오프야 선택지가 나와 초기치의 사정이야
	;1. 이벤트가 온이야 선택지가 나와
	;2. 이벤트가 오프야 선택지를 내지 않아
	;3. 이벤트가 온이야 선택지를 내지 않아
	;4. 이벤트가 오프야 선택지가 나와　　bit2가 진이야
	
	;1=온(bit0)　2=선택지를 내지 않아(bit1)
	;4=간단하게 조건을 첨가할 수 있어(bit2)
	
	;GETBIT (CFLAG:1500, 1) 여기의 1은 이진수의 2자리수째의 일 틀리기 쉬워
	;(CFLAG:1500, 1)의 수치진( 0이외 주인의 1) 위(0)를 봐
	
	;SETBIT STAIN:1, 7　이진수의 8자리수째를 진(주인의 1)으로 해
	;십진수로 말하면　+128되어 있지 않은 경우+128할게
	
	;CLEARBIT STAIN:1, 7　이진수의 8자리수째를 가짜(0)로 해
	;십진수로 말하면 이미+128되고 있는 경우에 한정해―128할게
	
	;INVATEBIT STAIN:1, 7　이진수의 8자리수째를 반전시켜
	;십진수로 말하면　+128되어 있지 않은 경우+128　이미+128되고 있는 경우- 128할게


	;시간정지 플래그가 한편 참조하는 일의 bit1(선택지를 내지 않아)가 가짜라면 선택지가 나온다
	SIF !FLAG:70 && !GETBIT(CFLAG:1500, 1)
		CALL M_KOJO_MESSAGE_COM_K44_180_선택지

	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:시간정지
			;시간 가동중에 시동하고 있는 경우의 모순 해소
			IF GETBIT(STAIN:1, 7)
				PRINTFORML 시간 가동중에 시동하고 있는 경우의 모순 해소처음
				RETURN 0
			;그 이외 시간정지 로션
			;시간정지 발라 칠분기는 없는 반드시 그것이 정의
			ELSE
				PRINTFORML 시간정지 로션처음
				RETURN 0
			ENDIF

		;이후!FLAG:시간정지　「발언」은 대체로 여기에서

		;분기 조건 초콜릿 바를 수 있는의 손 날름 날름 분기
		;다리의 더러움은 없기 때문에 손을 손발로서 대용
		;다른 형태로 초콜릿을 발랐을 때도 여기에 오므로 일단 이벤트 플래그도 조건에 추가
		ELSEIF GETBIT(STAIN:1, 7) && GETBIT(CFLAG:1500, 0)
			;연모
			IF TALENT:연모
				PRINTFORML 연모 날름 날름 처음
				;테테의 초콜릿 빨고 취했어
				;CLEARBIT STAIN:1, 7
				RETURN 0
			;그 이외 게임 밸런스 나름으로 연모 없음 연인 있어같은 케이스도 있을 수 있다
			ELSE
				PRINTFORML 날름 날름 처음
				RETURN 0
			ENDIF
		;분기 조건　CFLAG:1500의 bit0의 값이것진이라면 이벤트 발생 발라 칠분기
		ELSEIF GETBIT(CFLAG:1500, 0)
			;연모
			IF TALENT:연모
				PRINTFORML 연모 발라 칠 처음
				;테테 끈적끈적
				SETBIT STAIN:1, 7
				RETURN 0
			;그 이외 게임 밸런스 나름으로 연모 없음 연인 있어같은 케이스도 있을 수 있다
			ELSE
				PRINTFORML 발라 칠 처음
				;테테 끈적끈적
				SETBIT STAIN:1, 7
				RETURN 0
			ENDIF
		;그 이외 로션 분기
		ELSE
			;연모
			IF TALENT:연모
				PRINTFORML 연모 로션 분기처음
				RETURN 0
			;그 이외 게임 밸런스 나름으로 연모 없음 연인 있어같은 케이스도 있을 수 있다
			ELSE
				PRINTFORML 그 이외 로션 분기처음
				RETURN 0
			ENDIF
		ENDIF

	;이후!FIRSTTIME　시간정지중에 첫회를 맞이했을 경우와의 균형에 주의
	ELSE
		IF FLAG:시간정지
			;시간 가동중에 시동하고 있는 경우의 모순 해소
			IF GETBIT(STAIN:1, 7)
				PRINTFORML 시간 가동중에 시동하고 있는 경우의 모순 해소
				RETURN 0
			;그 이외 시간정지 로션
			ELSE
				PRINTFORML 시간정지 로션
				RETURN 0
			ENDIF

		;이후!FLAG:시간정지　「발언」은 대체로 여기에서

		;분기 조건 초콜릿 바를 수 있는의 손 날름 날름 분기
		;다리의 더러움은 없기 때문에 손을 손발로서 대용
		;다른 형태로 초콜릿을 발랐을 때도 여기에 오므로 일단 이벤트 플래그도 조건에 추가
		ELSEIF GETBIT(STAIN:1, 7) && GETBIT(CFLAG:1500, 0)
			;연모
			IF TALENT:연모
				PRINTFORML 연모 날름 날름
				;테테의 초콜릿 빨고 취했어
				;CLEARBIT STAIN:1, 7
				RETURN 0
			;그 이외 게임 밸런스 나름으로 연모 없음 연인 있어같은 케이스도 있을 수 있다
			ELSE
				PRINTFORML 날름 날름
				RETURN 0
			ENDIF
		;분기 조건　CFLAG:1500의 bit0의 값이것진이라면 이벤트 발생 발라 칠분기
		ELSEIF GETBIT(CFLAG:1500, 0)
			;연모
			IF TALENT:연모
				PRINTFORML 연모 발라 칠
				;테테 끈적끈적
				SETBIT STAIN:1, 7
				RETURN 0
			;그 이외 게임 밸런스 나름으로 연모 없음 연인 있어같은 케이스도 있을 수 있다
			ELSE
				PRINTFORML 발라 칠
				;테테 끈적끈적
				SETBIT STAIN:1, 7
				RETURN 0
			ENDIF
		;그 이외 로션 분기
		ELSE
			;여기에 연모 유무를 묻지 않는 묘사도 쓸 수 있어
			;PRINTFORML 
			;연모
			IF TALENT:연모
				PRINTFORML 연모 로션 분기
				RETURN 0
			;그 이외 게임 밸런스 나름으로 연모 없음 연인 있어같은 케이스도 있을 수 있다
			ELSE
				PRINTFORML 로션 분기
				RETURN 0
			ENDIF
			;여기에 연모 유무를 묻지 않는 묘사를 써도 2행상6행상의 RETURN 0으로 되돌리고 있기 때문에 표시되지 않아
			;PRINTFORML 
		ENDIF
	ENDIF
ENDIF
;리턴 하지 않으면 그대로다음까지 돌파해 가버려
;1(은)는 7행째의 CALL M_KOJO_MESSAGE_COM_K44_180_1에 보내지고 8행째의 RETURN RESULT에 인도해져
RETURN 1
