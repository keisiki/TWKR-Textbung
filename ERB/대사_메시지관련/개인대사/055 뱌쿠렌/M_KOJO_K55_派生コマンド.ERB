;==================================================
;6,挿入Ｇスポ責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K55_6
CALL M_KOJO_MESSAGE_SCOM_K55_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K55_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「후와%UNICODE(0x2764)%！응……입니까……지금의……」
		PRINTFORML  당신이 뱌쿠렌의 약한 곳을 찾아내면(자) 몇번이나 학같게 찌른다
		PRINTFORML 「거기%UNICODE(0x2764)%기분 좋다%UNICODE(0x2764)%안돼에%UNICODE(0x2764)%%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「지금%UNICODE(0x2764)%사정했다뿐인데%UNICODE(0x2764)%」
			PRINTFORML 「거기좋다%UNICODE(0x2764)%안돼%UNICODE(0x2764)%이지메등째%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「%UNICODE(0x2764)%거기 수기%UNICODE(0x2764)%있고 공주라고%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「응%UNICODE(0x2764)%거기는 약하기 때문에%UNICODE(0x2764)%안돼%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「기분 사촌뿐%UNICODE(0x2764)%이상해지면%UNICODE(0x2764)%%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
