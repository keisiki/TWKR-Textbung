RETURN 1

;==================================================
;7,挿入子宮口責め
;==================================================
@M_KOJO_MESSAGE_SCOM_K55_7
CALL M_KOJO_MESSAGE_SCOM_K55_7_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K55_7_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「%UNICODE(0x2764)%%UNICODE(0x2764)%안쪽까지 키스 되어 버린다%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「후~ 안돼%UNICODE(0x2764)%사정된지 얼마 안된 찔리면(자) 언덕 끈데 히%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「응,～～%UNICODE(0x2764)% , 둘 때까지, 오고 있는～～～%UNICODE(0x2764)%」
			PRINTFORMW %CALLNAME:MASTER%의 귀두와 뱌쿠렌의 자궁구가 서로 부딪치는 유육의 감각에, 뱌쿠렌은 완전히 만취하고 있다…….
			PRINTFORML 「아～～%UNICODE(0x2764)% , 후~%UNICODE(0x2764)% %CALLNAME:MASTER%,%CALLNAME:MASTER%～～%UNICODE(0x2764)%」
			PRINTFORMW 자궁구로부터 자궁 전체를 몇번이나 흔들어져 뱌쿠렌의 마음은 여자로서 최고의 즐거움에 채워졌다…….
		CASE 1
			PRINTFORML 「아%UNICODE(0x2764)% 아, , 아 아%UNICODE(0x2764)% 응응, 아아아 아～～～%UNICODE(0x2764)%」
			PRINTFORMW %CALLNAME:MASTER%의 강직이 뱌쿠렌의 부드러운 자궁구를 그니그니와 눌러 잡을 때에, 뱌쿠렌은 즐거움의 비명을 지르고 있다…….
			PRINTFORML 「쉿, 해 공%UNICODE(0x2764)% 아, 해 공이 , 히 좋다%UNICODE(0x2764)% 좋은 것～～%UNICODE(0x2764)%」
			PRINTFORMW 완전히 쾌락에 삼켜진 뱌쿠렌은, 심신 모두 흐트러지고 미쳤다…….
		ENDSELECT
	ELSEIF TALENT:78
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「응응%UNICODE(0x2764)% (은)는 아, 아, 아,%CALLNAME:MASTER%～～%UNICODE(0x2764)%」
			PRINTFORMW 꼼꼼한 개발의 덕분에 뱌쿠렌의 자궁구는 부드러워져, 때때로 수축하면서%CALLNAME:MASTER%의 귀두를 상냥하게 받아 들이고 있다.
			PRINTFORML 「아아, 아아～%UNICODE(0x2764)% 기분이야 지나, 후~%UNICODE(0x2764)% 갓난 아기, 그리고 위지우우～～%UNICODE(0x2764)%」
			PRINTFORMW 뱌쿠렌의 뇌리에 「임신」이라고 하는 단어가 반짝여, 뱌쿠렌의 흥분은 보다 한층 높아지고 있었다…….
		CASE 1
			PRINTFORML 「안쪽, 스쳐……굉장하다…！ 응, 아아───！」
			PRINTFORMW 뱌쿠렌의 자궁구를%CALLNAME:MASTER%의 귀두로 노크 하면(자), 뱌쿠렌은 전신을 가크가크와 진동시키면서 쾌락에 번농 되고 있다…….
			PRINTFORML 「……후~, 후~……머릿속, 새하얗게 되어버린다……%UNICODE(0x2764)%」
			PRINTFORMW %CALLNAME:MASTER%의 페니스가 가져오는 지고의 쾌락에 도취하면서, 뱌쿠렌은 빈의 육욕에 빠져 갔다…….
		CASE 2
			PRINTFORML 「후와 아, 아,%CALLNAME:MASTER%,%UNICODE(0x2764)% 좋아해, 너무 좋아……！%UNICODE(0x2764)%」
			PRINTFORMW 개발된 뱌쿠렌의 자궁구는 흡반과 같이%CALLNAME:MASTER%의 강직하게 달라붙어,%CALLNAME:MASTER%(을)를 자궁구의 더욱 오쿠로 부르려 하고 있다…….
			PRINTFORML 「질내, 너무 느껴버리는……응, 아 아！%UNICODE(0x2764)% 」
			PRINTFORMW %CALLNAME:MASTER%의 추송에 맞추어, 뱌쿠렌은 허리를 흔들면서 허덕여 흐트러졌다…….
		ENDSELECT
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「은…, 아, 아 아…！ 둘 때까지, 오고 있어……, 먹는다…, 아 아！！」
			PRINTFORMW 뱌쿠렌은%CALLNAME:MASTER%의 페니스에 의한 자궁구 고문을 받아 들이면서, 되어 거절해 상관하지 않고 허덕이고 있다…….
		CASE 1
			PRINTFORML 「이다, 안돼……！ , 깊으면 개, 너무 느껴…, 아, 나 , 아！！」
			PRINTFORMW 자궁구가 구이 구이라고 압박받을 때에, 뱌쿠렌은 숨을 황라고 교성을 계속 올렸다…….
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
