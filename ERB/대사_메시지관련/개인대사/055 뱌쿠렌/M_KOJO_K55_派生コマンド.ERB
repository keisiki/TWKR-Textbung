	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORML 「후아앗%UNICODE(0x2764)%！응읏…… 뭔가요…… 방금 그건……」
		PRINTFORML  당신은 뱌쿠렌의 약한 부분을 발견하자 그곳을 괴롭히듯이 쿡쿡거렸다
		PRINTFORML 「거깃%UNICODE(0x2764)% 기분 좋앗%UNICODE(0x2764)% 안대애앳 머리가앗 이상해져버려어엇%UNICODE(0x2764)%%UNICODE(0x2764)%」
		RETURN 1
	ENDIF
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「방금%UNICODE(0x2764)% 사정 했을 텐데엣%UNICODE(0x2764)%」
			PRINTFORML 「거깃 좋아앗%UNICODE(0x2764)% 안대애앳%UNICODE(0x2764)% 괴롭히면 안대애앳%UNICODE(0x2764)%%UNICODE(0x2764)%」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「햐웃%UNICODE(0x2764)% 거기 죠하아앗%UNICODE(0x2764)% 좀 더 해줘어엇%UNICODE(0x2764)%%UNICODE(0x2764)%」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「읏%UNICODE(0x2764)% 거긴 약하니까아%UNICODE(0x2764)% 안대애앳%UNICODE(0x2764)%」
		CASE 1
			PRINTFORML 「기분 좋아앗 아깃 만들고 싶어엇%UNICODE(0x2764)% 이상하게 되어버려어엇%UNICODE(0x2764)%」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
