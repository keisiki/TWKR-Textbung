CALL M_KOJO_MESSAGE_COM_K101_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K101_40_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「무엇…？이것을 아소코에…？」
		PRINTFORMW 「원！응…낯간지럽다」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 얼었을 때 중(안)에서 로터의 진동으로 토키코의 클리토리스가 흔들리고 있다.
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		PRINTFORMW 「…아♥그렇게 되어들♥…응응응♥」
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「응♥…♥…아 아♥」
		PRINTFORMW 「굉장하다…구리가 찌르르 하고 있다♥…」
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「응♥…아♥」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

