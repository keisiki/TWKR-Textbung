;140,イラマチオ
;==================================================
@M_KOJO_MESSAGE_COM_K101_140
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K101_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K101_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「응응응응！？응～～～～」
		PRINTFORMW 토키코는 괴로운 듯하게%CALLNAME:MASTER%의 음경을 질네라고 있다.
		RETURN 1
	ENDIF
	;基本セット
	;恋慕
	IF TALENT:연모
		PRINTFORMW 「응응～～～～～……응응응…후~…」
		PRINTFORMW 토키코는 우목으로%CALLNAME:MASTER%(을)를 보고 있다.
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「……응～～～～～」
		PRINTFORMW 토키코는 괴로운 듯하게 하면서도 어떻게든%CALLNAME:MASTER%(을)를 기분 좋게 시키려고 하고 있다.
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW 「응응응응응～～～～～」
		PRINTFORMW 「……심해요…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

