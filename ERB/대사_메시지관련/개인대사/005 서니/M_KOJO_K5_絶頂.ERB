@M_KOJO_MESSAGE_PALAMCNG_A_K5_22(ARG)
;-------------------------------------------------
;シックスナイン
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;同時射精
	IF NOWEX:사정 && NOWEX:MASTER:11
		PRINTFORMW
		RETURN 1
	;TARGETだけ射精
	ELSEIF NOWEX:사정
		;PLAYERについてる
		IF TALENT:PLAYER & 2
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	;PLAYERだけ射精
	ELSE
		;TARGETについてる
		IF TALENT:2 & 2
			PRINTFORMW
			RETURN 1
		;付いてない
		ELSE
			PRINTFORMW
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

