;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:C절정～4 CVABM絶頂
;NOWEX:이중절정 二重絶頂
;NOWEX:삼중절정 三重絶頂
;NOWEX:사중절정 四重絶頂
;NOWEX:오중절정 五重絶頂
;NOWEX:분유 噴乳
;NOWEX:사정 射精
;NOWEX:방뇨 放尿
;適当に組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K5
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;五重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:오중절정
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:사중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:사중절정
			;Ｃ＆Ｖ＆Ａ＆Ｂ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ＆Ｍ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:삼중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:삼중절정
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:이중절정
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		;[SKIPSTART]
		SELECTCASE NOWEX:이중절정
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:C절정
		;強絶頂Ｃ
		IF NOWEX:C절정 == 2
			PRINTFORMW 
		;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;強絶頂Ｖ
		IF NOWEX:V절정 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:A절정
		;強絶頂Ａ
		IF NOWEX:A절정 == 2
			PRINTFORMW 
		;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;強絶頂Ｂ
		IF NOWEX:B절정 == 2
			PRINTFORMW 
		;絶頂Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;強絶頂Ｍ
		IF NOWEX:M절정 == 2
			PRINTFORMW 
		;絶頂Ｍ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


