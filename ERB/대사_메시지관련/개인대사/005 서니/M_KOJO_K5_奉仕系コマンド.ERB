;==================================================
@M_KOJO_MESSAGE_COM_K5_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K5_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K5_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「이것을 가지고 움직이면 좋은거야？」
		PRINTFORML 「영차, 영차」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:연모
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「아, 자꾸자꾸 커지고 있다」
				PRINTFORMW 「 나의 손이 키모치 좋거든…」
			CASE 0
				PRINTFORML 「 나라고 양손으로 하지 않으면 안정되지 않을지도」
				PRINTFORMW 「응실마리, 응실마리」
			CASEELSE
				PRINTFORML 「이봐요 이봐요 , 빨리 사정해버릴 없음 있고」
				PRINTFORMW 서니는 일부러 손을 느슨한%CALLNAME:MASTER%의 페니스를 희롱한다.
		ENDSELECT
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「이군요 , 빨리 크게 해%UNICODE(0x2665) *1%」
				PRINTFORMW 
			CASE 0
				PRINTFORML 서니는%CALLNAME:MASTER%의 강직을 훑어 내면서 혀인 째를 하고 있다.
				PRINTFORMW 「빨리 이것을 나중에…%UNICODE(0x2665) *1%」
			CASEELSE
				PRINTFORMW 「이제 되지요, 빨리 빨리 해%UNICODE(0x2665) *1%」
		ENDSELECT
		RETURN 1
	;それ以外
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「미지근하고 이상한 느낌이군요」
			CASE 0
				PRINTFORMW 「원, 벌벌은 움직였다！」 
			CASEELSE
				PRINTFORMW 「응, 아직 나오지 않는거야？」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
RETURN 1

