;정사를 목격했다
;==================================================
@M_KOJO_EVENT_K92_6(ARG, ARG:1)
;말 주인은 TARGET
;ARG에 정사 상대
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;정사를 목격했다(친해져도:저)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 1
	$MOKUGEKI
		;태자님
		IF ARG == 56
			PRINTFORML 「에엑, , 태자님」
			PRINTFORMW 「대, 대단히 실례했습니다, 부디 천천히」
		;토지코
		ELSEIF ARG == 91
			PRINTFORML 「아니…… 토지코 하고, %CALLNAME:MASTER%」
			PRINTFORMW 「시, 실례했습니다―」
		ELSE	
			PRINTFORML 「뭐…… 자네들 설마……」
			PRINTFORMW 「시, 실례했습니다―」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;질 수 없어 참가(친해져도:고)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 2
			;태자님
			IF ARG == 56
				PRINTFORML 「에엑, , 태자님과 %CALLNAME:MASTER%」
				PRINTFORML 「음음……아무리 태자님이라고 하셔도……그래도……으으음……」
				PRINTFORML 「에에이! 나도 넣어 주세요! 거기―！」
				PRINTFORMW 후토는 %CALLNAME:MASTER% 미코의 사이에 끼어들어왔다……
			;토지코
			ELSEIF ARG == 91
				PRINTFORML 「아니…… 토지코에, %CALLNAME:MASTER%」
				PRINTFORML 「에、에에이！ 토지코라 해도 %CALLNAME:MASTER%는 건네주지 않아! 나도 넣어라 %CALLNAME:MASTER%! 거기―！」
				PRINTFORMW 후토는 %CALLNAME:MASTER% 토지코의 사이에 끼어들어왔다……
			ELSE
				PRINTFORML 「아니…… %CALLNAME:TARGET%에, %CALLNAME:MASTER%」
				PRINTFORML 「에、에에이！ 자네인 만큼 %CALLNAME:MASTER%는 건네주지 않아! 나도 넣어라 %CALLNAME:MASTER%! 거기―！」
				PRINTFORMW 후토는 %CALLNAME:MASTER%와 %CALLNAME:TARGET%의 사이에 끼어들어왔다……
			ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;어색한 듯이 떠난다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 3
		;목격과 같다
		GOTO MOKUGEKI
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

