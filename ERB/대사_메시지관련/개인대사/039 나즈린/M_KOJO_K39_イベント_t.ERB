		GOTO INPUT_LOOP_ONAMAE
	ELSEIF  FIRSTTIME("U39-1") && (!TALENT:연인 || !TALENT:연모) && STRCOUNT(LOCALS, "\(\(남편\|남편\)\(모양\|님\)\)\$")
