		ELSE
			IF RAND:3 == 0 
				PRINTFORMW 「응…고마워요. 많이 상쾌해졌어」
			ELSEIF RAND:2 == 0 
				PRINTFORMW 「…즐길 수 있었니?」
			ELSE
				PRINTFORMW 「…후우」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

