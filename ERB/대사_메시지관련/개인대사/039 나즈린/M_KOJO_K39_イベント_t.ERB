		CFLAG:1213 = 1
		;고백 성공 플래그를 세운다
		TFLAG:193 = 1
		;고백 말에 점프
		JUMP M_KOJO_MESSAGE_COM_K39_352_1
	ELSE
		GOTO INPUT_LOOP_OKOTOWARI
	ENDIF
	;고백을 거절한 플래그
	CFLAG:1202 = 3
	RETURN 1
ENDIF
RETURN 1

