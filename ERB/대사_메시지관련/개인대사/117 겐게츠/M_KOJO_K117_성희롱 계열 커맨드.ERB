	ELSEIF CFLAG:우후후 == 2
		SELECTCASE TCVAR:20
			;서로 껴안으면서
			CASE 32
				PRINTFORML 「하읏, 응, 후우……」
				PRINTFORMW 「온몸으로 %CALLNAME:MASTER%가 느껴져……」
			;수음
			CASE 50
				PRINTFORML 「응읏, 츄우……」
				PRINTFORMW 「후훗♡ 키스하면서 해 주는 쪽이 좋은 것 같네?」
			;딥키스
			CASE 60
				PRINTFORMW 「응, 응앗, 츄우…… 쪽♡ 쪼옥♡ 푸하아♡」
			;性交系
			CASE 85,87,88,89
				SELECTCASE RAND:3
					CASE 2	
						PRINTFORML 「응, 츄릅, 쿠츄우……」
						PRINTFORMW （위도 아래도…… 기분 좋아……）
					CASE 1
						PRINTFORML 「하읏, 읏, 쪼옥, 후아……」
						PRINTFORMW 겐게츠는 녹아내린 듯한 얼굴로 쾌감을 맛보고 있다
					CASE 0
						PRINTFORML 「으응♡ 앗♡ 푸하아……♡」
						PRINTFORMW 「%CALLNAME:MASTER%♡ 좋앗♡ 좋아앗♡」
				ENDSELECT
			;다이슈키홀드
			CASE 86
				PRINTFORML 「응, 응앗, 츄우…… 쪽♡ 쪼옥♡ 푸하아♡」
				;좋아한다고 쉽게 말해버리면 재미 없잖아?
				IF TALENT:연모 || TALENT:연인 || TALENT:애욕
					SELECTCASE RAND:2
						;약간 캐릭터 붕괴일지도
						CASE 1
							PRINTFORMW 「하앗, 좋아해 %CALLNAME:MASTER%......」
						CASE 0
							PRINTFORMW 「%CALLNAME:MASTER%, 갈 것 같으면…… 안쪽에……」
					ENDSELECT
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%...... 와줘......」
				ENDIF
		ENDSELECT
	;일상에서
	ELSE
		IF TALENT:연모 || TALENT:애욕
			PRINTFORML 「응…… 쪼옥, 후아......」
			SELECTCASE RAND:3
				CASE 2
					PRINTFORMW 「……%CALLNAME:MASTER%는 뭐랄까, 항상 갑자기 키스하려고 한단 말야」
				CASE 1
					PRINTFORMW 「%CALLNAME:MASTER%는, 키스하는 거 좋아하는구나?」
				CASE 0
					PRINTFORML 「%CALLNAME:MASTER%의 혀, 맛있네♡」
					PRINTFORMW 뭔가 무시무시한 말을 들어버린 것 같은데
			ENDSELECT
		ELSE
			PRINTFORML 「응, 츄우……」
			SELECTCASE RAND:2
				CASE 1
					PRINTFORMW 「……잠깐, 혀 집어넣지 말라고!」
				CASE 0
					PRINTFORMW 「제법 잘 하네?」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1
