	;반발각인 3>2레벨 다운 이벤트
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW （아직 완전히 용서해준 건 아니라고）
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"계열 이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;겐게츠의 방에서 조우
;==================================================
@M_KOJO_EVENT_K117_1(ARG,ARG:1)
;구상주-TARGET
;--------------------------------------------------
;기입 체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;MASTER가 들어왔을 때
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;이쪽 내용을 좀 더 작성하고 싶을지도
		IF (DAY:2 == 1 && DAY:3 == 1) || (DAY:2 == 2 && DAY:3 == 7) || (DAY:2 == 4 && DAY:3 == 14) 
			CALL SPECIALDAY_EVENT_K117
		ELSEIF TALENT:연모 || TALENT:사모
			PRINTFORML 「어머 %CALLNAME:MASTER%, 나를 만나러 온 걸까?」
		ELSE
			PRINTFORML 「오늘은 무슨 일이야?」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET이 들어왔을 때
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;여기도 조금 더
		IF (DAY:2 == 1 && DAY:3 == 1) || (DAY:2 == 2 && DAY:3 == 7) || (DAY:2 == 4 && DAY:3 == 14) 
			CALL SPECIALDAY_EVENT_K117
		ELSEIF TALENT:연모 || TALENT:사모
			PRINTFORML 「어머, %CALLNAME:MASTER%? 이렇게 얼굴 보니까 좋네」
		ELSE
			PRINTFORML 「안녕 %CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET이 욕실로 들어와서 함께 목욕
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「저기 %CALLNAME:MASTER%,」
		PRINTFORMW 「나도 같이 들어가도 될까……?」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET이 목욕 도중에 들이닥침
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		PRINTFORML 「…………」
		PRINTFORMW 「미, 미안!!」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET이 목욕하려고 들어와서 쫓겨남
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
	;구상 노트에서는 MASTER가 약점을 잡힌 상태라고 하네요
		PRINTFORML 「어머나,%CALLNAME:MASTER%?」
		PRINTFORMW 「나도 때마침 씻고 싶어졌는데」
		PRINTFORMW 「그러니까 좀 비켜 주겠니?」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출 도중에 조우
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF (DAY:2 == 1 && DAY:3 == 1) || (DAY:2 == 2 && DAY:3 == 7) || (DAY:2 == 4 && DAY:3 == 14) 
			CALL SPECIALDAY_EVENT_K117
		ELSE
			PRINTFORMW 「안녕 %CALLNAME:MASTER%」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;다른 캐릭터와 데이트 도중 조우
	;ARG:1에 데이트 상대
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:연모
			PRINTFORML 「부러워……」
			PRINTFORML 뭔가 겐게츠가 중얼거리는 소리가 들린 것 같은데
			PRINTFORMW 「아무 얘기 안 했어」
		ELSE
			PRINTFORML 「어머 %CALLNAME:MASTER%?」
			PRINTFORMW 「……꽤나 즐거워 보이네」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP에서 첫 인사 시
	;주석-이후 통상적인 조우 시의 구상 출력、ARG==1 OR ARG==2에 접속됩니다
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출 도중에 첫 인사
	;ARG:1은 데이트 상대、ARG:1이 0인 경우 데이트 도중 아님
	;주석-이 이후 일반 외출 시의 구상 출력、ARG==6 OR ARG==7에 접속됩니다
	;기입 체크（=0, 비표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		;ARG:1과 데이트 중
		IF ARG:1
		;혼자서 외출
		ELSE
		ENDIF
		PRINTFORML 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

