@M_KOJO_MESSAGE_COM_K63_302
;성공실패 판정
;TFLAG:192 (-2=커맨드 종료, -1＝강제실패, 0=COM의존, 1=강제성공or대성공)
IF TCVAR:발정
	IF TALENT:연모
		TFLAG:192 = 1
	ELSE
		TFLAG:192 = -1
	ENDIF
ELSEIF (TALENT:연모 || TALENT:사모) && DAY > K63_EYEBROW && AT_HOME(MASTER)
	TFLAG:192 = 1
ELSE
	CALL TRAIN_MESSAGE
ENDIF
CALL M_KOJO_MESSAGE_COM_K63_302_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_302_1
;-------------------------------------------------
;기입체크 (=0, 비표시、1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(302)
		PRINTFORMW 「자, 잠깐! 거리낌 없이 건드리지 마!」
		PRINTFORMW 「아, 미안… 그냥 놀랐을 뿐이야……」
		SIF TALENT:사모
			PRINTFORMW 「뭐어… %CALLNAME:MASTER%에게라면 조금 정도는……」
		RETURN 1
	ENDIF
	
	;연모（お風呂）
	IF TALENT:연모 && BATHROOM()
		PRINTFORMW 「등 밀어주는 거야?」
		PRINTFORMW 「고마워……」
		PRINTFORML 
		PRINTFORMW 「%TEXTR("그렇게… 물끄러미 보지 마……/아핫♪ 큭큭, 간지럽다구! 흐흑/좋아, 다음은 내가……")%」
		RETURN 1
	ENDIF
	IF TCVAR:발정
		FONTBOLD
		CALL HPH_PRINT,@"「햐앙아앗HPH」","W"
		FONTREGULAR
		IF TALENT:연모
			CFLAG:욕구불만도 += 100
			PALAM:TARGET:윤활 += 500
			BASE:TARGET:무드 += 500
			PRINTFORMW 「미, 미안해…… 지금 만지는 건 곤란하달까…… 그, 싫다는 건 아니고…」
			PRINTFORMW （으으, %CALLNAME:MASTER%에게 만져지면 몸이 멋대로 교미 준비해버려……）
			IF TALENT:자기애 >= 0
				CFLAG:TARGET:동행 = 60
				CFLAG:MASTER:동행 = 60
				PRINTFORMW 「봐…… 알겠어?　저기, 저쪽으로 갈래?」
				PRINTFORMW %조사처리(CALLNAME:TARGET, "는")% %CALLNAME:MASTER%의 소매를 꽉 쥐었다
			ENDIF
		ELSEIF TALENT:사모
			PRINTFORMW 「미, 미안해…… 지금은 가만히 놔뒀으면 하는데……」
		ELSE
			PRINTFORMW 「좀 만지지 마! 떨어져!」
			CFLAG:빡침 = 1
		ENDIF
		CALL COLORMESSAGE(@"욕구불만이 높아졌다",C_YELLOW,1)
		CFLAG:욕구불만도 += 100
		IF PALAM:TARGET:윤활 < 3000
			CALL COLORMESSAGE(@"윤활이 올랐다",C_YELLOW,1)
			PALAM:TARGET:윤활 += 500
		ENDIF
		RETURN 1
	ENDIF
	IF (TALENT:연모 || TALENT:사모) && DAY > K63_EYEBROW && AT_HOME(MASTER)
		CALL K63_眉カット
		RETURN 1
	ENDIF
	CALL KAGEROU_SKINSHIP
	;연모
	IF TALENT:연모
		SELECTCASE RESULT
		;手を握る
		CASE 0
			IF !RAND:3
				PRINTFORML 「역시 %CALLNAME:MASTER%의 손은 내 손이랑 다르네」
				PRINTFORML 「도구를 만들고 다루는, 인간의 손이야」
			ELSEIF RAND:2
				PRINTFORMW 「아… 잠ㄲ……」
				PRINTFORMW 「내 손, 손톱이 서 있으니까 위험해, 조심하도록 해…」
			ELSE
				PRINTFORMW 「후후, 희미하지만 %CALLNAME:MASTER%의 따스함이 전해져와……」
				PRINTFORMW 「이건 손깍지라고 했었던가……」
				PRINTFORMW 「조금 창피하니까 이 정도로 하자……」
			ENDIF
		;頬を撫でる
		CASE 1
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「앗… 왜 그래 %CALLNAME:MASTER%……」
					PRINTFORML 「후후, 나도 해줄게……」
				CASE 1
					PRINTFORML 「와앗! 갑자기 하면 놀라잖아」
					PRINTFORML 「ㄲ, 꼬집지 마아」
				CASE 2
					PRINTFORML 「응……」
					PRINTFORML 눈을 가늘게 뜨고, 쓰다듬고 있는 쪽으로 머리를 기울여 왔다
			ENDSELECT
		;耳に触る
		CASE 2
			IF RAND:2
				PRINTFORMW 「이쪽도 진짜 귀야」
				PRINTFORMW 「요수가 가진 불가사의의 원리야♪」
			ELSE
				PRINTFORMW 「이쪽 귀는 움직일 수 있어, 이거 봐」
				PRINTFORMW 「숨길 때는 이렇게 눕혀서……」
			ENDIF
		;顎を擽る
		CASE 3
			IF RAND:2
				PRINTFORML 「아, 그거 기분 좋다……」
				PRINTFORML 「그, 그치만 간지러워」
			ELSE
				PRINTFORML 「후아아… 간지러워……」
				PRINTFORML 「아아… 거기다 강아지 취급하는 것 같아서 부끄러워……」
			ENDIF
		;髪に触る
		CASE 4
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「제대로 손질은 하고 있긴 한데…」
					PRINTFORML 「그렇게 뚫어져라 쳐다보면 왠지 긴장되네……」
				CASE 1
					PRINTFORML 「그렇게 냄새 맡지 마아～…」
					PRINTFORML 「부끄러워……」
				CASE 2
					PRINTFORML 「감촉 기분 좋아？」
					PRINTFORML 「후후, 이것에 대해서는 조금 자신 있다구」
			ENDSELECT
			PRINTFORML 반들반들한 검은 머리를 만끽했다…
		;肩を揉む
		CASE 5
			PRINTFORMW 「아～ 기분 좋아～♪」
			IF RAND:2
				PRINTFORMW 「꽤 능숙하네」
				PRINTFORMW 「혹시 레이무에게 훈련이라도 받은 거야?」
			ELSE
				PRINTFORMW 「이번엔 내가 해줄게」
				PRINTL 
				PRINTFORMW 「손님 꽤나 뭉치셨네요～」
				PRINTFORMW 「어때? 잘하고 있어?」
			ENDIF
		;背を撫でる
		CASE 6
			IF RAND:2
				PRINTFORMW 「후아～……!」
				PRINTFORMW 「오싹거리면서도 왠지 차분해지네」
			ELSE
				PRINTFORMW 「아아… 왠지 기분 좋아……」
				PRINTFORMW 「너무 편해… 잠들 것 같아……」
			ENDIF
		;脇腹をつつく
		CASE 7
			IF RAND:2
				PRINTFORMW 「아하하! 간지럽잖아!」
				PRINTFORMW 「%CALLNAME:MASTER%에게도 해줄게」
				PRINTFORMW 「어때! 이건 어때!」
			ELSE
				PRINTFORMW 「잠깐 간지러어! 아하하!」
				PRINTFORMW 「꺄앗! 이제 그만해앳!」
				PRINTFORMW 「간지럽다니까아～!」
			ENDIF
		;鼻を擽る
		CASE 8
			IF RAND:2
				PRINTFORMW 「후아… 간질간질거려～……」
				PRINTFORMW 「엣취!」
			ELSE
				PRINTFORMW 「후후, 뭘 하고 싶은 걸까」
				PRINTFORMW 「%CALLNAME:MASTER%에게도 해줄게」
				PRINTFORMW 「어때? 기분이 이상하지?」
			ENDIF
		;ぺたぺた
		CASE 9
			PRINTFORMW 「아까부터 뭘 톡톡 건드리는 거야」
			IF RAND:2
				PRINTFORMW 「별로 싫지 않으니까 얼마든지 만져도 되지만…」
				PRINTFORMW 「왠지 수상해… %CALLNAME:MASTER%」
			ELSE
				PRINTFORMW 「그렇게 만지고 싶으면… 그……」
				PRINTFORMW 「그늘 쪽으로 가 준다면……」
			ENDIF
		CASE 10
			IF FIRSTTIME("肉球")
				PRINTFORMW 「육구를 만지고 싶다고？ 뭐, 변신하면 있지만……」
				PRINTFORML 카게로는 오른손을 왼쪽 소매에 넣고, 꼼지락거린다
				PRINTFORML 그리고 내민 오른손은 늑대의 그것이다
				PRINTFORMW 「자, 여기」
				PRINTFORML 부드럽……지는 않다, 밑창 같은 느낌이다
				PRINTFORMW 「히메들도 멋대로 실망했었지ー……」
			ELSE
				PRINTFORML 「%TEXTR("뭐가 재미있는건지……/또？ 어쩔 수 없네……")%」
				PRINTFORML 변신한 손을 만지게 해줬다
				IF !RAND:3
					PRINTFORML 「한쪽 손만 짐승이면 장갑의 동화책 같아？ 그런 이야기가 있어？」
					PRINTFORML 「아, 여우인가……늑대가 아니구나……」
					;역주, 手ぶくろを買いに라는 동화책
				ELSE
					PRINTFORML %TEXTR("평소의 여성스러운 손과는 전혀 다르지만, 이것도 카게로다/지면에 발톱을 세우고 박차기 위한, 강력한 짐승의 앞다리다")%
				ENDIF
			ENDIF
		;人狼
		CASE 100
			CALL ERATOHO_JINROU_GAME
		ENDSELECT
	;연모じゃない
	ELSE
		SELECTCASE RESULT
		;手を握る
		CASE 0
			PRINTFORMW 「어머, 손이 예쁘네」
			PRINTFORMW 「매일 손질하는거야?」
		;頬を撫でる
		CASE 1
			PRINTFORMW 「뭐야뭐야뭐야ー!?」
			PRINTFORMW 「정말, 언제나 갑작스러워서 놀란다구!」
		;耳に触る
		CASE 2
			PRINTFORMW 「이쪽도 진짜 귀야」
			PRINTFORMW 「어떤 원리냐구? 킥킥킥, 비밀♪」
		;顎を擽る
		CASE 3
			PRINTFORMW 「……이거, 개 취급 하는거야?」
			PRINTFORMW 「화낼거야?」
		;髪に触る
		CASE 4
			PRINTFORMW 「내 머리가 어때서……」
			PRINTFORMW 「머리는 여자의 목숨이니까」
			PRINTFORMW 「뭐야, 요괴가 말하면 이상한거야?」
		;肩を揉む
		CASE 5
			PRINTFORMW 「아ー 이거 좋아. 기분 좋아」
			PRINTFORMW 「더 해줘도 돼～」
		;背を撫でる
		CASE 6
			PRINTFORMW 「꺄앗! 뭐야 정말」
			PRINTFORMW 「오싹오싹～～해졌어」
		;脇腹をつつく
		CASE 7
			PRINTFORMW 「와아앗! 뭐하는 거야!」
			PRINTFORMW 「이게! 이게엣!」
		;鼻を擽る
		CASE 8
			PRINTFORMW 「후아…… 무슨…… 엣취!」
			PRINTFORMW 「정말ー! 뭐냐구ー!」
		;ぺたぺた
		CASE 9
			PRINTFORMW 「아까부터 뭘 톡톡 건드리는 거야」
			PRINTFORMW 「신사에 산다면 요괴도 드물지 않을 텐데」
			PRINTFORMW 「그런 짓 하다가 잡아먹혀도 아무 말 못 할걸?」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
