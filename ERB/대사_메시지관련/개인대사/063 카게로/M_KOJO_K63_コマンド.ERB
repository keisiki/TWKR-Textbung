@M_KOJO_MESSAGE_COM_K63_301_1
;-------------------------------------------------
;기입체크（=0, 비표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 
	;初回
	IF FIRSTTIME(301)
		PRINTFORMW 「어라, 차를 끓어줬구나. 고마워」
		SELECTCASE DAY:2
		;夏
		CASE 2
			PRINTFORMW 「무더운 날에 차가운 차는 몸에 스며드는 것 같아～」
			PRINTFORMW 「뜨거운 차도 마실 수 있냐고?」
		;夏じゃない
		CASEELSE
			PRINTFORMW 「뜨거운데 괜찮냐고?」
		ENDSELECT
			PRINTFORMW 「확실히 야생 늑대는 모두 고양이 혀야」
			PRINTFORMW 「그렇다고 해서 요괴인 늑대인간이랑 같다고 생각하면 큰 착각이야」
			PRINTFORMW 「것보다 왜 고양이 혀라고 하는걸까?… 후릅……」

	;연모
	ELSEIF TALENT:연모
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「역시 %CALLNAME:MASTER%, 눈치가 빠르네♪」
			PRINTFORMW 「으음～ 맛있어～♪」
		CASE 1
			PRINTFORMW 「어머 고마워♪ 잘 마실게…」
			PRINTL 후르릅…
			PRINTFORMW 「사람은 불을 쓸 줄 아니까 고양이 혀가 아니잖아?」
			PRINTFORMW 「그래서 그런지 자연에서 보면 인간의 혀가 이상한거야」
			PRINTFORMW 「즉 인간 혀란 뜻이야, 나도 %CALLNAME:MASTER%도」
			PRINTFORMW 「어머? 그치만 난 요괴니까 요괴 혀인가」
		CASE 2
			PRINTFORMW 「아～! 마침 목이 마른 참이었어! 고마워 %CALLNAME:MASTER%♪」
			PRINTFORMW 「차는 취하지 않으니까, 술보다 부담 없이 마실 수 있어서 좋아」
			PRINTFORMW 「그치만 둘 다 과음은 금물이야」
			PRINTFORMW 「차를 너무 많이 마시면 잠이 안오니까!」
		CASE 3
			PRINTFORMW 「전부터 생각하고 있었던 거지만……」
			PRINTFORMW 「이 차를 언제 우려냈는지 모르겠어」
			PRINTFORMW 「%조사처리(CALLNAME:MASTER, "는")% 이따금씩 이상한 짓을 하지」
		CASE 4
			PRINTFORMW 「%조사처리(CALLNAME:MASTER, "가")% 우려준 차, 기뻐♪」
			PRINTFORMW 「항상 고마워♪」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「어라 말차네, 보기 드문 녀석인걸. 고맙게 받을게」
				PRINTFORMW 「으읏, 조금 쓴 것 같네……」
			CASE 2
				PRINTFORMW 「여름에는 역시 보리차지」
				PRINTFORMW 「적당한 수분 보충을 빠뜨리면 안 돼!」
			CASE 3
				PRINTFORMW 「어머 오늘은 홍차네. 고맙게 받을게」
				PRINTFORMW 「으ー음, 맛도 좋지만 향기도 훌륭한걸」
			CASE 4
				PRINTFORMW 「와아～ 고마워!」
				PRINTFORMW 「따뜻해서 맛있어～!」
			ENDSELECT
		ENDSELECT

	;연모じゃない
	ELSE
		SELECTCASE RAND:6
		CASE 0
			PRINTFORMW 「어라, 눈치가 빠르네. 잘 마실게」
			PRINTFORMW 「음～ 맛있어～」
		CASE 1
			PRINTFORMW 「어머, 고마워. 잘 먹겠습니다」
			PRINTL 
			PRINTFORMW 「인간 이외의 동물은 모두 고양이 혀야」
			PRINTFORMW 「둔갑한 요괴는 어떨지 모르겠지만」
		CASE 2
			PRINTFORMW 「아ー! 마침 목이 마른 참이었어. 고마워」
			PRINTFORMW 「차와 술이 맛있다는 것은 어느 종족이나 공통 인식인가봐」
		CASE 3
			PRINTFORMW 「전부터 생각하고 있었던 건데……」
			PRINTFORMW 「이 차는 어느새 우려낸거야?」
			PRINTFORMW 「알고보니 요술?!」
		CASE 4
			PRINTFORMW 「항상 신세지네. 감사하고 있어」
			PRINTFORMW 「봐, 당신의 친절도 함께 몸 속으로 스며드는 것 같아～」
		CASE 5
			SELECTCASE DAY:2
			CASE 1
				PRINTFORMW 「어라 말차네, 보기 드문 녀석인걸. 고맙게 받을게」
				PRINTFORMW 「으읏, 조금 쓴 것 같네……」
			CASE 2
				PRINTFORMW 「여름에는 역시 보리차지」
				PRINTFORMW 「적당한 수분 보충을 빠뜨리면 안 돼!」
			CASE 3
				PRINTFORMW 「어머 오늘은 홍차네. 고맙게 받을게」
				PRINTFORMW 「으ー음, 맛도 좋지만 향기도 훌륭한걸」
			CASE 4
				PRINTFORMW 「와아～ 고마워!」
				PRINTFORMW 「따뜻해서 맛있어～!」
			ENDSELECT
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
