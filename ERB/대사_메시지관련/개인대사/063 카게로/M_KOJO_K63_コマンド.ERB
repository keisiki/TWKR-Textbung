;クラピ口上を参考に
@M_KOJO_MESSAGE_COM_K63_621_1
#DIM CHARA
;-------------------------------------------------
;기입체크 (=0, 비표시、1, 표시)
LOCAL = 1
;-------------------------------------------------
;地の文をカットしてるのでMESSAGE処理を内蔵する
IF LOCAL
	;CALL M_KOJO_COLOR_K63
	SELECTCASE TFLAG:193
	;釣れない
	CASE 0
		PRINTFORMDW 아무것도 낚지 못했다……
		PRINTFORMW 「%TEXTR("낚이지 않네/후와아아～～～")%……」
		IF !RAND:4 && !GROUPMATCH(TIME:2, 1, 4, 5, 6)
			PRINTFORML 「시간이 나빴을지도 몰라」
			PRINTFORML 「새벽이나 오후가 좋을 것 같아」
		ELSEIF CFLAG:MASTER:오늘의운세 / 10 - 50 + TFLAG:행운보정 * 10 < 0
			PRINTFORML 「오늘은 운이 좀 아닌 같아……」
		ENDIF
		RETURN 1
	;木片
	CASE 610
		IF TFLAG:194 > 1000
			PRINTL 
			PRINTFORMDL %조사처리(CALLNAME:TARGET, "가")% 나무조각을 낚았다…
			PRINTW
			PRINTFORMW 「……필요해? 이거」
		ELSE
			PRINTFORMDL 나무조각을 낚았다……
			PRINTW 
		ENDIF
		RETURN 1
	;紙切れ
	CASE 131
		IF TFLAG:194 > 1000
			PRINTFORML 「뭐지, 이건」
		ELSE
			PRINTFORMDL 종잇조각을 낚았다
		ENDIF
		PRINTDW 
		PRINTDL ┌─────────────┐
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　모리야 신사의 부적　　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　다섯 장을 모으면　　　｜
		PRINTDL ｜ 제비를 뽑을 수 있습니다　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　효험은 보증 못합니다  　｜
		PRINTDL ｜　　　　　　　　　　　　　｜
		PRINTDL ｜　　　　　　모리야 신사　 ｜
		PRINTDL └─────────────┘
		PRINTW 
		PRINTFORMW 「산에 있는 신사의 물건인가?」
		RETURN 1
	CASE 1 TO 20, 22 TO 60
		LOCALS = %FISH_NAME(TFLAG:193)%
	CASE 21
		LOCALS = 로터
	CASEELSE
		LOCALS = %ITEMNAME:(TFLAG:193)%
	ENDSELECT
	;카게로가 釣った
	IF TFLAG:194 > 1000
		PRINTFORMW 「!」
		PRINTFORMW 「왔어!」
		SELECTCASE LOCALS
			CASE "가아", "가물치", "실러캔스", "철갑상어", "일본연어", "피라루쿠", "왕연어"
				PRINTFORMW 「힘이 엄청나! 히메를 낚았을 때 이후로 처음이야!」
				PRINTFORMW 「%CALLNAME:MASTER%도 도와줘!」
		ENDSELECT
		PRINTL 
		PRINTFORMDL %조사처리(CALLNAME:TARGET, "가")% %조사처리(LOCALS, "를")% 낚았다!
		RESETCOLOR
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
	;MASTERが釣った
	ELSE
		PRINTFORMW 「%CALLNAME:MASTER%, 당기고 있어!」
		RESETCOLOR
		PRINTFORMDL %조사처리(LOCALS,"가")% 잡혔다！
		;ローター以降を弾いてサイズと漁獲数の処理
		IF INRANGE(TFLAG:193, 1, 60) && TFLAG:193 != 21
			;サイズの出力
			LOCAL:1 = FISHING_SIZE(TFLAG:193)
			PRINTFORML 　사이즈：{LOCAL:1}cm
			;ここまでの最高サイズと比較して大きければ記録
			IF LOCAL:1 > FISHSIZE_RECORD:(TFLAG:193)
				SETCOLOR C_YELLOW
				SIF FISH_EXP:(TFLAG:193)
					PRINTFORML 　지금까지 낚은 %LOCALS% 중에서 제일 큰 사이즈야!
				RESETCOLOR
				FISHSIZE_RECORD:(TFLAG:193) = LOCAL:1
			ENDIF
			;漁獲数の加算
			FISH_EXP:(TFLAG:193) ++
		ENDIF
		;説明を流す
		SELECTCASE TFLAG:193
		CASE 1 TO 20, 22 TO 60
			DRAWLINE
			PRINTFORML %LOCALS%
			CALL FISHING_EXPLANATION(TFLAG:193)
		CASE IS > 61
			DRAWLINE
			PRINTFORML %LOCALS%
			PRINTFORML 　　%ITEM_EXPLANATION(TFLAG:193)%
		ENDSELECT
		PRINTW 
		CALL M_KOJO_COLOR_K63
	ENDIF
	SELECTCASE TFLAG:193
		CASE 11 TO 20, 24 TO 60
			IF TIME_PROGRESS(TCVAR:MASTER:공복시각) && TIME_PROGRESS(TCVAR:TARGET:공복시각)
				PRINTFORMW 「저기, %CALLNAME:MASTER%, 배고프지 않아?」
				CALL ASK_YN("여기서 먹는다","먹지 않는다")
				IF !RESULT
					CALL PRINT_FACE, [[카게로]], "笑顔", "服", "", ""
					PRINTFORMW 「만세!」
					PRINTFORMW 낚은 %조사처리(LOCALS, "를")% 구워 먹었다
					CALL LOST_ITEM("맛있는물고기", 1, 1)
					TCVAR:[[카게로]]:공복시각 = 1440 * DAY + TIME + 200
					CALL RECOVER_PERMIL([[카게로]], 400, "체력", 0, "식사에 의한 회복")
					CALL RECOVER_PERMIL([[카게로]], 400, "기력", 0, "식사에 의한 회복")
					TCVAR:MASTER:공복시각 = 1440 * DAY + TIME + 200
					CALL RECOVER_PERMIL(MASTER, 400, "체력", 0, "식사에 의한 회복")
					CALL RECOVER_PERMIL(MASTER, 400, "기력", 0, "식사에 의한 회복")
					CALL RECOVER_PERMIL(MASTER, 200, "정력", 0, "식사에 의한 회복")
					TIME += 20
					CFLAG:1004 += 3
				ENDIF
			ENDIF
	ENDSELECT
ENDIF
RETURN 1

