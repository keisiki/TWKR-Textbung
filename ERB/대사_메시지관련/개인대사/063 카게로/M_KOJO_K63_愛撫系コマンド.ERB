		IF !RAND:2
			CALL HPH_PRINT,@"「좀 더……HPH」","L"
		ELSE
			CALL HPH_PRINT,@"「정말, 받아랏HPH」","L"
			IF !RAND:2
				;가슴애무or乳首吸い
				TCVAR:차회카운터 = 58 + RAND:2
			ELSEIF !RAND:2
				;딥키스
				TCVAR:차회카운터 = 60
			ELSEIF TEQUIP:50 == PLAYER
				TCVAR:차회카운터 = 88
			ELSE
				TCVAR:차회카운터 = 50
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

