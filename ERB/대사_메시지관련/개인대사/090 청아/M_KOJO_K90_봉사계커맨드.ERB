@M_KOJO_MESSAGE_COM_K90_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「하아♥ 자지 탁탁 해드릴 테니까 잔뜩 퓻퓻 해주세요~♥」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「어머? 벌써 나와버릴 것 같네♥」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「후훗♥ 쿠퍼액이 흘러나와.. 참지 않아도 괜찮아♥」
		ELSE
			PRINTFORMW 「자지즙 잔뜩 싸 줘♥」
		ENDIF
	;연모
	ELSEIF TALENT:연모
		IF RAND:3 == 0
			PRINTFORMW 「어서 정액 싸 줘♥」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「응♥ 대딸 해주니까 좋아?」
		ELSE
			PRINTFORMW 「앙♥ 난폭해서 잡기 힘든 자지네요♥」
		ENDIF
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
		IF RAND:3 == 0
			PRINTFORMW 「미끌미끌♥ 잔뜩 싸 주세요♥」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「읏♥ 씩씩하네요♥」
		ELSE
			PRINTFORMW 「자지즙.. 어서 주세요♥」
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「잔뜩 싸 주세요♥」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「움찔움찔 생기가 넘치네요♥ 어서 뷰룻뷰룻 해주세요♥」
		ELSE
			PRINTFORMW 「그럼.. 상냥하게 탁탁 해드릴게요♥」
		ENDIF
	ENDIF
ENDIF
RETURN 1

