			ELSEIF RAND:3 == 0
				PRINTFORMW 「아…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「응…거기는…」
				RETURN 1
			ELSE
				PRINTFORMW 「…응…」
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	IF NOWEX:V절정
		;강절정 V
		IF NOWEX:V절정 >= 2
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
