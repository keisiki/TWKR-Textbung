				RETURN 1
			ELSE
				PRINTFORMW 「…응 %UNICODE(0x2661) *1%」
				RETURN 1
			ENDIF
		;절정 A
		ELSE
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
				PRINTFORMW 「아앙…엉덩이…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「으흥 …나쁘지는 않은데」
				RETURN 1
			ELSE
				PRINTFORMW 「……」
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	IF NOWEX:B절정
		;강절정 B
		IF NOWEX:B절정 >= 2
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
				PRINTFORMW 「히…%UNICODE(0x2661) *1%」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「아앙…%UNICODE(0x2661) *1%」
				RETURN 1
			ELSE
				PRINTFORMW 「…아…%UNICODE(0x2661) *1%」
				RETURN 1
			ENDIF
		;절정 B
		ELSE
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
				PRINTFORMW 「응…거기…!」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「아…응…!」
				RETURN 1
			ELSE
				PRINTFORMW 「응…가슴…」
				RETURN 1
			ENDIF
		ENDIF
	ENDIF
	IF NOWEX:M절정
		;강절정 M
		IF NOWEX:M절정 >= 2
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
				PRINTFORMW 「응…!아…」
				PRINTFORMW 세이자는 구강의 쾌감을 만족해 받아들이고 있다.
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「으응…」
				PRINTFORMW 세이자는 구강의 쾌감을 만족해 받아들이고 있다.
				RETURN 1
			ELSE
				PRINTFORMW 「…으흥…응!」
				PRINTFORMW 세이자는 구강의 쾌감을 만족해 받아들이고 있다.
				RETURN 1
			ENDIF
		;절정 M
		ELSE
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
				PRINTFORMW 「아……」
