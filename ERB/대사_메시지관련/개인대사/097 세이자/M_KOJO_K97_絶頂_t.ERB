		;[SKIPSTART]
		SELECTCASE NOWEX:이중절정
			;C＆V절정
			CASE 1
				PRINTFORMW 
				RETURN 1
			;C＆A절정
			CASE 2
				PRINTFORMW 
				RETURN 1
			;V＆A절정
			CASE 3
				PRINTFORMW 
				RETURN 1
			;C＆B절정
			CASE 4
				PRINTFORMW 
				RETURN 1
			;V＆B절정
			CASE 5
				PRINTFORMW 
				RETURN 1
			;A＆B절정
			CASE 6
				PRINTFORMW 
				RETURN 1
			;A＆M절정
			CASE 7
				PRINTFORMW 
				RETURN 1
			;V＆M절정
			CASE 8
				PRINTFORMW 
				RETURN 1
			;C＆M절정
			CASE 9
				PRINTFORMW 
				RETURN 1
			;B＆M절정
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		;[SKIPEND]
	ENDIF
	IF NOWEX:C절정
		;강절정 C
		IF NOWEX:C절정 >= 2
			;시간중
			IF FLAG:70
				PRINTFORMW 얼어붙은 시간 속에서 세이자는 절정 했다.
			ELSEIF RAND:3 == 0
				PRINTFORMW 「아%UNICODE(0x2661) *1%」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「아앗 %UNICODE(0x2661) *1%」
				RETURN 1
			ELSE
				PRINTFORMW 「구%UNICODE(0x2661) *1%」
				RETURN 1
			ENDIF
