		PRINTFORMW 「응…기분이 좋다…」
		RETURN 1
	ENDIF
	;기본 세트
	;개시
	IF TEQUIP:샤워
		;연모
		IF TALENT:연모
			IF RAND:3 == 0
				PRINTFORMW 「…후우…」
				PRINTFORMW 「…빤히 보지 않지는…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「…아앙…」
				PRINTFORMW 「…어떻게 했어? 함께 받을까?」
				RETURN 1
			ELSE
				PRINTFORMW 「…아…」
				PRINTFORMW 「…이런 것, 역시 좋은데…」
				RETURN 1
			ENDIF
		;굴복 3
		ELSEIF MARK:괘씸각인 == 3
			IF RAND:3 == 0
				PRINTFORMW 「…기분이 좋은데…」
				PRINTFORMW 「…완전히…」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「아…」
				PRINTFORMW 「…좋다, 이것…」
				RETURN 1
			ELSE
				PRINTFORMW 「응…후우…」
				PRINTFORMW 「…기분이 좋다…」
				RETURN 1
			ENDIF
		;그 이외
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「…후우…」
				PRINTFORMW 「…이것으로 네가 없으면」
				RETURN 1
			ELSEIF RAND:2 == 0
				PRINTFORMW 「아…후우…」
				PRINTFORMW 「…응…」
				RETURN 1
			ELSE
				PRINTFORMW 「응…」
				PRINTFORMW 「…머리 정도 씻어 줄까?」
				RETURN 1
			ENDIF
		ENDIF
	;종료
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「…응」
			PRINTFORMW 「…마지막…」
			RETURN 1
		ELSEIF RAND:2 == 0
			PRINTFORMW 「…아…」
			PRINTFORMW 「…좀 더…」
			RETURN 1
		ELSE
			PRINTFORMW 「응…」
			PRINTFORMW 「…산뜻한…」
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

