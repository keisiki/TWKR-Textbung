;욕정이 LV2가 되었을 때 등 등
;-------------------------------------------------
;함수명 :OPPAI_DESCRIPTION
;개요:말이 아니고 마음으로 이해해라
;인수 :ARG…캐릭터등록번호
;반환값:꿈
;비고:문중 함수
;사용예:%OPPAI_DESCRIPTION(TARGET)%
;예문:가슴
;ARG:1을 1으로 하면(자) 상태에 응한 형용사가 도착한다
;사용예:%OPPAI_DESCRIPTION(TARGET, 1)%
;예문:벌 있 듯이 유괴
;조건문에 TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
;(을)를 지정하고 있는 것은 합체중의 묘사
;-------------------------------------------------
@OPPAI_DESCRIPTION(ARG,ARG:1)
#FUNCTIONS
VARSET LOCALS
IF ARG:1
	IF FLAG:Look표시용유방
		;폭유
		IF TALENT:ARG:가슴사이즈 == 2
			LOCALS += "벌 끊어질 듯이" 
		;거유
		ELSEIF TALENT:ARG:가슴사이즈 == 1
			LOCALS += "풍만한 "
		;미유
		ELSEIF TALENT:ARG:가슴사이즈 == 0
			LOCALS += "형이 좋은 "
		;작은 가슴
		ELSEIF TALENT:ARG:가슴사이즈 == -1
			LOCALS += "작은 흔들림인 "
		;절벽
		ELSEIF TALENT:ARG:가슴사이즈 == -2
			LOCALS += "사랑스러운 "
		ELSE
			LOCALS += "에러 보고, 상정외의수치를 나타내고 있습니다--이상"
		ENDIF
	ELSE
		;폭유
		IF TALENT:ARG:가슴사이즈 == 2
			IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
				SELECTCASE RAND:4
					CASE 0
						LOCALS += "종횡에 날뛰어 도는 "
					CASE 1
						LOCALS += "출렁출렁 날뛰고 미치는 "
					CASE 2
						LOCALS += "응과 흔들리고 춤추는 "
					CASE 3
						LOCALS += "출렁출렁 흔들리는 "
				ENDSELECT
			ENDIF
			IF TALENT:PLAYER:가슴사이즈 == -2 && RAND:5 == 0
				LOCALS += "눈이 휘둥그레 질 만큼 크게 여물은 "
			ELSEIF RAND:4 == 0
				LOCALS += "암소와 같은 "
			ELSEIF RAND:3 == 0
				LOCALS += "벌 있 듯이 "
			ELSE
				LOCALS += "감당하기 힘들 정도의 "
			ENDIF
		;거유
		ELSEIF TALENT:ARG:가슴사이즈 == 1
			IF RAND:3 == 0
				IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
					LOCALS += "부들부들 날뛰어 손바닥에 들어가지 않는 "
				ELSE
					LOCALS += "매우 손으로 숨길 수 없는 "
				ENDIF
			ELSEIF RAND:2 == 0
				IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
					LOCALS += "응 출렁하고 엄청난 박력으로 흔들리는 "
				ELSE
					LOCALS += "흔들리듯 여물은 "
				ENDIF
			ELSE
				IF TEQUIP:50 == PLAYER || TEQUIP:51 == PLAYER
					LOCALS += "출렁출렁 흔들리는 손으로부터 흘러넘칠 것 같은 "
				ELSE
					LOCALS += "손으로부터 흘러넘칠 것 같은 "
				ENDIF
			ENDIF
		;미유
		ELSEIF TALENT:ARG:가슴사이즈 == 0
			IF TEQUIP:ARG:50 == PLAYER || TEQUIP:ARG:51 == PLAYER
				IF !RAND:4
					LOCALS += "부들부들 떨리는 "
				ELSEIF !RAND:3
					LOCALS += "속시원하게 흔들리는 "
				ELSE
					LOCALS += "푹신푹신하고 떨리는 "
				ENDIF
			ENDIF
			IF TALENT:ARG:체형 < 0
				IF !RAND:2
					LOCALS += "체격에 비해 발육이 좋은 "
				ELSE
					LOCALS += "약간 큼직한 "
				ENDIF
			ELSEIF RAND:4 == 0
					LOCALS += "서들 까는 발육한 "
			ELSEIF RAND:3 == 0
					LOCALS += "손바닥에 들어가는 사이즈의 "
			ELSEIF RAND:2 == 0
					LOCALS += "좌우 균정이 잡힌 2개의 "
			ELSE
				LOCALS += "형이 좋은 "
			ENDIF
		;절벽
		ELSEIF TALENT:ARG:가슴사이즈 == -2
			IF RAND:4 == 0
				LOCALS += "희미하게 한 "
			ELSEIF RAND:2 == 0
				LOCALS += "거의 부푼 곳이 없는 "
			ELSE
				LOCALS += "살짝 부푼 곳이 느껴지는 정도의 "
			ENDIF
		;작은 가슴
		ELSEIF TALENT:ARG:가슴사이즈 == -1
			IF TALENT:ARG:체형 < 0
				LOCALS += "외형 상응하는 "
			ELSEIF RAND:2 == 0
				LOCALS += "완만한 "
			ELSE
				LOCALS += "사랑스러운 "
			ENDIF
		;상정외
		ELSE
			LOCALS += "에러 보고, 상정외의수치를 나타내고 있습니다--이상"
		ENDIF
	ENDIF
ENDIF
CALLF 상반신속옷체크(ARG)
IF FLAG:Look표시용유방
	;절벽
	IF TALENT:가슴사이즈 == -2
		LOCALS += "유두"
	;작은 가슴~폭유
	ELSEIF TALENT:가슴사이즈 >= -1 && TALENT:가슴사이즈 <= 2
