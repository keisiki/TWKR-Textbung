			PRINTFORML %타겟은% %SHOW_BOTTOM(TARGET, 1)%를 누르며 %CALLNAME:MASTER%에게 항의했다
		CASE 0
			PRINTFORML %타겟은% %SHOW_BOTTOM(TARGET, 1)%를 누르면서, 어딘가 기쁜듯이 %CALLNAME:MASTER%에게 항의했다
		CASE 1
			SIF !PANTS_ALREADY_COLLECTED()
				SETCOLOR C_PINK
			IF GET_TARGETNUM() == 1 && !DATE_HITOGOMI(CFLAG:PLAYER:현재위치) && !WITH_MOB()
				IF ABL:노출벽 > 3
					IF EQUIP:TARGET:바지
						PRINTFORML %타겟은% 스스로 %SHOW_BOTTOM(TARGET)%를 벗어 내리고,  %UNDER_SKIRT_DESCRIPTION(TARGET)%를 과시해 왔다…
					ELSE
						PRINTFORML %타겟은% 스스로 %SHOW_BOTTOM(TARGET, 1)%를 걷어 올리고, %UNDER_SKIRT_DESCRIPTION(TARGET)%를 과시해 왔다…
					ENDIF
				ELSE
					PRINTFORML %타겟은% 작게 소리를 높였지만, 그 이상의 저항은 없었다
					PRINTFORML %마스터는% %UNDER_SKIRT_DESCRIPTION(TARGET)%를 마음껏 시간(視姦) 했다…
					SIF PALAM:욕정 > PALAMLV:4
						PRINTFORML %CALLNAME:TARGET%의 눈동자는 물기를 띠며, %CALLNAME:MASTER%에 무엇인가를 기대하는 것 같은 시선을 보내고 있다…
				ENDIF
			ELSE
				IF ABL:노출벽 > 5
					IF EQUIP:TARGET:바지
						PRINTFORML %타겟은% 근처에 사람이 보이지 않자, 스스로 %SHOW_BOTTOM(TARGET)%를 벗어 내리고,  %UNDER_SKIRT_DESCRIPTION(TARGET)%를 과시해 왔다…
					ELSE
						PRINTFORML %타겟은% 근처에 사람이 보이지 않자, 스스로 %SHOW_BOTTOM(TARGET, 1)%를 걷어 올리고, %UNDER_SKIRT_DESCRIPTION(TARGET)%를 과시해 왔다…
					ENDIF
				ELSE
					IF EQUIP:TARGET:바지
						PRINTFORML %타겟은% 주위의 눈을 신경쓰며 %SHOW_BOTTOM(TARGET)%를 누르지만, 그 힘은 미약하다
						PRINTFORM 질질 끌어내려진 %SHOW_BOTTOM(TARGET)%로부터는
					ELSE
						PRINTFORML %타겟은% 주위의 눈을 신경쓰며 %SHOW_BOTTOM(TARGET, 1)%를 누르지만, 그 힘은 미약하다
						PRINTFORM 넘겨져버린 %SHOW_BOTTOM(TARGET, 1)%의 옷자락으로부터는
					ENDIF
					IF !CFLAG:291
						IF EQUIP:TARGET:레오타드|| EQUIP:TARGET:보디슈트
							PRINTFORML %CALLNAME:TARGET%은 아래에 아무것도 입지 않은 탓에, %SHOW_하반신속옷(TARGET, 3)%의 너머로도, 선명하게 형태가 떠오르는 %LOCALS%가 희미하게 드러나 보여지고 있다……
						ELSEIF EQUIP:TARGET:하반신속옷1 || EQUIP:TARGET:하반신속옷2
							PRINTFORML %타겟이% 입고 있는 %PANTS_DESCRIPTION(EQUIP:TARGET:하반신속옷2, TARGET) %%PANTSNAME(EQUIP:TARGET:하반신속옷2, TARGET)%가 희미하게 드러나 보여지고 있다……
						ELSE
							PRINTFORML 속옷을 입지 않아 %CALLNAME:TARGET%의 노출된 %LOCALS%가 선명하게 드러나 보여지고 있다……
						ENDIF
					ELSE
						IF EQUIP:TARGET:레오타드|| EQUIP:TARGET:보디슈트
							PRINTFORML %SHOW_BOTTOM(TARGET, 1)% 아래는 %SHOW_하반신속옷(TARGET, 3)%로 지켜지고 있다…
						ELSE
							PRINTFORML 속옷을 입지 않아 %CALLNAME:TARGET%의 노출된 %LOCALS%가 선명하게 드러나 보여지고 있다……
						ENDIF
					ENDIF
				ENDIF
				TFLAG:400 = RAND:2
				SIF TFLAG:400 && (DATE_HITOGOMI() || WITH_MOB() == 9)
					CALL DATE_HITOGOMI_MESSAGE()
			ENDIF
			RESETCOLOR
	ENDSELECT
ENDIF

;-------------------------------------------------
;외출처 모브 묘사
;-------------------------------------------------
@DATE_HITOGOMI_MESSAGE(ARG)
SIF !ARG
	ARG = CFLAG:PLAYER:현재위치
;외출처 지령님 및 지저 국내 영묘
IF (MAIN_MAP !=9 && ARG == 960) || (IN_HOME(ARG) && !CHK_FOCUS(927, CFLAG:MASTER:현재위치, 948) && (ROOMDATA:(CFLAG:MASTER:현재위치 % 100) & 장소_모브))
	IF GROUPMATCH(TIME:2, 1,6,7)
		PRINTFORML 근처에서 자는 %ANIMALNAME()%가 난폭한 숨소리를 내고 있다
	ELSE
		PRINTFORM 근처에 있던 %ANIMALNAME()%가 
		SELECTCASE TIME:1 % 25
			;소형 포유류
			CASE 0 TO 7
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 치근덕스럽게 몸을 비벼온다
					CASE 2
						PRINTFORML 이상한 듯 응시하고 있다
					CASE 1
						PRINTFORML 앞발을 뻗어 온다
					CASE 0
						PRINTFORML 바닥을 탁 두드리고 있다
				ENDSELECT
			;대형 포유류
			CASE 8 TO 13
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 호기심을 느꼈는지 얼굴을 접근해 온다
					CASE 2
						PRINTFORML 가만히 이쪽을 보고 있다는 생각이 든다
					CASE 1
						PRINTFORML 가볍게 몸을 흔들고 있다
					CASE 0
						PRINTFORML 더듬더듬 바닥을 두드리고 있다
				ENDSELECT
			;초대형 포유류(옥내에 들어가 이것 없지 않은 것인지?
			CASE 14 TO 15
				SELECTCASE RAND:2
					CASE 1
						PRINTFORML 가만히 이쪽을 내려다 본다
					CASE 0
						PRINTFORML 쿵 발을 구르고 있다
				ENDSELECT
			;조류
			CASE 16 TO 20
				SELECTCASE RAND:5
					CASE 4
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML \@ TIME:1 % 25 > 18 ? 펄럭펄럭 # 탁탁 \@ 날개를 펄럭이고 있다
						ENDIF
					CASE 3
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML \@ TIME:1 % 25 > 18 ? 울고 있다 # 재잘거리고 있다 \@
						ENDIF
					CASE 2
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML 바로 옆에 와서, 쓰다듬어지고 싶은 듯이 머리를 쑥 내밀어 온다
						ENDIF
					CASE 1
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML 여기저기 돌아다니고 있다
						ENDIF
					CASE 0
						IF TIME:1 % 25 == 18
							PRINTFORML %CALLNAME:MASTER%의 인사에, 깊숙이 고개를 숙여 온다
						ELSE
							PRINTFORML 위협해 온다
						ENDIF
				ENDSELECT
			;파충류
			CASE 21 TO 24
				SELECTCASE RAND:3
					CASE 2
						PRINTFORML 스르륵 접근해 가만히 이쪽을 응시하고 있다
					CASE 1
						PRINTFORML 위협의 의도인지 입을 벌리고 있다
					CASE 0
						PRINTFORML 흥미 없는 듯이 몸을 흔들고 있다
				ENDSELECT
		ENDSELECT
	ENDIF
ELSEIF IN_HOME(ARG) && CHK_FOCUS(927, CFLAG:MASTER:현재위치, 930) && (ROOMDATA:(CFLAG:MASTER:현재위치 % 100) & 장소_모브)
	PRINTFORM 근처에 있던 원령이, %CALLNAME:TARGET%
	SELECTCASE RAND:5
		CASE 4
			PRINTFORMDL 에 가까워지려고 했지만, %CALLNAME:MASTER%에 쫓아버려졌다
		CASE 3
			PRINTFORMDL 를 봐 조금씩 가까워져 오다가 갑자기 도망치기 시작했다
		CASE 2
			PRINTFORMDL 의 주위를 맴돌다가, 그대로 도망쳤다
		CASE 1
			PRINTFORMDL 를 보고 빠르게 움직였지만, %CALLNAME:MASTER%의 곁에는 다가오지 않는다
		CASE 0
			PRINTFORMDL 는 쳐다보지도 않고, 푹신푹신하고 맴돌고 있다
	ENDSELECT
ELSE
	PRINTFORM 주위에 있던 %DATE_HITOGOMI_SYUBETU(CFLAG:PLAYER:현재위치)%가 
	SELECTCASE RAND:7
		CASE 6
			PRINTFORML 기가 막힌 표정을 짓고 있다
		CASE 5
			PRINTFORML 당황해서 얼굴을 돌렸다
		CASE 4
			PRINTFORML 과시하지 말라고 투덜대고 있다
		CASE 3
			PRINTFORML 놀란 표정을 띠고 있다
		CASE 2
			PRINTFORML 몇 번이나 흘끔거린다
		CASE 1
			PRINTFORML 얼굴을 붉히며 시선을 딴 데로 돌렸다
		CASE 0
			PRINTFORML 제 눈을 의심하며 눈을 비비고 있다
	ENDSELECT
ENDIF
