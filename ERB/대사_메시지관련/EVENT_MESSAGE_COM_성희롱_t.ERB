@DATE_HITOGOMI_MESSAGE(ARG)
SIF !ARG
	ARG = CFLAG:PLAYER:현재위치
;외출처 지령님 및 지저 국내 영묘
IF (MAIN_MAP !=9 && ARG == 960) || (IN_HOME(ARG) && !CHK_FOCUS(927, CFLAG:MASTER:현재위치, 948) && (ROOMDATA:(CFLAG:MASTER:현재위치 % 100) & 장소_모브))
	IF GROUPMATCH(TIME:2, 1,6,7)
		PRINTFORML 근처에서 자고 있던 %ANIMALNAME()%가, 거친 숨소리를 내고 있다
	ELSE
		PRINTFORM 근처에 있던 %ANIMALNAME()%가 
		SELECTCASE TIME:1 % 25
			;소형 포유류
			CASE 0 TO 7
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 부비부비 치근덕거리며 몸을 비벼왔다 
					CASE 2
						PRINTFORML 신기하다는 듯이 바라보고 있다
					CASE 1
						PRINTFORML 앞발을 내밀어 온다
					CASE 0
						PRINTFORML 바닥을 탁 하고 두드리고 있다
				ENDSELECT
			;대형 포유류
			CASE 8 TO 13
				SELECTCASE RAND:4
					CASE 3
						PRINTFORML 호기심을 느꼈는지 얼굴을 들이민다
					CASE 2
						PRINTFORML 물끄러미 이쪽을 보고 있는 것 같다
					CASE 1
						PRINTFORML 가볍게 몸을 흔들고 있다
					CASE 0
						PRINTFORML 탁탁 바닥을 두드리고 있다
				ENDSELECT
			;초대형 포유류(옥내에 들어가 이것 없지 않은 것인지?
			CASE 14 TO 15
				SELECTCASE RAND:2
					CASE 1
						PRINTFORML 가만히 이쪽을 내려다보고 있다
					CASE 0
						PRINTFORML 쿵 하고 발을 구르고 있다
				ENDSELECT
			;조류
			CASE 16 TO 20
				SELECTCASE RAND:5
					CASE 4
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML \@ TIME:1 % 25 > 18 ? 퍼덕퍼덕 # 탁탁 \@ 날개를 펄럭이고 있다
						ENDIF
					CASE 3
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML \@ TIME:1 % 25 > 18 ? 울고 있다 # 재잘거리고 있다 \@
						ENDIF
					CASE 2
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML 바로 옆에 다가와서, 쓰다듬 받고 싶다는 듯이 머리를 스윽 내밀어 왔다
						ENDIF
					CASE 1
						IF TIME:1 % 25 == 18
							PRINTFORML 가만히 움직이지 않고 있다
						ELSE
							PRINTFORML 여기저기 돌아다니고 있다
						ENDIF
					CASE 0
						IF TIME:1 % 25 == 18
							PRINTFORML %CALLNAME:MASTER%의 인사에, 깊숙이 고개를 숙여 보였다
						ELSE
							PRINTFORML 위협해 온다
						ENDIF
				ENDSELECT
			;파충류
			CASE 21 TO 24
				SELECTCASE RAND:3
					CASE 2
						PRINTFORML 슬쩍 다가가서 물끄러미 이쪽을 바라보고 있다
					CASE 1
						PRINTFORML 위협의 의도인지 입을 벌리고 있다
					CASE 0
						PRINTFORML 흥미 없다는 듯이 몸을 흔들고 있다
				ENDSELECT
		ENDSELECT
	ENDIF
ELSEIF IN_HOME(ARG) && CHK_FOCUS(927, CFLAG:MASTER:현재위치, 930) && (ROOMDATA:(CFLAG:MASTER:현재위치 % 100) & 장소_모브)
	PRINTFORM 근처에 있던 원령이, %CALLNAME:TARGET%
	SELECTCASE RAND:5
		CASE 4
			PRINTFORMDL 에게 다가가려고 했지만, %CALLNAME:MASTER%에게 쫓겨났다
		CASE 3
			PRINTFORMDL 를 보고 한발한발 조금씩 다가오다 갑자기 도망치기 시작했다
		CASE 2
			PRINTFORMDL 의 주위를 어지러이 날아다니다, 그대로 도망쳤다
		CASE 1
			PRINTFORMDL 를 보고 서둘러 움직였지만, %CALLNAME:MASTER%의 곁에는 다가오지 않았다
		CASE 0
			PRINTFORMDL 에는 눈길도 주지 않고, 둥실둥실 떠다니고 있다
	ENDSELECT
ELSE
	PRINTFORM 주위에 있던 %DATE_HITOGOMI_SYUBETU(CFLAG:PLAYER:현재위치)%가 
	SELECTCASE RAND:7
		CASE 6
			PRINTFORML 기가 막힌 표정을 짓고 있다
		CASE 5
			PRINTFORML 당황해서 얼굴을 돌렸다
		CASE 4
			PRINTFORML 과시하지 말라고 투덜대고 있다
		CASE 3
			PRINTFORML 놀란 표정을 짓고 있다
		CASE 2
			PRINTFORML 몇 번이나 힐끔 엿본다
		CASE 1
			PRINTFORML 얼굴을 붉히며 시선을 딴 데로 돌렸다
		CASE 0
			PRINTFORML 제 눈을 의심하며 눈을 비비고 있다
	ENDSELECT
ENDIF
