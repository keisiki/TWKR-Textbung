CASE "주택" ;KR판 오리지날 주택/저택 기능
	SELECTCASE ARGS
	CASE "SALES"
		IF ITEM:주택 < 4
			RETURN 0
		ELSE
			RETURN -2
		ENDIF
	CASE "구입"
		VARSET LOCAL ;가격
		VARSET LOCALS ;출력텍스트
		SELECTCASE ITEM:주택
			CASE 0
				LOCAL = ITEMPRICE:ARG * (100 - ARG:1) / 100
				LOCALS '= @"주택을 구입하겠습니까? 가격: " + 금액표시(LOCAL)
			CASE 1
				LOCAL = ITEMPRICE:ARG * (100 - ARG:1) / 100 * 3
				LOCALS '= @"저택으로 증축하겠습니까? 가격: " + 금액표시(LOCAL)
			CASE 2,3
				LOCAL = ITEMPRICE:ARG * (100 - ARG:1) / 100 * 3
				LOCALS '= @"더욱 증축하겠습니까? 가격: " + 금액표시(LOCAL)
			ENDSELECT
		PRINTSL LOCALS
		CALL ASK_YN
		IF LOCAL <= MONEY
			MONEY -= LOCAL
			PRINTFORMW %조사처리(ITEMNAME:ARG,"를")% %금액표시(LOCAL)%에 구입했습니다
		ELSE
			PRINTFORMW 돈이 부족하다…
			RETURN 0
		ENDIF
		IF !RESULT
			ITEM:ARG ++
			;주거설정 처리 ;주택/저택 첫구매
			SIF GROUPMATCH(ITEM:ARG, 1,2)
