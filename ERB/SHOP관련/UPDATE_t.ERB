		CALL SET_MINIMUM_EXP(C_ID, GETNUM(EXP, "전투경험"), 최저능력치)
		CONTINUE
	ENDIF
	ABL:C_ID:갱신능력번호 = MAX(최저능력치, ABL:C_ID:갱신능력번호)
