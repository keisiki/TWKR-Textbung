ENDSELECT

IF ボーナス内容
	SELECTCASE ボーナス内容
		CASE 4
			授与個数 = 1
		CASE IS > 8
			授与個数 = 8
		CASE IS > 5
			授与個数 = 5
		CASEELSE
			授与個数 = 3
	ENDSELECT
	IF ITEM:TB_ITEM() + 授与個数 > 99
		ITEM:TB_ITEM() = 99
	ELSE
		ITEM:TB_ITEM() += 授与個数
	ENDIF
	IF ボーナス内容 == 4
		CALL COLORMESSAGE(@"%ITEMNAME:TB_ITEM()%을 받았다!", C_YELLOW, 2, 1)
	ELSE
		CALL COLORMESSAGE(@"%ITEMNAME:TB_ITEM()%을 {授与個数}개 받았다!", C_YELLOW, 2, 1)
	ENDIF
	CLEARLINE 1
ENDIF

