;-----------------------------------------------------------
;지점의 연결 설정
;ARG    기준지점
;ARG:1  대상지점
;RETURNF 0=이동할 수 없다, 보이지 않는다
;        1=이동할 수 있다, 보이지 않는다
;        2=이동할 수 없다, 보인다
;        3=이동할 수 있다, 보인다 (직통)
;-----------------------------------------------------------
@CAN_MOVE_1(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P101산문
		SIF GROUPMATCH(ARG:1, P102경내, P140지장길)
			RETURNF 3
	CASE P102경내
		SIF GROUPMATCH(ARG:1, P104강당, P105경당, P107봉당, P119별채, P121묘지, P137마미조의거처)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P101산문, P122범종루)
			RETURNF 3
	CASE P103본당
		SIF GROUPMATCH(ARG:1, P104강당)
			RETURNF 3
	CASE P104강당
		SIF GROUPMATCH(ARG:1, P102경내)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P103본당)
			RETURNF 3
	CASE P105경당
		SIF GROUPMATCH(ARG:1, P102경내)
			RETURNF 1
	CASE P106뒷마당
		SIF GROUPMATCH(ARG:1, P120헛간, P121묘지)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P110툇마루)
			RETURNF 3
	CASE P107봉당
		SIF GROUPMATCH(ARG:1, P102경내, P108거실, P112목욕탕)
			RETURNF 1
	CASE P108거실
		SIF GROUPMATCH(ARG:1, P107봉당, P109조온방, P135복도)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P111부엌)
			RETURNF 3
	CASE P109조온방
		SIF GROUPMATCH(ARG:1, P108거실, P118뱌쿠렌방, P135복도)
			RETURNF 1
	CASE P110툇마루
		SIF GROUPMATCH(ARG:1, P113나즈린방, P115쇼방)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P106뒷마당, P135복도)
			RETURNF 3
	CASE P111부엌
		SIF GROUPMATCH(ARG:1, P108거실)
			RETURNF 3
	CASE P112목욕탕
		SIF GROUPMATCH(ARG:1, P107봉당)
			RETURNF 1
	CASE P113나즈린방
		SIF GROUPMATCH(ARG:1, P110툇마루, P116이치린방, P135복도)
			RETURNF 1
	CASE P114무라사방
		SIF GROUPMATCH(ARG:1, P116이치린방, P117누에방, P135복도)
			RETURNF 1
	CASE P115쇼방
		SIF GROUPMATCH(ARG:1, P110툇마루, P118뱌쿠렌방, P135복도)
			RETURNF 1
	CASE P116이치린방
		SIF GROUPMATCH(ARG:1, P113나즈린방, P114무라사방, P135복도)
			RETURNF 1
	CASE P117누에방
		SIF GROUPMATCH(ARG:1, P114무라사방, P136쿄코방, P135복도)
			RETURNF 1
	CASE P118뱌쿠렌방
		SIF GROUPMATCH(ARG:1, P109조온방, P115쇼방, P135복도)
			RETURNF 1
	CASE P119별채
		SIF GROUPMATCH(ARG:1, P102경내)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P112목욕탕)
			RETURNF 2
	CASE P120헛간
		SIF GROUPMATCH(ARG:1, P106뒷마당)
			RETURNF 1
	CASE P121묘지
		SIF GROUPMATCH(ARG:1, P102경내, P106뒷마당)
			RETURNF 1
	CASE P122범종루
		SIF GROUPMATCH(ARG:1, P102경내)
			RETURNF 3

	CASE P123영묘대문
		SIF GROUPMATCH(ARG:1, P124영묘광장, P140지장길)
			RETURNF 3
	CASE P124영묘광장
		SIF GROUPMATCH(ARG:1, P126도장, P133목욕장, P134곳간, P138종루, P139사선루)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P123영묘대문)
			RETURNF 3
	CASE P125토지코방
		SIF GROUPMATCH(ARG:1, P130복도)
			RETURNF 1
	CASE P126도장
		SIF GROUPMATCH(ARG:1, P124영묘광장, P130복도)
			RETURNF 1
	CASE P127후토방
		SIF GROUPMATCH(ARG:1, P130복도)
			RETURNF 1
	CASE P128미코방
		SIF GROUPMATCH(ARG:1, P130복도)
			RETURNF 1
	CASE P129응접실
		SIF GROUPMATCH(ARG:1, P130복도)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P131고산수)
			RETURNF 3
	CASE P130복도
		SIF GROUPMATCH(ARG:1, P126도장, P125토지코방, P127후토방, P128미코방, P129응접실, P132조제실)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P131고산수)
			RETURNF 3
	CASE P131고산수
		SIF GROUPMATCH(ARG:1, P129응접실, P130복도)
			RETURNF 3
	CASE P132조제실
		SIF GROUPMATCH(ARG:1, P130복도)
			RETURNF 1
	CASE P133목욕장
		SIF GROUPMATCH(ARG:1, P124영묘광장)
			RETURNF 1
	CASE P134곳간
		SIF GROUPMATCH(ARG:1, P124영묘광장)
			RETURNF 1

	CASE P135복도
		SIF GROUPMATCH(ARG:1, P108거실, P109조온방, P113나즈린방, P114무라사방, P115쇼방, P116이치린방, P117누에방, P118뱌쿠렌방, P136쿄코방)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P110툇마루)
			RETURNF 3
	CASE P136쿄코방
		SIF GROUPMATCH(ARG:1, P117누에방, P135복도)
			RETURNF 1
	CASE P137마미조의거처
		SIF GROUPMATCH(ARG:1, P102경내)
			RETURNF 1
	CASE P138종루
		SIF GROUPMATCH(ARG:1, P124영묘광장)
			RETURNF 1
	CASE P139사선루
		SIF GROUPMATCH(ARG:1, P124영묘광장)
			RETURNF 1
	CASE P140지장길
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P101산문, P123영묘대문)
			RETURNF 3

	CASE MAXROOM
		SIF ARG:1 == P140지장길
			RETURNF 1
ENDSELECT
RETURNF 0