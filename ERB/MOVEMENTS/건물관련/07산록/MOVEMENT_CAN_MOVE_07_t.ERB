
;지점 관련성 응답 처리
;-----------------------------------------------------------
@CAN_MOVE_7(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
;수해도 입구
CASE 701
	SIF GROUPMATCH(ARG:1,702,734)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE 702
	SIF GROUPMATCH(ARG:1,701,703,715,716)
		RETURNF 1
;현무의늪
CASE 703
	SIF GROUPMATCH(ARG:1,702,704,705,709,735)
		RETURNF 1
CASE 704,735
	SIF ARG:1 == 703
		RETURNF 1
CASE 705
	SIF GROUPMATCH(ARG:1,703,707)
		RETURNF 3
	SIF ARG:1 == 733
		RETURNF 1
CASE 706,708,710
	SIF ARG:1 == 707
		RETURNF 1
CASE 707
	SIF GROUPMATCH(ARG:1,705,706,708,710,711,731)
		RETURNF 1
CASE 709
	SIF GROUPMATCH(ARG:1,703)
		RETURNF 1
CASE 711
	SIF GROUPMATCH(ARG:1,707,712,732,736)
		RETURNF 1
CASE 712,732
	SIF GROUPMATCH(ARG:1,711,736,713)
		RETURNF 1
CASE 713
	SIF GROUPMATCH(ARG:1,712)
		RETURNF 1
CASE 715
	SIF GROUPMATCH(ARG:1,702,716)
		RETURNF 1
CASE 716
	SIF GROUPMATCH(ARG:1,702,715,717,718,719)
		RETURNF 1
CASE 717
	SIF GROUPMATCH(ARG:1,716,718,719)
		RETURNF 1
CASE 718
	SIF GROUPMATCH(ARG:1,716,717,719)
		RETURNF 1
CASE 719
	SIF GROUPMATCH(ARG:1,715,716,717,718)
		RETURNF 1
;선인의저택카센!
;부엌으로부터는 현관, 도장과 목욕탕, 2층 응접실
CASE 720
	SIF GROUPMATCH(ARG:1,721,722,723,729)
		RETURNF 1
;목욕탕으로부터는 도장과 부엌 현관, 2층 응접실
CASE 721
	SIF GROUPMATCH(ARG:1,720,722,723,729)
		RETURNF 1
;도장으로부터는 목욕탕과 부엌 현관, 2층 응접실
CASE 722
	SIF GROUPMATCH(ARG:1,720,721,723,729)
		RETURNF 1
;2층 응접실로부터는 도장과 부엌, 현관, 목욕탕, 서재, 2층 출입구, 3층 거실
CASE 723
	SIF GROUPMATCH(ARG:1,720,721,722,724,725,726,729)
		RETURNF 1
;서재
CASE 724
	SIF GROUPMATCH(ARG:1,723,725)
		RETURNF 1
CASE 725
	SIF GROUPMATCH(ARG:1,723,724,730)
		RETURNF 1
CASE 726
	SIF GROUPMATCH(ARG:1,723,727,728)
		RETURNF 1
CASE 727
	SIF GROUPMATCH(ARG:1,726,728)
		RETURNF 1
CASE 728
	SIF GROUPMATCH(ARG:1,726,727)
		RETURNF 1
;선인의 뜰과 직통
CASE 729
	SIF ARG:1 == 730
		RETURNF 3
	SIF GROUPMATCH(ARG:1,720,721,722,723)
		RETURNF 1
;선인의 뜰
CASE 730
	SIF GROUPMATCH(ARG:1,729,731)
		RETURNF 1
;목
CASE 731
	SIF ARG:1 == 730
		RETURNF 1
	SIF ARG:1 == 707
		RETURNF 1
CASE 733
	SIF ARG:1 == 705
		RETURNF 1
CASE 734
	SIF ARG:1 == 701
		RETURNF 1
CASE 736
	SIF ARG:1 == 711
		RETURNF 1
CASE MAXROOM
	SIF ARG:1 == 701
		RETURNF 1
ENDSELECT

RETURNF 0