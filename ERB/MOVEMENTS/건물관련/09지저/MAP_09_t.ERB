@DRAW_MAP_BG_9(SLICED)
#DIMS SLICED
#DIM CONST C_WATER = 0x25b7c0
SELECTCASE SLICED
;▼着色
CASE "■", "┳", "ⅰ", "╋"
	SELECTCASE TFLAG:맵변환
	CASE 0
		SETCOLOR C_WOOD1
	CASE 1
		SETCOLOR C_STONE1
	CASE 2
		SETCOLOR C_WOOD1
	CASE 3
		SETCOLOR 0x006a6c
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "│", "─", "┐", "┌", "└", "┘", "|"
	SELECTCASE TFLAG:맵변환
	CASE 2, 0
		SETCOLOR C_WOOD1
	CASE 3
		SETCOLOR C_ORANGE
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "□", "○"
	SELECTCASE TFLAG:맵변환
	CASE 3, 0
		SETCOLOR 0x00ffcc
	CASEELSE
		SETCOLOR C_WOOD2
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "◇", "├"
	SETCOLOR C_STONE1
	PRINTS SLICED
	RESETCOLOR
CASE "●", "┃"
	SELECTCASE TFLAG:맵변환
	CASE 0
		SETCOLOR C_WOOD1
	CASE 2
		SETCOLOR C_WOOD2
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "ｖ"
	SETCOLOR C_L_GREEN
	PRINTS SLICED
	RESETCOLOR
CASE "＃"
	SETCOLOR C_FOREST1
	PRINTS SLICED
	RESETCOLOR
CASE "＠"
	SETCOLOR C_RED
	PRINTS SLICED
	RESETCOLOR
CASE "~", "♨"
	SETCOLOR C_WATER
	PRINTS SLICED
	RESETCOLOR
CASE "火", "炎", "▆", "▅", "▃", "▂", "▁"
	SETCOLOR C_FLAME
	PRINTS SLICED
	RESETCOLOR
CASE "＿"
	SELECTCASE TFLAG:맵변환
	CASE 3
		SETCOLOR C_D_RED
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "／", "＼"
	SELECTCASE TFLAG:맵변환
	CASE 3
		SETCOLOR C_ORANGE
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "◎", "❤"
	SELECTCASE TFLAG:맵변환
	CASE 2
		SETCOLOR C_PINK
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "∞"
	SETCOLOR C_FUTON
	PRINTS SLICED
	RESETCOLOR
CASE "\,", ".", "-", "―", "‐", "､", "\"", "´", "`", "｀", "'", ";", "_"
	SETCOLOR C_ORANGE
	PRINTS SLICED
	RESETCOLOR
;▼特殊な表示
CASE "#a", "#b", "#c", "#d", "#e", "#f", "#g", "#h", "#i", "#j", "#k", "#l", "#m", "#n", "#o"
	CALL MAP_PRINT(SLICED)
CASE "#A"
	SETCOLOR C_WOOD2
	PRINT ─
	RESETCOLOR
CASE "#B"
	SETCOLOR C_WOOD2
	PRINT │
	RESETCOLOR
CASE "#C"
	SETCOLOR C_D_RED
	PRINT ■
	RESETCOLOR
CASE "#D"
	SETCOLOR C_D_RED
	PRINT □
	RESETCOLOR
CASE "#E"
	SETCOLOR C_D_RED
	PRINT ／
	RESETCOLOR
CASE "#F"
	SETCOLOR C_D_RED
	PRINT ＼
	RESETCOLOR
CASE "#G"
	SETCOLOR C_WATER
	PRINT ◎
	RESETCOLOR
CASE "#H"
	SETCOLOR C_STONE1
	PRINT ■
	RESETCOLOR
CASE "#I"
	SETCOLOR 0x006a6c
	PRINT ■
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT