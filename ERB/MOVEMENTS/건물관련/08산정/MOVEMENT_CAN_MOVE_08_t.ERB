;-----------------------------------------------------------
;지점의 연결 설정
;ARG    기준지점
;ARG:1  대상지점
;RETURNF 0=이동할 수 없다, 보이지 않는다
;        1=이동할 수 있다, 보이지 않는다
;        2=이동할 수 없다, 보인다
;        3=이동할 수 있다, 보인다 (직통)
;-----------------------------------------------------------
@CAN_MOVE_8(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
CASE P801산정입구
	SIF GROUPMATCH(ARG:1, P802다리)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P807모미지댁)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
CASE P802다리
	SIF GROUPMATCH(ARG:1, P801산정입구, P803삭도역)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P806폭포뒤, P807모미지댁, P808아야댁, P809하타테댁)
		RETURNF 1
CASE P803삭도역
	SIF GROUPMATCH(ARG:1, P802다리)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P804모리야신사토리이, P808아야댁, P809하타테댁, P810산막)
		RETURNF 1
CASE P804모리야신사토리이
	SIF GROUPMATCH(ARG:1, P811경내)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P803삭도역, P805정상, P810산막)
		RETURNF 1
CASE P805정상
	SIF GROUPMATCH(ARG:1, P804모리야신사토리이, P830현운해, P851쿠다카의오두막, P852코마쿠사의오두막, P853미스마루의오두막, P854메구무댁, P855홍룡동)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P801산정입구, P802다리, P803삭도역)
		RETURNF 2
CASE P806폭포뒤
	SIF GROUPMATCH(ARG:1, P802다리)
		RETURNF 1
CASE P807모미지댁
	SIF GROUPMATCH(ARG:1, P801산정입구, P802다리)
		RETURNF 1
CASE P808아야댁, P809하타테댁
	SIF GROUPMATCH(ARG:1, P802다리, P803삭도역)
		RETURNF 1
CASE P810산막
	SIF GROUPMATCH(ARG:1, P803삭도역, P804모리야신사토리이)
		RETURNF 1

CASE P811경내
	SIF GROUPMATCH(ARG:1, P804모리야신사토리이, P813신사사무소, P814호숫가)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P812본전, P821봉당)
		RETURNF 1
CASE P812본전
	SIF GROUPMATCH(ARG:1, P811경내, P826복도)
		RETURNF 1
CASE P813신사사무소
	SIF GROUPMATCH(ARG:1, P811경내)
		RETURNF 3
CASE P814호숫가
	SIF GROUPMATCH(ARG:1, P811경내, P815마당)
		RETURNF 3
CASE P815마당
	SIF GROUPMATCH(ARG:1, P814호숫가, P816툇마루)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P821봉당)
		RETURNF 1
CASE P816툇마루
	SIF GROUPMATCH(ARG:1, P817카나코방, P818스와코방, P819거실, P821봉당, P826복도)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P815마당)
		RETURNF 3
CASE P817카나코방
	SIF GROUPMATCH(ARG:1, P816툇마루, P818스와코방, P819거실)
		RETURNF 1
CASE P818스와코방
	SIF GROUPMATCH(ARG:1, P816툇마루, P817카나코방, P820사나에방, P821봉당)
		RETURNF 1
CASE P819거실
	SIF GROUPMATCH(ARG:1, P816툇마루, P817카나코방, P820사나에방, P826복도)
		RETURNF 1
CASE P820사나에방
	SIF GROUPMATCH(ARG:1, P818스와코방, P819거실, P821봉당, P826복도)
		RETURNF 1
CASE P821봉당
	SIF GROUPMATCH(ARG:1, P811경내, P815마당, P826복도, P816툇마루, P818스와코방, P820사나에방, P823빈방, P824화장실, P825목욕탕)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P822부엌)
		RETURNF 3
CASE P822부엌
	SIF GROUPMATCH(ARG:1, P821봉당)
		RETURNF 3
CASE P823빈방, P824화장실, P825목욕탕
	SIF GROUPMATCH(ARG:1, P821봉당)
		RETURNF 1
CASE P826복도
	SIF GROUPMATCH(ARG:1, P812본전, P816툇마루, P819거실, P820사나에방, P821봉당)
		RETURNF 1

CASE P830현운해
	SIF GROUPMATCH(ARG:1, P805정상)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P832유정천거리)
		RETURNF 3
CASE P831히나나위저택앞
	SIF GROUPMATCH(ARG:1, P832유정천거리, P834히나나위저택봉당)
		RETURNF 1
CASE P832유정천거리
	SIF GROUPMATCH(ARG:1, P830현운해)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P831히나나위저택앞, P833도원)
		RETURNF 1
CASE P833도원
	SIF GROUPMATCH(ARG:1, P832유정천거리)
		RETURNF 1
CASE P834히나나위저택봉당
	SIF GROUPMATCH(ARG:1, P831히나나위저택앞, P835히나나위저택응접실, P836히나나위저택주방, P837히나나위저택목욕탕, P838히나나위저택거실)
		RETURNF 1
CASE P835히나나위저택응접실
	SIF GROUPMATCH(ARG:1, P834히나나위저택봉당, P836히나나위저택주방)
		RETURNF 1
CASE P836히나나위저택주방
	SIF GROUPMATCH(ARG:1, P834히나나위저택봉당, P835히나나위저택응접실)
		RETURNF 1
CASE P837히나나위저택목욕탕
	SIF GROUPMATCH(ARG:1, P834히나나위저택봉당)
		RETURNF 1
CASE P838히나나위저택거실
	SIF GROUPMATCH(ARG:1, P834히나나위저택봉당, P839히나나위저택다다미방, P840텐시방, P841이쿠방, P842히나나위저택헛간)
		RETURNF 1
CASE P839히나나위저택다다미방
	SIF GROUPMATCH(ARG:1, P838히나나위저택거실, P840텐시방)
		RETURNF 1
CASE P840텐시방
	SIF GROUPMATCH(ARG:1, P838히나나위저택거실, P839히나나위저택다다미방)
		RETURNF 1
CASE P841이쿠방, P842히나나위저택헛간
	SIF GROUPMATCH(ARG:1, P838히나나위저택거실)
		RETURNF 1

CASE P851쿠다카의오두막, P852코마쿠사의오두막, P853미스마루의오두막, P854메구무댁
	SIF GROUPMATCH(ARG:1, P805정상)
		RETURNF 1
CASE P855홍룡동
	SIF GROUPMATCH(ARG:1, P805정상, P856모모요의거처)
		RETURNF 1
CASE P856모모요의거처
	SIF GROUPMATCH(ARG:1, P855홍룡동)
		RETURNF 1

CASE MAXROOM
	SIF ARG:1 == P801산정입구
		RETURNF 1
ENDSELECT

RETURNF 0