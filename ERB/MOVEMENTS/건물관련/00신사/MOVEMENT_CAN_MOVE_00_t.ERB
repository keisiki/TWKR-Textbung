;-----------------------------------------------------------
;지점의 연결 설정
;ARG    기준지점
;ARG:1  대상지점
;RETURNF 0=이동할 수 없다, 보이지 않는다
;        1=이동할 수 있다, 보이지 않는다
;        2=이동할 수 없다, 보인다
;        3=이동할 수 있다, 보인다 (직통)
;-----------------------------------------------------------
@CAN_MOVE_0(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P1토리이
		SIF GROUPMATCH(ARG:1, P2경내, P22참배길)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
	CASE P2경내
		SIF GROUPMATCH(ARG:1, P6헛간, P16낡은오두막, P20본전뒷편, P56아운의거처)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P1토리이, P3새전함, P5개수대, P21모리야신사분사)
			RETURNF 3
	CASE P3새전함
		SIF GROUPMATCH(ARG:1, P4본당)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P2경내)
			RETURNF 3
	CASE P4본당
		SIF GROUPMATCH(ARG:1, P3새전함)
			RETURNF 1
	CASE P5개수대
		SIF GROUPMATCH(ARG:1, P2경내)
			RETURNF 3
	CASE P6헛간
		SIF GROUPMATCH(ARG:1, P2경내, P20본전뒷편)
			RETURNF 1
	CASE P7봉당
		SIF GROUPMATCH(ARG:1, P9거실, P20본전뒷편)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P8부엌)
			RETURNF 3
	CASE P8부엌
		SIF GROUPMATCH(ARG:1, P9거실, P10툇마루)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P7봉당)
			RETURNF 3
	CASE P9거실
		SIF GROUPMATCH(ARG:1, P7봉당, P8부엌, P15레이무방, P17작은창고, P53창고, P54난쟁이벌레통, P55일본식방)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P10툇마루)
			RETURNF 3
	CASE P10툇마루
		SIF GROUPMATCH(ARG:1, P8부엌, P12탈의실, P14화장실, P19큰창고, P54난쟁이벌레통)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P9거실, P11마당)
			RETURNF 3
	CASE P11마당
		SIF GROUPMATCH(ARG:1, P20본전뒷편, P52별채)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P10툇마루)
			RETURNF 3
	CASE P12탈의실
		SIF GROUPMATCH(ARG:1, P10툇마루, P13목욕탕)
			RETURNF 1
	CASE P13목욕탕
		SIF GROUPMATCH(ARG:1, P12탈의실)
			RETURNF 1
	CASE P14화장실
		SIF GROUPMATCH(ARG:1, P10툇마루)
			RETURNF 1
	CASE P15레이무방
		SIF GROUPMATCH(ARG:1, P9거실, P17작은창고, P18벽장)
			RETURNF 1
	CASE P16낡은오두막
		SIF GROUPMATCH(ARG:1, P2경내, P20본전뒷편)
			RETURNF 1
	CASE P17작은창고
		SIF GROUPMATCH(ARG:1, P9거실)
			RETURNF 1
	CASE P18벽장
		SIF GROUPMATCH(ARG:1, P15레이무방)
			RETURNF 1
	CASE P19큰창고
		SIF GROUPMATCH(ARG:1, P10툇마루)
			RETURNF 1
	CASE P20본전뒷편
		SIF GROUPMATCH(ARG:1, P6헛간, P7봉당, P11마당, P16낡은오두막)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P2경내)
			RETURNF 3
	CASE P21모리야신사분사
		SIF GROUPMATCH(ARG:1, P2경내)
			RETURNF 3
	CASE P22참배길
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P1토리이, P36몽환유적앞)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P23온천・탈의실, P25수호신의숲)
			RETURNF 1
	CASE P23온천・탈의실
		SIF GROUPMATCH(ARG:1, P22참배길, P24온천, P25수호신의숲)
			RETURNF 1
	CASE P24온천
		SIF GROUPMATCH(ARG:1, P23온천・탈의실)
			RETURNF 1
	CASE P25수호신의숲
		SIF GROUPMATCH(ARG:1, P22참배길)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P23온천・탈의실, P27삼요정의집・객실)
			RETURNF 1

	CASE P27삼요정의집・객실
		SIF GROUPMATCH(ARG:1, P25수호신의숲, P30삼요정의집・목욕탕, P31삼요정의집・2층복도, P35삼요정의집・지하실)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P29삼요정의집・부엌)
			RETURNF 3
	CASE P29삼요정의집・부엌
		SIF GROUPMATCH(ARG:1, P27삼요정의집・객실)
			RETURNF 3
	CASE P30삼요정의집・목욕탕
		SIF GROUPMATCH(ARG:1, P27삼요정의집・객실)
			RETURNF 1
	CASE P31삼요정의집・2층복도
		SIF GROUPMATCH(ARG:1, P27삼요정의집・객실, P32서니방, P33루나방, P34스타방)
			RETURNF 1
	CASE P32서니방, P33루나방, P34스타방
		SIF GROUPMATCH(ARG:1, P31삼요정의집・2층복도)
			RETURNF 1
	CASE P35삼요정의집・지하실
		SIF GROUPMATCH(ARG:1, P27삼요정의집・객실)
			RETURNF 1

	CASE P36몽환유적앞
		SIF GROUPMATCH(ARG:1, P22참배길)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P37몽환유적・1층통로)
			RETURNF 1
	CASE P37몽환유적・1층통로
		SIF GROUPMATCH(ARG:1, P36몽환유적앞, P38몽환유적・세면소, P41몽환유적・식당겸휴게실, P43유메미방, P44치유리방, P46몽환유적・2층통로)
			RETURNF 1
	CASE P38몽환유적・세면소
		SIF GROUPMATCH(ARG:1, P37몽환유적・1층통로, P39몽환유적・욕실, P40몽환유적・화장실)
			RETURNF 1
	CASE P39몽환유적・욕실, P40몽환유적・화장실
		SIF GROUPMATCH(ARG:1, P38몽환유적・세면소)
			RETURNF 1
	CASE P41몽환유적・식당겸휴게실
		SIF GROUPMATCH(ARG:1, P37몽환유적・1층통로)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P42몽환유적・조리실)
			RETURNF 3
	CASE P42몽환유적・조리실
		SIF GROUPMATCH(ARG:1, P41몽환유적・식당겸휴게실)
			RETURNF 3
	CASE P43유메미방, P44치유리방
		SIF GROUPMATCH(ARG:1, P37몽환유적・1층통로)
			RETURNF 1
	CASE P46몽환유적・2층통로
		SIF GROUPMATCH(ARG:1, P37몽환유적・1층통로, P47몽환유적・함교, P48몽환유적・제1자료실, P49몽환유적・연구실, P50몽환유적・제2자료실, P51몽환유적・창고)
			RETURNF 1
	CASE P47몽환유적・함교
		SIF GROUPMATCH(ARG:1, P46몽환유적・2층통로)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P36몽환유적앞)
			RETURNF 2
	CASE P48몽환유적・제1자료실, P49몽환유적・연구실, P50몽환유적・제2자료실, P51몽환유적・창고
		SIF GROUPMATCH(ARG:1, P46몽환유적・2층통로)
			RETURNF 1

	CASE P52별채
		SIF GROUPMATCH(ARG:1, P11마당)
			RETURNF 1
	CASE P53창고, P55일본식방
		SIF GROUPMATCH(ARG:1, P9거실)
			RETURNF 1
	CASE P54난쟁이벌레통
		SIF GROUPMATCH(ARG:1, P9거실, P10툇마루)
			RETURNF 1
	CASE P56아운의거처
		SIF GROUPMATCH(ARG:1, P2경내)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P22참배길
			RETURNF 1
ENDSELECT
RETURNF 0