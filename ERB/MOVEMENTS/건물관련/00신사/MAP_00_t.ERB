@DRAW_MAP_BG_0(SLICED)
#DIMS SLICED
#DIM CONST C_MUGEN = 0x4d5aaf
SELECTCASE SLICED
;▼着色
CASE "ｖ"
	SETCOLOR C_PLANT
	PRINTS SLICED
	RESETCOLOR
CASE "林", "木"
	SETCOLOR C_FOREST1
	PRINTS SLICED
	RESETCOLOR
CASE "森"
	SETCOLOR C_FOREST2
	PRINTS SLICED
	RESETCOLOR
CASE "■", "│", "─", "┐", "┌", "└", "┘", "￣", "／", "＼", "ｘ", "目"
	SELECTCASE TFLAG:맵변환
	CASE 2
		SETCOLOR C_MUGEN
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "□", "○", "┏", "┓", "┗", "┛", "〓"
	SELECTCASE TFLAG:맵변환
	CASE 2
		SETCOLOR 0x706caa
	CASE 4, 0
		SETCOLOR C_MUGEN
	CASEELSE
		SETCOLOR C_WOOD2
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "|"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
CASE "γ", "⌒", "ヽ", "ゝ", "＿", "ノ"
	SELECTCASE TFLAG:맵변환
	CASE 4, 0
		SETCOLOR C_MUGEN
	CASEELSE
		SETCOLOR C_AQUA
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "石", "狛", "回", "┃", "┳", "┻", "┣", "┫"
	SELECTCASE TFLAG:맵변환
	CASE 3
		SETCOLOR C_WOOD2
	CASEELSE
		SETCOLOR C_STONE1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "━"
	SELECTCASE TFLAG:맵변환
	CASE 2
		SETCOLOR C_STONE1
	CASEELSE
		SETCOLOR C_WOOD2
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "☆"
	SETCOLOR C_L_GREEN
	PRINTS SLICED
	RESETCOLOR
CASE "三"
	SETCOLOR C_MUGEN
	PRINTS SLICED
	RESETCOLOR
CASE "。"
	SETCOLOR 0xec6d71
	PRINTS SLICED
	RESETCOLOR
CASE "◆", "Π"
	SETCOLOR C_MUGEN
	PRINTS SLICED
	RESETCOLOR
CASE "●"
	SETCOLOR C_TORII
	PRINTS SLICED
	RESETCOLOR
CASE "♨"
	SETCOLOR 0x59b9c6
	PRINTS SLICED
	RESETCOLOR
CASE "/", "X", "ヾ", "v", "ﾉ", "､"
	SETCOLOR C_FOREST1
	PRINTS SLICED
	RESETCOLOR
CASE "\"", "`", "ゞ", "ヽ", "y"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
;▼特殊な表示
CASE "#a", "#b", "#c", "#d", "#e", "#f", "#g", "#h", "#i", "#j", "#k", "#l", "#m", "#n", "#p"
	CALL MAP_PRINT(SLICED)
CASE "#A"
	SETCOLOR C_WINDOW
	PRINT □
	RESETCOLOR
CASE "#B"
	SETCOLOR C_MUGEN
	PRINT ■
	RESETCOLOR
CASE "#C"
	SETCOLOR C_MUGEN
	PRINT ニ
	RESETCOLOR
CASE "#D"
	SETCOLOR C_ORANGE
	PRINT ☀
	RESETCOLOR
CASE "#E"
	SETCOLOR C_YELLOW
	PRINT 🌙
	RESETCOLOR
CASE "#F"
	SETCOLOR 0x2ca9e1
	PRINT ☆
	RESETCOLOR
CASE "#G"
	SETCOLOR C_STONE1
	PRINT □
	RESETCOLOR
CASE "#H"
	SETCOLOR C_MUGEN
	PRINT ＼
	RESETCOLOR
CASE "#I"
	SETCOLOR C_MUGEN
	PRINT ｜
	RESETCOLOR
CASE "#J"
	SETCOLOR C_WOOD2
	PRINT ヽ
	RESETCOLOR
CASE "#K"
	SETCOLOR C_WOOD2
	PRINT Π
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT