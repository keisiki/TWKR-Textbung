@DRAW_MAP_BG_5(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;▼着色
CASE "森"
	SETCOLOR 0x00a4a4
	PRINTS SLICED
	RESETCOLOR
CASE "林"
	SETCOLOR C_TEAL
	PRINTS SLICED
	RESETCOLOR
CASE "木"
	SETCOLOR 0x006060
	PRINTS SLICED
	RESETCOLOR
CASE "Ω"
	SELECTCASE RAND:8
	CASE 0
		SETCOLOR 0x507ea4
	CASE 1
		SETCOLOR 0xc7dc68
	CASE 2
		SETCOLOR 0xce5242
	CASEELSE
		SETCOLOR 0xcd8c5c
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "■", "│", "─", "┐", "┌", "└", "┘", "|", "／", "＼", "┴", "┏", "━", "┓", "┗", "┛", "┃", "＝", "￣", "＿"
	SETCOLOR C_WOOD1
	PRINTS SLICED
	RESETCOLOR
CASE "\"", "`", "ゞ", "ヽ", "囲", "□", "┣", "┫", "○", "井", "●", "薪"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
CASE "/", "X", "ヾ", "v", "ﾉ", "､"
	SETCOLOR C_TEAL
	PRINTS SLICED
	RESETCOLOR
CASE "Ψ"
	SETCOLOR 0xc53d43
	PRINTS SLICED
	RESETCOLOR
CASE "ｖ"
	SETCOLOR 0x8491c3
	PRINTS SLICED
	RESETCOLOR
CASE "桜"
	SETCOLOR 0xc0a2c7
	PRINTS SLICED
	RESETCOLOR
CASE "П", "∩"
	SELECTCASE TFLAG:맵변환
	CASE 1, 0
		SETCOLOR C_STONE1
	CASEELSE
		SETCOLOR C_D_ORANGE
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "形"
	SELECTCASE TFLAG:맵변환
	CASE 2
		SETCOLOR 0xfbdac8
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
;▼特殊な表示
CASE "#a", "#b", "#c", "#d", "#e", "#f", "#g", "#h", "#i", "#j", "#k", "#l", "#m", "#n"
	CALL MAP_PRINT(SLICED)
CASE "#A"
	SETCOLOR C_WOOD2
	PRINT |y
	RESETCOLOR
CASE "#B"
	SETCOLOR C_WOOD2
	PRINT ||
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT