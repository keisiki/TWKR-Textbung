;-----------------------------------------------------------
;지점의 연결 설정
;ARG    기준지점
;ARG:1  대상지점
;RETURNF 0=이동할 수 없다, 보이지 않는다
;        1=이동할 수 있다, 보이지 않는다
;        2=이동할 수 없다, 보인다
;        3=이동할 수 있다, 보인다 (직통)
;-----------------------------------------------------------
@CAN_MOVE_2(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P201인간마을문
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P203남쪽거리)
			RETURNF 3
	CASE P202중앙광장
		SIF GROUPMATCH(ARG:1, P215꽃집, P216식료품가게, P220망루, P227야채가게)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P203남쪽거리, P204동쪽거리, P205북쪽거리, P206서쪽거리, P207용신상)
			RETURNF 3
	CASE P203남쪽거리
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P211술집, P212카페, P214주택가앞, P216식료품가게, P218일품요릿집)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P201인간마을문, P202중앙광장)
			RETURNF 3
	CASE P204동쪽거리
		SIF GROUPMATCH(ARG:1, P203남쪽거리, P205북쪽거리, P213스즈나안, P217과자가게, P219집회소, P222서당, P223대중목욕탕, P224케이네댁, P227야채가게)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202중앙광장)
			RETURNF 3
	CASE P205북쪽거리
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P206서쪽거리, P219집회소, P221히에다저택, P222서당, P224케이네댁)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202중앙광장, P207용신상)
			RETURNF 3
	CASE P206서쪽거리
		SIF GROUPMATCH(ARG:1, P205북쪽거리, P214주택가앞, P215꽃집, P220망루, P221히에다저택, P225혼숙여관, P238예탄정)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P202중앙광장)
			RETURNF 3
	CASE P207용신상
		SIF GROUPMATCH(ARG:1, P202중앙광장, P205북쪽거리)
			RETURNF 3
	CASE P211술집, P212카페
		SIF GROUPMATCH(ARG:1, P203남쪽거리)
			RETURNF 1
	CASE P213스즈나안
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P226코스즈방)
			RETURNF 1
	CASE P214주택가앞
		SIF GROUPMATCH(ARG:1, P203남쪽거리, P206서쪽거리)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P230빈방, P231반키방, P232렌코방, P233메리방, P234유키방, P235마이방)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P208라이코방, P209야츠하시방, P210벤벤방, P241빈방, P236공동변소, P237공동우물)
			RETURNF 1
	CASE P215꽃집
		SIF GROUPMATCH(ARG:1, P202중앙광장, P206서쪽거리)
			RETURNF 1
	CASE P216식료품가게
		SIF GROUPMATCH(ARG:1, P202중앙광장, P203남쪽거리)
			RETURNF 1
	CASE P217과자가게
		SIF GROUPMATCH(ARG:1, P204동쪽거리)
			RETURNF 1
	CASE P218일품요릿집
		SIF GROUPMATCH(ARG:1, P203남쪽거리)
			RETURNF 1
	CASE P219집회소
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P205북쪽거리)
			RETURNF 1
	CASE P220망루
		SIF GROUPMATCH(ARG:1, P202중앙광장, P206서쪽거리)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P201인간마을문, P203남쪽거리, P204동쪽거리, P205북쪽거리, P207용신상)
			RETURNF 2
	CASE P221히에다저택
		SIF GROUPMATCH(ARG:1, P205북쪽거리, P206서쪽거리, P229아큐방)
			RETURNF 1
	CASE P222서당
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P205북쪽거리, P224케이네댁)
			RETURNF 1
	CASE P223대중목욕탕
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P228대절욕탕)
			RETURNF 1
	CASE P224케이네댁
		SIF GROUPMATCH(ARG:1, P204동쪽거리, P205북쪽거리, P222서당)
			RETURNF 1
	CASE P225혼숙여관
		SIF GROUPMATCH(ARG:1, P206서쪽거리)
			RETURNF 1
	CASE P226코스즈방
		SIF GROUPMATCH(ARG:1, P213스즈나안)
			RETURNF 1
	CASE P227야채가게
		SIF GROUPMATCH(ARG:1, P202중앙광장, P204동쪽거리)
			RETURNF 1
	CASE P228대절욕탕
		SIF GROUPMATCH(ARG:1, P223대중목욕탕)
			RETURNF 1
	CASE P229아큐방
		SIF GROUPMATCH(ARG:1, P221히에다저택)
			RETURNF 1
	CASE P230빈방, P231반키방, P241빈방, P236공동변소, P237공동우물
		SIF GROUPMATCH(ARG:1, P214주택가앞)
			RETURNF 1
	CASE P232렌코방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P233메리방)
			RETURNF 1
	CASE P233메리방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P232렌코방)
			RETURNF 1
	CASE P234유키방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P235마이방)
			RETURNF 1
	CASE P235마이방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P234유키방)
			RETURNF 1
	CASE P208라이코방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P209야츠하시방, P210벤벤방)
			RETURNF 1
	CASE P209야츠하시방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P208라이코방, P210벤벤방)
			RETURNF 1
	CASE P210벤벤방
		SIF GROUPMATCH(ARG:1, P214주택가앞, P209야츠하시방, P208라이코방)
			RETURNF 1
	CASE P238예탄정
		SIF GROUPMATCH(ARG:1, P206서쪽거리, P239미요이방)
			RETURNF 1
	CASE P239미요이방
		SIF GROUPMATCH(ARG:1, P238예탄정)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P201인간마을문
			RETURNF 1
ENDSELECT
RETURNF 0