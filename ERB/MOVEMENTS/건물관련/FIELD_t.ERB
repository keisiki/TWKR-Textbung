@MAPBUTTON(ARGS, ARG)
SETCOLOR C_P_BLUE
SELECTCASE ARG
	CASE MAIN_MAP
		SIF !CHK_DATENOW(CFLAG:MASTER:데이트중)
			SETCOLOR 0 , 255, 0
	CASE 0
		SIF CFLAG:MASTER:데이트중 == ARG
			SETCOLOR 0 , 255, 0
	CASE 1
		SIF CFLAG:MASTER:데이트중 == ARG
			SETCOLOR 0 , 255, 0
	CASE 2
		SIF CFLAG:MASTER:데이트중 == ARG
			SETCOLOR 0 , 255, 0
	CASE 3
		SIF CFLAG:MASTER:현재위치 == 안개의호수 || CFLAG:MASTER:현재위치 == 얼음집
			SETCOLOR 0 , 255, 0
	CASE 4
		SIF CFLAG:MASTER:현재위치 >= 죽림입구 && CFLAG:MASTER:현재위치 <= 토끼동굴
			SETCOLOR 0 , 255, 0
	CASE 5
		SIF CFLAG:MASTER:현재위치 >= 숲의입구 && CFLAG:MASTER:현재위치 <= 재고의길
			SETCOLOR 0 , 255, 0
	CASE 6
		SIF CFLAG:MASTER:현재위치 >= 중유의길 && CFLAG:MASTER:현재위치 <= 삼도천
			SETCOLOR 0 , 255, 0
	CASE 7
		;SIF CFLAG:MASTER:데이트중 == ARG && CFLAG:MASTER:현재위치 != 마요이가
		;	SETCOLOR 0 , 255, 0
	CASE 8
		SIF CFLAG:MASTER:데이트중 == ARG && CFLAG:MASTER:현재위치 != 천계
			SETCOLOR 0 , 255, 0
	CASE 9
		SIF CFLAG:MASTER:데이트중 == ARG
			SETCOLOR 0 , 255, 0
	CASE 10
		SIF CFLAG:MASTER:현재위치 == 괴안통로
			SETCOLOR 0 , 255, 0
ENDSELECT
PRINTBUTTON ARGS, ARG
RESETCOLOR

