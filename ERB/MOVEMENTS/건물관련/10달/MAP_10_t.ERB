@DRAW_MAP_BG_10(SLICED)
#DIMS SLICED
#DIM CONST C_R_WOOD = 0xa73836
;▼着色
SELECTCASE SLICED
CASE "木", "林", "森"
	SETCOLOR C_FOREST1
	PRINTS SLICED
	RESETCOLOR
CASE "ｖ"
	SETCOLOR C_L_GREEN
	PRINTS SLICED
	RESETCOLOR
CASE "＃", "╋"
	SETCOLOR 0x9664ff
	PRINTS SLICED
	RESETCOLOR
CASE "桃"
	SETCOLOR C_PINK
	PRINTS SLICED
	RESETCOLOR
CASE "石"
	SETCOLOR C_STONE1
	PRINTS SLICED
	RESETCOLOR
CASE "■", "l", "＝"
	SETCOLOR C_R_WOOD
	PRINTS SLICED
	RESETCOLOR
CASE "□"
	SELECTCASE TFLAG:맵변환
	CASE 1, 0
		SETCOLOR C_WINDOW
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "●"
	SELECTCASE TFLAG:맵변환
	CASE 1, 3
		SETCOLOR C_P_PURPLE
	CASEELSE
		SETCOLOR C_R_WOOD
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "～"
	SETCOLOR C_WINDOW
	PRINTS SLICED
	RESETCOLOR
CASE "│", "─", "╷", "○", "┐", "└", "三", "|"
	SELECTCASE TFLAG:맵변환
	CASE 3
		SETCOLOR C_P_PURPLE
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "┃", "━"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
CASE "／", "γ", "⌒", "ヽ", "i", "ゝ", "_", "\"", "ｉ", "ノ", ".", "'", "￣", "｀", ",", "l", "ノ", "!", "/", "＿", "、"
	SETCOLOR 0x2ca9e1
	PRINTS SLICED
	RESETCOLOR
;▼特殊な表示
CASE "#a", "#b", "#c", "#d", "#e", "#f", "#g", "#h", "#i", "#j", "#k", "#l", "#m", "#n"
	CALL MAP_PRINT(SLICED)
CASE "#A"
	SETCOLOR C_R_WOOD
	PRINT ●
	RESETCOLOR
CASE "#B"
	SETCOLOR C_WOOD1
	PRINT □
	RESETCOLOR
CASE "#C"
	SETCOLOR C_R_WOOD
	PRINT ||
	RESETCOLOR
CASE "#D"
	SETCOLOR 0x2ca9e1
	PRINT | 
	RESETCOLOR
CASE "#E"
	SETCOLOR C_WOOD1
	PRINT ／
	RESETCOLOR
CASE "#F"
	SETCOLOR C_WOOD1
	PRINT ￣
	RESETCOLOR
CASE "#G"
	SETCOLOR C_WOOD1
	PRINT ＼
	RESETCOLOR
CASE "#H"
	SETCOLOR C_WINDOW
	PRINT □
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT