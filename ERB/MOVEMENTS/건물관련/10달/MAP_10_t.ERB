@DRAW_MAP_BG_10(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;채색 싶을 때는 이런 느낌
CASE "木","林"
	SETCOLOR 0x90EE90
	PRINTS SLICED
	RESETCOLOR
;다른 문자를 표시시키는 예
CASE "#W"
	SETCOLOR 0x90EE90
	PRINT 木
	RESETCOLOR
CASE "#E"
	SIF TALENT:MASTER:위치짐작 && COUNTCHARA:(MAXROOM) > 0 && FLAG:짐작모드 == 1
		SETCOLOR 255,100,200
	PRINT ↓
	RESETCOLOR
;보통의
CASEELSE
	PRINTS SLICED
ENDSELECT