@DRAW_MAP_BG_3(SLICED)
#DIMS SLICED
#DIM CONST C_BLOOD = 0xc85a6e
#DIM CONST C_KIRI_WATER = 0x82b4c8
SELECTCASE SLICED
;▼着色
CASE "ｖ"
	SETCOLOR C_L_GREEN
	PRINTS SLICED
	RESETCOLOR
CASE "林", "木"
	SETCOLOR C_FOREST1
	PRINTS SLICED
	RESETCOLOR
CASE "森"
	SETCOLOR C_FOREST2
	PRINTS SLICED
	RESETCOLOR
CASE "∧", "r", "ｔ", "_", "⊥", "/", "i", "!", "／", "＼", "'", ",", "i", "土", "ﾖ", ":", "ｌ", "|"
	SETCOLOR C_BLOOD
	PRINTS SLICED
	RESETCOLOR
CASE "●", "￣", "三", "l", "├", "Ⅰ", "＝"
	SETCOLOR C_BLOOD
	PRINTS SLICED
	RESETCOLOR
CASE "■", "│", "─", "┐", "┌", "└", "┘", "┬", "＿"
	SELECTCASE TFLAG:맵변환
	CASE 4
		SETCOLOR C_STONE1
	CASEELSE
		SETCOLOR C_BLOOD
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "門"
	SETCOLOR C_STONE1
	PRINTS SLICED
	RESETCOLOR
CASE "彡"
	SETCOLOR 0xaf0082
	PRINTS SLICED
	RESETCOLOR
CASE "□", "━", "┃", "┏", "┓"
	SETCOLOR C_WOOD1
	PRINTS SLICED
	RESETCOLOR
CASE "◎"
	SELECTCASE TFLAG:맵변환
	CASE 1, 0
		SETCOLOR 0xeddc44
	CASEELSE
		SETCOLOR C_PINK
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "-", "‐", "､", "｀", "丶", "´", "*", "~", "+", "～", "―", "♨"
	SETCOLOR C_KIRI_WATER
	PRINTS SLICED
	RESETCOLOR
CASE "ｒ", "、"
	SETCOLOR 0xFFFFFF
	PRINTS SLICED
	RESETCOLOR
;▼特殊な表示
CASE "#a", "#b", "#c", "#d", "#e", "#f", "#g", "#h", "#i", "#j", "#k", "#l", "#m", "#n"
	CALL MAP_PRINT(SLICED)
CASE "#3"
	RESETCOLOR
	PRINT 三
CASE "#A"
	SETCOLOR C_BLOOD
	PRINT //
	RESETCOLOR
CASE "#B"
	SETCOLOR C_STONE2
	PRINT ■
	RESETCOLOR
CASE "#C"
	SETCOLOR 0xFFFFFF
	PRINT | 
	RESETCOLOR
CASE "#D"
	SETCOLOR 0xFFFFFF
	PRINT │
	RESETCOLOR
CASE "#E"
	SETCOLOR C_KIRI_WATER
	PRINT _ 
	RESETCOLOR
CASE "#F"
	SETCOLOR C_KIRI_WATER
	PRINT ／
	RESETCOLOR
CASE "#G"
	SETCOLOR C_KIRI_WATER
	PRINT ＼
	RESETCOLOR
CASE "#H"
	SETCOLOR C_KIRI_WATER
	PRINT /*
	RESETCOLOR
CASE "#I"
	SETCOLOR 0xFFFFFF
	PRINT - 
	RESETCOLOR
CASE "#J"
	SETCOLOR C_RED
	PRINT ┃
	RESETCOLOR
CASE "#K"
	SETCOLOR C_WOOD1
	PRINT ●
	RESETCOLOR
CASE "#L"
	SETCOLOR C_BLOOD
	PRINT 】
	RESETCOLOR
CASE "#M"
	SETCOLOR C_BLOOD
	PRINT 【
	RESETCOLOR
CASE "#N"
	SETCOLOR C_STONE1
	PRINT □
	RESETCOLOR
;ふつうの
CASEELSE
	PRINTS SLICED
ENDSELECT