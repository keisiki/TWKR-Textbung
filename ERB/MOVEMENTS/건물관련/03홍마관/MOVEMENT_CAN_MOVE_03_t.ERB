;-----------------------------------------------------------
;지점의 연결 설정
;ARG    기준지점
;ARG:1  대상지점
;RETURNF 0=이동할 수 없다, 보이지 않는다
;        1=이동할 수 있다, 보이지 않는다
;        2=이동할 수 없다, 보인다
;        3=이동할 수 있다, 보인다 (직통)
;-----------------------------------------------------------
@CAN_MOVE_3(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	CASE P301정문
		SIF GROUPMATCH(ARG:1, P303수위실, P304빈오두막, P343호숫가)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302정원)
			RETURNF 3
	CASE P302정원
		SIF GROUPMATCH(ARG:1, P303수위실, P306뒤뜰, P309객실)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P301정문, P307동쪽테라스, P308서쪽테라스)
			RETURNF 3
	CASE P303수위실, P304빈오두막
		SIF GROUPMATCH(ARG:1, P301정문, P302정원, P306뒤뜰)
			RETURNF 1
	CASE P305헛간
		SIF GROUPMATCH(ARG:1, P306뒤뜰)
			RETURNF 1
	CASE P306뒤뜰
		SIF GROUPMATCH(ARG:1, P302정원, P303수위실, P304빈오두막, P305헛간, P313주방)
			RETURNF 1
	CASE P307동쪽테라스
		SIF GROUPMATCH(ARG:1, P310도서관)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302정원)
			RETURNF 3
	CASE P308서쪽테라스
		SIF GROUPMATCH(ARG:1, P309객실)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302정원)
			RETURNF 3
	CASE P309객실
		SIF GROUPMATCH(ARG:1, P302정원, P310도서관, P314식당, P315응접실, P3161층화장실, P3192층복도, P328지하복도)
			RETURNF 1
	CASE P310도서관
		SIF GROUPMATCH(ARG:1, P307동쪽테라스, P309객실, P311파츄리방, P312소악마방)
			RETURNF 1
	CASE P311파츄리방, P312소악마방
		SIF GROUPMATCH(ARG:1, P310도서관)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306뒤뜰)
			RETURNF 2
	CASE P313주방
		SIF GROUPMATCH(ARG:1, P306뒤뜰, P314식당)
			RETURNF 1
	CASE P314식당
		SIF GROUPMATCH(ARG:1, P309객실, P313주방)
			RETURNF 1
	CASE P315응접실
		SIF GROUPMATCH(ARG:1, P308서쪽테라스, P309객실)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302정원)
			RETURNF 2
	CASE P3161층화장실
		SIF GROUPMATCH(ARG:1, P309객실)
			RETURNF 1
	CASE P3192층복도
		SIF GROUPMATCH(ARG:1, P309객실, P320사쿠야방, P321빈방, P3222층화장실, P323메이드요정대기소, P326레밀리아방)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P324큰발코니)
			RETURNF 3
	CASE P320사쿠야방, P321빈방
		SIF GROUPMATCH(ARG:1, P3192층복도)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P306뒤뜰)
			RETURNF 2
	CASE P3222층화장실, P323메이드요정대기소
		SIF GROUPMATCH(ARG:1, P3192층복도)
			RETURNF 1
	CASE P324큰발코니
		SIF GROUPMATCH(ARG:1, P3192층복도)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P302정원, P301정문)
			RETURNF 2
	CASE P326레밀리아방
		SIF GROUPMATCH(ARG:1, P3192층복도)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P327작은발코니)
			RETURNF 3
	CASE P327작은발코니
		SIF GROUPMATCH(ARG:1, P326레밀리아방)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P302정원, P301정문, P324큰발코니)
			RETURNF 2
	CASE P328지하복도
		SIF GROUPMATCH(ARG:1, P309객실, P329창고, P330고용인실, P331지하실, P332탈의실)
			RETURNF 1
	CASE P329창고, P330고용인실, P331지하실
		SIF GROUPMATCH(ARG:1, P328지하복도)
			RETURNF 1
	CASE P332탈의실
		SIF GROUPMATCH(ARG:1, P328지하복도, P333대욕탕)
			RETURNF 1
	CASE P333대욕탕
		SIF GROUPMATCH(ARG:1, P332탈의실)
			RETURNF 1

	CASE P334폐양관・입구
		SIF GROUPMATCH(ARG:1, P343호숫가)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P335폐양관・객실)
			RETURNF 3
	CASE P335폐양관・객실
		SIF GROUPMATCH(ARG:1, P338폐양관・목욕탕, P339폐양관・2층복도)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P334폐양관・입구, P337폐양관・부엌)
			RETURNF 3
	CASE P337폐양관・부엌
		SIF GROUPMATCH(ARG:1, P335폐양관・객실)
			RETURNF 3
	CASE P338폐양관・목욕탕
		SIF GROUPMATCH(ARG:1, P335폐양관・객실)
			RETURNF 1
	CASE P339폐양관・2층복도
		SIF GROUPMATCH(ARG:1, P335폐양관・객실, P340루나사방, P341메를랑방, P342리리카방)
			RETURNF 1
	CASE P340루나사방, P341메를랑방, P342리리카방
		SIF GROUPMATCH(ARG:1, P339폐양관・2층복도)
			RETURNF 1

	CASE P343호숫가
		SIF GROUPMATCH(ARG:1, P347안개의호수)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
		SIF GROUPMATCH(ARG:1, P301정문, P334폐양관・입구, P344레티의움집, P345치르노의움집, P346대요정의거처)
			RETURNF 1
	CASE P344레티의움집, P345치르노의움집, P346대요정의거처
		SIF GROUPMATCH(ARG:1, P343호숫가)
			RETURNF 1
	CASE P347안개의호수
		SIF GROUPMATCH(ARG:1, P343호숫가)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P348안개의호수・바닥)
			RETURNF 1
	CASE P348안개의호수・바닥
		SIF GROUPMATCH(ARG:1, P347안개의호수)
			RETURNF 1
	CASE MAXROOM
		SIF ARG:1 == P343호숫가
			RETURNF 1
ENDSELECT
RETURNF 0