	SELECTCASE TFLAG:맵변환
	CASE 1
		SETCOLOR C_STONE1
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "┓", "┏", "┗", "┛", "□", "●", "┃", "━", "n", "_", "／", "＼", "二"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
CASE "o"
	SETCOLOR C_STONE1
	PRINTS SLICED
	RESETCOLOR
CASE "～", "~"
	SELECTCASE TFLAG:맵변환
	CASE 1, 0
		SETCOLOR 0x66cdaa
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "*", "\"", "`", "心", "´", "、", "桜"
	SETCOLOR C_PINK
	PRINTS SLICED
	RESETCOLOR
CASE "ﾛ", "目", "○", "="
	SETCOLOR C_CHIKUSHO
	PRINTS SLICED
	RESETCOLOR
CASE "ｖ"
	SETCOLOR C_PLANT
	PRINTS SLICED
	RESETCOLOR
CASE "ρ"
	SETCOLOR 0xFFFFFF
	PRINTS SLICED
	RESETCOLOR
CASE "△", "▲"
	SETCOLOR C_REICHO
	PRINTS SLICED
	RESETCOLOR
;▼特殊な表示
