@DRAW_MAP_BG_6(SLICED)
#DIMS SLICED
#DIM CONST C_CHIKUSHO = 0xb33e5c
#DIM CONST C_REICHO = 0x82cddd
#DIM CONST C_BUILDING = 0x5050a0
SELECTCASE SLICED
;▼着色
CASE "木", "林", "森"
	SETCOLOR C_TEAL
	PRINTS SLICED
	RESETCOLOR
CASE "Ψ"
	SETCOLOR 0xc53d43
	PRINTS SLICED
	RESETCOLOR
CASE "💀"
	SETCOLOR 0x8b474b
	PRINTS SLICED
	RESETCOLOR
CASE "火", "炎"
	SETCOLOR C_FLAME
	PRINTS SLICED
	RESETCOLOR
CASE "池", "卍"
	SETCOLOR C_RED
	PRINTS SLICED
	RESETCOLOR
CASE "丘"
	SETCOLOR 0xb5b538
	PRINTS SLICED
	RESETCOLOR
CASE "ヾ", "ゞ", "从", "/", "ﾂ", "y", "i", "ﾍ", "ｲ", "ﾄ"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
CASE "■"
	SELECTCASE TFLAG:맵변환
	CASE 5
		SETCOLOR C_CHIKUSHO
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
