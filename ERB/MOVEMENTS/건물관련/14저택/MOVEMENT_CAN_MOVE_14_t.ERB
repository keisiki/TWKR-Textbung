
;-----------------------------------------------------------
;지점의 연결 설정
;ARG    기준지점
;ARG:1  대상지점
;RETURNF 0=이동할 수 없다, 보이지 않는다
;        1=이동할 수 있다, 보이지 않는다
;        2=이동할 수 없다, 보인다
;        3=이동할 수 있다, 보인다(직통)
;-----------------------------------------------------------
@CAN_MOVE_14(ARG, ARG:1)
#FUNCTION
SELECTCASE ARG
	;저택 밖에서 올 때
	CASE MAXROOM
		SIF ARG:1 == 1401
			RETURNF 1
	;일층 현관
	CASE P1401현관
		SIF GROUPMATCH(ARG:1, P1413·1층통로)
			RETURNF 3
		SIF ARG:1 == MAXROOM
			RETURNF 2
	;일층 거실
	CASE P1402거실
		SIF GROUPMATCH(ARG:1, P1409뜰)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P1413·1층통로)
			RETURNF 1
	;일층 응접실
	CASE P1403응접실
		SIF GROUPMATCH(ARG:1, P1413·1층통로,P1404객실,P1405식당)
			RETURNF 1
	;일층 객실
	CASE P1404객실
		SIF GROUPMATCH(ARG:1, P1413·1층통로,P1403응접실)
			RETURNF 1
	;일층 식당
	CASE P1405식당
		SIF GROUPMATCH(ARG:1, P1413·1층통로,P1403응접실,P1406부엌)
			RETURNF 1
	;일층 부엌
	CASE P1406부엌
		SIF GROUPMATCH(ARG:1, P1413·1층통로,P1405식당)
			RETURNF 1
	;일층 탈의실
	CASE P1407탈의실
		SIF GROUPMATCH(ARG:1, P1413·1층통로,P1408욕실)
			RETURNF 1
	;일층 욕실
	CASE P1408욕실
		SIF GROUPMATCH(ARG:1, P1407탈의실)
			RETURNF 1
	;일층 안뜰
	CASE P1409뜰
		SIF GROUPMATCH(ARG:1, P1402거실,P1411창고)
			RETURNF 1
		SIF GROUPMATCH(ARG:1, P1410연못)
			RETURNF 3
        SIF GROUPMATCH(ARG:1, P1412능무대)
			RETURNF 2
	;일층 연못
	CASE P1410연못
		SIF GROUPMATCH(ARG:1, P1409뜰)
			RETURNF 3
		;능무대정도는 보일듯
		SIF GROUPMATCH(ARG:1, P1412능무대)
			RETURNF 3
	;일층 창고
	CASE P1411창고
		SIF GROUPMATCH(ARG:1, P1409뜰,P1412능무대)
			RETURNF 1
	;일층 능무대
	CASE P1412능무대
		SIF GROUPMATCH(ARG:1, P1411창고)
			RETURNF 1
		;연못정도는 보일듯
		SIF GROUPMATCH(ARG:1, P1410연못)
			RETURNF 3
        SIF GROUPMATCH(ARG:1, P1409뜰)
			RETURNF 2
	;일층 통로
	CASE P1413·1층통로
		SIF GROUPMATCH(ARG:1,P1423·2층통로,P1443·지하통로)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P1401현관,P1402거실,P1403응접실,P1404객실,P1405식당,P1406부엌,P1407탈의실)
			RETURNF 1
	;이층 각 방
	CASE P1414·201 TO P1422·2층서재
		SIF GROUPMATCH(ARG:1, P1423·2층통로)
			RETURNF 1
	;이층 통로
	CASE P1423·2층통로
		SIF GROUPMATCH(ARG:1,P1413·1층통로,P1433·3층통로)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P1414·201,P1415·202,P1416·203,P1417·204,P1418·205,P1419·206,P1420·2층휴게실,P1421·2층유희실,P1422·2층서재)
			RETURNF 1
	;삼층 각 방
	CASE P1424·301 TO P1430·3층다용도실, P1432·3층휴게실
		SIF GROUPMATCH(ARG:1, P1433·3층통로)
			RETURNF 1
	;삼층 전망대
	CASE P1431·3층전망대
		SIF GROUPMATCH(ARG:1, P1433·3층통로)
			RETURNF 1
		;전망대인데 옥외 정도는 보여야
		SIF GROUPMATCH(ARG:1, P1409뜰,P1410연못, MAXROOM)
			RETURNF 2
	;삼층 통로
	CASE P1433·3층통로
		SIF GROUPMATCH(ARG:1,P1423·2층통로)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P1424·301,P1425·302,P1426·303,P1427·304,P1428·305,P1429·306,P1430·3층다용도실,P1431·3층전망대,P1432·3층휴게실)
			RETURNF 1
		SIF GROUPMATCH(ARG:1,P1445·지하감옥)
			RETURNF 1
	;지하 각 방
	CASE P1434·B01 TO P1441·지하도장
		SIF GROUPMATCH(ARG:1, P1443·지하통로)
			RETURNF 1
	;지하 탈의실
	CASE P1442·지하탈의실
		SIF GROUPMATCH(ARG:1, P1443·지하통로,P1444·지하온천)
			RETURNF 1
	;지하 통로
	CASE P1443·지하통로
		SIF GROUPMATCH(ARG:1,P1413·1층통로)
			RETURNF 3
		SIF GROUPMATCH(ARG:1, P1434·B01,P1435·B02,P1436·B03,P1437·B04,P1438·B05,P1439·B06,P1440·지하다용도실,P1441·지하도장,P1442·지하탈의실)
			RETURNF 1
	;지하 온천
	CASE P1444·지하온천
		SIF GROUPMATCH(ARG:1, P1442·지하탈의실)
			RETURNF 1
	CASE P1445·지하감옥
		SIF GROUPMATCH(ARG:1, P1445·지하통로)
			RETURNF 1
ENDSELECT
RETURNF 0