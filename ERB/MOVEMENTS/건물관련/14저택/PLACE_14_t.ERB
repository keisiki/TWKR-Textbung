@MAP_SEARCH_REPLACEMENT_14(ARG)
#FUNCTION
SELECTCASE ARG
;;1 층 옥내
	CASE 1401 TO 1408, 1411
		RETURNF 1410
;;1층 옥외
	CASE 1409 TO 12
		RETURNF 1420
;;2층옥내
	CASE 1414 TO 1413
		RETURNF 1430
	;3층 옥내
	CASE 1424 TO 1433
		RETURNF 1440
	;지하 옥내
	CASE 1434 TO 1445
		RETURNF 1450
ENDSELECT
RETURNF 0


@MAP14_AREANAME(PLACE_ID)
#FUNCTIONS
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM PLACE_ID
IF MAIN_MAP == 14
	SIF INRANGE(PLACE_ID, 1401, 1408)||PLACE_ID == 1413
		RETURNF "%CALLNAME:MASTER%의 저택 1층"
	SIF INRANGE(PLACE_ID, 1409, 1412)
		RETURNF "%CALLNAME:MASTER%의 저택 마당"
	SIF INRANGE(PLACE_ID, 1414, 1423)
		RETURNF "%CALLNAME:MASTER%의 저택 2층"
	SIF INRANGE(PLACE_ID, 1424, 1433)
		RETURNF "%CALLNAME:MASTER%의 저택 3층"
	SIF INRANGE(PLACE_ID, 1434, 1445)
		RETURNF "%CALLNAME:MASTER%의 저택 지하"
ELSE
	SELECTCASE PLACE_ID
	CASE 1410
		RETURNF "%CALLNAME:MASTER%의 저택 1층"
	CASE 1420
		RETURNF "%CALLNAME:MASTER%의 저택 마당"
	CASE 1430
		RETURNF "%CALLNAME:MASTER%의 저택 2층"
	CASE 1440
		RETURNF "%CALLNAME:MASTER%의 저택 3층"
	CASE 1450
		RETURNF "%CALLNAME:MASTER%의 저택 지하"
	ENDSELECT
ENDIF
