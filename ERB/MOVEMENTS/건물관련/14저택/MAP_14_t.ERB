@DRAW_MAP_BG_14(SLICED)
#DIMS SLICED
SELECTCASE SLICED
;채색 싶을 때는 이런 느낌
CASE "∬","∫","\"","\'","γ","`","~","⌒","ﾞ","ヽ", "ノ", ")", ",", "ﾞ", "r",")","ﾄ","i","l","､","｡","ｒ",".","＿","_","ﾉ"
	SETCOLOR C_AQUA
	PRINTS SLICED
	RESETCOLOR
CASE "#Z"
	SIF TALENT:MASTER:위치짐작 && COUNTCHARA:(MAXROOM) > 0 && FLAG:짐작모드 == 1
		SETCOLOR 255,100,200
	PRINT ↓
	RESETCOLOR
;다른 문자를 표시시키는 예
CASE "#A","#B","#C","#D","#E", "#F","#G","#H","#I","#J","#K", "#L","#M","#N","#O","#P","#Q"
	PRINTS 저택방호칭출력(SLICED)
CASEELSE
	PRINTS SLICED
ENDSELECT

@저택방호칭출력(SLICED)
#FUNCTIONS
#DIMS SLICED
#DIMS 코드_2층 = "#A","#B","#C","#D","#E"
#DIMS 코드_3층 = "#F","#G","#H","#I","#J","#K"
#DIMS 코드_지하 = "#L","#M","#N","#O","#P","#Q"

VARSET LOCAL

IF FINDELEMENT(코드_2층, SLICED) != -1
	LOCAL = 저택2층독실배정:(FINDELEMENT(코드_2층, SLICED))
ELSEIF FINDELEMENT(코드_3층, SLICED) != -1
	LOCAL = 저택3층독실배정:(FINDELEMENT(코드_3층, SLICED))
ELSEIF FINDELEMENT(코드_지하, SLICED) != -1
	LOCAL = 저택지하독실배정:(FINDELEMENT(코드_지하, SLICED))
ENDIF

IF LOCAL > 0 ; 빈 방이 아닐 때
	LOCALS '= SUBSTRING(CALLNAME:LOCAL, 0,4)
	SELECTCASE STRLENS(LOCALS)
		CASE 1 ;반각 1글자
			LOCALS '= @" %LOCALS%  "
		CASE 2 ;전각 1글자 또는 반각 2글자
			LOCALS '= @" %LOCALS% "
		CASE 3 ;전각 1글자 + 반각 1글자 또는 반각 3글자
			LOCALS += " "
		CASE 4 ;전각 2글자 또는 반각 4글자
	ENDSELECT
	RETURNF LOCALS
ELSE
	RETURNF "독실"
ENDIF
