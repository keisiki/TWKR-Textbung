;21~30번까지의 캐릭터
;CALL		USER
;ARG		0:호출 데이터 NO, 1:이동 경향 취득시의 방NO
;RETURN		캐릭터별 호출 데이터치
;COMMENT	
;-----------------------------------------------------------
;메를랑
@CHARAMOVE_DATA_21(ARG,ARG:2)
LOCAL = 21
SELECTCASE ARG
	;이동 대상 멤버 체크
	CASE 1
		;연동 처리(리리카와 함께)
		IF TALENT:22:연모 || (TALENT:22:사모 && !TALENT:21:사모 && !TALENT:20:사모)
			CFLAG:LOCAL:현재위치 = CFLAG:22:만난위치 ?  CFLAG:22:만난위치 # SUKIMA()
		ELSEIF TALENT:21:연모 || (TALENT:21:사모 && (!TALENT:22:사모 || !TALENT:20:사모))
			CFLAG:LOCAL:현재위치 = CFLAG:21:만난위치 ?  CFLAG:21:만난위치 # SUKIMA()
		ELSE
			CFLAG:LOCAL:현재위치 = CFLAG:20:만난위치 ?  CFLAG:20:만난위치 # SUKIMA()
		ENDIF
		RETURN 0
	;이동 확률
	CASE 2
	;기상시간대
	CASE 3
		RETURN 수면시간(LOCAL)
	;이동 경향
	CASE 4
		SELECTCASE ARG:2
			;개인 방에는 못 들어간다
			CASE 15, 17, 19, 53, 55
				RETURN -100
			CASE 18, 43, 44, 51, 52
				RETURN -100
			;헛간과 저택의 3층과 지하, 및 각 개인방
			CASE 303, 304, 305, 311, 312, 318, 320, 321, 323, 325, 344, 345, 346, 348
				RETURN -100
		ENDSELECT
	;일시간대
	CASE 5
	;가는 지역(0=박1=사2=리3=호4=죽5=삼6=명7=산8=수9=지10=월)
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 3, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;출현 장소
	CASE 7
		SELECTCASE ARG:2
			CASE 21,22,23, 31,32,33,34, 61,62,65,66
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

