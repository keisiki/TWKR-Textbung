;キャラ別_各種データ
;131～140番までのキャラ
;CALL		USER
;ARG		0:呼出データNO, 1:移動傾向取得時の部屋NO
;RETURN		キャラ別呼出データ値
;COMMENT	
;-----------------------------------------------------------
;磨弓
@CHARAMOVE_DATA_131(ARG,ARG:2)
LOCAL = 131
SELECTCASE ARG
	;移動対象メンバーチェック
	CASE 1
	;移動確率
	CASE 2
	;起床時間帯
	CASE 3
		RETURN 수면시간(LOCAL)
	;移動傾向
	CASE 4
		SELECTCASE ARG:2
			;是非曲直庁、八雲邸には行かない（入り口部分は出る時用）
			CASE 610, 613, 630, 631
				RETURN -10
			CASE 610 TO 618, 630 TO 639
				RETURN -100
			;他人の家には入らない
			CASE 608, 609
				RETURN -100
			;組長宅には行かない
			CASE 646 TO 652
				RETURN -100
			;とくになし
			CASEELSE
				RETURN 0
		ENDSELECT
	;仕事時間帯
	CASE 5
	;行く地域(0=博1=寺2=里3=湖4=竹5=森6=冥7=山8=守9=地10=月）
	CASE 6
		SELECTCASE ARG:2
			CASE 2, 6
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 21, 22, 23, 27, 61, 62, 64, 68, 69
				RETURN 1
			CASEELSE
				RETURN 0
		ENDSELECT
ENDSELECT

