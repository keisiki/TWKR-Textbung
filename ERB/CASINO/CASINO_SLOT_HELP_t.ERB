	페이지수 %= 전페이지수
	RESTART
CASE 1
	페이지수 ++
	페이지수 %= 전페이지수
	RESTART
CASE 666
	전페이지수 = 17
	RESTART
CASEELSE
	RETURN
ENDSELECT

