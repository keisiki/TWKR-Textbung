				ELSE
					NEXTCOLOR = C_MAP_夕焼け_通常
				ENDIF
			CASE 6, 7, 11
				NEXTCOLOR = C_MAP_霧_昼
			CASE 4, 5, 13, 14, 15
				NEXTCOLOR = C_MAP_雨_昼
			CASE 8, 9, 10, 12
				NEXTCOLOR = C_MAP_雪_昼
			CASEELSE
				NEXTCOLOR = C_MAP_夕焼け_通常
		ENDSELECT
	
	;PM8~AM4　夜・深夜
	CASE 0 TO 239, IS >= 1200
		SELECTCASE TIME:5
			CASE 0, 1, 2
				NEXTCOLOR = C_MAP_晴れ_夜
			CASE 6, 7, 11
				NEXTCOLOR = C_MAP_霧_夜
