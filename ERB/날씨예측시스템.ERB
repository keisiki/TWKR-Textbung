;天候傾向によって寒気・暖気による補正方法が変化する
SELECTCASE NOW_WEATHER_CONTROL
	;1 春・秋の天気(基本)
	CASE 1
		気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2
	;2 春・秋の天気(温暖前線接近時)
	CASE 2
		気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2
	;3 春・秋の天気(暖域時)
	CASE 3
		気流補正 = POWER_WARM_AIR * 2
	;4 春・秋の天気(寒冷前線通過時)
	CASE 4
		気流補正 = (0 - POWER_COLD_AIR) * 2
	;5 春・秋の天気(寒冷渦接近時)
	CASE 5
		気流補正 = (0 - POWER_COLD_AIR) * 2
	;6 夏の天気(基本)
	CASE 6
		気流補正 = (POWER_WARM_AIR - 3) * 3
	;7 夏の天気(夕立接近時)
	CASE 7
		SELECTCASE CAN_CUMULUS:0
			CASE 1,2,3
				気流補正 = (POWER_WARM_AIR - 3) * 3 - (POWER_COLD_AIR / 2)
			CASE 4,5
				気流補正 = 0 - POWER_COLD_AIR
		ENDSELECT
	;8 夏の天気(夕立通過後)
	CASE 8
		SELECTCASE CAN_CUMULUS:0
			CASE 1,2,3
				気流補正 = (POWER_WARM_AIR - 3) * 3 - POWER_COLD_AIR
			CASE 4,5
				気流補正 = (0 - POWER_COLD_AIR) * 2
		ENDSELECT
	;9 冬の天気(基本)
	CASE 9
		気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2
	;10 冬の天気(日本海低気圧接近時)
	CASE 10
		気流補正 = POWER_WARM_AIR * 2
	;11 冬の天気(日本海低気圧通過時)
	CASE 11
		気流補正 = (0 - POWER_COLD_AIR) * 2
	;12 冬の天気(南岸低気圧接近時)
	CASE 12
		気流補正 = (0 - POWER_COLD_AIR) * 2
	;13 冬の天気(寒冷渦接近時)
	CASE 13
		気流補正 = (0 - POWER_COLD_AIR) * 2
	;14 梅雨・秋雨の天気(基本)
	CASE 14
		IF POWER_WARM_AIR >= POWER_COLD_AIR
			気流補正 = POWER_WARM_AIR
		ELSE
			気流補正 = 0 - POWER_COLD_AIR
		ENDIF
	;15 梅雨・秋雨(梅雨末期・秋雨+台風接近)・先駆降雨帯発生時の天気
	CASE 15
		気流補正 = POWER_WARM_AIR * 2
	;16 台風接近時の天気(強風域)
	CASE 16
		SELECTCASE DAY:2
			CASE 2
				気流補正 = (POWER_WARM_AIR - 3) * 3 + IS_TYPHOON:1
			CASEELSE
				気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2 + IS_TYPHOON:1
		ENDSELECT
	;21 台風接近時の天気(暴風域・最接近)
	CASE 21
		SELECTCASE DAY:2
			CASE 2
				気流補正 = (POWER_WARM_AIR - 3) * 3 + IS_TYPHOON:1
			CASEELSE
				気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2 + IS_TYPHOON:1
		ENDSELECT
	;17 台風接近時の天気(台風の目)
	CASE 17
		SELECTCASE DAY:2
			CASE 2
				気流補正 = (POWER_WARM_AIR - 3) * 3 + IS_TYPHOON:1
			CASEELSE
				気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2 + IS_TYPHOON:1
		ENDSELECT
	;18 霧の天気
	CASE 18
		SELECTCASE DAY:2
			CASE 2
				気流補正 = (POWER_WARM_AIR - 3) * 3
			CASEELSE
				気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2
		ENDSELECT
	;19 疑似好天
	CASE 19
		気流補正 = (0 - POWER_COLD_AIR) * 15 / 10
	;20 集中豪雨時の天気
	CASE 20
		気流補正 = (POWER_WARM_AIR - POWER_COLD_AIR) * 2
ENDSELECT

現在マップ = GET_MAPID(CFLAG:MASTER:현재위치)
高度補正 = 0
現在お出かけマップ = GET_MAP_REPLACEMENT(CFLAG:MASTER:현재위치)

;マップごとに高度による気温低下の補正値を決定
SELECTCASE 現在マップ
	CASE 0
		高度補正 = 0
		
