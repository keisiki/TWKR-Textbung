;-------------------------------------------------
;天候システム上の時間帯に応じた名前を返す
;-------------------------------------------------
@GET_NAME_WEATHER_TIMEZOME(ARG = -1)
#FUNCTIONS

SIF ARG == -1
	ARG = TIME

SELECTCASE ARG
	;AM0~3
	CASE 0 TO 179
		;RETURNF "未明"
		RETURNF "미명"
	;AM3~6
