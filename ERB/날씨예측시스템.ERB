	CASE 5
		高度補正 = 0
		
		補正温度 = 基礎気温 + 気流補正 * 10 - 高度補正 * 10
	CASE 6
		高度補正 = 10
		
		補正温度 = 基礎気温 + 気流補正 * 10 - 高度補正 * 10
	CASE 7
		高度補正 = 6
		
		補正温度 = 基礎気温 + 気流補正 * 10 - 高度補正 * 10
	CASE 8
		IF (MAIN_MAP == 現在マップ && 現在お出かけマップ == 天界) || (MAIN_MAP != 現在マップ && CFLAG:MASTER:현재위치 == 天界)
			高度補正 = 20
			
			補正温度 = 基礎気温 + 気流補正 * 10 - 高度補正 * 10
		ELSE
			高度補正 = 12
			
			補正温度 = 基礎気温 + 気流補正 * 10 - 高度補正 * 10
		ENDIF
	;灼熱地獄跡は40℃,その他地底は20℃
	CASE 9
		IF (MAIN_MAP == 現在マップ && 現在お出かけマップ == 灼熱地獄跡) || (MAIN_MAP != 現在マップ && CFLAG:MASTER:현재위치 == 灼熱地獄跡)
			補正温度 = 400
		ELSE
			補正温度 = 200
		ENDIF
	CASE 10
		高度補正 = 0
		
		補正温度 = 基礎気温 + 気流補正 * 10 - 高度補正 * 10
ENDSELECT

;屋内では20℃に近づくように気温が上下する
;屋外では日射がある場合、気温上昇
日射補正 = 0
IF OUTROOF(CFLAG:MASTER:현재위치)
	SELECTCASE TIME:5
		CASE 0,1
			SELECTCASE TIME:0
				CASE 360 TO 599, 960 TO 1079
					日射補正 += 15
				CASE 600 TO 959
					日射補正 += 20
			ENDSELECT
			
			SELECTCASE DAY:2
				CASE 1,3
					日射補正 += 10
				CASE 2
					日射補正 += 15
				CASE 4
					日射補正 += 10
			ENDSELECT
		CASE 2
			日射補正 += 5
	ENDSELECT
	補正温度 += 日射補正
ELSE
	SELECTCASE 補正温度
		CASE IS <= 250
			建造物補正 = 50
			
			補正温度 = MIN(200, 補正温度 + 建造物補正)
		CASEELSE
			建造物補正 = -40
			
			補正温度 = MAX(250, 補正温度 + 建造物補正)
	ENDSELECT
ENDIF

;チルノがいると気温低下
IF CFLAG:MASTER:현재위치 == CFLAG:14:현재위치
	補正温度 -= 100
ENDIF

;風呂では30℃に近づくように気温が上昇する
IF BATHCHECK(CFLAG:MASTER:현재위치) && 補正温度 < 300
	建造物補正 = 100
	
	補正温度 = MIN(300, 補正温度 + 建造物補正)
ENDIF

;炬燵では25℃に近づくように気温が上昇する
;炬燵は気温10℃未満で出す
IF 코타츠가있는장소(CFLAG:MASTER:현재위치) && 補正温度 < 100
	建造物補正 = 100
	
	補正温度 = MIN(250, 補正温度 + 建造物補正)
ENDIF

RETURNF 補正温度

