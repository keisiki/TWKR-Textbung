;*********************************************************************************************
;カウンターコマンド：엉덩이를 쓰다듬는다
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOMF25(ARG)

	;몸의자세の変更
	;=============================
	CALL TOUCH_RESET_T(ARG)

	;ソース計算および内部処理系統
	;=============================
	SOURCE:ARG:유혹 += 100
	SOURCE:ARG:도발 += 300
	SOURCE:ARG:치욕 += 200

	;マスターカウンター制御
	;=============================
	;なし

RETURN 1

;*********************************************************************************************
;カウンター実行可否出力関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ABLE25(ARG)



RETURN 1

;*********************************************************************************************
;カウンター実行判定値算出関数
;ARG = 対象のキャラ番号
;*********************************************************************************************
@CCOM_ORDER25(커맨드번호, ARG, 기본치)
#DIM 커맨드번호
#DIM 기본치
#DIMS 카운터타입

	;카운터타입設定
	;=============================
	카운터타입 = 소프트

	;実行判定値算出式
	;=============================
	LOCAL = 기본치 + CFLAG:ARG:적극성 + CFLAG:ARG:질

	;카운터타입別計算処理 実質実行判定処理を含む
	;=============================
	LOCAL *= COUNTER_TYPE(카운터타입, ARG)

RETURN LOCAL
