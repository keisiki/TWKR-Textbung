;────────────────────────────────────
;절정계의 처리 등
;────────────────────────────────────
;-------------------------------------------------
;절정 처리
;-------------------------------------------------
@ORGASM_ADD(ARG)
#DIM 합계절정수
#DIM 초조하게해중
#DIM MultipleEc
#DIM Multiplier

VARSET LOCAL
초조하게해중 = 0
LOCAL = ((CUP:ARG:쾌C + PALAM:ARG:쾌C) >= PALAMLV:4) + (CUP:ARG:쾌V + PALAM:ARG:쾌V >= PALAMLV:4) + (CUP:ARG:쾌A + PALAM:ARG:쾌A >= PALAMLV:4) + (CUP:ARG:쾌B + PALAM:ARG:쾌B >= PALAMLV:4) + (CUP:ARG:쾌M + PALAM:ARG:쾌M >= PALAMLV:4)
SELECTCASE LOCAL
	CASE 5
		LOCAL:1 = 5000
	CASE 4
		LOCAL:1 = 3000
	CASE 3
		LOCAL:1 = 2000
	CASE 2
		LOCAL:1 = 1500
	CASE 1
		LOCAL:1 = 1000
ENDSELECT

;시인성의 문제로 남겨 둔다
C = 0
V = 0
A = 0
B = 0
M = 0
;절정 C
;무심코 절정
CALL 절정강도판정(ARG, 0, LOCAL:1)
C = RESULT
;절정 V
CALL 절정강도판정(ARG, 1, LOCAL:1)
V = RESULT

;절정 A
CALL 절정강도판정(ARG, 2, LOCAL:1)
A = RESULT

;절정 B
CALL 절정강도판정(ARG, 3, LOCAL:1)
B = RESULT
	
;절정 M
CALL 절정강도판정(ARG, 4, LOCAL:1)
M = RESULT
IF TEQUIP:ARG:애태우기모드
	IF 초조하게해발생(ARG, C + V + A + B + M)
		EX:ARG:C절정직전 += C
		EX:ARG:V절정직전 += V
		EX:ARG:A절정직전 += A
		EX:ARG:B절정직전 += B
		EX:ARG:M절정직전 += M
		TCVAR:ARG:초조하게해회수 ++
		초조하게해중 = 1
	ENDIF
ENDIF
SIF 초조하게해중
	GOTO JIRASI
IF CUP:ARG:쾌C
	C += EX:ARG:C절정직전
	EX:ARG:C절정직전 = 0
ENDIF
IF CUP:ARG:쾌V
	V += EX:ARG:V절정직전
	EX:ARG:V절정직전 = 0
ENDIF
IF CUP:ARG:쾌A
	A += EX:ARG:A절정직전
	EX:ARG:A절정직전 = 0
ENDIF
IF CUP:ARG:쾌B
	B += EX:ARG:B절정직전
	EX:ARG:B절정직전 = 0
ENDIF
IF CUP:ARG:쾌M
	M += EX:ARG:M절정직전
	EX:ARG:M절정직전 = 0
ENDIF
;-------------------------------------------------
;절정시의 추가 처리
;-------------------------------------------------
NOWEX:ARG:TotalEX = C + V + A + B + M 
MultipleEc = MIN(C, 1) + MIN(V, 1) + MIN(A, 1) + MIN(B, 1) + MIN(M, 1) - 1
POWER Multiplier, 2, MultipleEc
; PRINTFORML {C + V + A + B + M} ec {MultipleEc} {Multiplier}


IF C && V && A && B && M
	C *= 16
	V *= 16
	A *= 16
	B *= 16
	M *= 16
	NOWEX:ARG:오중절정 = 1
ELSEIF C && V && A && B
	C *= 8
	V *= 8
	A *= 8
	B *= 8
	NOWEX:ARG:사중절정 = 1
ELSEIF C && V && A && M
	C *= 8
	V *= 8
	A *= 8
	M *= 8
	NOWEX:ARG:사중절정 = 2
ELSEIF C && V && B && M
	C *= 8
	V *= 8
	B *= 8
	M *= 8
	NOWEX:ARG:사중절정 = 3
ELSEIF C && A && B && M
	C *= 8
	A *= 8
	B *= 8
	M *= 8
	NOWEX:ARG:사중절정 = 4
ELSEIF V && A && B && M
	V *= 8
	A *= 8
	B *= 8
	M *= 8
	NOWEX:ARG:사중절정 = 5
ELSEIF C && V && A
	C *= 4
	V *= 4
	A *= 4
	NOWEX:ARG:삼중절정 = 1
ELSEIF B && V && A
	B *= 4
	V *= 4
	A *= 4
	NOWEX:ARG:삼중절정 = 2
ELSEIF C && B && A
	C *= 4
	B *= 4
	A *= 4
	NOWEX:ARG:삼중절정 = 3
ELSEIF C && V && B
	C *= 4
	V *= 4
	B *= 4
	NOWEX:ARG:삼중절정 = 4
ELSEIF C && V && M
	C *= 4
	V *= 4
	M *= 4
	NOWEX:ARG:삼중절정 = 5
ELSEIF B && V && M
	B *= 4
	V *= 4
	M *= 4
	NOWEX:ARG:삼중절정 = 6
ELSEIF A && V && M
	A *= 4
	V *= 4
	M *= 4
	NOWEX:ARG:삼중절정 = 7
ELSEIF B && C && M
	C *= 4
	B *= 4
	M *= 4
	NOWEX:ARG:삼중절정 = 8
ELSEIF A && B && M
	A *= 4
	B *= 4
	M *= 4
	NOWEX:ARG:삼중절정 = 9
ELSEIF C && A && M
	C *= 4
	A *= 4
	M *= 4
	NOWEX:ARG:삼중절정 = 10
ELSEIF C && V
	C *= 2
	V *= 2
	NOWEX:ARG:이중절정 = 1
ELSEIF C && A
	C *= 2
	A *= 2
	NOWEX:ARG:이중절정 = 2
ELSEIF V && A
	V *= 2
	A *= 2
	NOWEX:ARG:이중절정 = 3
ELSEIF C && B
	C *= 2
	B *= 2
	NOWEX:ARG:이중절정 = 4
ELSEIF V && B
	V *= 2
	B *= 2
	NOWEX:ARG:이중절정 = 5
ELSEIF A && B
	A *= 2
	B *= 2
	NOWEX:ARG:이중절정 = 6
ELSEIF A && M
	A *= 2
	M *= 2
	NOWEX:ARG:이중절정 = 7
ELSEIF V && M
	V *= 2
	M *= 2
	NOWEX:ARG:이중절정 = 8
ELSEIF C && M
	C *= 2
	M *= 2
	NOWEX:ARG:이중절정 = 9
ELSEIF B && M
	B *= 2
	M *= 2
	NOWEX:ARG:이중절정 = 10
ENDIF


IF (C + V + A + B + M) > 0
	TCVAR:ARG:초조하게해회수 = 0
	TEQUIP:ARG:애태우기모드 = 0
ENDIF
합계절정수 = C + V + A + B + M
;시간각인
IF FLAG:70 && ARG
	IF  TALENT:ARG:스침 == 1
		IF 합계절정수 >= 4 && MARK:ARG:시간각인 < 3
			TCVAR:ARG:시간각인취득용 = 3
		ELSEIF 합계절정수 >= 2 && MARK:ARG:시간각인 < 2
			TCVAR:ARG:시간각인취득용 = 2
		ELSEIF  합계절정수 >= 1 && MARK:ARG:시간각인 < 1
			TCVAR:ARG:시간각인취득용 = 1
		ENDIF
	ELSE
		IF 합계절정수 >= 16 && MARK:ARG:시간각인 < 3
			TCVAR:ARG:시간각인취득용 = 3
		ELSEIF 합계절정수 >= 8 && MARK:ARG:시간각인 < 2
			TCVAR:ARG:시간각인취득용 = 2
		ELSEIF  합계절정수 >= 1 && MARK:ARG:시간각인 < 1
			TCVAR:ARG:시간각인취득용 = 1
		ENDIF
	ENDIF
ELSE
;쾌락각인의 처리
;절정의 부위와 강도의 합계가 일정 이상으로 각인을 취득한다
;합계절정수가 8이상으로 쾌락각인 3에 상당
	IF  TALENT:ARG:스침 == 1
		IF 합계절정수 >= 4 && MARK:ARG:쾌락각인 <= 3
			TCVAR:ARG:쾌락강도 = 3
		;합계절정수가 5이상으로 쾌락각인 2에 상당
		ELSEIF 합계절정수 >= 2 && MARK:ARG:쾌락각인 < 2
			TCVAR:ARG:쾌락강도 = 2
		;합계절정수가 2이상으로 쾌락각인 1에 상당
		ELSEIF 합계절정수 >= 1 && MARK:ARG:쾌락각인 < 1
			TCVAR:ARG:쾌락강도 = 1
		ELSE
			TCVAR:ARG:쾌락강도 = 0
		ENDIF
	ELSEIF 합계절정수 >= 24 && MARK:ARG:쾌락각인 <= 3
		TCVAR:ARG:쾌락강도 = 3
	;합계절정수가 12이상으로 쾌락각인 2에 상당
	ELSEIF 합계절정수 >= 12 && MARK:ARG:쾌락각인 < 2
		TCVAR:ARG:쾌락강도 = 2
	;합계절정수가 2이상으로 쾌락각인 1에 상당
	ELSEIF 합계절정수 >= 2 && MARK:ARG:쾌락각인 < 1
		TCVAR:ARG:쾌락강도 = 1
	ELSE
		TCVAR:ARG:쾌락강도 = 0
	ENDIF
ENDIF
IF FLAG:70 == 1
ELSE

IF C
	SOURCE:ARG:노출 += 500 * C
	SOURCE:ARG:굴종 += 200 * C * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:반감 += (40 - (CFLAG:ARG:만져도돼 * 35)) * C * (CFLAG:ARG:우후후 ?  1 # 3)
	DOWNBASE:ARG:체력 += 20
	DOWNBASE:ARG:기력 += 10
ENDIF
IF V
	SOURCE:ARG:욕정 += 800 * V
	SOURCE:ARG:복종 += 500 * V * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:노출 += 700 * V
	SOURCE:ARG:굴종 += 400 * V * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:반감 += (80 - (CFLAG:ARG:만져도돼 * 75)) * V * (CFLAG:ARG:우후후 ?  1 # 3)
	DOWNBASE:ARG:체력 += 30
	DOWNBASE:ARG:기력 += 20
ENDIF
IF A
	SOURCE:ARG:욕정 += 500 * A
	SOURCE:ARG:복종 += 700 * A * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:노출 += 900 * A
	SOURCE:ARG:굴종 += 500 * A * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:반감 += (120 - (CFLAG:ARG:만져도돼 * 115)) * A * (CFLAG:ARG:우후후 ?  1 # 3)
	DOWNBASE:ARG:체력 += 40
	DOWNBASE:ARG:기력 += 20
ENDIF
IF B
	SOURCE:ARG:노출 += 500 * B
	SOURCE:ARG:굴종 += 200 * B * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:반감 += (50 - (CFLAG:ARG:만져도돼 * 45)) * B * (CFLAG:ARG:우후후 ?  1 # 3)
	DOWNBASE:ARG:체력 += 20
	DOWNBASE:ARG:기력 += 10
ENDIF
IF M
	SOURCE:ARG:사랑 += 1500 * M * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	SOURCE:ARG:복종 += 1000 * M * (SYNCED_ORGASM(ARG) ? 120 # 100) / 100
	DOWNBASE:ARG:체력 += 10
	DOWNBASE:ARG:기력 += 5
ENDIF
IF C && V && A && B && M
	DOWNBASE:ARG:체력 -= 80
	DOWNBASE:ARG:기력 -= 70
ELSEIF (C && V && A && B) || (C && V && A && M) || (B && V && A && M) || (C && B && A && M) || (C && V && B && M)
	DOWNBASE:ARG:체력 -= 40
	DOWNBASE:ARG:기력 -= 30

ELSEIF (C && V && A) || (B && V && A) || (C && B && A) || (C && V && B) || (C && V && M) || (B && V && M) || (C && B && M) || (C && A && M) || (V && A && M) || (B && A && M)
	DOWNBASE:ARG:체력 -= 20
	DOWNBASE:ARG:기력 -= 10
ENDIF
ENDIF
;NOWEX에 데이터를 넣는다(절정시 말에 사용한다)
NOWEX:ARG:C절정 = C
NOWEX:ARG:V절정 = V
NOWEX:ARG:A절정 = A
NOWEX:ARG:B절정 = B
NOWEX:ARG:M절정 = M
;절정회수를 늘린다
EX:ARG:C절정 += C
EX:ARG:V절정 += V
EX:ARG:A절정 += A
EX:ARG:B절정 += B
EX:ARG:M절정 += M

;시간정지중 혹은 수면중이라면, 무자각절정회수를 늘린다(다만, 당신을 제외하다)
IF FLAG:70 > 0 || CFLAG:ARG:수면 && ARG
	EX:ARG:무자각C절정 += C
	EX:ARG:무자각V절정 += V
	EX:ARG:무자각A절정 += A
	EX:ARG:무자각B절정 += B
	EX:ARG:무자각M절정 += M
ENDIF

;절정경험을 늘린다
EXP:ARG:절정경험 += 합계절정수

IF TALENT:ARG:스침 == 1 && 합계절정수
	IF ABL:ARG:C감각 < 3 && C
		ABL:ARG:C감각 ++
		CALL COLORMESSAGE(@"%CALLNAME:ARG%의 C감각이 {ABL:ARG:C감각} 가 되었다.",C_YELLOW,2)
	ENDIF
	IF ABL:ARG:B감각 < 3 && B
		ABL:ARG:B감각 ++
		CALL COLORMESSAGE(@"%CALLNAME:ARG%의 B감각이 {ABL:ARG:B감각} 가 되었다.",C_YELLOW,2)
	ENDIF
	IF ABL:ARG:V감각 < 3 && V
		ABL:ARG:V감각 ++
		CALL COLORMESSAGE(@"%CALLNAME:ARG%의 V감각이 {ABL:ARG:V감각} 가 되었다.",C_YELLOW,2)
	ENDIF
	IF ABL:ARG:A감각 < 3 && A
		ABL:ARG:A감각 ++
		CALL COLORMESSAGE(@"%CALLNAME:ARG%의 A감각이 {ABL:ARG:A감각} 가 되었다.",C_YELLOW,2)
	ENDIF
	IF ABL:ARG:M감각 < 3 && M
		ABL:ARG:M감각 ++
		CALL COLORMESSAGE(@"%CALLNAME:ARG%의 M감각이 {ABL:ARG:M감각} 가 되었다.",C_YELLOW,2)
	ENDIF
	IF MAX(GETPALAMLV(PALAM:욕정,5), 합계절정수) > ABL:ARG:욕망 + 1
		ABL:ARG:욕망 ++
		CALL COLORMESSAGE(@"%CALLNAME:ARG%의 욕망이 {ABL:ARG:욕망} 가 되었다.",C_YELLOW,2)
	ENDIF
ENDIF

;체위별 절정경험
IF TEQUIP:ARG:50 > -1
	LOCAL:3 = TEQUIP:ARG:체위
ELSEIF TEQUIP:ARG:51 > -1
	LOCAL:3 = TEQUIP:ARG:체위 + 5
ELSE
	LOCAL:3 = 0
ENDIF
SIF LOCAL:3
	EXP:ARG:(LOCAL:3 + 100) += 1

;절정경험의 추가, 무자각만 당신에게 가산되지 않는다
SIF !SHIRAHU(ARG) && ARG
	EXP:ARG:무자각절정경험 += C + V + A + B + M
EXP:ARG:C절정경험 += C
EXP:ARG:V절정경험 += V
EXP:ARG:A절정경험 += A
EXP:ARG:B절정경험 += B
EXP:ARG:M절정경험 += M

;비디오
IF TEQUIP:ARG:28
	IF NOWEX:ARG:8
		LOCALS = O2/O3/O4/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:7
		LOCALS = O2/O3/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:6
		LOCALS = O2/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:0 == 2
		LOCALS = C/C/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:0 == 1
		LOCALS = C/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:1 == 2
		LOCALS = V/V/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:1 == 1
		LOCALS = V/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:2 == 2
		LOCALS = A/A/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:2 == 1
		LOCALS = A/
		TSTR:1 += LOCALS
	ENDIF
	IF NOWEX:ARG:3 == 2
		LOCALS = B/B/
		TSTR:1 += LOCALS
	ELSEIF NOWEX:ARG:3 == 1
		LOCALS = B/
		TSTR:1 += LOCALS
	ENDIF
ENDIF
;絶頂時、利尿剤orおもらし癖で漏らす
IF FLAG:오줌유효 > 0 && MultipleEc > 0 && ( TCVAR:이뇨제 > 0 || TALENT:오줌싸개버릇 > 0 )
	NOWEX:ARG:방뇨 = MultipleEc
	EX:ARG:방뇨 += MultipleEc
	EXP:ARG:방뇨경험 += Multiplier
	;獲得経験値*5 だけ利尿剤の持続時間を短縮
	TCVAR:이뇨제 = MAX( TCVAR:이뇨제 - (Multiplier * 5) , 0)
ENDIF

$JIRASI
;IF 초조하게해중


RETURN 1

@MESSAGE_PALAMCNG_B2
IF NOWEX:오중절정
	IF !GETBIT(CFLAG:3, 2)
		EXP:이상경험 ++
		SETBIT CFLAG:3, 2
	ENDIF
	PRINTL 오중절정
	PRINTL (각각 16배의 구슬을 얻을 수 있습니다)
ELSEIF NOWEX:사중절정
	PRINTL 사중절정
	SELECTCASE NOWEX:사중절정
		CASE 1
			PRINTL C＆V＆A＆B절정
		CASE 2
			PRINTL C＆V＆A＆M절정
		CASE 3
			PRINTL C＆V＆B＆M절정
		CASE 4
			PRINTL C＆A＆B＆M절정
		CASE 5
			PRINTL V＆A＆B＆M절정
	ENDSELECT
	PRINTL (각각 8배의 구슬을 얻을 수 있습니다)
ELSEIF NOWEX:삼중절정
	SELECTCASE NOWEX:삼중절정
		CASE 1
			PRINTL C＆V＆A절정
		CASE 2
			PRINTL B＆V＆A절정
		CASE 3
			PRINTL C＆B＆A절정
		CASE 4
			PRINTL C＆V＆B절정
		CASE 5
			PRINTL C＆V＆M절정
		CASE 6
			PRINTL B＆V＆M절정
		CASE 7
			PRINTL A＆V＆M절정
		CASE 8
			PRINTL C＆B＆M절정
		CASE 9
			PRINTL A＆B＆M절정
		CASE 10
			PRINTL C＆A＆M절정
	ENDSELECT
	PRINTL (각각 4배의 구슬을 얻을 수 있습니다)
ELSEIF NOWEX:이중절정
	SELECTCASE NOWEX:이중절정
		CASE 1
			PRINTL C＆V절정
		CASE 2
			PRINTL C＆A절정
		CASE 3
			PRINTL V＆A절정
		CASE 4
			PRINTL C＆B절정
		CASE 5
			PRINTL V＆B절정
		CASE 6
			PRINTL A＆B절정
		CASE 7
			PRINTL A＆M절정
		CASE 8
			PRINTL V＆M절정
		CASE 9
			PRINTL C＆M절정
		CASE 10
			PRINTL B＆M절정
	ENDSELECT
	PRINTL (각각 2배의 구슬을 얻을 수 있습니다)
ELSEIF NOWEX:B절정
	IF NOWEX:B절정 == 3
		PRINTL 초강 절정 B
	ELSEIF NOWEX:B절정 == 2
		PRINTL 강절정 B
	ELSE
		PRINTL 절정 B
	ENDIF
ELSEIF NOWEX:A절정
	IF NOWEX:A절정 == 3
		PRINTL 초강 절정 A
	ELSEIF NOWEX:A절정 == 2
		PRINTL 강절정 A
	ELSE
		PRINTL 절정 A
	ENDIF
ELSEIF NOWEX:V절정
	IF NOWEX:V절정 == 3
		PRINTL 초강 절정 V
	ELSEIF NOWEX:V절정 == 2
		PRINTL 강절정 V
	ELSE
		PRINTL 절정 V
	ENDIF
ELSEIF NOWEX:C절정
	IF NOWEX:C절정 == 3
		PRINTL 초강 절정 C
	ELSEIF NOWEX:C절정 == 2
		PRINTL 강절정 C
	ELSE
		PRINTL 절정 C
	ENDIF
ELSEIF NOWEX:M절정
	IF NOWEX:M절정 == 3
		PRINTL 초강 절정 M
	ELSEIF NOWEX:M절정 == 2
		PRINTL 강절정 M
	ELSE
		PRINTL 절정 M
	ENDIF
ENDIF

;절정에 의한 욕망 LV업
LOCAL = 0
SIF NOWEX:C절정 >= 2 || NOWEX:B절정 >= 2 || NOWEX:V절정 || NOWEX:A절정
	LOCAL = 1
SIF NOWEX:V절정 >= 2 || NOWEX:A절정 >= 2 || NOWEX:M절정 >= 1
	LOCAL = 3
SIF (NOWEX:C절정 >= 2 || NOWEX:B절정 >= 2) && NOWEX:이중절정
	LOCAL = 3
SIF NOWEX:삼중절정
	LOCAL = 5
SIF NOWEX:사중절정
	LOCAL = 7
SIF NOWEX:오중절정
	LOCAL = 10
;자제심이 있으면(자) 1내린다
SIF TALENT:자제심
	LOCAL -= 1

IF ABL:욕망 < LOCAL
	ABL:욕망 = LOCAL
	PRINTFORML 그리고 욕망이 LV{LOCAL}가 되었다
	;욕망의 상승에 의한[억압]의 소멸을 체크
	CALL YOKUBO_UP_CHECK
ENDIF
PRINTL 
;-------------------------------------------------
