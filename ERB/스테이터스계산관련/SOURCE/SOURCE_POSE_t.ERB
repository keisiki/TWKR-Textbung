	;파이즈리
	CASE 82
		IF TEQUIP:ARG:P접촉부위 == 6 || TEQUIP:ARG:P접촉부위 == 7
			CALL 삽입해제(ARG)
		ENDIF
		CALL TOUCH_SET(SET_FROM_B,SET_P,ARG)
	;가랑이
	CASE 83
		IF TEQUIP:ARG:P접촉부위 == 6 || TEQUIP:ARG:P접촉부위 == 7
