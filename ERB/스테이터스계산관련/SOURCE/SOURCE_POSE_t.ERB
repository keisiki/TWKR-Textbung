;────────────────────────────────────
;애무, 성교등의 계속, 해제, 처리
;────────────────────────────────────
@SHOW_TOUCH
;(1=페니스2=클리토리스 3=지 4=구 5=흉 6=질 7=애널 8=다리)
FOR LOCAL, 1, GET_TARGETNUM() + 1
	SIF TARGET:LOCAL <= 0
		CONTINUE
	SELECTCASE TEQUIP:(TARGET:LOCAL):P접촉부위
		CASE 3
			PRINT 수음한다 
		CASE 4
			PRINT 펠라한다 
		CASE 6
			PRINT 역강간 
		CASE 7
			PRINT 역애널 
		CASE 8
			PRINT 풋잡한다 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):C접촉부위
		CASE 1
			PRINT 가랑이 
		CASE 3
			PRINT 클리애무 
		CASE 4
			PRINT 쿠니 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):손가락접촉부위
		CASE 1
			PRINT 수음 
		CASE 5
			PRINT 가슴주무르기 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):입접촉부위
		CASE 1
			PRINT 펠라 
		CASE 2
			PRINT 쿠니 시킨다 
		CASE 4
			PRINT 키스 
		CASE 7
			PRINT 애널 봉사 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):B접촉부위
		CASE 1
			PRINT 파이즈리 
		CASE 3
			PRINT 가슴애무 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):V접촉부위
		CASE 1
			PRINT 성교 
		CASE 3
			PRINT 손가락삽입 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):A접촉부위
		CASE 1
			PRINT 애널 섹스 
		CASE 3
			PRINT 애널애무 
		CASE 4
			PRINT 애널빨기 
	ENDSELECT
	SELECTCASE TEQUIP:(TARGET:LOCAL):다리접촉부위
		CASE 1
			PRINT 풋잡 
	ENDSELECT
	PRINTFORML (%CALLNAME:(TARGET:LOCAL)%)
NEXT
PRINTL

@TOUCH_SUCCESSION(ARG)
VARSET LOCAL
SELECTCASE SELECTCOM
	;애무손가락과 입을 리셋
	CASE 0
		CALL TOUCH_RESET_M(SET_YUBI)
		SIF !TEQUIP:ARG:계속키스
			CALL TOUCH_RESET_M(SET_M)
		;본래는 2개소 동시이지만, 시스템적으로는 한 개소 밖에 무리예요로, 고육지책
		IF !RAND:2
			CALL TOUCH_SET(SET_FROM_C,SET_YUBI,ARG)
		ELSE
			CALL TOUCH_SET(SET_FROM_B,SET_YUBI,ARG)
		ENDIF
	;쿠니 성교 해제해 대상의 C에 입을 세트
	CASE 1
		CALL 삽입해제(ARG)
		CALL TOUCH_RESET_M(SET_YUBI)
		IF GETBIT(TALENT:ARG:성별,1)
			CALL TOUCH_SET(SET_FROM_P,SET_M,ARG)
		ELSE
			CALL TOUCH_SET(SET_FROM_C,SET_M,ARG)
		ENDIF
	;펠라하는 성교 해제해 대상의 C에 입을 세트
	CASE 2
		;성교 해제
		CALL 삽입해제(ARG)
		CALL TOUCH_SET(SET_FROM_P,SET_M,ARG)
	;지삽
	CASE 3
		IF TEQUIP:ARG:바이브
		;손가락 리셋
			CALL TOUCH_RESET_M(SET_YUBI)
		ELSE
			;손가락을 대상의 V에 세트
			CALL TOUCH_SET(SET_FROM_V,SET_YUBI,ARG)
		ENDIF
	;애널빨기
	CASE 4
		;성교 해제해 입을 대상의 A에 세트
		CALL 삽입해제(ARG)
		CALL TOUCH_RESET_M(SET_YUBI)
		CALL TOUCH_SET(SET_FROM_A,SET_M,ARG)
	;애널애무
	CASE 5
		IF TEQUIP:ARG:애널바이브 || TEQUIP:ARG:애널비즈 || TEQUIP:ARG:관장
			;손가락 리셋
			CALL TOUCH_RESET_M(SET_YUBI)
		ELSE
			CALL TOUCH_SET(SET_FROM_A,SET_YUBI,ARG)
		ENDIF
	;가슴애무
	CASE 6
		CALL TOUCH_SET(SET_FROM_B,SET_YUBI,ARG)
	;유두고문
	CASE 7
		CALL TOUCH_SET(SET_FROM_B,SET_YUBI,ARG)
	;음부개장
	CASE 8
		IF TEQUIP:ARG:A접촉부위 == 1 && TEQUIP:ARG:체위 == 2
			TEQUIP:ARG:체위 = 1
		ELSEIF TEQUIP:ARG:A접촉부위 == 1 && TEQUIP:ARG:체위 == 5
			TEQUIP:ARG:체위 = 4
		ELSEIF TEQUIP:ARG:A접촉부위 != 1
			CALL TOUCH_RESET_T(ARG)
		ENDIF
	;자위
	CASE 9
	;유두빨기
	CASE 10
		CALL TOUCH_SET(SET_FROM_B,SET_M,ARG)
;--------------------------------------
	CASE 11
		CALL TOUCH_SET(SET_FROM_B,SET_M,ARG)
;--------------------------------------
	;가슴주무르기
	CASE 12
		CALL TOUCH_SET(SET_FROM_B,SET_YUBI,ARG)
	;애널개장
	CASE 13
		IF TEQUIP:ARG:V접촉부위 == 1 && TEQUIP:ARG:체위 == 1
			TEQUIP:ARG:체위 = 2
		ELSEIF TEQUIP:ARG:V접촉부위 == 1 && TEQUIP:ARG:체위 == 4
			TEQUIP:ARG:체위 = 5
		ELSEIF TEQUIP:ARG:V접촉부위 != 1
			CALL TOUCH_RESET_T(ARG)
		ENDIF
	;클리애무
	CASE 15
		;指を対象のＣにセット
		CALL TOUCH_SET(SET_FROM_C,SET_YUBI,ARG)
	;키스한다
	CASE 19,20
		CALL TOUCH_SET(SET_FROM_M,SET_M,ARG)
	;아무것도하지않는다
	CASE 21
	;권유 접수
	CASE 22
	;로터
	CASE 40
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:P접촉부위 = 0
	;E마사지기
	CASE 41
		CALL TOUCH_SET(0,0,0, SET_P)
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:P접촉부위 = 0
	;클리캡
	CASE 42
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:P접촉부위 = 0
	;오나홀
