			CALL 삽입해제(ARG)
		ENDIF
		CALL TOUCH_SET(0,0,0,1)
	;足コキ
	CASE 85
		IF TEQUIP:ARG:P접촉부위 == 6 || TEQUIP:ARG:P접촉부위 == 7
			CALL 삽입해제(ARG)
		ENDIF
		CALL TOUCH_SET(SET_FROM_LEG,SET_P,ARG)
	;진공펠라
	CASE 88
		IF TEQUIP:ARG:P접촉부위 == 6 || TEQUIP:ARG:P접촉부위 == 7
			CALL 삽입해제(ARG)
		ENDIF
		CALL TOUCH_SET(SET_FROM_M,SET_P,ARG)
	;애널애무된다
	CASE 90
		CALL TOUCH_SET(SET_FROM_YUBI,SET_A,ARG)
	;애널빨기된다
	CASE 91
		IF TEQUIP:ARG:P접촉부위 == 6 || TEQUIP:ARG:P접촉부위 == 7
			CALL 삽입해제(ARG)
		ENDIF
		CALL TOUCH_SET(SET_FROM_M,SET_A,ARG)
	;A정상위된다
	CASE 92
		CALL TOUCH_SET(SET_FROM_P,SET_A,ARG)
		TEQUIP:MASTER:체위 = 1
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:V접촉부위 = 0
	;A후배위된다
	CASE 93
		CALL TOUCH_SET(SET_FROM_P,SET_A,ARG)
		TEQUIP:MASTER:체위 = 2
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:V접촉부위 = 0
	;A기승위한다
	CASE 94
		CALL TOUCH_SET(SET_FROM_P,SET_A,ARG)
		TEQUIP:MASTER:체위 = 3
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:V접촉부위 = 0
	;스팬킹
	CASE 100
		CALL TOUCH_RESET_M(SET_YUBI)
	;편
	CASE 101
		CALL TOUCH_RESET_M(SET_YUBI)
	;초
	CASE 102
		CALL TOUCH_RESET_M(SET_YUBI)
	;침
	CASE 103
		CALL TOUCH_RESET_M(SET_YUBI)
	;눈가리개
	CASE 104
		CALL TOUCH_RESET_M(SET_YUBI)
	;젖스팬킹
	CASE 108
		CALL TOUCH_RESET_M(SET_YUBI)
	;커닐링구스강제
	CASE 120
		CALL TOUCH_SET(SET_FROM_M,SET_C,ARG)
	;짝맞추기놀이
	CASE 121
		CALL TOUCH_SET(SET_FROM_C,SET_C,ARG)
	;수음한다
	CASE 124
