		TEQUIP:MASTER:C접촉부위 = 0
		TEQUIP:MASTER:P접촉부위 = 0
	;오나홀(위)
	CASE 743
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:MASTER:C접촉부위 = 0
		TEQUIP:MASTER:P접촉부위 = 0
	;바이브(위)
	CASE 744
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:MASTER:V접촉부위 = 0
	;애널바이브(위)
	CASE 745
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:MASTER:A접촉부위 = 0
	;애널비즈(위)
	CASE 746
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:MASTER:A접촉부위 = 0
	;유두캡(위)
	CASE 747
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:MASTER:B접촉부위 = 0
	;착유기(위)
	CASE 748
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:MASTER:B접촉부위 = 0
	;유두로터(위)
	CASE 749

	CASE IS < 200
		CALL TOUCH_SET(0,0,0,1)
	CASE IS > 500
		CALL TOUCH_SET(0,0,0,1)
	;청소
	CASE 410
		CALL TOUCH_SET(0,0,0,1)
ENDSELECT


;MASTER와 TARGET의 접촉부위를 설정한다
@TOUCH_SET(ARG,ARG:1,ARG:2,ARG:3)
;ARG TARGET의 접촉부위(101=페니스 102=클리토리스 103=지 104=구 105=흉 106=질 107=애널 108=다리)
;ARG:1 MASTER의 접촉부위(1=페니스2=클리토리스 3=지 4=구 5=흉 6=질 7=애널 8=다리)
;ARG:2 대상의 캐릭터 번호
;ARG:3 진이라면 리셋 한다
VARSET LOCAL

IF ARG:3
	FOR LOCAL,1,CHARANUM
		FOR LOCAL:1,1,9
		;COM60~70의 경우, 입은 리셋 하지 않는다
			SIF LOCAL == ARG:2 && LOCAL:1 == 4 && TFLAG:3 >= 60 && TFLAG:3 <= 70
				CONTINUE
			TEQUIP:LOCAL:(100 + LOCAL:1) = 0
		NEXT
		;내친 김에 체위도 리셋
		TEQUIP:LOCAL:체위 = 0
	NEXT
ENDIF
SIF ARG <= 100
	RETURN 0
FOR LOCAL,1,CHARANUM
	FOR LOCAL:1,1,9
		SIF TEQUIP:LOCAL:(100 + LOCAL:1) == ARG:1
			TEQUIP:LOCAL:(100 + LOCAL:1) = 0
	NEXT
NEXT
TEQUIP:(ARG:2):ARG = ARG:1
;계속계 커맨드를 해제

IF MASTER_POSE(SET_M,SET_M) != ARG:2 && TEQUIP:(ARG:2):계속키스 && SELECTCOM != 0
	FOR LOCAL,1,CHARANUM
		TEQUIP:LOCAL:계속키스 = 0
	NEXT
	PRINTL ＜키스 해제＞
ENDIF

IF MASTER_POSE(SET_B,SET_YUBI) != ARG:2 && TEQUIP:(ARG:2):가슴주무르기
	FOR LOCAL,1,CHARANUM
		TEQUIP:LOCAL:가슴주무르기 = 0
	NEXT
	PRINTL ＜가슴주무르기 해제＞
ENDIF

IF MASTER_POSE(SET_B,SET_M) != ARG:2 && TEQUIP:(ARG:2):유두빨기 && SELECTCOM != 6 && SELECTCOM != 12
	FOR LOCAL,1,CHARANUM
		TEQUIP:LOCAL:유두빨기 = 0
	NEXT
	PRINTL ＜유두빨기 해제＞
ENDIF


;MASTER의 접촉부위를 리셋 한다
@TOUCH_RESET_M(ARG)
;ARG MASTER의 접촉부위(1=페니스2=클리토리스 3=지 4=구 5=흉 6=질 7=애널 8=다리)
FOR LOCAL, 1, GET_TARGETNUM() + 1
	SIF TARGET:LOCAL <= 0
		CONTINUE
	FOR LOCAL:1,1,9
		SIF TEQUIP:(TARGET:LOCAL):(100 + LOCAL:1) == ARG
				TEQUIP:(TARGET:LOCAL):(100 + LOCAL:1) = 0
	NEXT
NEXT

;TARGET의 접촉부위를 리셋 한다
@TOUCH_RESET_T(ARG)
FOR LOCAL,1,9
	TEQUIP:ARG:(100 + LOCAL) = 0
NEXT

@MASTER_POSE(ARG,ARG:1,ARG:2)
;ARG MASTER의 부위를 점유중의 TARGET:X의 부위(1=페니스2=클리토리스 3=지 4=구 5=흉 6=질 7=애널 8=다리)
;ARG 0이라면 누가 점유 하고 있을까 돌려준다
;ARG:1 MASTER의 부위(1=페니스2=클리토리스 3=지 4=구 5=흉 6=질 7=애널 8=다리)
;ARG:2 진이라면 전턴의 이력을 본다
;반환값 MASTER의 부위를 점유중의 TARGET:X가 있다면 그 캐릭터 번호를 돌려준다
;MASTER_POSE(SET_M, SET_P, 1)라면 전턴( 제3 인수)에 누군가가 MASTER의 페니스( 제2 인수)를 입( 제1 인수)으로 점유 하고 있었을 경우, 그 캐릭터 번호를 돌려준다
;ARG로 MASTER의 ARG:1에 접하고 있는 캐릭터를 돌려준다는 것
#FUNCTION
VARSET LOCAL
IF !ARG
	FOR LOCAL,1,CHARANUM
		SIF CFLAG:LOCAL:현재위치 != CFLAG:MASTER:현재위치
			CONTINUE
		FOR LOCAL:1,1,9
			SIF TEQUIP:LOCAL:(100 + LOCAL:1) == ARG:1
				RETURNF LOCAL
		NEXT
	NEXT
ENDIF
ARG:2 = ARG:2 ? 1 # 0
FOR LOCAL, 1, GET_TARGETNUM() + 1
	SIF TARGET:LOCAL <= 0
		CONTINUE
	SIF TEQUIP:(TARGET:LOCAL):(100 + ARG + 10 * ARG:2) == ARG:1
		RETURNF TARGET:LOCAL
NEXT

@삽입해제(ARG)
SIF TEQUIP:ARG:V접촉부위 == 1
	TEQUIP:ARG:V접촉부위 = 0
SIF TEQUIP:ARG:A접촉부위 == 1
	TEQUIP:ARG:A접촉부위 = 0
TEQUIP:ARG:C접촉부위 = 0
TEQUIP:ARG:P접촉부위 = 0
TEQUIP:ARG:입접촉부위 = 0
TEQUIP:ARG:V섹스 = -1
TEQUIP:ARG:A섹스 = -1 
TEQUIP:ARG:체위 = 0
TCVAR:ARG:오폭삽입플래그 = 0
