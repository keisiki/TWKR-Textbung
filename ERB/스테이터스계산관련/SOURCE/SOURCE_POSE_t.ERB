		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:C접촉부위 = 0
		TEQUIP:ARG:P접촉부위 = 0
	;바이브
	CASE 44
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:V접촉부위 = 0
	;애널바이브
	CASE 45
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:A접촉부위 = 0
	;애널비즈
	CASE 46
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:A접촉부위 = 0
	;유두캡
	CASE 47
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:B접촉부위 = 0
	;착유기
	CASE 48
		CALL TOUCH_RESET_M(SET_YUBI)
		TEQUIP:ARG:B접촉부위 = 0
	;유두로터
	CASE 49
		CALL TOUCH_RESET_M(SET_YUBI)
	;정상위
