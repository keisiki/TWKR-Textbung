;FileName_SOURCE1.ERB ------------------------------ Rev1.00
;고통의 소스 처리
;CALL		USER
;ARG		캐릭터 NO
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@SOURCE_고통(ARG)
VARSET LOCAL
LOCAL = SOURCE:ARG:고통
LOCAL:1 = SOURCE:ARG:고통
LOCAL:2 = SOURCE:ARG:고통 / 20

;마조끼
SELECTCASE ABL:ARG:마조끼
	CASE 0
		TIMES LOCAL:1 , 0.00
	CASE 1
		TIMES LOCAL:1 , 0.10
	CASE 2
		TIMES LOCAL:1 , 0.20
	CASE 3
		TIMES LOCAL:1 , 0.30
	CASE 4
		TIMES LOCAL:1 , 0.45
	CASE 5
		TIMES LOCAL:1 , 0.60
	CASE 6
		TIMES LOCAL:1 , 0.75
	CASE 7
		TIMES LOCAL:1 , 0.90
	CASE 8
		TIMES LOCAL:1 , 1.20
	CASE 9
		TIMES LOCAL:1 , 1.50
	CASEELSE
		TIMES LOCAL:1 , 3.00
ENDSELECT

;난쟁이 체형
IF TALENT:ARG:체형 < -2
	TIMES LOCAL , 2.00
	TIMES LOCAL:1 , 0.75
ENDIF

;조교자가【새드】
SIF TALENT:PLAYER:새드
	TIMES LOCAL:1 , 2.50

;겁쟁이
SIF TALENT:ARG:담력 < 0
	TIMES LOCAL:2 , 2.00

;마조마음이 있으면(자) PALAM:욕정도 오른다
;그 때 조교자가【새드】라면 효과적
;고통에 응해 공포의 소스가 증가한다
IF D_고통변동:ARG:D_변동치 != 0
	LOCAL = LOCAL * (100 + D_고통변동:ARG:D_변동치) / 100
ENDIF
IF D_욕정변동:ARG:D_변동치 != 0
	LOCAL:1 = LOCAL:1 * (100 + D_욕정변동:ARG:D_변동치) / 100
ENDIF
IF D_공포변동:ARG:D_변동치 != 0
	LOCAL:2 = LOCAL:2 * (100 + D_공포변동:ARG:D_변동치) / 100
ENDIF
CUP:ARG:고통 += LOCAL
CUP:ARG:욕정 += LOCAL:1
CUP:ARG:공포 += LOCAL:2

