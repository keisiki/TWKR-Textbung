LOCAL = SOURCE:ARG:유혹
LOCAL:1 = SOURCE:ARG:유혹
;상대 주도
IF ARG != MASTER && CFLAG:ARG:우후후 == 2
	TIMES LOCAL, 2.00
	TIMES LOCAL:1, 2.00
ENDIF
CUP:ARG:우월 += LOCAL
LOCAL = LOCAL * (100 + GET_REVISION(ABL:PLAYER:욕망, 500, 20)) / 100
CUP:PLAYER:욕정 += LOCAL
LOCAL:1 = LOCAL:1 * (100 + GET_REVISION(ABL:PLAYER:순종, 500, 5)) / 100
CUP:PLAYER:공순 += LOCAL:1

