;FileName_SOURCE1.ERB ------------------------------ Rev1.00
;노출의 소스 처리
;CALL		USER
;ARG		캐릭터 NO
;RETURN		VOID
;COMMENT	
;-----------------------------------------------------------
@SOURCE_노출(ARG)
VARSET LOCAL
;수줍음과 수치심 없음은 여기서 처리
SIF TALENT:ARG:수치심 > 0
	TIMES SOURCE:ARG:노출 , 2.00
SIF TALENT:ARG:수치심 < 0
	TIMES SOURCE:ARG:노출 , 0.50
SIF WITH_MOB()
	TIMES SOURCE:ARG:노출 , 1.50
;윤활 업의 50%를 노출의 소스에 가세한다
;다만 액체 추가의 소스의 만큼은 당긴다
SOURCE:ARG:노출 += (CUP:ARG:윤활 - SOURCE:ARG:액체) / 2

LOCAL = SOURCE:ARG:노출
;상대 주도
SIF ARG != MASTER && CFLAG:ARG:우후후 == 2
	TIMES LOCAL, 0.75
;ABL:노출벽을 본다
SELECTCASE ABL:ARG:노출벽
	CASE 0
		TIMES LOCAL , 0.00
	CASE 1
		TIMES LOCAL , 0.10
	CASE 2
		TIMES LOCAL , 0.20
	CASE 3
		TIMES LOCAL , 0.40
	CASE 4
		TIMES LOCAL , 0.60
	CASE 5
		TIMES LOCAL , 0.80
	CASE 6
		TIMES LOCAL , 1.00
	CASE 7
		TIMES LOCAL , 1.20
	CASE 8
		TIMES LOCAL , 1.40
	CASE 9
		TIMES LOCAL , 1.60
	CASEELSE
		TIMES LOCAL , 2.00
ENDSELECT
IF D_욕정변동:ARG:D_변동치 != 0
	LOCAL = LOCAL * (100 + D_욕정변동:ARG:D_변동치) / 100
ENDIF
CUP:ARG:욕정 += LOCAL

LOCAL = SOURCE:ARG:노출

;PALAM:치정을 본다
;부끄러움을 느끼면 느낄수록 너무 부끄럽지 않게 된다
IF PALAM:ARG:치정 < PALAMLV:1
	TIMES LOCAL , 1.00
ELSEIF PALAM:ARG:치정 < PALAMLV:2
	TIMES LOCAL , 0.90
ELSEIF PALAM:ARG:치정 < PALAMLV:3
	TIMES LOCAL , 0.80
ELSEIF PALAM:ARG:치정 < PALAMLV:4
	TIMES LOCAL , 0.70
ELSEIF PALAM:ARG:치정 >= PALAMLV:4
	TIMES LOCAL , 0.60
ENDIF

SIF LOCAL < 0
	LOCAL = 0
IF D_치정변동:ARG:D_변동치 != 0
	LOCAL = LOCAL * (100 + D_치정변동:ARG:D_변동치) / 100
ENDIF
CUP:ARG:치정 += LOCAL

