            SETCOLOR 0X181818
        PRINTFORM [{LOCAL,3,RIGHT}] %DISH_NAME,20,LEFT%　　　
        LOCAL:1++
        IF LOCAL:1 == 3
            LOCAL:1 = 0
            PRINTL
        ENDIF
    ENDIF
