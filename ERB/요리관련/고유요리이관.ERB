@Y_고유재료소비표(ARG)
DRAWLINE
PRINTFORM %ARG의% 고유 요리 : %Y_고유요리이관정보(ARG)%
IF LEARNED_MENU:ARG <= 100
    PRINTFORM 　숙련도:{LEARNED_MENU:ARG}％
ENDIF
PRINTL
TRYCALLFORM Y_고유요리{ARG}()
FOR LOCAL,0,22
    IF Y_고유재료:LOCAL > 0
        IF Y_고유재료:LOCAL > 40
            PRINTFORM [%ITEMNAME:(Y_고유재료:LOCAL)%]
        ELSE
            IF LOCAL >= 11
                SELECTCASE Y_고유재료:LOCAL
                    CASE 1
                        PRINTFORM [소금]
                    CASE 2
                        PRINTFORM [후추]
                    CASE 3
                        PRINTFORM [설탕]
                    CASE 4
                        PRINTFORM [식초]
                    CASE 5
                        PRINTFORM [된장]
                    CASE 6
                        PRINTFORM [기름]
                    CASE 7
                        PRINTFORM [간장]
                    CASE 8
                        PRINTFORM [고춧가루]
                    CASE 9
                        PRINTFORM [젓갈]
                    CASE 10
                        PRINTFORM [버터]
                    CASE 11
                        PRINTFORM [강황]
                ENDSELECT
                LOCAL:1 += 100
            ELSE
                SELECTCASE Y_고유재료:LOCAL
                    CASE 1
                        PRINTFORM [국수]
                    CASE 2
                        PRINTFORM [빵]
                    CASE 3
                        PRINTFORM [육수]
                    CASE 4
                        PRINTFORM [밀가루]
                    CASE 5
                        PRINTFORM [계란]
                    CASE 6
                        PRINTFORM [콩]
                    CASE 7
                        PRINTFORM [양파]
                    CASE 8
                        PRINTFORM [마늘]
                    CASE 9
                        PRINTFORM [파프리카]
                    CASE 10
                        PRINTFORM [치즈]
                ENDSELECT
                LOCAL:1 += 500
            ENDIF
        ENDIF
        PRINTL
    ENDIF
NEXT
PRINTFORML 소비되는 비용 : {LOCAL:1}\\
CALL Y_고유요리재료소비(1)
IF RESULT == 1
    PRINTFORML [제작 가능]
ELSE
    PRINTFORML [제작 불가]
ENDIF
DRAWLINE
RETURN RESULT

@Y_고유요리재료소비(ARG)
#DIM DYNAMIC 소비금
#DIM DYNAMIC 소비할아이템,30
;ARG : 1 = 가능성여부만
LOCAL:1 = 0
;주재료
IF Y_고유재료:0 <= 40
    ;기본제공품
    IF Y_고유재료:0 != -1
        소비금 += 500
    ENDIF
ELSE
    IF ITEM:(Y_고유재료:0)
        소비할아이템:(LOCAL:1) = ITEM:(Y_고유재료:0)
        LOCAL:1++
    ELSE
        RETURN -1
    ENDIF
ENDIF
;부재료
FOR LOCAL,1,10
    IF Y_고유재료:LOCAL <= 40
        ;기본제공품
        IF Y_고유재료:LOCAL != -1
            소비금 += 500
        ENDIF
    ELSE
        IF ITEM:(Y_고유재료:LOCAL)
            소비할아이템:(LOCAL:1) = ITEM:(Y_고유재료:LOCAL)
            LOCAL:1++
        ELSE
            RETURN -1
        ENDIF
    ENDIF
NEXT
;양념
FOR LOCAL,11,20
    IF Y_고유재료:LOCAL <= 40
        ;기본제공품
        IF Y_고유재료:LOCAL != -1
            소비금 += 100
        ENDIF
    ELSE
        IF ITEM:(Y_고유재료:LOCAL)
            소비할아이템:(LOCAL:1) = ITEM:(Y_고유재료:LOCAL)
            LOCAL:1++
        ELSE
            RETURN -1
        ENDIF
    ENDIF
NEXT
;첨가물
IF Y_고유재료:21 > 0
    IF ITEM:(Y_고유재료:21)
        소비할아이템:(LOCAL:1) = ITEM:(Y_고유재료:21)
        LOCAL:1++
    ELSE
        RETURN -1
    ENDIF
ENDIF
IF Y_고유재료:22 > 0
    IF ITEM:(Y_고유재료:22)
        소비할아이템:(LOCAL:1) = ITEM:(Y_고유재료:22)
        LOCAL:1++
    ELSE
        RETURN -1
    ENDIF
ENDIF

IF ARG != 1
    SIF MONEY < 소비금
        RETURN -1
    FOR LOCAL,0,LOCAL:1
        SIF 소비할아이템:(LOCAL) > 0
            ITEM:(소비할아이템:(LOCAL)) --
    NEXT
    MONEY -= 소비금
ENDIF
RETURN 1

