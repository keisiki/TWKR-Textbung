@EXIST_의상세트93
#LOCALSIZE 1
#LOCALSSIZE 1

@의상세트93(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "이름"
	CALLF MAKE_STR(V_NAME, "마리사 침실 세트")
CASE "캐릭터 특수 의상 세트"
	CALLF MAKE_INT(V_NAME, 1)
;여기서부터 의상 설정계
CASE "상반신속옷1"
	CALLF MAKE_INT(V_NAME, 13)
CASE "OVERRIDE_UNDERWEAR_TOP"
	CALLF MAKE_INT(V_NAME, 1)
CASE "양말"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "가터 스타킹"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("양말", "양말"))
	ENDIF
ENDSELECT

