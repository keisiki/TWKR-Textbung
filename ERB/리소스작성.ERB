;もう作ってある/リソース未導入なら帰る
SIF SPRITECREATED("別顔_裸_通常_30") || !SPRITECREATED("30_bodyparts_00")
	RETURN
;素体ベース生成
DO 
	L_COUNT ++
	IF PAT:L_COUNT == "開始"
		G_ID = 0
		CALL 画像合成(G_ID, "30_BASE")
		CONTINUE
	ELSEIF PAT:L_COUNT == "合成"
		SPRITECREATE @"30_%素体:TEMP%", G_ID
		TEMP ++
		CONTINUE
	ENDIF
	CALL 画像合成(G_ID, @"30_bodyparts_%PAT:L_COUNT%")
