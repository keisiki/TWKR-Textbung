;手っ取り早く別顔をつくるための関数
@COMPOSE_SPRITE_PRESET(C_ID, BODY, EXPRESSION, 別顔ID)
#DIM C_ID
#DIM G_ID
#DIM TEMP
#DIM 素体COUNT
#DIM 表情COUNT
#DIMS 表情,15
#DIMS EXPRESSION
#DIMS 素体,10
#DIMS BODY
#DIM 別顔ID
;BODYに素体名を登録
SIF BODY == ""
	BODY = 別顔_裸_/別顔_服_
SPLIT BODY, "/", 素体

;EXPRESSIONに表情パターンを登録
SIF EXPRESSION == ""
	EXPRESSION = 通常/笑顔/怒り/発情/トロ顔/イキ顔
SPLIT EXPRESSION, "/", 表情

;もう作ってある/リソース未導入なら帰る
SIF SPRITECREATED(@"%素体:0%笑顔_%TOSTR(C_ID)%")
	RETURN

;立ち絵画像合成
;素体に各表情を重ねてスプライトとして登録していく
FOR 素体COUNT, 0, 10
	SIF 素体:素体COUNT == ""
		BREAK
	FOR 表情COUNT, 1, 15
		SIF 表情:表情COUNT == ""
			BREAK
		;only load alternate image sets if they're in use
	IF SUBSTRINGU(素体:素体COUNT, 0, 2) == "別顔"
		LOCALS '= SUBSTRINGU(素体:素体COUNT, 2, 1)
		IF LOCALS == "_" && CFLAG:C_ID:대체적용 != 1
			CONTINUE
		ELSEIF ISNUMERIC(LOCALS) && TOINT(LOCALS) != CFLAG:C_ID:대체적용
			CONTINUE
		ENDIF
	ENDIF
		G_ID = 0
		CALL 画像合成(G_ID, @"%素体:素体COUNT%通常_%TOSTR(C_ID)%")
		IF 別顔ID
			CALL 画像合成(G_ID, @"%TOSTR(C_ID)%別顔%TOSTR(別顔ID)%_表情%表情:表情COUNT%")
		ELSE
			CALL 画像合成(G_ID, @"%TOSTR(C_ID)%別顔_表情%表情:表情COUNT%")
		ENDIF
		SPRITECREATE @"%素体:素体COUNT%%表情:表情COUNT%_%TOSTR(C_ID)%", G_ID
	NEXT
NEXT
