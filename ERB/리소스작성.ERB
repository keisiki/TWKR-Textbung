	識別 = %\@ STRFIND(素体:素体COUNT, "顔") > -1 ? # _s \@%
	FOR 表情COUNT, 0, 14
		G_ID = 0
		CALL 画像合成(G_ID, @"30_%素体:素体COUNT%")
		CALL 画像合成(G_ID, @"30_faceparts%識別%_%表情NO:表情COUNT%")
		SPRITECREATE @"%素体:素体COUNT%_%表情:表情COUNT%_30", G_ID
