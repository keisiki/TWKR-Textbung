;-------------------------------------------------
;함수명 :GET_TALENTNAME
;개요:소질 나토리 이득 함수
;인수 :ARG:0…TALENT 등록 번호
;　　　:ARG:1…TALENT의 내용
;반환값:소질명
;비고:문중 함수
;소질의 이름을 돌려준다.
;-------------------------------------------------
@GET_TALENTNAME(ARG, ARG:1)
#FUNCTIONS
SELECTCASE ARG
	;처녀
	CASE 0
		IF ARG:1 == -1
			RETURNF "무자각비처녀"
		ELSEIF ARG:1 
			RETURNF "처녀"
		ENDIF
	;동정
	CASE 1
		SIF !ARG:1
			RETURNF "동정"
	;성별
	CASE 2
		IF ARG:1 == 3
			RETURNF "후타나리"
		ELSEIF ARG:1 == 2
			RETURNF "남자"
		ENDIF
	;키스미경험
	CASE 6
		IF ARG:1 == -1
			RETURNF "키스미경험?"
		ELSEIF ARG:1 == 1
			RETURNF "키스미경험"
		ENDIF
	;;연인
	CASE 7
		IF GETBIT(ARG:1,1)
			RETURNF "아내"
		ELSEIF ARG:1 == 1
			RETURNF "연인"
		ENDIF
		
	;담력
	CASE 10
		IF ARG:1 == -1
			RETURNF "겁장이"
		ELSEIF ARG:1 == 1
			RETURNF "다부짐"
		ENDIF
	;태도
	CASE 11
		IF ARG:1 == -1
			RETURNF "솔직함"
		ELSEIF ARG:1 == 1
			RETURNF "반항적"
		ENDIF
	;응답
	CASE 12
		IF ARG:1 == -1
			RETURNF "얌전함"
		ELSEIF ARG:1 == 1
			RETURNF "거만함"
		ENDIF
	;자존심
	CASE 13
		IF ARG:1 == -1
			RETURNF "자존심 낮음"
		ELSEIF ARG:1 == 1
			RETURNF "자존심 높음"
		ENDIF
	;기분
	CASE 15
		IF ARG:1 == -1
			RETURNF "기분 나쁨"
		ELSEIF ARG:1 == 1
			RETURNF "기분 좋음"
		ENDIF
	;용모
	CASE 16
		IF ARG:1 == -1
			RETURNF "추악"
		ELSEIF ARG:1 == 1
			RETURNF "미형"
		ENDIF
	;연령
	CASE 17
		IF ARG:1 == -1
			RETURNF "아이"
		ELSEIF ARG:1 == 1
			RETURNF "중년"
		ELSEIF ARG:1 == 2
			RETURNF "노인"
		ENDIF
	;성적흥미
	CASE 23
		IF ARG:1 == -1
			RETURNF "보수적"
		ELSEIF ARG:1 == 1
			RETURNF "호기심"
		ENDIF
	;명랑／음침,
	CASE 24
		IF ARG:1 == -1
			RETURNF "음침함"
		ELSEIF ARG:1 == 1
			RETURNF "명랑함"
		ENDIF
	;정조 관념
	CASE 30
		IF ARG:1 == -1
			RETURNF "정조무관심"
		ELSEIF ARG:1 == 1
			RETURNF "정조관념"
		ENDIF
	;자기애
	CASE 31
		IF ARG:1 == -1
			RETURNF "억압"
		ELSEIF ARG:1 == 1
			RETURNF "해방"
		ENDIF
	;수치심
	CASE 33
		IF ARG:1 == -1
			RETURNF "수치심없음"
		ELSEIF ARG:1 == 1
			RETURNF "수줍음"
		ENDIF
	;통각
	CASE 40
		IF ARG:1 == -1
			RETURNF "고통에 강함"
		ELSEIF ARG:1 == 1
			RETURNF "고통에 약함"
		ENDIF
	;젖기쉬움
	CASE 41
		IF ARG:1 == -1
			RETURNF "젖기 어려움"
		ELSEIF ARG:1 == 1
			RETURNF "젖기 쉬움"
		ENDIF
	;습득속도
	CASE 50
		IF ARG:1 == -1
			RETURNF "습득 느림"
		ELSEIF ARG:1 == 1
			RETURNF "습득 빠름"
		ENDIF
	;낚시 채집 조제 Lv
	CASE 47, 48, 49
		IF ARG:1 > 0
			LOCALS = %TALENTNAME:ARG%
			LOCALS += TOSTR(ARG:1)
			RETURNF LOCALS
		ENDIF
	;악취내성
	CASE 61
		IF ARG:1 == -2
			RETURNF "결벽증"
		ELSEIF ARG:1 == -1
			RETURNF "악취 민감"
		ELSEIF ARG:1 == 1
			RETURNF "악취 둔감"
		ELSEIF ARG:1 == 2
			RETURNF "불결 무시"
		ENDIF
	;쾌감응답
	CASE 70
		IF ARG:1 == -1
			RETURNF "쾌감에 부정"
		ELSEIF ARG:1 == 1
			RETURNF "쾌감에 솔직"
		ENDIF
	;성별기호
	CASE 81
		IF ARG:1 == -1
			RETURNF "양도"
		ELSEIF ARG:1 == 3
			RETURNF "인간 불신"
		ELSEIF ARG:1 == 1
			RETURNF "남성기피"
		ELSEIF ARG:1 == 2
			RETURNF "여성기피"
		ENDIF
	;체형
	CASE 100
		IF ARG:1 == -5
			RETURNF "난쟁이"
		ELSEIF ARG:1 == -2
			RETURNF "유아 체형"
		ELSEIF ARG:1 == -1
			RETURNF "작은 체형"
		ELSEIF ARG:1 == 1
			RETURNF "장신"
		ELSEIF ARG:1 == 2
			RETURNF "거구"
		ELSEIF ARG:1 == 3
			RETURNF "거인"
		ENDIF
	;C감도
	CASE 101
		IF ARG:1 == -1
			RETURNF "C둔감"
		ELSEIF ARG:1 == 1
			RETURNF "C민감"
		ENDIF
	;V감도
	CASE 102
		IF ARG:1 == -1
			RETURNF "V둔감"
		ELSEIF ARG:1 == 1
			RETURNF "V민감"
		ENDIF
	;A감도
	CASE 103
		IF ARG:1 == -1
			RETURNF "A둔감"
		ELSEIF ARG:1 == 1
			RETURNF "A민감"
		ENDIF
	;B감도
	CASE 104
		IF ARG:1 == -1
			RETURNF "B둔감"
		ELSEIF ARG:1 == 1
			RETURNF "B민감"
		ENDIF
	;가슴사이즈
	CASE 105
		IF ARG:1 == -2
			RETURNF "절벽"
		ELSEIF ARG:1 == -1
			RETURNF "작은 가슴"
		;ELSEIF ARG:1 == 0
			;RETURNF "보유"
		ELSEIF ARG:1 == 1
			RETURNF "거유"
		ELSEIF ARG:1 == 2
			RETURNF "폭유"
		ENDIF
	;M감도
	CASE 106
		IF ARG:1 == -1
			RETURNF "M둔감"
		ELSEIF ARG:1 == 1
			RETURNF "M민감"
			ELSEIF ARG:1 == 2
			RETURNF "M과민"
		ENDIF
	;술내성
	CASE 121
		IF ARG:1 == -2
			RETURNF "술을 못함"
		ELSEIF ARG:1 == -1
			RETURNF "술에 약하다"
		ELSEIF ARG:1 == 1
			RETURNF "술에 강하다"
		ELSEIF ARG:1 == 2
			RETURNF "애주가"
		ELSEIF ARG:1 == 3
			RETURNF "술고래"
		ENDIF
	;회복속도
	CASE 130
		IF ARG:1 == -1
			RETURNF "회복 느림"
		ELSEIF ARG:1 == 1
			RETURNF "회복 빠름"
		ENDIF
	CASE 142
		SELECTCASE ARG:1
			CASE 1
				RETURNF "빗치"
		ENDSELECT
	CASE 143
		SELECTCASE ARG:1
			CASE 1
				RETURNF "동물귀"
			CASE 2
				RETURNF "토끼귀"
			CASE 3
				RETURNF "이누미미"
			CASE 4
				RETURNF "네코미미"
		ENDSELECT
	CASE 130
		IF ARG:1 == 1
			RETURNF "수유가능"
		ELSEIF ARG:1 == 2
			RETURNF "모유체질"
		ENDIF
	;음악지식
	CASE 161
		SELECTCASE ARG:1
			CASE 1
				RETURNF "악기지식"
			CASE 2
				RETURNF "가창지식"
			CASE 3
				RETURNF "무용지식"
		ENDSELECT
	;음감
	CASE 162
		IF ARG:1 == -1
			RETURNF "음치"
		ELSEIF ARG:1 == 1
			RETURNF "음감"
		ELSEIF ARG:1 == 2
			RETURNF "절대음감"
		ENDIF
	;한기선호
	CASE 164
		IF ARG:1 == 1
			RETURNF "한기선호"
		ELSEIF ARG:1 == 2
			RETURNF "설녀"
		ENDIF
	;채양보음／채음보양
	CASE 179
		IF GETBIT(ARG:1,0)&&GETBIT(ARG:1,2)
			RETURNF "채양보음"
		ELSEIF GETBIT(ARG:1,1)&&GETBIT(ARG:1,3)
			RETURNF "채음보양"
		ENDIF
	;섹프
	CASE 185
		IF ARG:1 == 1
			RETURNF "섹프"
		ELSEIF ARG:1 == 2
			RETURNF "정부"
		ENDIF
	;비생물
	CASE 192
		IF ARG:1 == 1
			RETURNF "비생물"
		ELSEIF ARG:1 == 2
			RETURNF "영체"
		ENDIF
	;신령
	CASE 193
		IF ARG:1 == 2
			RETURNF "사신"
		ELSEIF ARG:1 == 3
			RETURNF "염라"
		ENDIF
	;인간
	CASE 194
		IF ARG:1 == 1
			RETURNF "인간"
		ELSEIF ARG:1 == 2
			RETURNF "마법사"
		ELSEIF ARG:1 == 3
			RETURNF "무녀"
		ELSEIF ARG:1 == 4
			RETURNF "천인"
		ELSEIF ARG:1 == 5
			RETURNF "선인"
		ELSEIF ARG:1 == 6
			RETURNF "월인"
		ENDIF
	;형상
	CASE 250
		IF ARG:1 == 1
			RETURNF "유아 자지"
		ELSEIF ARG:1 == 2
			RETURNF "허접 자지"
		ELSEIF ARG:1 == 4
			RETURNF "반딧 자지"
		ELSEIF ARG:1 == 5
			RETURNF "돌기 자리"
		ELSEIF ARG:1 == 6
			RETURNF "거근"
		ELSEIF ARG:1 == 10
			RETURNF "초거근"
		ENDIF
	CASE 360
		IF ARG:1 == 1
	 		RETURNF "불임"
		ENDIF
	CASEELSE
		SIF ARG:1
			RETURNF @"%TALENTNAME:ARG%"
ENDSELECT