	SELECTCASE TALENT:CHARA:ARG
	CASE 1
		RETURNF "인간"
	CASE 2
		RETURNF "선인"
	CASE 3
		RETURNF "천인"
	CASE 4
		RETURNF "월인"
	CASE 5
		RETURNF "마계인"
	CASE 6
