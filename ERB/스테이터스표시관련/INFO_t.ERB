					CALL KOJO_MESSAGE_SEND("EVENT",5,LOCAL,MOODBREAKER)
				;장난 OFF
				CALL SET_PRANK(0, LOCAL, 2)
				;은밀중 OFF
				CALL SET_SNEAK(0, LOCAL, 2)
			NEXT
		ENDIF
	ENDIF
ENDIF
