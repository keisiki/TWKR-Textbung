		RESETCOLOR
	ELSE
		PRINTFORML 각성:%"!" * LOCAL%
	ENDIF
ELSE
	IF CFLAG:빡침
		IF TALENT:담력 < 0
			SETCOLOR C_BLUE
			PRINTL 눈물
		ELSE
			SETCOLOR 255,0,0
			PRINTL 분노
		ENDIF
		RESETCOLOR
	ELSEIF CFLAG:장난 || TCVAR:만취
		PRINTFORML 각성:%"!" * LOCAL%
	ELSE
		PRINTFORM 분노:%"!" * LOCAL%
		PRINTFORM   %"　" * (5 - MIN(LOCAL,5))%
		SELECTCASE CFLAG:TARGET:385
			CASE IS > 240
				PRINT <졸음쫒기·극>
			CASE IS > 180
				PRINT <졸음쫒기·강>
			CASE IS > 120
				PRINT <졸음쫒기·중>
			CASE IS > 0
				PRINT <졸음쫒기·약>
			CASEELSE
				PRINT
		ENDSELECT
	ENDIF
ENDIF
PRINTL

