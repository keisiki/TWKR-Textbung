;RETURN		VOID
;COMMENT	まとめて表示するように改良,TARGETの＜道具や使用状態＞以外の状態はここ
;-----------------------------------------------------------
@SHOW_EQUIP_2
;상황의 표시
;조교 대상에는 사용하지 않는, 특수한 착탈식의 도구를 추가했을 경우는 여기에 추가를 해 주세요
;(장소 변경, 마스터나 조수가 사용하는 도구등 )
LOCALS = （

IF TALENT:TARGET:기분 == 1
	LOCALS += "・기분좋음" ;ご機嫌
ELSEIF TALENT:TARGET:기분 == -1
	LOCALS += "・기분나쁨" ;フキゲン
ENDIF
SIF TCVAR:TARGET:축복
	LOCALS += "・축복"
SIF TCVAR:TARGET:인챈트
	LOCALS += "・파워업"
SIF TCVAR:TARGET:가호
	LOCALS += "・가호"

IF CFLAG:TARGET:수면
	SELECTCASE TCVAR:TARGET:수면제강도
	CASE 1
		LOCALS += "・깊은 수면"
	CASE 2
		LOCALS += "・매우 깊은 수면"
	CASE 3
		LOCALS += "・혼수중"
	CASEELSE
		LOCALS += "・수면중"
	ENDSELECT
ELSEIF TCVAR:TARGET:수면제강도
	SELECTCASE TCVAR:TARGET:수면제강도
	CASE 1
	LOCALS += "・졸음을 느낌"
	CASE 2
	LOCALS += "・강한 졸음을 느낌"
	CASE 3
	LOCALS += "・몽롱한 상태"
	ENDSELECT
ENDIF

SIF TCVAR:발정
	LOCALS += "・발정중"
SIF TCVAR:만취
	LOCALS += "・만취중"

SELECTCASE TCVAR:발원함보정
	CASE 23
		LOCALS += "・성적흥미 상승중"
	CASE 31
		LOCALS += "・해방 상승중"
	CASE 41
		LOCALS += "・젖기쉬움 상승중"
	CASE 70
		LOCALS += "・쾌감 긍정 상승중"
	CASE 80
		LOCALS += "・도착적 부여중"
	CASE 82
		LOCALS += "・새드 부여중"
	CASE 83
		LOCALS += "・마조 부여중"
	CASE 101
		LOCALS += "・C감도 상승중"
	CASE 102
		LOCALS += "・V감도 상승중"
	CASE 103
		LOCALS += "・A감도 상승중"
	CASE 104
		LOCALS += "・B감도 상승중"
	CASE 106
		LOCALS += "・M감도 상승중"
	ENDSELECT

;カラー表示,上に行くほど優先度が高い
IF CFLAG:TARGET:수면
	SETCOLOR C_BLUE
ELSEIF TCVAR:TARGET:발정
	SETCOLOR C_H_PINK
ELSEIF TCVAR:TARGET:만취
	SETCOLOR C_ORANGE
ELSEIF TCVAR:TARGET:수면제강도
	SETCOLOR C_BLUE
ELSEIF TALENT:TARGET:기분 == 1
	SETCOLOR C_AQUA
ELSEIF TALENT:TARGET:기분 == -1
	SETCOLOR C_RED
ENDIF

;表示部分
PRINTFORM %NAME:TARGET%

;通常時
IF LOCALS== "（"
	RETURN
;状態時
ELSE
	LOCALS += "）"
	PRINTFORM %REPLACE(LOCALS, "（・", "（")%
ENDIF
RESETCOLOR

