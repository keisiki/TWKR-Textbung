;-------------------------------------------------
;攻守交替
;-------------------------------------------------
@COM24
VARSET LOCAL
CFLAG:MASTER:우후후 = 2
TFLAG:COMABLE관리 = 3
FOR LOCAL,1,CHARANUM
	IF CFLAG:MASTER:현재위치 == CFLAG:LOCAL:현재위치
		CFLAG:LOCAL:우후후 = 2
		CFLAG:LOCAL:원래의우후후 = 0
		LOCAL:1 ++
	ENDIF
NEXT
PRINTFORML %CALLNAME:TARGET%\@ LOCAL:1 > 1 ? 일행 #  \@ 에게 주도권을 양보했다
TIME += 5

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE24
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(150)
	RETURN RESULT
;相手の욕정
SIF GETPALAMLV(PALAM:욕정,10) < 5
	RETURN 0
SIF TCVAR:억지로
	RETURN 0
;시간정지中も駄目
SIF !SHIRAHU(TARGET)
	RETURN 0
;なんか装着してるとダメ
FOR LOCAL,11,25
	SIF TEQUIP:LOCAL
		RETURN 0
NEXT
;挿入中は駄目
SIF TEQUIP:50 != -1
	RETURN 0
SIF TEQUIP:51 != -1
	RETURN 0
;샤워中はダメ
SIF TEQUIP:32
	RETURN 0
;유두빨기中はダメ
SIF TEQUIP:41
	RETURN 0
;乳揉み中はダメ
SIF TEQUIP:42
	RETURN 0
;계속키스
SIF TEQUIP:계속키스
	RETURN 0

RETURN 1
