;-------------------------------------------------
;요리の材料のレベルを設定する関数
;요리に使いそうなのはとりあえず個別に設定しておく
;その他に関しては値段で適当に設定
;-------------------------------------------------
@식재레벨(ARG)
#FUNCTION
VARSET LOCAL
SELECTCASE ITEMNAME:ARG
CASE "싸구려식재"
	LOCAL = 1
CASE "상급식재"
	LOCAL = 3
CASE "고급식재"
	LOCAL = 5
CASE "감미재"
	LOCAL = 3
CASE "물고기"
	LOCAL = 2
CASE "맛있는물고기"
	LOCAL = 4
CASE "벌레"
	LOCAL = 2
CASE "레어한벌레"
	LOCAL = 4
CASE "개구리"
	LOCAL = 2
CASE "로터"
	LOCAL = 4
CASE "양배추", "순무", "오이", "토마토", "양파", "가지", "호박", "배추", "당근", "파", "딸기", "수박"
	;小作人だったりするとランク上がる予定,漁師とかMASTER側にロールを設定するのもいいかもしれない
	IF 0
		LOCAL = 5
	ELSE
		LOCAL = 3
	ENDIF
CASE "새고기", "짐승고기", "정체불명고기"
	LOCAL = 3
CASE "산새"
	LOCAL = 4
CASE "우유"
	LOCAL = 5
CASE "탁주", "감주"
	LOCAL = 2
CASE "청주", "와인", "매실주", "증류주"
	LOCAL = 3
CASE "약주"
	LOCAL = 4
CASE "오니고로시", "신주"
	LOCAL = 5
CASE "초콜릿"
	LOCAL = 3
CASE "고급초콜릿"
	LOCAL = 5
CASE "아이스크림", "엿"
	LOCAL = 3
CASE "포션"
	LOCAL = 2
CASE "매직포션"
	LOCAL = 4
CASE "엘릭서", "선단"
	LOCAL = 5
CASE "미라다의돌"
	LOCAL = 4
CASE "보통버섯", "독버섯", "끈적끈적버섯", "약초", "독초", "쓴뿌리"
	LOCAL = 2
CASE "위험한버섯", "산나물", "참마", "우콘", "나무딸기", "벌꿀", "으름덩굴", "죽순"
	LOCAL = 3
CASE "동충하초", "마법의버섯", "상약초", "피안화", "만드라고라", "악화의유액", "선도", "방황하는영혼", "다크매터"
	LOCAL = 4
CASE "용의손톱", "얼음의비늘", "특약초", "눈의영초", "고려인삼", "용혈", "봉황의꼬리"
	LOCAL = 5
CASEELSE
	SELECTCASE ITEMPRICE:ARG
	CASE IS < 500
		LOCAL = 1
	CASE IS < 1000
		LOCAL = 2
	CASE IS < 5000
		LOCAL = 3
	CASE IS < 10000
		LOCAL = 4
	CASEELSE
		LOCAL = 5
	ENDSELECT
ENDSELECT
RETURNF LOCAL

