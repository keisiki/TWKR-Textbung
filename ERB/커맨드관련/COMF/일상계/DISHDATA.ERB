	CASE 301 TO 350
		TYPECHECK = 디저트
	CASEELSE
		IF DISH_NAME != ""
			PRINTL 
			PRINTFORML TYPECHECKが設定されてないよ！ DISH_NUM:{DISH_NUM} DISH_NAME:%DISH_NAME%
			THROW
		ENDIF
	ENDSELECT
ENDIF
IF !SKILL_LV
	SELECTCASE TYPECHECK
	CASE "경식"
		SKILL_LV = 0
	CASE "주식"
		SKILL_LV = 2
	CASE "디저트"
		SKILL_LV = 3
	ENDSELECT
ENDIF
IF !DISH_TIME
	SELECTCASE TYPECHECK
	CASE "경식"
		DISH_TIME = 20
	CASE "주식"
		DISH_TIME = 40
	CASE "디저트"
		DISH_TIME = 40
	ENDSELECT
ENDIF
SIF !DISH_MAXLIMIT
	DISH_MAXLIMIT = 120
SIF ITEM:보온전기밥통
	TIMES DISH_MAXLIMIT, 1.5

;RESULT設定,デフォルトは1(材料不足）
VARSET RESULT
RESULT = 1
;材料判定,いずれかの材料を1つでも持っていれば作れる
FOR LOCAL, 1, 11
	SIF DISH_ING:LOCAL < 0
		BREAK
	SIF ITEM:(DISH_ING:LOCAL) > 0
		RESULT = 10
NEXT

;必要技能判定
SIF ABL:MASTER:요리기능 < SKILL_LV
	RESULT = 2

;練習時は判定を無視してDISH_TYPEを決定する
IF 연습모드
	DISH_TYPE = %TYPECHECK%
ELSE
	;TYPECHECK判定
	SIF TYPECHECK != ARGS
		RESULT = 0

	;고유요리판정
	IF INRANGE(DISH_NUM, 400, 400 + CHARANUM)
		SIF LEARNED_MENU:SP_CHARA < 100
			RESULT = 0
	ENDIF
ENDIF

;存在判定
SIF DISH_NAME == ""
	RESULT = 0

RESULT:1 = SKILL_LV
RESULTS:1 = %TYPECHECK%
RETURN RESULT
;-------------------------------------------------
;요리の材料を設定する関数
;材料は１つで作れるので多いほど作る手段が多い≒作りやすい
;"범용"を入れると범용食材が세트される
;"물고기"を入れると魚食材が세트される
;"작성불능"を入れるとどうやっても作れないようになる(表示はされる）
;-------------------------------------------------
@재료설정(ARGS:1, ARGS:2, ARGS:3, ARGS:4, ARGS:5, ARGS:6, ARGS:7, ARGS:8, ARGS:9, ARGS:10)
VARSET DISH_ING, -1
IF ARGS:1 == "범용"
	DISH_ING:1 = GETNUM(ITEM, "고급식재")
	DISH_ING:2 = GETNUM(ITEM, "상급식재")
	DISH_ING:3 = GETNUM(ITEM, "싸구려식재")
	RETURN
ELSEIF ARGS:1 == "물고기"
	DISH_ING:1 = GETNUM(ITEM, "맛있는물고기")
	DISH_ING:2 = GETNUM(ITEM, "물고기")
ELSEIF ARGS:1 == "벌레"
	DISH_ING:1 = GETNUM(ITEM, "희귀한벌레")
	DISH_ING:2 = GETNUM(ITEM, "벌레")
	RETURN
	RETURN
ELSEIF ARGS:1 == "작성불능"
	FOR LOCAL, 1, 11
		DISH_ING:LOCAL = -1
	NEXT
	RETURN
ENDIF
FOR LOCAL, 1, 11
	IF ARGS:LOCAL == ""
		;로터が0番なので無いものは-1を入れておく
		DISH_ING:LOCAL = -1
	ELSE
		DISH_ING:LOCAL = GETNUM(ITEM, ARGS:LOCAL)
		IF GETNUM(ITEM, ARGS:LOCAL) == -1
			PRINTFORML 아이템명이 間違ってるよ！＞%ARGS:LOCAL%
			THROW
		ENDIF
	ENDIF
NEXT

