				CASE 3
					モブ子二人称 = %\@ 年齢差 == 2 ? ばあ # おば \@%ちゃん
					SIF 年齢差 == 1 && RAND:3
						モブ子二人称 = %TEXTR("姉ちゃん/お姉ちゃん/おねえちゃん")%
				CASE 5
					モブ子二人称 = %\@ 年齢差 == 2 ? お婆 # オバ \@%さん
			ENDSELECT
		ENDIF
	ELSEIF 体格差 > 0 || 年齢差 < 0 
		モブ子二人称 = %\@ 呼称 ? 坊や # お嬢ちゃん \@%
	ENDIF
ENDIF
$SKIP
SIF モブ子一人称 == CALLNAME:ARG && !RAND:3
	モブ子一人称 += "ちゃん"
RETURN
