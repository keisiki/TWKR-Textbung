;オブジェクト本体
@술데이터50(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "명칭"
	CALLF MAKE_STR(V_NAME, "하룻밤의쿠시나다")
CASE "변화처"
	CALLF MAKE_STR(V_NAME, "")
CASE "양조일수"
	CALLF MAKE_INT(V_NAME, )
;CASE "양조수"
;	CALLF MAKE_INT(V_NAME, )
CASE "술기운증가량"
	LOCAL = 500 + RAND:200
	;一夜のクシナダは술내성が高いほど酔いが回りやすい
	IF ARG != MASTER
		IF TALENT:ARG:술내성 == -1
			TIMES LOCAL, 1.1
		ELSEIF TALENT:ARG:술내성 == 1
			TIMES LOCAL, 1.2
		ELSEIF TALENT:ARG:술내성 == 2
			TIMES LOCAL, 1.7
		ELSEIF TALENT:ARG:술내성 == 3
			TIMES LOCAL, 2.5
		ELSEIF TALENT:ARG:술내성 == -2
			IF CFLAG:ARG:술을못함 >= 5
				TIMES LOCAL, 2.0
			ELSEIF CFLAG:ARG:술을못함 >= 2
				TIMES LOCAL, 1.5
			ELSE
				TIMES LOCAL, 1.0
			ENDIF
		ENDIF
	ENDIF
	CALLF MAKE_INT(V_NAME, LOCAL)
CASE "TASTE"
	IF TALENT:ARG:술내성 < 0
		LOCAL = 8
	ELSE
		LOCAL = 25
	ENDIF
	CALLF MAKE_INT(V_NAME, LOCAL)
ENDSELECT

