;酒番号から名称を取得
@ALC_NAME(ARG)
#FUNCTIONS
#DIM CHARA
RETURNF GET_STR(0, "술데이터", ARG, "명칭")

;名称から酒番号を取得
@ALC_ID(ARGS)
#FUNCTION
SIF ARGS == ""
	ARGS = %VAR_FERMENT%
FOR LOCAL,1,100
	SIF GET_STR(0, "술데이터", LOCAL, "명칭") == ARGS
		RETURNF LOCAL
NEXT
RETURNF 0

@IS_ALC(ARG)
#FUNCTION
#DIM 술ID
LOCALS = %ITEMNAME:ARG%
FOR 술ID, 1, 100
	IF GET_STR(0, "술데이터", 술ID, "명칭") == LOCALS
		IF GET_INT(0, "술데이터", 술ID, "술기운증가량") > 0
			RETURNF 1
		ELSE
			RETURNF 0
		ENDIF
	ENDIF
NEXT
RETURNF 0

;廃止
;@양조일수(ARG)
;@醸造できた量(ARG)


@주조
#DIMS ALTER
DAY:10 ++
LOCAL = GET_INT(0, "술데이터", ALC_ID(), "양조일수")
IF LOCAL > 0 && LOCAL <= DAY:10
	ALTER = %GET_STR(0, "술데이터", ALC_ID(), "변화처")%
	CALL COLORMESSAGE(@"술벌레 병 안의 %조사처리(VAR_FERMENT,"는")% %조사처리(ALTER,"으로")% 변화했다",C_YELLOW, 2)
	DAY:10 = 0
	VAR_FERMENT = %ALTER%
ENDIF

@DRUNKEN(C_ID, itemnum)
#DIM C_ID
#DIM 술ID
#DIM itemnum
#DIM 술기운증가량

LOCALS = %ITEMNAME:itemnum%

술ID = ALC_ID(LOCALS)

술기운증가량 = GET_INT(C_ID, "술데이터", 술ID, "술기운증가량")
;一夜のクシナダを除く
IF LOCALS != "하룻밤의쿠시나다"
	IF TALENT:C_ID:술내성 == -1
		TIMES 술기운증가량, 1.2
	ELSEIF TALENT:C_ID:술내성 == 1
		TIMES 술기운증가량, 0.9
	ELSEIF TALENT:C_ID:술내성 == 2
		TIMES 술기운증가량, 0.75
	ELSEIF TALENT:C_ID:술내성 == 3
		TIMES 술기운증가량, 0.5
	ELSEIF TALENT:C_ID:술내성 == -2
		IF CFLAG:C_ID:술을못함 >= 5
			TIMES 술기운증가량, 2.0
		ELSEIF CFLAG:C_ID:술을못함 >= 2
			TIMES 술기운증가량, 1.5
		ELSE
			TIMES 술기운증가량, 1.0
		ENDIF
	ENDIF
ENDIF
SIF C_ID == MASTER
