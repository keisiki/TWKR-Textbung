;-------------------------------------------------
;애널전극
;하드한 조교 커맨드, 레벨 4
;-------------------------------------------------
@COM148
;착탈의처리
TEQUIP:애널전극 = !TEQUIP:애널전극
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:TARGET:2 = 12
;강도
TCVAR:TARGET:4 = 1000

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
IF FLAG:70 == 1
BASE:MASTER:TSP -= 300
	SOURCE:PLAYER:성행동=25
	SOURCE:PLAYER:강요=750
SOURCE:쾌A = 500
SOURCE:고통 = 750

;피스트등의 A에의 큰 고통은 먼저 처리해 둔다
;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
	TIMES SOURCE:고통 , 3.00
ELSEIF EXP:A경험 < EXPLV:2
	TIMES SOURCE:고통 , 2.50
ELSEIF EXP:A경험 < EXPLV:3
	TIMES SOURCE:고통 , 2.40
ELSEIF EXP:A경험 < EXPLV:4
	TIMES SOURCE:고통 , 2.00
ELSEIF EXP:A경험 < EXPLV:5
	TIMES SOURCE:고통 , 1.80
ELSEIF EXP:A경험 >= EXPLV:5
	TIMES SOURCE:고통 , 1.60
ENDIF

;PALAM:윤활을 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:고통 , 3.00
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:고통 , 2.50
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:고통 , 2.00
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:고통 , 1.50
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES SOURCE:고통 , 1.00
ENDIF

ELSE

DOWNBASE:체력 += 300
DOWNBASE:기력 += 450
	SOURCE:PLAYER:성행동=25

	SOURCE:PLAYER:강요=1500
SOURCE:쾌A = 500
SOURCE:고통 = 1500
SOURCE:노출 = 300
SOURCE:굴종 = 800
SOURCE:일탈 = 1200
SOURCE:반감 = 1200

;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:자기애 < 0 || TALENT:저항
	SOURCE:울굴 += 1200

;피스트등의 A에의 큰 고통은 먼저 처리해 둔다
;EXP:A경험을 본다
IF EXP:A경험 < EXPLV:1
	TIMES SOURCE:고통 , 3.00
	TIMES SOURCE:반감 , 4.00
ELSEIF EXP:A경험 < EXPLV:2
	TIMES SOURCE:고통 , 2.50
	TIMES SOURCE:반감 , 3.50
ELSEIF EXP:A경험 < EXPLV:3
	TIMES SOURCE:고통 , 2.40
	TIMES SOURCE:반감 , 3.00
ELSEIF EXP:A경험 < EXPLV:4
	TIMES SOURCE:고통 , 2.00
	TIMES SOURCE:반감 , 2.50
ELSEIF EXP:A경험 < EXPLV:5
	TIMES SOURCE:고통 , 1.80
	TIMES SOURCE:반감 , 2.00
ELSEIF EXP:A경험 >= EXPLV:5
	TIMES SOURCE:고통 , 1.60
	TIMES SOURCE:반감 , 1.50
ENDIF

;PALAM:윤활을 본다
IF PALAM:윤활 < PALAMLV:1
	TIMES SOURCE:고통 , 3.00
	TIMES SOURCE:반감 , 2.00
ELSEIF PALAM:윤활 < PALAMLV:2
	TIMES SOURCE:고통 , 2.50
	TIMES SOURCE:반감 , 1.50
ELSEIF PALAM:윤활 < PALAMLV:3
	TIMES SOURCE:고통 , 2.00
	TIMES SOURCE:반감 , 1.20
ELSEIF PALAM:윤활 < PALAMLV:4
	TIMES SOURCE:고통 , 1.50
ELSEIF PALAM:윤활 >= PALAMLV:4
	TIMES SOURCE:고통 , 1.00
ENDIF
ENDIF
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;A경험
EXP:A경험 += 4

SIF !GETBIT (CFLAG:3, 8)
	EXP:이상경험 += 1

;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF FLAG:70 == 1
	IF HETEROSEX(TARGET, PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 1
	ENDIF
ELSE
	IF HETEROSEX(TARGET, PLAYER)
		EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 1
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 1
	ENDIF
ENDIF
RETURN 1
;-------------------------------------------------
;장착 계속의 처리
;-------------------------------------------------
@EQUIP_COM148(ARG)
;애널전극

PRINTL ＜애널전극 삽입중＞

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:TARGET:2 = 12
;강도
TCVAR:TARGET:4 = 500

IF FLAG:70 == 1
BASE:MASTER:TSP -= 200
SOURCE:ARG:쾌A += 250

;피스트등의 A에의 큰 고통은 먼저 처리해 둔다
;EXP:A경험을 본다
LOCAL = 750
LOCAL:1 = 600
IF EXP:ARG:A경험 < EXPLV:1
	TIMES LOCAL , 3.00
ELSEIF EXP:ARG:A경험 < EXPLV:2
	TIMES LOCAL , 2.50
ELSEIF EXP:ARG:A경험 < EXPLV:3
	TIMES LOCAL , 2.40
ELSEIF EXP:ARG:A경험 < EXPLV:4
	TIMES LOCAL , 2.00
ELSEIF EXP:ARG:A경험 < EXPLV:5
	TIMES LOCAL , 1.80
ELSEIF EXP:ARG:A경험 >= EXPLV:5
	TIMES LOCAL , 1.60
ENDIF

;PALAM:윤활을 본다
IF PALAM:ARG:윤활 < PALAMLV:1
	TIMES LOCAL , 3.00
ELSEIF PALAM:ARG:윤활 < PALAMLV:2
	TIMES LOCAL , 2.50
ELSEIF PALAM:ARG:윤활 < PALAMLV:3
	TIMES LOCAL , 2.00
ELSEIF PALAM:ARG:윤활 < PALAMLV:4
	TIMES LOCAL , 1.50
ELSEIF PALAM:ARG:윤활 >= PALAMLV:4
	TIMES LOCAL , 1.00
ENDIF
SOURCE:ARG:고통 += LOCAL

ELSE


DOWNBASE:ARG:체력 += 200
DOWNBASE:ARG:기력 += 250

SOURCE:ARG:쾌A += 250
SOURCE:ARG:굴종 += 400
SOURCE:ARG:일탈 += 600


;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:ARG:자기애 < 0 || TALENT:ARG:저항
	SOURCE:ARG:울굴 += 600

;피스트등의 A에의 큰 고통은 먼저 처리해 둔다
;EXP:A경험을 본다
LOCAL = 750
LOCAL:1 = 600
IF EXP:ARG:A경험 < EXPLV:1
	TIMES LOCAL , 3.00
	TIMES LOCAL:1 , 4.00
ELSEIF EXP:ARG:A경험 < EXPLV:2
	TIMES LOCAL , 2.50
	TIMES LOCAL:1 , 3.50
ELSEIF EXP:ARG:A경험 < EXPLV:3
	TIMES LOCAL , 2.40
	TIMES LOCAL:1 , 3.00
ELSEIF EXP:ARG:A경험 < EXPLV:4
	TIMES LOCAL , 2.00
	TIMES LOCAL:1 , 2.50
ELSEIF EXP:ARG:A경험 < EXPLV:5
	TIMES LOCAL , 1.80
	TIMES LOCAL:1 , 2.00
ELSEIF EXP:ARG:A경험 >= EXPLV:5
	TIMES LOCAL , 1.60
	TIMES LOCAL:1 , 1.50
ENDIF

;PALAM:윤활을 본다
IF PALAM:ARG:윤활 < PALAMLV:1
	TIMES LOCAL , 3.00
	TIMES LOCAL:1 , 2.00
ELSEIF PALAM:ARG:윤활 < PALAMLV:2
	TIMES LOCAL , 2.50
	TIMES LOCAL:1 , 1.50
ELSEIF PALAM:ARG:윤활 < PALAMLV:3
	TIMES LOCAL , 2.00
	TIMES LOCAL:1 , 1.20
ELSEIF PALAM:ARG:윤활 < PALAMLV:4
	TIMES LOCAL , 1.50
ELSEIF PALAM:ARG:윤활 >= PALAMLV:4
	TIMES LOCAL , 1.00
ENDIF
SOURCE:ARG:고통 += LOCAL
SOURCE:ARG:반감 += LOCAL:1
ENDIF
;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
;A경험
EXP:ARG:A경험 += 1

RETURN 1

;-------------------------------------------------
;실행 판정
;-------------------------------------------------
@COM_ABLE148
;애널전극 실행 판정
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(148)
	RETURN RESULT
;하반신착의
SIF TEQUIP:0 > 1
	RETURN 0
;해제는 언제라도 가능
SIF TEQUIP:23
	RETURN 1
;아이템 을 가지고 있는인가, 아이템 없음 설정이 아니면 안 돼
SIF ITEM:24 == 0 && NOITEM == 0
	RETURN 0
;애널바이브 사용중은 불가
SIF TEQUIP:14
	RETURN 0
;애널비즈 사용중은 불가
SIF TEQUIP:15
	RETURN 0
;보통 관장 사용중은 불가
SIF TEQUIP:21
	RETURN 0
;풍선 사용중은 불가
SIF TEQUIP:22
	RETURN 0
;목욕탕플레이중은 안 돼
SIF TEQUIP:31
	RETURN 0
RETURN 1

