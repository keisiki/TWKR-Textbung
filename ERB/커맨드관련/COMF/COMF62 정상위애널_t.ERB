;-------------------------------------------------
;정상위애널
;섹스계 커맨드, 레벨 1
;-------------------------------------------------
@COM62
;-------------------------------------------------
;특수 파생 처리
;-------------------------------------------------
;전회의 조교가 3 P 때 3 P에
IF PREVCOM == 71
	CALL COM_ABLE71
	SIF RESULT
		JUMP COM71
ENDIF
;침대가 없는 or욕실에서 플레이매트가 없으면 립 정도에
SIF !BEDROOM(CFLAG:PLAYER:현재위치) && !(BATHROOM() && ITEM:20)
	TFLAG:193 = 1
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = PLAYER

;봉사 쾌락 플래그
TCVAR:PLAYER:108 = 1

CSTR:PLAYER:10 += @"{TARGET}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:PLAYER:2 = 2
TCVAR:PLAYER:동정상실 = 1
TCVAR:A상실발생 = 1
;강도
TCVAR:PLAYER:4 = 1700

CSTR:TARGET:10 += @"{PLAYER}/"
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수 12=A고문 13=V 꾸짖고)
TCVAR:TARGET:2 = 12
;강도
TCVAR:TARGET:4 = 1500

DOWNBASE:PLAYER:체력 += 40
DOWNBASE:PLAYER:기력 += 40
CALL 몸사용하기("C",1)
CALL 몸사용하기("P삽입",1)
;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 120
	SOURCE:PLAYER:쾌C = 400
	;SOURCE:PLAYER:성행동+= 20
	SOURCE:쾌A = 400
	SOURCE:고통 = 400

ELSE

	DOWNBASE:체력 += 80
	DOWNBASE:기력 += 80

	SOURCE:PLAYER:쾌C = 400
	;SOURCE:PLAYER:성행동+= 20
	SOURCE:쾌A = 400
	SOURCE:사랑 = 100
	SOURCE:고통 = 800
	SOURCE:노출 = 60
	SIF TFLAG:193
		SOURCE:노출 = 300
	SOURCE:일탈 = 200
	SOURCE:반감 = 500

	;위 쪽으로 계산한 더러운 데이터
	SOURCE:불결 = Y*10 + 60

	;성기술보정
	LOCAL = 1 + (ABL:PLAYER:허리 - 2) / 10
	SOURCE:쾌A *= LOCAL
	SOURCE:사랑 *= LOCAL
	;-------------------------------------------------
	;특수소스처리
	;-------------------------------------------------
	;억압, 저항 소유에는, 억울소스 추가
	SIF TALENT:자기애 < 0 || TALENT:저항
		SOURCE:울굴 += 500
ENDIF
;A에의 고통은 먼저 처리해 둔다
CALL PAIN_CHECK_A(TARGET)

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;사정에 관계없이 행해지는 처리는 여기에서
;노예의 A⇔조교자의 P의 더러움이 이동
CALL STAIN_애널P삽입

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:A경험 ++
EXP:PLAYER:삽입경험 ++
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF FLAG:70 == 1 || CFLAG:장난
	IF HETEROSEX(TARGET, PLAYER)
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 4
	ENDIF
	IF CFLAG:PLAYER:장난 == 2
		EXP:PLAYER:수면간경험 += 1
		EXP:TARGET:수면간경험 += 1
	ENDIF
		SIF !CFLAG:장난
		EXP:PLAYER:시간경험 += 1
ELSE
	IF HETEROSEX(TARGET, PLAYER)
		EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 4
		EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 4
	ENDIF
	EXP:A성교경험 ++
	;애정경험
	SIF !ASSIPLAY
		EXP:애정경험 += TALENT:연모
ENDIF
RETURN 1

@CAN_COM62(ARG)
;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
IF FLAG:70 == 1 || CFLAG:장난
	SKIPDISP ARG
	IF !BEDROOM(CFLAG:PLAYER:현재위치)
		PRINTL 대면입위로 애널을 범한다
	ELSE
		PRINTL 정상위로 애널을 범한다
	ENDIF
ELSE

	;밧줄로 묶여 있을 경우는 무시(밧줄이 없을 때만 실행 판정을 실시한다)
	IF !TEQUIP:밧줄
		SKIPDISP ARG
		IF !BEDROOM(CFLAG:PLAYER:현재위치) && !(BATHROOM() && ITEM:20)
			PRINTL 대면입위 애널
		ELSE
			PRINTL 정상위애널
		ENDIF
		A = 0
		S = 0
		;모든 명령에 공통의 요소를 고려
		;(순종이 높다고 명령에 따르기 쉬운 등)
		CALL COM_ORDER

		;ABL:욕망
		IF ABL:욕망
			SIF S
				PRINT  + 
			A += ABL:욕망 * 3
			PRINT 욕망
			PRINTV 'LV, ABL:욕망, '(, ABL:욕망 * 3, ')
			S = 1
		ENDIF
		;ABL:A감각
		IF ABL:A감각
			SIF S
				PRINT  + 
			A += ABL:A감각 * 2
			PRINT A감각
			PRINTV 'LV, ABL:A감각, '(, ABL:A감각 * 2, ')
			S = 1
		ENDIF
		;쾌락각인
		IF MARK:쾌락각인
			SIF S
				PRINT  + 
			A += MARK:쾌락각인 * 3
			PRINT 쾌락각인
			PRINTV 'LV, MARK:쾌락각인, '(, MARK:쾌락각인 * 3, ')
			S = 1
		ENDIF
		;PALAM:윤활(의 부족)
		IF PALAM:윤활 < PALAMLV:3
			PRINT  - 
			A -= 5
			PRINT 윤활 부족
			PRINTV '(, 5, ')
			S = 1
		ENDIF
		;PALAM:욕정
		IF GETPALAMLV(PALAM:욕정, 5)
			SIF S
				PRINT  + 
			A += GETPALAMLV(PALAM:욕정, 5) * 3
			PRINT 욕정
			PRINTV 'LV, GETPALAMLV(PALAM:욕정, 5), '(, GETPALAMLV(PALAM:욕정, 5) * 3, ')
			S = 1
		ENDIF
		;수줍음
		IF TALENT:수치심 > 0
			PRINT  - 
			A -= 2
			PRINT 수줍음
			PRINTV '(, 2, ')
			S = 1
		ENDIF
		;쾌감에 부정
		IF TALENT:쾌감응답 < 0 && !TALENT:음란
			PRINT  - 
			A -= 5
			PRINT 쾌감에 부정
			PRINTV '(, 5, ')
			S = 1
		ENDIF
		;남성기피(연모, 친애로 무효)
		IF OTOKOGIRAI(TARGET)
			PRINT  - 
			A -= 12
			PRINT 남성기피
			PRINTV '(, 12, ')
			S = 1
		ENDIF
		;음란
		IF TALENT:음란 && ASSIPLAY == 0
			SIF S
				PRINT  + 
			A += 15
			PRINT 음란
			PRINTV '(, 15, ')
			S = 15
		ENDIF
		;창부
		IF TALENT:음란 > 1 && ASSIPLAY == 0
			SIF S
				PRINT  + 
			A += 5
			PRINT 창부
			PRINTV '(, 5, ')
			S = 5
		ENDIF
		;A경험
		IF EXP:A경험 < EXPLV:2
			PRINT  - 
			A -= 30
			PRINT A경험부족
			PRINTV '(, 30, ')
			S = 1
		ENDIF
		;조교자가 후타나리
		IF (TALENT:PLAYER:2 & 3) == 3
			SIF S
				PRINT  + 
			A += 4
			PRINT 후타나리
			PRINTV '(, 4, ')
			S = 1
		ENDIF

		;미약
		IF TCVAR:미약
			SIF S
				PRINT  + 
			A += 6
			PRINT 춘약
			PRINTV '(, 6, ')
			S = 1
		ENDIF
		;레이프중
		IF TCVAR:레이프중
			SIF S
				PRINT  + 
			A += 30
			PRINT 레이프중
			PRINTV '(, 30, ')
			S = 30
		ENDIF
		;창녀
		IF TALENT:창녀||TALENT:렌탈창녀
			SIF S
				PRINT  + 
			A += 100
			PRINT 창녀
			PRINTV '(, 100, ')
			S = 100
		ENDIF
		Y = 0
		;애액의 더러움
		SIF STAIN:PLAYER:2 & 1
			Y += 1
		;정액의 더러움
		SIF STAIN:PLAYER:2 & 4
			Y += 3
		;애널의 더러움
		SIF STAIN:PLAYER:2 & 8
			Y += 7
		SIF STAIN:PLAYER:2 & 16
			Y += 15
		Y = Y * (2 - TALENT:악취내성) / 2

		;더러움 있음
		IF Y
			PRINT  - 
			A -= Y
			PRINT 더러움 있음
			;악취 둔감
			IF TALENT:악취내성 > 0
				PRINT , 악취 둔감
			ELSEIF TALENT:악취내성 < 0
				PRINT , 악취 민감
			ENDIF
			PRINTV '(, Y, ')
			S = 1
		ENDIF

		;합계를 표시(30이상으로 실행)
		PRINT  = 
		PRINTV A

		V = 30
		SIF A < V
			PRINT  < 
		SIF A == V
			PRINT  = 
		SIF A > V
			PRINT  > 
		PRINT 실행치
		PRINTV V
	ELSE
		SKIPDISP ARG
		IF !BEDROOM(CFLAG:PLAYER:현재위치) && !(BATHROOM() && ITEM:20)
			PRINTL 속박 대면입위 애널
		ELSE
			PRINTL 속박 정상위애널
		ENDIF
	ENDIF
	WAIT
	SKIPDISP 0
	;실행할 수 없다
	SIF A < V
		RETURN 0
	IF TALENT:임신 && (CFLAG:903 <= 30 / 2|| CFLAG:903 >= 71)
		PRINTFORMW 배의 아이를 위해서라도 지금은 %CALLNAME:ARG%에게 무리시키지 않는 게 좋을 것이다
		RETURN 0
	ENDIF

ENDIF
SKIPDISP 0
RETURN 1

;-------------------------------------------------
;실행 판정
;-------------------------------------------------
@COM_ABLE62
;정상위애널 실행 판정
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(62)
	RETURN RESULT
;독인형이라면 항문이 붙어있지 않기 때문에 안돼
SIF TALENT:196 == 99
	RETURN 0
;조교자가 남자, 후타나리의 경우는 어느 정도 발기하고 있지 않으면 안 돼
SIF (TALENT:PLAYER:2 & 2) && BASE:PLAYER:발기 < 1000
	RETURN 0
;애널바이브 삽입중은 안 돼
SIF TEQUIP:14
	RETURN 0
;애널비즈 사용중은 불가
SIF TEQUIP:15
	RETURN 0
;보통 관장 사용중은 불가
SIF TEQUIP:21
	RETURN 0
;풍선 사용중은 불가
SIF TEQUIP:22
	RETURN 0
;전극 사용중은 불가
SIF TEQUIP:23
	RETURN 0
;시간정지중은 스킵
IF FLAG:70 != 1
	;일정한 A경험이 필요
	SIF EXP:2 < EXPLV:3
		RETURN 0
ENDIF
;체격 차이가 있는 경우는 경험이 없으면 안 돼
SIF 삽입불가(TARGET) == 1 && EXP:A경험 < 50 && !TALENT:PLAYER:금단의지식
	RETURN 0
;한층 더 체격 차이가 있으면(자) 금단의지식이 필요
SIF 삽입불가(TARGET) == 2 && (!TALENT:PLAYER:금단의지식 || EXP:A경험 < 50)
	RETURN 0
SIF 삽입불가(TARGET) == 3
	RETURN 0
;조교자가 남자, 반인반요, 후타나리, 혹은 페니스밴드가 없으면 안 돼
SIF !(TALENT:PLAYER:2 & 2)  && ITEM:PBAND == 0
	RETURN 0
;윤활 부족해, 순종 3 이하·레즈끼 3 이하의 ASSI의 경우는 할 수 없다(새드라면 OK)
IF PALAM:윤활 < PALAMLV:2 && ASSIPLAY
	SIF (ABL:ASSI:순종 <= 3 || ABL:ASSI:기교 <= 3) && TALENT:ASSI:새드 == 0
		RETURN 0
ENDIF
;하반신착의
SIF TEQUIP:0 & 4
	RETURN 0
;목욕탕플레이중은 플레이매트가 없으면 안 돼
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;후배위로부터는 안 돼
SIF TEQUIP:체위 == 5
	RETURN 0

RETURN 1

