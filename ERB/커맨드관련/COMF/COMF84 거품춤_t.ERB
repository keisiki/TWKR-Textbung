	RETURN 0
;풍선 사용중은 불가
SIF TEQUIP:22
	RETURN 0
;전극 사용중은 불가
SIF TEQUIP:23
	RETURN 0
;정지 중에는 불가
SIF FLAG:70 == 1
	RETURN 0
RETURN 1

