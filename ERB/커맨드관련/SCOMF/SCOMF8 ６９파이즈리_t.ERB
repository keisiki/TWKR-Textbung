;-------------------------------------------------
;69파이즈리
;봉사계 파생 커맨드
;-------------------------------------------------
@SCOM8
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = TARGET
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수)


LOCALS = {TARGET}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 5
;강도
TCVAR:PLAYER:4 = 2000

LOCALS = {PLAYER}/
CSTR:TARGET:10 += LOCALS
TCVAR:TARGET:2 = 4
;강도
TCVAR:TARGET:4 = 1200

;봉사 쾌락 플래그
TCVAR:PLAYER:108 = 1
TCVAR:108 = 1

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
DOWNBASE:체력 += 10
DOWNBASE:기력 += 100
SOURCE:MASTER:쾌C = 150
	SOURCE:PLAYER:성행동 =15

SOURCE:쾌C = 150
SOURCE:쾌B = 150
SOURCE:사랑 = 200
SOURCE:성행동 = 500
SOURCE:달성 = 300
SOURCE:액체 = 80
SOURCE:노출 = 100
SOURCE:굴종 = 1200
SOURCE:일탈 = 700
SOURCE:반감 = 700

;위 쪽으로 계산한 더러운 데이터
SOURCE:불결 = Y*10 + 60


;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:자기애 < 0 || TALENT:저항
	SOURCE:울굴 += 1000

;혀를 사용하는 조교에서는, 조교자의 혀놀림에 의해, 쾌감의 소스 증가
SIF TALENT:PLAYER:혀놀림
	TIMES SOURCE:쾌C , 1.50

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 B⇔조교자의 P의 더러움이 이동
STAIN:5 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:5
;노예의 V⇔조교자의 입의 더러움이 이동
STAIN:3 |= STAIN:PLAYER:구
STAIN:PLAYER:구 |= STAIN:3


;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:파이즈리경험 ++
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF HETEROSEX(TARGET, PLAYER)
	EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 8
	EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 8
ENDIF

;애정경험
SIF !ASSIPLAY
	EXP:애정경험 += TALENT:연모

;노예가 고양이혀 소유의 경우는, 조교자에게 가학쾌락경험을 추가
SIF TALENT:TARGET:고양이혀
	EXP:가학쾌락경험 += 1

RETURN 1

@CAN_SCOM8(ARG)
SKIPDISP ARG
PRINTL 69파이즈리
;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
A = 0
S = 0

;모든 명령에 공통의 요소를 고려
;(순종이 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;ABL:욕망
IF ABL:욕망
	SIF S
		PRINT  + 
	A += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV, ABL:욕망, '(, ABL:욕망 * 1, ')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF S
		PRINT  + 
	A += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV, ABL:봉사정신, '(, ABL:봉사정신 * 4, ')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF S
		PRINT  + 
	A += ABL:정액중독 * 3
	PRINT 정액중독
	PRINTV 'LV, ABL:정액중독, '(, ABL:정액중독 * 3, ')
	S = 1
ENDIF

IF MARK:쾌락각인
	SIF S
		PRINT  + 
	A += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV, MARK:쾌락각인, '(, MARK:쾌락각인 * 1, ')
	S = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정, 5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:욕정, 5) * 1
	PRINT 욕정
	PRINTV 'LV, GETPALAMLV(PALAM:욕정, 5), '(, GETPALAMLV(PALAM:욕정, 5) * 1, ')
	S = 1
ENDIF
;'수줍음
IF TALENT:수치심 > 0
	PRINT  - 
	A -= 3
	PRINT 수줍음
	PRINTV '(, 3, ')
	S = 1
ENDIF
;악취 둔감
IF TALENT:악취내성 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 악취 둔감
	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 민감
IF TALENT:악취내성 < 0
	PRINT  - 
	A -= 3
	PRINT 악취 민감
	PRINTV '(, 3, ')
	S = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF S
		PRINT  + 
	A += 6
	PRINT 헌신적
	PRINTV '(, 6, ')
	S = 1
ENDIF
;쾌감에 부정(음란, 창부로 무효)
IF TALENT:쾌감응답 < 0 && !TALENT:음란
	PRINT  - 
	A -= 3
	PRINT 쾌감에 부정
	PRINTV '(, 3, ')
	S = 1
ENDIF
;남성기피(연모, 친애로 무효)
IF OTOKOGIRAI(TARGET)
	PRINT  - 
	A -= 12
	PRINT 남성기피
	PRINTV '(, 12, ')
	S = 1
ENDIF
;연모
IF TALENT:연모 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 연모
	PRINTV '(, 5, ')
	S = 5
ENDIF
;친애
IF TALENT:연모 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 친애
	PRINTV '(, 5, ')
	S = 5
ENDIF

;조교자가 후타나리
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 후타나리
	PRINTV '(, 8, ')
	S = 1
ENDIF

Y = 0
;애액의 더러움
SIF STAIN:PLAYER:2 & 1
	Y += 1
;정액의 더러움
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널의 더러움
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15

Y = Y * (2 - TALENT:악취내성) / 2

;서로 더러워져
IF Y
	PRINT  - 
	A -= Y
	PRINT 서로 더러워져
	;악취 둔감
	IF TALENT:악취내성 > 0
		PRINT , 악취 둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT , 악취 민감
	ENDIF
	PRINTV '(, Y, ')
	S = 1
ENDIF


;합계를 표시(34이상으로 실행)
PRINT  = 
PRINTV A

V = 34
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

WAIT
SKIPDISP 0
;실행할 수 없다
SIF A < V
	RETURN 0
RETURN 1

;-------------------------------------------------
;선택 가능 판정
;-------------------------------------------------
@COM_ABLE508
;69파이즈리
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(508)
	RETURN RESULT
;수면간중
SIF CFLAG:장난 > 1
	RETURN 0
;남자라면 안 돼
SIF !(TALENT:2 & 1)
	RETURN 0
;체격 차이가 있으면(자) 안 돼
SIF ABS(TALENT:체형 - TALENT:PLAYER:체형) > 2
	RETURN 0
;[절벽]이라면 불가
SIF TALENT:가슴사이즈 == -2
	RETURN 0
;[폭유]라면 기교 LV2 이상,[거유]가 아니면 기교 4이상,[작은 가슴]이라면 기교 5 필요
SIF TALENT:가슴사이즈 + ABL:기교 < 4
	RETURN 0
;조교자가 남자나 반인반요인가 후타나리이 아니면 안 돼
SIF !(TALENT:PLAYER:2 & 2)
	RETURN 0
;고양이혀의 조교자의 경우는 기교 Lv2 이상 필요
SIF TALENT:PLAYER:고양이혀 && ABL:PLAYER:기교 < 2
	RETURN 0
;고양이혀의 노예의 경우는 기교 Lv2 이상 필요
SIF TALENT:고양이혀 && ABL:기교 < 2
	RETURN 0
;상반신착의
SIF TEQUIP:1 > 1
	RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:밧줄
	RETURN 0
;침대 룸이 아니면 안 돼
SIF !BEDROOM(CFLAG:PLAYER:현재위치) && !(BATHROOM() && ITEM:20)
	RETURN 0
;정지 중에는 불가
SIF FLAG:70 == 1
	RETURN 0

RETURN 1

