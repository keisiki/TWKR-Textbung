;-------------------------------------------------
;더블 파이즈리
;조수에 의한 커맨드, 및 레즈비언 플레이, 레벨 3
;-------------------------------------------------
@SCOM11
;-------------------------------------------------
;실행 결정
;-------------------------------------------------
STR:0 = 더블 파이즈리
;-------------------------------------------------
;COM별 FLAG의 설정
;-------------------------------------------------
;행위자
TCVAR:116 = TARGET
;TCVAR:0 사정 대상 플래그(비트 0=TARGET 1=PLAYER 2=MASTER 3=ASSI)
;TCVAR:1 예비 사정 대상 플래그(비트를 사용하고 싶지 않은 인용, CFLAG:100으로 번역됩니다)	(1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI)
;TCVAR:2 사정장소 플래그(-1=콘돔 1=질내 2=애널 3=수음 4=펠라티오 5=파이즈리 6=가랑이 7=풋잡 8=체표 9=애널 봉사 10=도구 11=촉수)

;두 명째
LOCAL = MASTER_POSE(SET_B, SET_P, 1)
LOCALS = {TARGET}/{LOCAL}/
CSTR:PLAYER:10 += LOCALS
TCVAR:MASTER:2 = 5
;강도
TCVAR:MASTER:4 = 2000
;봉사 쾌락
TCVAR:108 = 1
TCVAR:LOCAL:108 = 1

;-------------------------------------------------
;소스 추가 처리
;-------------------------------------------------
;소비 체력, 기력
DOWNBASE:체력 += 10
DOWNBASE:기력 += 100
SOURCE:MASTER:쾌C = 150

SOURCE:쾌B = 150
SOURCE:사랑 = 200
SOURCE:성행동 = 500
SOURCE:달성 = 250
SOURCE:굴종 = 900
SOURCE:일탈 = 400
SOURCE:반감 = 400

;위 쪽으로 계산한 더러운 데이터
SOURCE:불결 = Y*40 + 100

;소비 체력, 기력
DOWNBASE:LOCAL:체력 += 10
DOWNBASE:LOCAL:기력 += 100

SOURCE:LOCAL:쾌B = 150
SOURCE:LOCAL:사랑 = 100
SOURCE:LOCAL:성행동 = 250
SOURCE:LOCAL:달성 = 120
SOURCE:LOCAL:굴종 = 450
SOURCE:LOCAL:일탈 = 400
SOURCE:LOCAL:반감 = 400

;위 쪽으로 계산한 더러운 데이터
SOURCE:LOCAL:불결 = Y*40 + 100
;-------------------------------------------------
;특수소스처리
;-------------------------------------------------
;억압, 저항 소유에는, 억울소스 추가
SIF TALENT:자기애 < 0 || TALENT:저항
	SOURCE:울굴 += 600
SIF TALENT:LOCAL:자기애 < 0 || TALENT:LOCAL:저항
	SOURCE:LOCAL:울굴 += 600

;-------------------------------------------------
;더러움의 처리
;-------------------------------------------------
;노예의 B⇔조교자의 P의 더러움이 이동
STAIN:5 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:5

;조수의 B⇔조교자의 P의 더러움이 이동
STAIN:LOCAL:5 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:LOCAL:5

;-------------------------------------------------
;경험의 처리
;-------------------------------------------------
EXP:LOCAL:파이즈리경험 ++
EXP:파이즈리경험 ++
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF HETEROSEX(TARGET, PLAYER)
	EXP:(50 + HETEROSEX(TARGET, PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(TARGET, PLAYER)) += 3
ENDIF
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF HETEROSEX(TARGET, LOCAL)
	EXP:LOCAL:(50 + HETEROSEX(TARGET, LOCAL)) += 3
	EXP:TARGET:(50 + HETEROSEX(TARGET, LOCAL)) += 3
ENDIF
;동성애 레즈비언이라면 반환값 1 동성애자라면 반환값 2
IF HETEROSEX(LOCAL, PLAYER)
	EXP:LOCAL:(50 + HETEROSEX(LOCAL, PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(LOCAL, PLAYER)) += 3
ENDIF

RETURN 1

@CAN_SCOM11(ARG)
SKIPDISP ARG
PRINTL 더블 파이즈리
;-------------------------------------------------
;실행 판정 처리
;-------------------------------------------------
A = 0
S = 0

;모든 명령에 공통의 요소를 고려
;(순종이 높다고 명령에 따르기 쉬운 등)
CALL COM_ORDER

;ABL:욕망
IF ABL:욕망
	SIF S
		PRINT  + 
	A += ABL:욕망 * 1
	PRINT 욕망
	PRINTV 'LV, ABL:욕망, '(, ABL:욕망 * 1, ')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF S
		PRINT  + 
	A += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV, ABL:봉사정신, '(, ABL:봉사정신 * 4, ')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:정액중독
	SIF S
		PRINT  + 
	A += ABL:정액중독 * 3
	PRINT 정액중독
	PRINTV 'LV, ABL:정액중독, '(, ABL:정액중독 * 3, ')
	S = 1
ENDIF

IF MARK:쾌락각인
	SIF S
		PRINT  + 
	A += MARK:쾌락각인 * 1
	PRINT 쾌락각인
	PRINTV 'LV, MARK:쾌락각인, '(, MARK:쾌락각인 * 1, ')
	S = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정, 5)
	SIF S
		PRINT  + 
	A += GETPALAMLV(PALAM:욕정, 5) * 1
	PRINT 욕정
	PRINTV 'LV, GETPALAMLV(PALAM:욕정, 5), '(, GETPALAMLV(PALAM:욕정, 5) * 1, ')
	S = 1
ENDIF
;수줍음
IF TALENT:수치심 > 0
	PRINT  - 
	A -= 1
	PRINT 수줍음
	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 둔감
IF TALENT:악취내성 > 0
	SIF S
		PRINT  + 
	A += 1
	PRINT 악취 둔감
	PRINTV '(, 1, ')
	S = 1
ENDIF
;악취 민감
IF TALENT:악취내성 < 0
	PRINT  - 
	A -= 3
	PRINT 악취 민감
	PRINTV '(, 3, ')
	S = 1
ENDIF
;헌신적
IF TALENT:헌신적
	SIF S
		PRINT  + 
	A += 6
	PRINT 헌신적
	PRINTV '(, 6, ')
	S = 1
ENDIF
;쾌감에 부정(음란, 창부로 무효)
IF TALENT:쾌감응답 < 0 && !TALENT:음란
	PRINT  - 
	A -= 1
	PRINT 쾌감에 부정
	PRINTV '(, 1, ')
	S = 1
ENDIF
;남성기피(연모, 친애로 무효)
IF OTOKOGIRAI(TARGET) == 1
	PRINT  - 
	A -= 12
	PRINT 남성기피
	PRINTV '(, 12, ')
	S = 1
ENDIF
;음란
IF TALENT:음란 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 음란
	PRINTV '(, 5, ')
	S = 5
ENDIF
;창부
IF TALENT:음란 > 1 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINT 창부
	PRINTV '(, 5, ')
	S = 5
ENDIF

;조교자가 후타나리
IF (TALENT:PLAYER:2 & 3) == 3
	SIF S
		PRINT  + 
	A += 8
	PRINT 후타나리
	PRINTV '(, 8, ')
	S = 1
ENDIF

Y = 0
;애액의 더러움
SIF STAIN:PLAYER:2 & 1
	Y += 1
;정액의 더러움
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널의 더러움
SIF STAIN:PLAYER:2 & 8
	Y += 7
SIF STAIN:PLAYER:2 & 16
	Y += 15
Y = Y * (2 - TALENT:악취내성) / 2

;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	PRINT 더러움 있음
	;악취 둔감
	IF TALENT:악취내성 > 0
		PRINT , 악취 둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT , 악취 민감
	ENDIF
	PRINTV '(, Y, ')
	S = 1
ENDIF

;합계를 표시(38이상으로 실행)
PRINT  = 
PRINTV A

V = 38
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행치
PRINTV V

WAIT
SKIPDISP 0
;실행할 수 없다
SIF A < V
	RETURN 0
RETURN 1

;-------------------------------------------------
;선택 가능 판정
;-------------------------------------------------
@COM_ABLE511
;더블 파이즈리
SIF !TFLAG:100
	RETURN 0
;일괄관리
SIF GLOBAL_COMABLE(511)
	RETURN RESULT
LOCAL = MASTER_POSE(SET_B, SET_P, 1)
;주인이 남자나 반인반요인가 후타나리이 아니면 안 돼
SIF !(TALENT:PLAYER:2 & 2)
	RETURN 0
;고양이혀의 노예의 경우는 기교 Lv2 이상 필요
SIF TALENT:고양이혀 && ABL:기교 < 2
	RETURN 0
;고양이혀의 조수의 경우는 기교 Lv2 이상 필요
SIF TALENT:LOCAL:고양이혀 && ABL:LOCAL:기교 < 2
	RETURN 0

;조수가 봉사정신 LV3 이상, 기교 LV4 이상이 아니면 안 돼
SIF ABL:LOCAL:기교 < 4 || ABL:LOCAL:봉사정신 < 3
	RETURN 0
;조수와 TARGET의 양쪽 모두에 가슴이 없으면 안 돼
SIF !(TALENT:2 & 1) || !(TALENT:LOCAL:2 & 1)
	RETURN 0
;상반신착의
SIF TEQUIP:1 > 1
	RETURN 0
;밧줄 사용 중에는 불가
SIF TEQUIP:밧줄 || TEQUIP:LOCAL:밧줄
	RETURN 0
;정지 중에는 불가
SIF FLAG:70 == 1
	RETURN 0
RETURN 1

