@시작소지금설정
FOR LOCAL, 1, CHARANUM
	SELECTCASE 소득수준:LOCAL
		CASE M_거지
			CFLAG:LOCAL:시작소지금 = 1000 ; 월 6000원
		CASE M_저소득층
			CFLAG:LOCAL:시작소지금 = 5000 ; 월 30000원
		CASE M_차상위
			CFLAG:LOCAL:시작소지금 = 8000 ; 월 48000원
		CASE M_중하위
			CFLAG:LOCAL:시작소지금 = 10000 ; 월 60000원
		CASE M_일반
			CFLAG:LOCAL:시작소지금 = 13000 ; 월 78000원
		CASE M_부유층
			CFLAG:LOCAL:시작소지금 = 25000 ; 월 150000원
		CASE M_부자
			CFLAG:LOCAL:시작소지금 = 40000 ; 월 240000원
		CASE M_갑부
			CFLAG:LOCAL:시작소지금 = 70000 ; 월 420000원
		CASEELSE
			CFLAG:LOCAL:시작소지금 = 10000 ; 월 60000원
	ENDSELECT 
NEXT
;-------------
FOR LOCAL, 1, CHARANUM
	CFLAG:LOCAL:소지금 = CFLAG:LOCAL:시작소지금
NEXT


