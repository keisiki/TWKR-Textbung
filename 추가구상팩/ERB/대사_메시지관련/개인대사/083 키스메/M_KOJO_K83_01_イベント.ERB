		ENDIF
	ENDSELECT
;通常
CASEELSE
	IF TALENT:[[키스메]]:연인
		PRINTFORMW 「아, %CALLNAME:MASTER%!」
	ELSEIF TALENT:[[키스메]]:연모
		PRINTFORMW 「아, %CALLNAME:MASTER%……!」
	ELSEIF TALENT:[[키스메]]:사모
		PRINTFORMW 「아, %CALLNAME:MASTER%……」
	ELSE
		PRINTFORMW 「앗! 아, 안녕……」
	ENDIF
ENDSELECT

