				TALENT:[[키스메]]:기분 = -1
			ELSEIF TALENT:[[키스메]]:연모
				PRINTFORMW 「아, %CALLNAME:MASTER%…… 아, 안… 녕……」
				;良くならない
				TALENT:[[키스메]]:기분 = MIN(TALENT:[[키스메]]:기분, 0)
			ELSEIF TALENT:[[키스메]]:사모
				PRINTFORMW 「아, %CALLNAME:MASTER%……」
				;良くならない
				TALENT:[[키스메]]:기분 = MIN(TALENT:[[키스메]]:기분, 0)
			ELSE
				PRINTFORMW 「앗! 아, 안녕……」
			ENDIF
			;バレンタイン
			SIF DAY:2 == 4 && DAY:3 == 14
				CALL M_KOJO_K83_ENCOUNT("バレンタイン")
