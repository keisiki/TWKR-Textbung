		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6 && !TCVAR:[[키스메]]:오늘만났는지
		;ハロウィン
		IF DAY:2 == 3 && DAY:3 == 30
			CALL M_KOJO_K83_ENCOUNT("ハロウィン")
		ELSE
			CALL M_KOJO_K83_ENCOUNT("通常")
			;バレンタイン
			SIF DAY:2 == 4 && DAY:3 == 14
				CALL M_KOJO_K83_ENCOUNT("バレンタイン")
		ENDIF
		RETURN 1
	ENDIF
