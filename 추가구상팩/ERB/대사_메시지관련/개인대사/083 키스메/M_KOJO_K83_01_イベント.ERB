;==================================================
;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K83_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = M_KOJO_K83_UFUFU([[키스메]])
;-------------
IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「하아, 하아…… 피, 피곤해……」
		PRINTFORMW 「미, 미안해…… %CALLNAME:MASTER%……」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		PRINTFORMW 「하아, 하아…… 이, 이제, 충분해……」
		PRINTFORMW 「%CALLNAME:MASTER%…… 고, 고마워……」
		TCVAR:[[키스메]]:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		PRINTFORMW 「햐앗!!」
		IF TALENT:[[키스메]]:연인
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「응…… 벌을 줘야겠네……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「응…… 교대할 차례네……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				PRINTFORMW 「으…… 미, 미안해……!」
			CASEELSE
				PRINTFORMW 「으…… 요, 용서해 줘……!」
			ENDSELECT
		ENDIF
		TCVAR:[[키스메]]:351 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		IF TALENT:[[키스메]]:연인
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「%CALLNAME:MASTER%…… 좋아하는구나……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「자, 많이 놀아 줄게……HPH」", "W")
			ENDSELECT
		ELSE
			SELECTCASE RAND:2
			CASE 1
				CALL HPH_PRINT(@"「후회해도 늦은 거야……HPH」", "W")
			CASEELSE
				CALL HPH_PRINT(@"「후후…… 그럼, 봐 주지 않을게……HPH」", "W")
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

