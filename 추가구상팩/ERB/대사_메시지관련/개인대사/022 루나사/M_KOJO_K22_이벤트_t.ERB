@M_KOJO_SPEVENT_K22_3(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;통상 귀가
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		IF TALENT:연인
			PRINTFORMW 「…이만 돌아갈 시간이구나. 내일, 또 봐…」
		ELSE
		PRINTFORMW 「즐거웠어. 가는 길 조심해」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인 소거 이벤트
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「…나도, 조금 끈질겼지. 미안…」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인 2>1 레벨 다운 이벤트
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「…즐거웠어, 또 기회가 있으면 좋아요」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;반발각인 3>2 레벨 다운 이벤트
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「당신이 한 짓, 용서한 게 아냐」
		RETURN 1
	ENDIF
	PRINTFORML 
ENDIF
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"계 이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;방에서 만남
;==================================================
@M_KOJO_EVENT_K22_1(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;MASTER가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:연인
			PRINTFORMW 「응, 여기에 오며…」
		ELSE
			PRINTFORMW 「…아아, 상태는 어때?」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 들어 왔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연인
			PRINTFORMW 「찾았어… %CALLNAME:MASTER%」
		ELSE
			PRINTFORMW 「아아, 아무래도」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 함께 들어간다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「아… 괜찮을까」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 나와 갔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 「에엣, 미, 미안」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET가 목욕해 와 내쫓아졌다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
	;루나사는 이렇게 그림들 깐데 있고, 는 두
		PRINTFORMW 「들어가고 싶은거야. …나올 수 있어?」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출처에서 조우했다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMW 「아, 안녕하세요. 오늘의 기압은…」
		;일기 예보 시키고 싶지만 무리겠네요?
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;타캐릭터와 데이트중에 조우했다
	;ARG:1에 데이트상대
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW 「질투할 수 있어요」
		RETURN 1
	ENDIF
	;그 외
	PRINTFORML  
ENDIF
RETURN 1

;==================================================
;아침에 일어 났을 때(안녕 말)
;==================================================
@M_KOJO_EVENT_K22_2(ARG, ARG:1)
;말 주인은 TARGET
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;비동거(同棲) 한편 상대가 먼저 일어나고 있다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET 방에 MASTER가 들어가 있다(연모시 동침중취침시만?
