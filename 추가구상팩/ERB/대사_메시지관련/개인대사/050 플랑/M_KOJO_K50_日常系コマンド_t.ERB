		;PRINTFORML 「」
		RETURN 1
	ENDIF
	;상대 주도 밀어 넘어뜨려지고 안
	IF CFLAG:우후후 == 2
		;고추 붙어 있다
		IF TALENT:TARGET:2 & 2
			;PRINTFORML 「」
		;고추 붙지 않다
		ELSE
			;PRINTFORML 「」
		ENDIF
		RETURN 1
	ENDIF
	;당신 주도 덮치기중
	IF CFLAG:우후후 == 1
		;PRINTFORML 「」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;특수 분기 사용시에는 코멘트 아웃을 해방해 주세요
	;-------------------------------------------------
	;;임신중
	;IF TALENT:임신
