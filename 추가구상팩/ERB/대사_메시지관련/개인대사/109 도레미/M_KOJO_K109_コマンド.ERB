LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF FLAG:70
			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORML 「물에 잘 씻겨진다고요? 그런 문제라면…하아…알겠습니다」
			PRINTFORMDW 도레미는 얌전히 로션을 발라지고 있다
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
			CALL HPH_PRINT,@"「후훗HPH질척질척 거리네요」","L"
			PRINTFORMDL 체온에 데워진 로션을 도레미에게 바르고 있다
			CALL HPH_PRINT,@"「자…다음에는 어떤걸？HPH」","W"
		RETURN 1
	;屈服3
	ELSEIF MARK:괘씸각인 == 3
			CALL HPH_PRINT,@"「으응…미끌미끌해서…HPH」","L"
			PRINTFORMDL %마스터는% 손바닥에 로션을 바르고는,도레미에게 발라주었다
			PRINTFORMDW 민감한 부분에 손가락이 미끄러져 내릴때마다, 도레미는 흠칫하고 몸을 떨었다
		RETURN 1
	;それ以外
	ELSE
			PRINTFORML 「…아, 차갑지 않네요…」
			PRINTFORMDW 데워둔 거니까！
		RETURN 1
	ENDIF
ENDIF
RETURN 1

