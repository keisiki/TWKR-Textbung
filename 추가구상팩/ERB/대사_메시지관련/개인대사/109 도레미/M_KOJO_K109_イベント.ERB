;相手が押し倒しを解除
;==================================================
@M_KOJO_EVENT_K109_30(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェ옷ク（=0, 非表示,1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
	;疲れた
		PRINTFORML 「하아…조금 피곤하네요…」
		PRINTFORMDL 덮쳐 넘어뜨리고는 참으로 제멋대로의 말투다
		PRINTFORMDL %마스터는% 한숨을 쉬며 옷을 정리했다
		TCVAR:109:발정 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		IF TALENT:연모
			CALL HPH_PRINT,@"「하아…기분 좋았다…HPH」","L"
			PRINTFORMDL 욕망을 완전히 발산한 도레미는 피부에 반들거리게 윤기를 흘리며 만족스러워하고 있다
			CALL HPH_PRINT,@"「……또, 하자구요…HPH」","L"
			PRINTFORMDW 녹아내린 목소리로 귓가에 속삭여지자, %마스터는% 당장이라도 해버리고 싶은 충동을 느꼈다…
			TCVAR:109:발정 = 0
		ELSE
			PRINTFORML 「그…이런 일에 말려들게 해서 정말로 죄송합니다」
			PRINTFORMDL 도레미는 %CALLNAME:MASTER%를 성적으로 덮쳐버린 것을 후회하고 있는 것 같다
			PRINTFORMDL %마스터는%, 기분 좋아서 좋았다。라고 도레미를 위로했다
			TCVAR:109:발정 = 0
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを押し倒された
		CALL HPH_PRINT,@"「쿡HPH다음은 %CALLNAME:MASTER%의 차례인가요…HPH」","L"
		PRINTFORMDL 도레미는 입맛을 다시면서 이쪽을 열렬하게 쳐다본다
		PRINTFORMDW 열에 들뜬 한 쌍의 커플은 서로 녹아내리듯이 피부를 겹쳐갔다…
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		PRINTFORML 「별로, %CALLNAME:MASTER%를 망가뜨려버려도 상관없는 거겠죠？」
		PRINTFORMDW 도레미는 입맛을 다시며 애무를 재개했다
		RETURN 1
	ENDIF
ENDIF
RETURN 1


;==================================================
;一日の終了時(AFTERTRAニーの前),面識がなければ呼び出されない
;==================================================
@M_KOJO_EVENT_K109_31(ARG,ARG:1)
;口上主はTARGET
;ARGは会읏てない日数
;何日以上会읏てなか읏たら愚痴を言うとかできます
;CFLAG:オナバレで分岐入れてもよさそう
;--------------------------------------------------
;記入チェ옷ク（=0, 非表示,1, 表示）
LOCAL = 0
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;今日会읏ていない
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今日会읏ている
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		RETURN 1
	ENDIF
ENDIF
RETURN 1

