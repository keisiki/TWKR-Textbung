@M_KOJO_EVENT_K109_3(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェ옷ク（=0, 非表示,1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に帰る（定時にな읏て寝る）
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「그럼, 좋은 꿈 꾸세요」
		PRINTFORMDW 도레미는 돌아갔다
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:연모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「………졸지 않았다고요？」
				PRINTFORMDW 도레미는 졸려보인다
			CASE 1
				PRINTFORML 「…과연…」
				PRINTFORMDW 졸린 것인지, 맞장구가 적당하다
			CASE 2
				PRINTFORML 도레미가 조용해졌기 때문에, 코를 막아서 숨을 쉴 수 없게 해보았다
				PRINTFORMW 잠시 뒤, 숨이 갑갑해진 것인지 벌떡 일어났다
			ENDSELECT
		RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会話というよりは寝言？）
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:5
		CASE 0
			PRINTFORML 「으으…이 꿈의 맛은 청산가리 맛…」
			PRINTFORMDL 무슨 맛인거야
		CASE 1
			PRINTFORML 「……구우…」
			PRINTFORMDW 도레미는 깊게 잠들어있는 것 같다
		CASE 2
			PRINTFORML 「…사쿠라모찌…」
			PRINTFORMDW 무슨 꿈인 걸까
		CASE 3
			PRINTFORML 「…%CALLNAME:MASTER%…」
			PRINTFORMDW 잠꼬대로 이름이 불려졌다
		CASE 4
			PRINTFORML 「…스페이스 데브리가…」
			PRINTFORMDL 우주에 있는 꿈인건가
		ENDSELECT
	RETURN 1
	ENDIF
	;-------------------------------------------------
	;イタズラ中
	;記入チェ옷ク（=0, 非表示,1, 表示）
	;LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		IF TALENT:연모
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「아…으으…」
			CASE 1
				PRINTFORMW 「후…으응…」
			CASE 2
				PRINTFORMW 「됐어…앗…」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「……으응…」
			CASE 1
				PRINTFORMW 「…아…응…」
			CASE 2
				PRINTFORMW 「…읏…」
			ENDSELECT
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;쇠약中（연모付き拠点内キャラがMAIN_MAPで쇠약状態にな읏た場合か,오招き部屋で相手が쇠약した場合に発生
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML 「조금 쉬게 해주세요…」
		PRINTFORMDW 도레미는 녹초가 되어 있다
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（部屋まで運ばれる）
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORML 「…좋은 꿈 꾸세요。%CALLNAME:MASTER%」
		PRINTFORMDL 도레미는 몸을 기대어왔다
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが自室まで戻읏てきた
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た
		;記入チェ옷ク（=0, 非表示,1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 0
			PRINTFORML 「맥이라고 해도 수면은 필요하니까요…」
			PRINTFORMDW 도레미는 파자마로 갈아입고서, 작은 숨소리를 내기 시작했다…
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;部屋から追い出された
		;記入チェ옷ク（=0, 非表示,1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;他キャラとのＨを止めさせて寝た
		;記入チェ옷ク（=0, 非表示,1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 2
			IF TALENT:연인
				PRINTFORMW 「%CALLNAME:MASTER%…」
				PRINTFORMDW 움찔 해서 뒤돌아보면, 도레미가 유령처럼 서있었다
				PRINTFORML 「오늘의 꿈은 기대가 되는걸요？…각오해두세요」
				PRINTFORMDW 옷을 고쳐 입으면서 %마스터는% 유서를 남겨야 할지 고민했다
			ELSE
				PRINTFORMW 「즐기시는 중에 죄송하지만…」
				PRINTFORMDW 목소리에 돌아보자,도레미가 경멸의 시선으로 내려다보고 있었다
				PRINTFORML 「그런 행위를 그만두라고 하지는 않겠습니다…만, 제 방에서 발정이 나서 교미하는 꼴은 그만두면 안될까요？」
				PRINTFORMDW 도레미에게 경고를 받고서, 행위를 계속할 분위기도 사라져버렸다
			ENDIF
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;非在住TARGETがMAIN_MAPで休憩
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;前のターンから一緒の部屋にいる状態で相手が寝る（오招き部屋含む（
	;記入チェ옷ク（=0, 非表示,1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		PRINTFORMW 
		RETURN 1
	ENDIF
	;その他
	PRINTFORML  
ENDIF
RETURN 1
