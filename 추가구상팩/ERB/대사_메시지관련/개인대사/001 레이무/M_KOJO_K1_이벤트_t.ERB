@M_KOJO_EVENT_K1_23(ARG, ARG:1)
;말 주인은 LOCAL
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;거절당했다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:사모
			PRINTL 
			PRINTFORMW 「――응? 같이 목욕하자고..?」
			PRINTL
			PRINTFORMW 레이무는 머리를 갸웃하며 %CALLNAME:MASTER%에게 되물었다.
			PRINTL
			PRINTFORMW 「으으응…… 안 된다구, 안 돼, 아무리 생각해도 안 돼」
			PRINTL
