LOCAL = 1
;-------------------------------------------------
IF LOCAL
	VARSET LOCAL
	IF BASE:발기 >= 1000
		IF !CFLAG:291
			발기상태 += "팬티를 찢을 것 같을 만큼(정도) 발기했다"
		ELSE
			발기상태 += "딱딱하게 우뚝 선다"
		ENDIF
	ELSE
		IF !CFLAG:291
			발기상태 += "팬티의 안에 어떻게든 들어가 있다"
		ELSE
			발기상태 += "힘없이 처진다"
		ENDIF
	ENDIF
	발기상태 += TINKO(TARGET)
	IF !CFLAG:291
		IF PALAM:윤활 > PALAMLV:4
			은밀한곳상태 += "은밀한 곳이 젖어 비치는"
		ELSEIF PALAM:윤활 > PALAMLV:2
			은밀한곳상태 += "애액의 얼룩이 떠오른"
		ENDIF
		은밀한곳상태 += "팬티"
	ELSE
		IF PALAM:윤활 > PALAMLV:4
			은밀한곳상태 += "애액으로 흠뻑 젖어 있는"
		ELSEIF PALAM:윤활 > PALAMLV:2
			은밀한곳상태 += "애액이 촉촉한"
		ENDIF
		은밀한곳상태 += "은밀한 곳"
