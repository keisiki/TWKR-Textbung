;==================================================
;100,스팽킹
;==================================================
@M_KOJO_MESSAGE_COM_K105_100
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K105_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K105_100_1
;-------------------------------------------------
;기입 체크（=0, 비표시, 1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;특수 상황
	IF FLAG:70 ;시간정지
		PRINTFORMW 
		RETURN 1
	ELSEIF CFLAG:MASTER:장난 == 2
		PRINTFORMW 
		RETURN 1
	ELSEIF TCVAR:만취 == 1
		PRINTFORMW 
		RETURN 1
	;통상
	ELSE
		IF ABL:마조끼 < 2
			IF TALENT:105:연인 || TALENT:105:연모
				PRINTFORML 「아얏！　잠깐, 뭐하는거야」
				PRINTFORMW 「미얀하지만, 그런거 취미가 아냐」
			ELSE
				PRINTFORMW 「으읏……뭐야, 갑자기」
				SIF TALENT:사모
					PRINTFORMW （아파……마음대로 해버리고는, 이거 완전 터무니없는 사디스트네）
			ENDIF
		ELSE
			IF TALENT:105:연인 || TALENT:105:연모
				PRINTFORML 「아앗%UNICODE(0x2665) *1%　정말, 아프다구……」
				PRINTFORMW 「아, 뭐……그, 싫다는건, 아니고……」
			ELSE
					PRINTFORML 「읏！　아……%UNICODE(0x2665) *1%」
					PRINTFORMW 「정말……」
				SIF TALENT:사모
					PRINTFORMW （위험해, 이거……기분좋아……%UNICODE(0x2665) *1%）
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

