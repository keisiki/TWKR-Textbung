;이름을 가르치는 이벤트(함수)
;/훌륭한 쿄코 말의 환파크라고 감탄하지만 어디도 이상하지는 않다
;-------------------------------------------------

@NAME_SET_K39
PRINTL 이름을 입력해 주세요(직접 입력하는지, 버튼으로 선택해 주세요)
IF NAME:MASTER != "당신"
	LOCALS = %NAME:MASTER%
ELSE
	LOCALS = %CALLNAME:MASTER%
ENDIF
PRINTBUTTON @"[이름(%LOCALS%)을 자칭한다]", @"%LOCALS%"
PRINTL 　
;이름을 입력하는 루프
$INPUT_LOOP_ONAMAE
INPUTS
; 줄머리행 꼬리의 공백 문자 삭제
LOCALS = %RESULTS%
REPLACE LOCALS, "\(\^\\s\+\|\\s\+\$)", ""
;10문자 이상은 받아들이지 않는다
IF STRLENS(LOCALS) > 10
	CLEARLINE 1
	REUSELASTLINE 「…아무리 그래도 너무 길어. 조금 더 짧게는 안될까?」
	GOTO INPUT_LOOP_ONAMAE
;공백은 받아들이지 않는다
ELSEIF RESULTS == ""
	PRINTL 이름을 입력해 주세요
	GOTO INPUT_LOOP_ONAMAE
ELSE
	; 동성 동명
	IF FLAG:역극 != 39 && STRCOUNT(LOCALS, "나즈린")
		CLEARLINE 1
		REUSELASTLINE 「…내 이름을 대는건 그다지 유쾌하지 않네」
		GOTO INPUT_LOOP_ONAMAE
	; 뱌쿠렌이 주인은 아니고, 뱌쿠렌이라고 하는 프레이즈가 포함되어 있다. 혹은
	; 별이 주인은 아니고, 토라마루 혹은 쇼라고 하는 프레이즈가 포함되어 있다
	ELSEIF (FLAG:역극 != 55 && STRCOUNT(LOCALS, "히지리\*\\s\*뱌쿠렌")) || (FLAG:역극 != 87 && STRCOUNT(LOCALS, "\(토라마루\)\*\\s\*쇼\$"))
		CLEARLINE 1
		REUSELASTLINE 「…시시한 농담은 그만해」
		GOTO INPUT_LOOP_ONAMAE
	; 묘렌사세는 곧
	; 응 그렇네요 타타라 승고로라든지 가르치는 사람도 있을지도 모르는 걸
	ELSEIF (FLAG:역극 != 40 && STRCOUNT(LOCALS, "\(타타라\)\*\\s\*코가사")) || (FLAG:역극 != 85 && STRCOUNT(LOCALS, "\(쿠모이\)\*\\s\*이치린")) || (FLAG:역극 != 86 && STRCOUNT(LOCALS, "\(\(무라사\)*\\s\*미나미츠\|무라사\)"))
		CLEARLINE 1
		REUSELASTLINE 「…우리 사람과 혼동하기 쉬워. 다른걸로 해주지 않을래?」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FLAG:역극 != 69 && STRCOUNT(LOCALS, "\(후타츠이와\)\*\\s\*마미조")
		CLEARLINE 1
		REUSELASTLINE 「…그 녀석은 꼬리가 튀어나와 있어.」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF (FLAG:역극 != 41 && STRCOUNT(LOCALS, "\(호쥬\)\*\\s\*누에"))
		CLEARLINE 1
		REUSELASTLINE 「…누에는 지금 지붕 위에 있는데.」
		GOTO INPUT_LOOP_ONAMAE
	; 도교 세력은 곧
	ELSEIF (FLAG:역극 != 89 && STRCOUNT(LOCALS, "\(미야코\)\*\\s\*요시카")) || (FLAG:역극 != 90 && STRCOUNT(LOCALS, "\^곽\*\\s\*청아")) || (FLAG:역극 != 91 && STRCOUNT(LOCALS, "\(소가\)\*\\s\*토지코")) || (FLAG:역극 != 92 && STRCOUNT(LOCALS, "\(모노노베\)\*\\s\*후토")) || (FLAG:역극 != 56 && STRCOUNT(LOCALS, "\(\(토요사토미미\)\*\\s\*미코\|성덕태자\)"))
		CLEARLINE 1
		REUSELASTLINE 
		GOTO INPUT_LOOP_ONAMAE
	; 하쿠레이 레이무라고 하는 프레이즈가 포함되어 있다. 혹은
	; 코치야 사나에라고 하는 프레이즈가 포함되어 있다
	ELSEIF STRCOUNT(LOCALS, "\(하쿠레이\)\*\\s\*레이무") || STRCOUNT(LOCALS, "\(코치야\)\*\\s\*사나에")
		PRINTFORML 「그 상식 밖의 무녀랑 가족이야? 뭐 괜찮지만.」
	; 키리사메 마리사라고 하는 프레이즈가 포함되어 있다.
	ELSEIF STRCOUNT(LOCALS, "\(키리사메\)\*\\s\*마리사")
		PRINTFORML 「그 이상한 마법사와 묘한 인연이 있나보네. 뭐 괜찮지만.」
	; 직접 입력이라면 안된다고 생각했는지? 연주하군!
	ELSEIF FIRSTTIME("U39-1") && ABL:친밀 < 4 && STRCOUNT(LOCALS, "\(\(\(형(오빠)\|에게 있고\)\|\(언니(누나)\|응\)\)\(씨\|\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「바보냐 너는.」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U39-1") && ABL:친밀 < 7 && STRCOUNT(LOCALS, "\(\(\(형(오빠)\|에게 있고\)\|\(언니(누나)\|응\)\)\(모양\|님\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「바보냐 너는.」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF FIRSTTIME("U39-1") && (ABL:순종 < 7 || ABL:봉사정신 < 7) && STRCOUNT(LOCALS, "\(\(\|\)\(주인\|주인\)\(모양\|님\)\)$")
		CLEARLINE 1
		REUSELASTLINE 「바보냐 너는.」
		GOTO INPUT_LOOP_ONAMAE
	ELSEIF  FIRSTTIME("U39-1") && (!TALENT:연인 || !TALENT:연모) && STRCOUNT(LOCALS, "\(\(남편\|남편\)\(모양\|님\)\)\$")
		CLEARLINE 1
		REUSELASTLINE 「바보냐 너는.」
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTFORML 「%RESULTS%…구나」
	ENDIF
	PRINTL [0] 네
	PRINTL [1] 아니오
	;입력한 이름을 확정하는 루프
	$INPUT_LOOP_KAKUTEI_1
	INPUT
	IF RESULT == 0
		;당신의 이름을%MASTERNAME:39%에 격납
		MASTERNAME:39 = %RESULTS%
		PRINTFORML 
	ELSEIF RESULT == 1
		PRINTL 이름을 입력해 주세요
		GOTO INPUT_LOOP_ONAMAE
	ELSE
		PRINTL 이름을 입력해 주세요
		GOTO INPUT_LOOP_ONAMAE
	ENDIF
