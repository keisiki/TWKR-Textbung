CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K61_1_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K61_1_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;처음
	;기입 체크
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		;연모
        IF TALENT:연모
            PRINTFORMW 「응읏…… %MASTERNAME:61%, 괜찮아? 자세, 힘들지 않아……?」
        ;그 이외
        ELSE
            PRINTFORMW 「일부러 이런 이상한 자세까지 하고 싶어, 이런 것 때문에……?」
        ENDIF 
		RETURN 1
	ENDIF
	;기본 세트
	;시간중
	IF FLAG:70
		;PRINTFORMW 
		RETURN 1
	;연모
	ELSEIF TALENT:연모
		IF ABL:0 >= 15
            PRINTFORMW 「응, 아, 앗, 나, %MASTERNAME:61%의 자지, 핥지 않으면, 후아앗♪」
            PRINTFORMW 애무로 꽤 느끼고 있는지, 모코우의 뜨거운 한숨이 이쪽의 성기에 닿는다……
        ELSE
            PRINTFORMW 「후, 우으응, %MASTERNAME:61%, 지쳤다면 쉬고 있어도 괜찮아, 나 혼자 할 테니까……」
        ENDIF
		RETURN 1
	;C감각 B이상
	ELSEIF ABL:0 >= 15
	     PRINTFORMW 「하읏, 후아, 무리……그렇게 하면, 입, 움직일 수 없어……」
	     RETURN 1
	;굴복 3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 「읏, 아우우, 그렇게 자극되면, 집중할 수 없어……」
		RETURN 1
	;그 이외
	ELSE
		PRINTFORMW (토할 것 같아……)
		RETURN 1
	ENDIF
ENDIF
RETURN 1

