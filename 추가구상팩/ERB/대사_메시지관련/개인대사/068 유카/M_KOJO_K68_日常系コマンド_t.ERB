;360, 몸을맡긴다
;TCVAR:20(27=골짜기를 보이는 32=앞에서 안겨든 35=치마안을 보이는 53=파이즈리 73= 77=젖을 들이마시게 하는 85=V삽입하는 86=다이스키 홀드 87=A삽입하는 88=허리 모습 V 89=허리 모습 A
;==================================================
@M_KOJO_MESSAGE_COM_K68_360
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K68_360_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K68_360_1
;-------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE TCVAR:20
		;골짜기를 보인다
		CASE 27
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「흐응……\@ TALENT:MASTER:성별 == 2 ?  당신 # 당신 \@, 그런 반응하군요……후훗」
				CASEELSE
					PRINTFORMW 「후훗……보고 있을 뿐으로 좋은거야?」
			ENDSELECT
		;앞에서 안겨든다
		CASE 32
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「\@ TALENT:MASTER:성별 == 2 ?  당신 # 당신 \@이 그 기분이라면, 이대로 질식시켜 줄까……후훗」
				CASEELSE
					PRINTFORMW 「응……!　좀, 너무 흥분하지 않아……?」
			ENDSELECT
		;치마안을 보인다
		CASE 35
			PRINTFORMW 「후훗……어때?」
		;파이즈리
		CASE 53
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW (응……이런 일 당하는 것도, 기분은 나쁘지 않네요)
				CASEELSE
					PRINTFORMW 「우후훗……기분이 좋은거야?　좀 더 격렬하게 해 준다……!」
			ENDSELECT
		;
		CASE 73
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「그렇게 멍한 얼굴 해……떨어져 버렸어 까」
				CASEELSE
					PRINTFORMW 「그렇게 응석부려……부끄럽지 않은 것일까, 후훗」
			ENDSELECT
		;젖을 들이마시게 한다
		CASE 77
			SELECTCASE RAND:2
				CASE 0
					PRINTFORM 「흐응, 싫지 않은 거예요……그러면, 두와 젖가슴 들이마셔 짊어질까……후훗
					CALL HEARTCB_K68(0xD02830)
					PRINTFORMW 」
				CASEELSE
					PRINTFORM 「아기같아요,\@ TALENT:MASTER:성별 == 2 ?  당신 # 당신 \@……아앙
					CALL HEARTCB_K68(0xD02830)
					PRINTFORMW 」
			ENDSELECT
		;85=V삽입하는 87=A삽입하는 88=허리 모습 V 89=허리 모습 A
		CASE 85,87 TO 89
			SELECTCASE RAND:2
				CASE 0
					PRINTFORM 「하아, 좀 더, 허리 거절하세요……
					CALL HEARTCB_K68(0xD02830)
					PRINTFORM !　기분 좋아지는 것은\@ TALENT:MASTER:성별 == 2 ?  당신 # 당신 \@이 아니고, 나니까……
					CALL HEARTCB_K68(0xD02830)
					PRINTFORMW !」
				CASEELSE
					PRINTFORM 「하앗
					CALL HEARTCB_K68(0xD02830)
					PRINTFORM 　\@ NOWEX:MASTER:사정 ?  이제(벌써) 안 돼?　이것……?　 # 언제까지 유지하는 것일까…… \@우후훗
					CALL HEARTCB_K68(0xD02830)
					PRINTFORMW 」
			ENDSELECT
		;다이스키 홀드
		CASE 86
			PRINTFORM 「아앙, 하아, 하앗
			CALL HEARTCB_K68(0xD02830)
			PRINTFORM 　좀,\@ TALENT:MASTER:성별 == 2 ?  당신 # 당신 \@도 움직이세요……
			CALL HEARTCB_K68(0xD02830)
			PRINTFORMW 」
		CASEELSE
			PRINTFORML 「그래요, 얌전하게 해 주세요……」
	ENDSELECT
ENDIF

RETURN 1
