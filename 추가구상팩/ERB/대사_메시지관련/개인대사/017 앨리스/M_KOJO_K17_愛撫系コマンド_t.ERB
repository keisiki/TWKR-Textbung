@M_KOJO_MESSAGE_COM_K17_9_1
#DIM iType = 0
#DIMS cText
VARSET cText
;-------------------------------------------------
;감각 부위를 선정
;-------------------------------------------------
IF TEQUIP:바이브
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_부위V)
ELSEIF TEQUIP:애널바이브
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_부위A)
ELSEIF TEQUIP:샤워
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), K17C_부위C)
ELSE
	CALL K17_SET_AUTO_PVOICE(K17_GRT_EX(), RAND:2? K17C_부위C#K17C_부위B)
ENDIF

;-------------------------------------------------
;9, 자위
;버릇 있어
;-------------------------------------------------
IF K17_RT_FG() & K17C_RTF_버릇 && !(CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐) && GET_TARGETNUM() == 1
	;버릇 있어 졸라 계 둘이서
	;망상 발표
	;일단 타입 변환
	iType = CFLAG:K17C_CF_허덕여타입
	cText = %K17_H망상()%
	CALL K17_SET_AUTO_PVOICE(, , K17_허덕여타입_H망상(RESULT:0, RESULT:1))
	
	PRINTFORMDL %마스터는% %CALLNAME:TARGET%에게 평소에 하던 것처럼 %K17_RS("자위/비밀연구")%를 했으면 좋겠다, 라고 전했다
	
	IF FIRSTTIME(@"{SELECTCOM}말")
		;첫회
		PRINTFORML 「읏!?　우으…… 사람의 약점을 이용하려 들다니……」
		PRINTFORMDL 부끄러운 듯이 뾰로통 해졌지만, 착실히 응해 주는 %CALLNAME:TARGET%였다
		PRINTW
		PRINTFORML 「잠깐 들어줘, 있잖아……하는 건 좋지만…… 혼자서…엣찌, 하고 있으면, 저기……굉장히 쓸쓸해져서……」
		PRINTFORML 「그러니까 끝나고, 제대로 잔뜩 귀여워해 주지 않으면, 안 돼?」
		PRINTFORMDL %마스터는% %CALLNAME:TARGET%의 머리를 상냥하게 쓰다듬으면서, 무릎 위에 앉혔다
	ELSE
		;2회째 이후
		PRINTFORML 「또……시키고 싶은거야?　정말 ……%K17_RS("엣찌/변태/색골")%」
		PRINTFORMDL %K17_RS("부끄러운 듯이 뾰로통 해졌지만/입으로는 그렇게 말하면서도")%, 착실히 응해 주는 %CALLNAME:TARGET%였다
		PRINTFORML 「끝나고, %K17_CNAME()%도 제대로 잔뜩 %K17_RS("귀여워해/기분 좋게 해")%주지 않으면, %K17_RS("싫어할 거니/용서하지 않을 테니/안 되니")%까 %K17_HEART()%」
		PRINTFORMDL %마스터는% %CALLNAME:TARGET%%K17_RS("의 머리를 상냥하게 쓰다듬으면서/를 상냥하게 꼭 껴안으면서")%, 완전히 지정석이 된 무릎 위에 %K17_RS("올렸다/앉혔다")%
	ENDIF
	
	PRINTW
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	PRINTFORMDL "연구열심" %타겟은%, 눈을 감고서 망상 자위에 몰두하고 있었다……
	PRINTW
	PRINTFORMDL %마스터는% %CALLNAME:TARGET%에게 도대체 어떤 망상으로 쓰여지고 있는지 귓속말로 물어봤다
	PRINTFORML 「%cText% %K17_RS("망상/상상")%으로 , %K17_RS("썼/써먹었")%습니다 %K17_BLUSH(1)%」
	PRINTW
	PRINTFORMDL %타겟은% 얼굴이 새빨갛게 되었지만, 조금 흥분한 듯한 모습으로 그렇게 고백했다
	PRINTFORMDL 망상을 폭로하는 것도 이제 완전히 플레이의 일종으로 되어 버린 듯하다……
	CFLAG:K17C_CF_허덕여타입 = iType
	RETURN 1
	
;-------------------------------------------------
;9, 자위
;범용
;-------------------------------------------------
ELSE
	;버릇 없음 or 덮쳐 계
	CALL K17_MESSAGE
	CALL K17_CLL_COUNTER_REACTIONMESSAGE
	PRINTFORML 「%K17_AUTO_PVOICE()%」
	IF !(CFLAG:K17C_CF_허덕여타입 & K17C_허덕여F_덮쳐)
		;졸라 계
		PRINTFORMDL 쾌락을 탐내는 %CALLNAME:TARGET%의 손놀림은 꽤 손에 익은 듯했다……
	ELSE
		;덮쳐 계
		PRINTFORMDL 싫어하면서도 말하는 대로 따르는 %CALLNAME:TARGET%였지만, 손놀림은 꽤 손에 익은 듯했다……
	ENDIF
	RETURN 1
ENDIF



