;### 대사색깔판정 ###################################
@M_KOJO_COLOR_K17
SETCOLOR K17C_앨리스COLOR
;여기서 SETCOLOR 하는 것으로 말에 첨가할 수 있습니다
;예：SETCOLOR 0xC07070
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"UPDATE 판정"
;※※※※※※※※※※※※※※※※※※※※※※※※※※
@M_KOJO_UPDATE_K17

;말 독자적인 UPDATE용
;ENCOUNTER 전용에 독자 분기 플래그를 가르쳤을 경우등에 부디
;UPDATE 판정으로 CFLAG를 사용하는 경우, 캐릭터 번호의 지정이 필요하게 됩니다
#DIM DYNAMIC TEMP=0
CALL COLORMESSAGE("망상 에로 아가씨 앨리스 설정·UPDATE 개시", K17C_앨리스COLOR, 1)
;******************************************************
;소질 변경 설정
;******************************************************
IF CFLAG:K17C_ID:K17C_CF_소질변화플래그 == 0
	PRINTL
	CALL CHOICE ("본 대사에 따라 앨리스의 소질을 변화시키겠습니까?","변화시킨다","변화시키지 않는다")
	PRINTL
	SELECTCASE RESULT
		CASE 0
			CFLAG:K17C_ID:K17C_CF_소질변화플래그 = 1
			PRINTFORMDL 이후의 행동에 따라 소질이 변할 때가 있습니다
		CASE 1
			CFLAG:K17C_ID:K17C_CF_소질변화플래그 = -1
			PRINTFORMDL 이후, 본 대사에 따른 소질 변화는 이루어지지 않습니다
	ENDSELECT
ENDIF

;******************************************************
;인형에 대해서 캐릭터가 있으면 정지한다
;******************************************************
TEMP = K17_GET_DOLL(0)
SIF !TEMP
	CFLAG:K17C_ID:K17C_CF_인형대사표시설정 = -1
SIF !(TEMP & K17C_샹하이)
	CFLAG:K17C_ID:K17C_CF_샹하이이벤트 = -1

IF CFLAG:K17C_ID:K17C_CF_인형대사표시설정 == 0
	PRINTL
	PRINTFORMDL 이 앨리스 대사 내에서는 가끔 인형(\@K17_GET_DOLL(K17C_샹하이) ? 샹하이# \@　\@K17_GET_DOLL(K17C_호라이) ? 호라이#\@)가 말할 때가 있습니다
	PRINTFORMD 이런 느낌→
	TRYCALL K17_인형대사(K17C_샹하이, 0)
	TRYCALL K17_인형대사(K17C_호라이, 1)
	RESETCOLOR
	PRINTFORMDL 현재의 설정[보류중-말합니다]
	PRINTL
	CALL CHOICE("인형의 대사를 표시하겠습니까? ","일단 보류","말해도 돼","입 다물게 한다")
	SELECTCASE RESULT
		CASE 0
			PRINTFORMDW 보류했습니다. UPDATE하면 다시 선택지가 나옵니다
			SIF !FLAG:대사색깔
				PRINTFORMW 또한 대사 색의 설정이 OFF라면 분간할 수 없을 겁니다
		CASE 1
			PRINTFORMDW 말을 하도록 설정했습니다
			CFLAG:K17C_ID:K17C_CF_인형대사표시설정 = 1
			SIF !FLAG:대사색깔
				PRINTFORMW 또한 대사 색의 설정이 OFF라면 분간할 수 없을 겁니다
		CASE 2
			PRINTFORMDW 입을 다물도록 설정했습니다
			CFLAG:K17C_ID:K17C_CF_인형대사표시설정 = -1
	ENDSELECT
ENDIF

;******************************************************
;이 말을 사용하기 위한만의 버그를 마음대로 수정
;******************************************************
;eraTW4. 43 수정전의 세이브 데이터
;데이트후이벤트플래그 버그 수정 보고 수정이 끝난 상태
;목욕탕의 값이 대입되고 있었기 때문에, 커맨드 선택할 때에 시간이 들어가 있던 모양
;플래그에서는 3까지 밖에 안 될 것이므로 우선 4이상이라면 수정
IF CFLAG:K17C_ID:데이트후이벤트플래그 > 3
	PRINTFORM 「데이트후이벤트플래그 버그를 검지...
	CFLAG:K17C_ID:데이트후이벤트플래그 = 0
	PRINTFORM 초기화...
	
	IF CFLAG:K17C_ID:K17C_CF_데이트이벤트 && (CFLAG:K17C_ID:기정사실 & 합의_키스)
		SETBIT CFLAG:K17C_ID:데이트후이벤트플래그 , 0
		PRINTFORM 1 갱신...
	ENDIF
	IF CFLAG:K17C_ID:K17C_CF_고백이벤트 && (CFLAG:K17C_ID:기정사실 & 합의_연인)
		SETBIT CFLAG:K17C_ID:데이트후이벤트플래그 , 1
		PRINTFORM 2 갱신...
	ENDIF
	
	PRINTFORMW 수정했습니다」
ENDIF

;찾아내 버렸다 드문 녀석
;연령 버그의 수정
;IF TALENT:MASTER:연령 == -2
;	PRINTFORM 「연령 수치 버그를 검지...
;	TALENT:MASTER:연령 = 0
;	PRINTFORMW 수정했습니다」
;ENDIF
;
;;체형 버그의 수정
;IF TALENT:MASTER:체형 == -3
;	PRINTFORM 「체형 수치 버그를 검지...
;	TALENT:MASTER:체형 = 0
;	PRINTFORMW 수정했습니다」
;ENDIF

;******************************************************
;말 넣고 나서의 로드시의 대응
;우선 관계성을 증감했을 경우는 대응할 수 있도록(듯이)
;******************************************************
;보존 플래그의 갱신
IF K17_RT_FG(K17C_ID) != CFLAG:K17C_ID:K17C_CF_RTF
	
	;이벤트 플래그재설정 어쩐지 능숙한 방식 생각한다
	;세이브 데이터로부터 키스 커맨드 선택했는지 억지로 찾는다
	IF !(FIRSTTIME(312,1, K17C_ID) || !FIRSTTIME(20,1, K17C_ID)) && !CFLAG:K17C_ID:K17C_CF_FK마스터
		PRINTFORM 「특정 SELECTCOM FIRSTTIME를 검지...
		CFLAG:K17C_ID:K17C_CF_FK마스터 = 1
		PRINTFORMW 플래그를 갱신했습니다」
	ENDIF
	
	PRINTFORM 「보존 플래그의 차이를 검지...
	CFLAG:K17C_ID:K17C_CF_RTF = K17_RT_FG(K17C_ID)
	PRINTFORMW 플래그를 갱신했습니다」
	PRINTFORML 다음번 힌트를 표시합니다
	PRINTL
	TRYCALL K17_HINT()
	PRINTW
ENDIF
;******************************************************
;말의 버전 관리
;INT 어째서 소수점 제 2를 1자리수째로 할까
;이런 취약한 것으로 좋은 것인지……? (무서워하고
;******************************************************
IF CFLAG:K17C_ID:K17C_CF_VER < K17C_VER
	
	PRINTFORM 대사의 버전 차이를 검지, UPDATE, 자기 진단을 개시...
	
	;------------
	;Ver 0.02
	;------------
	IF CFLAG:K17C_ID:K17C_CF_VER < 002
		;치마걷어 올리기로 준 기독을 취소해 수정 동작 테스트k
		;CSTR로부터 지정 캐릭터라인을 검색해 삭제　/절약하고
		TEMP = K17_AUTO_SPLIT_NUM(CSTR:K17C_ID:0,"/","330 말")
		
		;기독＆현조건식을 본다
		IF CFLAG:K17C_ID:호감도 <= 5000 && TEMP != -1
			;기독을 취소해
			CSTR:K17C_ID:0 = %K17_CHANGE_SPLIT(CSTR:K17C_ID:0,"/", TEMP)%/
			;수정 공지
			CALL COLORMESSAGE("\n 커맨드：치마를 들춘다.의 읽음을 읽지 않음으로 재설정했습니다...", K17C_호라이COLOR, 1)
			CALL COLORMESSAGE("판정 기준이 전 Ver에서 바뀌었습니다. 다시 선택 도전해보세요...", K17C_호라이COLOR, 1)
		ENDIF
	ENDIF
	;------------
	;Ver 0.03
	;------------
	IF CFLAG:K17C_ID:K17C_CF_VER < 003
		;신플래그의 초기설정 하지 않아도 괜찮지만 일단
		IF !CFLAG:K17C_ID:K17C_CF_허덕여RT
			PRINTFORM \n 새롭게 추가된 플래그의 초기 설정을 합니다...
			CALL K17_SET_AUTO_PVOICE(-1,-1, K17C_접수_애원계)
			PRINTFORMW 설정했습니다
		ENDIF
	ENDIF
	;------------
	;Ver 0.10
	;------------
	IF CFLAG:K17C_ID:K17C_CF_VER < 010
		;신플래그의 초기 설정 신음 타입의 보존
		PRINTFORML \nVer0. 10의 갱신을 실시합니다...
		IF CFLAG:K17C_ID:기정사실 & 합의_우후후
			PRINTFORM 우후후 합의를 검지...
			PRINTFORM \n 새롭게 추가된 플래그의 초기 설정을 합니다...
			CALL K17_SET_AUTO_PVOICE(, , K17C_접수_애원계)
			PRINTFORMW 설정했습니다
		ENDIF
		;성희롱
		IF !FIRSTTIME("(313|315|316) 말", 1, K17C_ID)
			PRINTFORM 성희롱 커맨드의 경험을 검지...
			PRINTFORM \n 새롭게 추가된 플래그의 초기 설정을 합니다...
			CALL K17_SET_AUTO_PVOICE(, , K17C_접수_합의덮쳐계)
			PRINTFORMW 설정했습니다
		ENDIF
		;오나바렉스 경험하고 있으면 억지로 계 보존
		IF !FIRSTTIME("오나바렉스 첫회", 1, K17C_ID)
			PRINTFORM 딸딸이들킴섹스의 경험을 검지...
			PRINTFORM \n 새롭게 추가된 플래그의 초기 설정을 합니다...
			CALL K17_SET_AUTO_PVOICE(, , K17C_접수_억지로계)
			PRINTFORMW 설정했습니다
		ENDIF
		;덮쳐진 적 있으면 기본을 보존
		IF !FIRSTTIME("밀어넘어뜨려지고", 1, K17C_ID)
			PRINTFORM 덮쳐짐 경험을 검지...
			PRINTFORM \n 새롭게 추가된 플래그의 초기 설정을 합니다...
			CALL K17_SET_AUTO_PVOICE(, , K17C_공격_도움계)
			PRINTFORMW 설정했습니다
		ENDIF
		
	ENDIF
	;------------
	;Ver 0.13
	;------------
	IF CFLAG:K17C_ID:K17C_CF_VER < 013
		;MIN내에서 인크리먼트하던 버그가 있었기 때문에
		;경험하고 있어도, 플래그치가 가산되어 있지 않은 데이터용
		IF CFLAG:K17C_ID:K17C_CF_샹하이이벤트 == 0 && FIRSTTIME("샹하이 이벤트", 1, K17C_ID)
			PRINTFORM 샹하이 이벤트 경험을 검지...
			CFLAG:K17C_ID:K17C_CF_샹하이이벤트++
			PRINTFORM \n플래그값을 가산했습니다. 오랫동안 버그 발견하지 못해 죄송합니다\n
		ENDIF
		
		IF CFLAG:K17C_ID:K17C_CF_고백이벤트 == 0 && FIRSTTIME("고백 이벤트", 1, K17C_ID)
			PRINTFORM 고백 이벤트 경험을 검지...
			CFLAG:K17C_ID:K17C_CF_고백이벤트++
			PRINTFORM \n플래그값을 가산했습니다. 오랫동안 버그 발견하지 못해 죄송합니다\n

			IF K17_RT_FG(K17C_ID) != CFLAG:K17C_ID:K17C_CF_RTF
				PRINTFORM 「보존 플래그의 차이를 검지...
				CFLAG:K17C_ID:K17C_CF_RTF = K17_RT_FG(K17C_ID)
				PRINTFORMW 플래그를 갱신했습니다」
				PRINTFORML 다음번 힌트를 표시합니다
				PRINTL
				TRYCALL K17_HINT()
				PRINTW
			ENDIF
		ENDIF

	ENDIF
	;------------
	;대사 버전의 갱신
	;------------
	CFLAG:K17C_ID:K17C_CF_VER = K17C_VER
	PRINTFORMW ...버전을 갱신했습니다
	
ELSEIF CFLAG:K17C_ID:K17C_CF_VER  == K17C_VER
	PRINTFORML 현재 최신 버전입니다
ELSE
	PRINTFORML 세이브 데이터와 대사의 버전이 일치하지 않았습니다
	PRINTFORML 세이브 데이터　Ver.  {CFLAG:K17C_ID:K17C_CF_VER/100}. {CFLAG:K17C_ID:K17C_CF_VER%100/10}{CFLAG:K17C_ID:K17C_CF_VER%10}
	PRINTFORML 현재의 말　　Ver.  {K17C_VER/100}. {K17C_VER%100/10}{K17C_VER%10}
ENDIF

CALL COLORMESSAGE(@"망상 에로 아가씨 앨리스 설정·UPDATE 종료", K17C_앨리스COLOR, 1)
;뭐야 이 계산은(경악 어라-무엇인가 이렇게 말하는 명령 없었던가
PRINTFORMW Ver.  {K17C_VER/100}. {K17C_VER%100/10}{K17C_VER%10}
RETURN 1

