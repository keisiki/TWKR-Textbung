@M_KOJO_EVENT_K17_7(ARG, ARG:1)
;말 주인은 TARGET
;ARG에 목격자
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;소분기
	;-------------------------------------------------
	;정사를 볼 수 있었다(목격자의 친해져도:저)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 1
		;아는 사람인가 본다
		SELECTCASE K17_GET_FRIENDS(ARG)
			;거리낌 없는 계
			CASE K17C_레이무ID, K17C_마리사ID, K17C_파츄리ID, K17C_유카ID, K17C_사쿠야ID, K17C_메디슨ID
				PRINTFORML 「%K17_CNAME(ARG)%!」
				PRINTFORML 「아니……그……그러니까, 미안……」
			;존경계
			CASE K17C_미마ID, K17C_루이즈ID, K17C_신키ID, K17C_유메코ID
				PRINTFORML 「읏!　%K17_CNAME(ARG)%!」
				PRINTFORML 「이건, 그, 그러니까……」
				PRINTFORML 「죄, 죄송해요, 경솔했습니다……」
			;그 외
			CASEELSE
				PRINTFORML 「읏!　……그, 죄송합니다」
		ENDSELECT
		;--------------
		;우후후가 종료되었을 경우의 처리
		;--------------
		TCVAR:K17C_ID:K17C_TC_우후후주도권 = 0
		TCVAR:K17C_ID:K17C_TC_욕구불만도 = 0
		
		;플래그 보존
		SIF CFLAG:기정사실 & 합의_우후후
			TRYCALL K17_RELATION_EV( K17C_RTF_우후후 )
	ENDIF
	;-------------------------------------------------
	;목격자가 질 수 없어 참가(목격자의 친해져도:고)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 2
		;아는 사람인가 본다
		SELECTCASE ARG
			;거리낌 없는 계
			CASE K17C_레이무ID, K17C_마리사ID, K17C_파츄리ID, K17C_유카ID, K17C_사쿠야ID, K17C_메디슨ID
				PRINTFORML 「읏……%K17_CNAME(ARG)%?」
				;상대가 연인인가 보는 아와와와……
				IF !TALENT:ARG:연인
					PRINTFORML 「……저기, 끼어들지 마……%K17_CNAME(ARG)%는 저쪽으로 가」
				ENDIF
				
			;존경계
			CASE K17C_미마ID, K17C_루이즈ID, K17C_신키ID, K17C_유메코ID
				PRINTFORML 「읏!　%K17_CNAME(ARG)%!」
				PRINTFORML 「엣, %K17_CNAME(ARG)%도……같이!　네, 넷!, 부디……」
				
			CASEELSE
				PRINTFORML 「읏!　놀랐잖아……」
		ENDSELECT
		TRYCALL K17_난교개시(ARG)
	ENDIF
	;-------------------------------------------------
	;목격자가 떠난다(연인 같은 종류 한정인가
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG:1 == 3
		;아는 사람인가 본다
		SELECTCASE K17_GET_FRIENDS(ARG)
			;거리낌 없는 계
			CASE K17C_레이무ID, K17C_마리사ID, K17C_파츄리ID, K17C_유카ID, K17C_사쿠야ID, K17C_메디슨ID
				PRINTFORML 「앗, %K17_CNAME(ARG)%……」
				PRINTFORML 「미안, 신경 쓰이게 해버렸네……」
				PRINTFORML 「%K17_CNAME()%, 이참에 %K17_CNAME(ARG)%를 제대로 도와 줘」
			;존경계
			CASE K17C_미마ID, K17C_루이즈ID, K17C_신키ID, K17C_유메코ID
				PRINTFORML 「읏!　%K17_CNAME(ARG)%!」
				PRINTFORML 「……확실히 %K17_CNAME(ARG)%에게 신경 쓰이게 되는 건 좋지 않네요……」
			;그 외
			CASEELSE
				PRINTFORML 「읏!　……보, 보여 버렸다……」
		ENDSELECT
		PRINTFORML (나보다, %K17_CNAME()%의 몸 보는 게……더 싫으니까)
	ENDIF
	;-------------------------------------------------
	;MASTER에 이끌려 목격자도 참가
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG:1 == 4
		;PRINTFORML 
		;RETURN 1
	;ENDIF
	;그 외
	PRINTW
	
ENDIF
RETURN 1

