;분노 말
;==================================================
@M_KOJO_EVENT_K17_29(ARG, ARG:1)
;말 주인은 TARGET
;ARG는 TARGET의 담력으로 분기
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL && !FLAG:시간정지 && !CFLAG:TARGET:수면
	PRINTL
	
	;-------------------------------------------------
	;우후후 안
	;-------------------------------------------------
	;억지로 플래그 본다
	IF CFLAG:TARGET:우후후
		CALL K17_억지로질내사정(TARGET)
		IF RESULT
			PRINTW
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;화나게 해 버렸다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		SELECTCASE K17_GRT()
			CASE IS >= K17C_사모
				PRINTFORML 「……응, 조금 %K17_RS("과했던 거/심했던 거")% 같아, 서로 머리 좀 식히자」
				PRINTFORM 「아무리 나라고 %K17_RS("해도 자꾸 그러면 화낼 거니까/해도 자꾸 그러면 화낼 거다?")%」
				TRYCALL K17_인형대사(-1, 1, K17_RS("바보아냐?/화낼거야/흥"))
			CASEELSE
				PRINTFORM 「……정말, 조금 심했어」
				TRYCALL K17_인형대사(-1, 1, K17_RS("바보아냐?/화낼거야/흥"))
		ENDSELECT
		PRINTW
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;울려 버렸다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「%K17_SPLIT_SHUFFLE(@"%K17_RS("/흐윽//읏/흑","/", 2)%/……/……/%RESULTS:1%")%」
		TRYCALL K17_인형대사(-1, 1, K17_RS("앨리스/울지마/눈물닦아/옳지옳지")+"……")
		PRINTW
		RETURN 1
	ENDIF
ENDIF
RETURN 1


