@M_KOJO_EVENT_K17_10(ARG, ARG:1)
#DIM DYNAMIC iRT = 0
#DIM DYNAMIC iFriends = 0

;----------
;앨리스 이외의TARGET를 취득 친구 우선
;----------
iFriends = K17_GET_TARGETS_ID()
iRT = K17_GRT_EX(TARGET)
;--------------------------------------------------
;말 주인은 TARGET
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	
	;-------------------------------------------------
	;MASTER를 덮치기(개시시)
	;둘이서
	;밀어 넘어뜨려진 순간에는 기정사실 들어 있지 않는 곧 빠져 나가면(자) 합의를 얻지 않는다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------
		;개시시의 보존
		;허덕여타입 결정
		;개시시의 주도권
		;욕구불만도의 일시 보존
		;-------------------------
		TCVAR:K17C_ID:K17C_TC_우후후주도권 = TARGET
		TCVAR:K17C_ID:K17C_TC_질내사정카운트 = 0
		IF TALENT:MASTER:성별 >= 2
			TCVAR:K17C_ID:K17C_TC_욕구불만도 = BASE:MASTER:정력
		ELSE
			TCVAR:K17C_ID:K17C_TC_욕구불만도 = CFLAG:욕구불만도
		ENDIF
		
		;-----------
		;껴안기모드
		;-----------
		IF ARG:1 || CFLAG:K17C_ID:K17C_CF_허덕여타입 == K17C_공격_애원계
			CALL K17_SET_AUTO_PVOICE(iRT,-1, K17C_공격_애원계)
			PRINTFORML 「후훗……뭔가 어리광 부리고 싶은 기분이야……%K17_RS("냐응/흐아앙")%~」
			PRINTFORMDL %타겟이% 어리광 부리듯이 %CALLNAME:MASTER%에게 바짝 달라붙더니 그대로 껴안았다
			PRINTW
			RETURN 1
		ENDIF
		;-----------
		;약점잡음의 벌
		;-----------
		IF CFLAG:약점잡음 || CFLAG:K17C_ID:K17C_CF_허덕여타입 == K17C_공격_억지로계
			PRINTFORMDL %마스터는% 외형과는 다르게 상상도 할 수 없을 정도의 괴력을 발휘하는 %CALLNAME:TARGET%에게 짓눌려졌다
			PRINTFORML 「후훗……그럼 벌 받을 시간이야……각오는 된 거지……?」
			PRINTFORMDL 그렇게 귓속말로 요염하게 속삭인 %CALLNAME:TARGET%의 눈빛은 평소와 많이 달라서, %마스터는% 조금 공포를 느꼈다……
			CALL K17_SET_AUTO_PVOICE(iRT,-1, K17C_공격_억지로계)
			PRINTW
			RETURN 1
		ENDIF
		
		;-----------
		;통상 카운터 우후후 첫회
		;-----------
		IF CFLAG:기정사실 & 합의_우후후 && FIRSTTIME("밀어넘어뜨려지고", 1)
			PRINTFORML 「저, 저기……오늘은……그……그러니까……」
			PRINTFORMDL %타겟은% 뭔가 망설이듯이 다리를 꼬아대더니, %CALLNAME:MASTER%을 껴안았다
			PRINTFORML (뭐……뭐야, 평소엔 제멋대로……하는 주제에………우으…)
			PRINTW
			PRINTFORML 「그, 그쪽이 그런 식으로 나올 거라면, 나한테도 생각이 있어……」
			PRINTFORML 「이렇게 보여도 마녀니까……」
			PRINTFORMDL %타겟이% 그렇게 말하면서 손을 흔들더니 마법의 실로 %CALLNAME:MASTER%을 속박했다
			PRINTFORMDL 겉으로 봐선 꽉 묶인 것처럼 보이지만 그렇게 아프진 않고, 오히려 상냥하게 감싸인 듯한 느낌을 받았다
			PRINTFORML 「후훗……어때?　……앗, 아프진 않아?　괜찮아?」
			PRINTW
			PRINTFORML 「왜 웃는 거야, 정말……」
			PRINTFORMDL 이렇게 사랑스러운 마녀에게 습격 당하는 거라면 나쁘지 않다……
			PRINTFORML 「방심했다간 잡아먹힐 테니깐……어, 어흥」
			PRINTFORMDL 얼굴이 새빨개진 %CALLNAME:TARGET%는, %CALLNAME:MASTER%을 아기 재우듯 상냥히 넘어트렸다
			CALLF FIRSTTIME("밀어넘어뜨려지고")
			CALL K17_SET_AUTO_PVOICE(iRT,-1, K17C_공격_도움계)
		;-----------
		;합의없음으로 카운터 우후후 첫회에 왔을 경우도 싶다 있고 것으로 욕구불만도반 넘어도 이것으로
		;-----------
		ELSEIF !(CFLAG:기정사실 & 합의_우후후) || CFLAG:욕구불만도 >= 500 || CFLAG:K17C_ID:K17C_CF_허덕여타입 == K17C_공격_합의덮쳐계
			PRINTFORML 「후우―……후우―……」
			PRINTFORML 「이래봐도……이래봐도 상당히, 많이 참았어……%K17_CNAME()%?　알고는 있어?」
			PRINTFORMDL %마스터는% 뭐가 뭔지 전혀 짐작조차 하지 못한 채, 그대로 괴력에 짓눌려졌다
			PRINTFORMDL 움직이려고 해봐도, 평상시엔 인형 조작할 때나 쓰이던 마법의 실이 %CALLNAME:MASTER%의 몸에 감겨 있어서 움직일 수가 없었다
			PRINTW
			PRINTFORML 「미안, 너무 무서워하진 마… 그렇지만, 이제 참을 수가 없어, 이 기분……」
			PRINTFORML 「%K17_CNAME()%을……버, 범할 거야……」
			PRINTFORMDL 얼굴을 새빨갛게 물들인 채로, 이마에 얇은 땀이 맺힌 %타겟이% %CALLNAME:MASTER%을 덮쳤다
			CALL K17_SET_AUTO_PVOICE(iRT,-1, K17C_공격_합의덮쳐계)
		;-----------
		;2회째 이후 통상 처리
		;-----------
		ELSE
			CALL K17_SET_AUTO_PVOICE(iRT,-1, K17C_공격_도움계)
			iRT = K17_LOWER_LIMIT_RAND(iRT, K17C_사모, K17C_연모)
			SELECTCASE iRT
				CASE IS >= K17C_연모
					;대사
					PRINTDATAL
						DATAFORM 「있잖아, 그러니까……그……%K17_RS("안아줘/엣찌하자/섹스하자")%? %K17_RS("응? /") %%K17_HEART()%」
						DATAFORM 「%K17_CNAME()%이 이런 %K17_RS("음란한 /엣찌한 ")%몸으로 만들었잖아……책임을 지라구……」
						DATAFORM 「%K17_RS("해, 해버릴/더, 덮쳐버릴/버, 범해버릴")%……거니까……」
					ENDDATA
					;정경
					PRINTDATAD
						DATAFORM %마스터는% 달라붙어선 스스로 몸을 비벼대는 %CALLNAME:TARGET%에게,
						DATAFORM %CALLNAME:MASTER%의 손을 잡아, 살며시 자신의 %K17_RS("가슴/뺨/허벅지")%에 갖다대는 %CALLNAME:TARGET%에게
						DATAFORM %CALLNAME:MASTER%의 무릎 위에서 몸을 흔들며, %K17_RS("고간/가슴")%을 비벼대는 %CALLNAME:TARGET%에게 그대로
					ENDDATA
					PRINTFORMDL %K17_RS("상냥하게/천천히") % %K17_RS("껴안아졌다/떠밀려졌다")%
				CASE IS >= K17C_사모
					;대사
					PRINTDATAL
						DATAFORM 「……하자?　응? %K17_RS("　응? /") %%K17_HEART()%」
						DATAFORM 「……하자, 안 할 거야?　그럼 %K17_RS("이 쪽/내 쪽")%에서……」
						DATAFORM 「하, 할 거니깐……%K17_RS("해버릴 /할 기분이 들게 만들 ")%거니깐……」
					ENDDATA
					;정경
					PRINTDATAD
						DATAFORM %CALLNAME:MASTER%의 몸에 바짝 달라붙는 %CALLNAME:TARGET%에게,
						DATAFORM %CALLNAME:MASTER%의 %K17_RS("손을 상냥하게 잡아/손을 꼭 잡아/손에 손가락을 걸어")%오는 %CALLNAME:TARGET%에게
						DATAFORM %CALLNAME:MASTER%의 %K17_RS("허벅지를 간지럽히는 /가슴에 머리를 문지르는 /뺨을 쿡쿡 찔러오는 ")% %CALLNAME:TARGET%에게,
					ENDDATA
					PRINTFORMDL %K17_RS("상냥하게/부드럽게") % %K17_RS("껴안아졌다/떠밀려졌다")%
				CASEELSE
					PRINTFORML 「저기 ……괜찮지?　뭘까 이거…… 중독 된 거 같아…… 그러니까」
					PRINTFORMDL %마스터는% %CALLNAME:TARGET%에게 %K17_RS("상냥하게/천천히")% 떠밀려졌다
			ENDSELECT
			PRINTFORML 「%K17_CNAME()%이 %K17_RS("나쁜/이런 몸으로 만든")%거야, 이제 %K17_RS("나~쁜/음란한") %%K17_RS("마법사/마녀")%한테 덮쳐질 테니깐……%K17_RS("후훗/후후훗")%」
			
		ENDIF
		
		;----------------------------
		;난교
		;----------------------------
		IF iFriends
			PRINTW
			SELECTCASE iFriends
				CASE K17C_레이무ID, K17C_마리사ID, K17C_파츄리ID, K17C_유카ID, K17C_사쿠야ID, K17C_메디슨ID
					;앨리스가 연인경우
					IF TALENT:TARGET:연인
						PRINTFORML 「%K17_CNAME()%, %K17_CNAME(iFriends)%도 제대로 사랑해 줘」
					ENDIF
				CASE K17C_미마ID, K17C_루이즈ID, K17C_신키ID, K17C_유메코ID
					IF TALENT:TARGET:연인
						PRINTFORML 「이번만 함께 해줄 거지만……%K17_CNAME(iFriends)%랑 너무 비교하진 마?」
					ENDIF
				CASEELSE
					;앨리스가 연인경우
					IF TALENT:TARGET:연인
						PRINTFORML 「%K17_CNAME()%, 그 애도 제대로 사랑해 줘」
					ENDIF
			ENDSELECT
			TRYCALL K17_난교개시(iFriends)
			CALL K17_SET_AUTO_PVOICE(iRT,-1, K17C_공격_애원계)
		ENDIF
		PRINTW
		RETURN 1
	ENDIF
	
	
	;-------------------------------------------------
	;껴안기모드 만족
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2 && ARG:1 == 1
		PRINTFORML 「응~ 만족했다……후훗」
		PRINTW
		;타이프리세
		CFLAG:K17C_ID:K17C_CF_허덕여타입 = K17C_공격_애원계
		TCVAR:K17C_ID:K17C_TC_우후후주도권 = 0
		TCVAR:K17C_ID:K17C_TC_욕구불만도 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;껴안기모드 강제 종료
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2 && ARG:1 == 2
		PRINTFORML 「뭐, 뭐야, 그렇게 싫어하지 않아도 되잖아……」
		PRINTW
		;타이프리세
		CFLAG:K17C_ID:K17C_CF_허덕여타입 = K17C_공격_애원계
		TCVAR:K17C_ID:K17C_TC_우후후주도권 = 0
		TCVAR:K17C_ID:K17C_TC_욕구불만도 = 0
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;달랠 수 있었다(우후후 종료시)
	;밀어 넘어뜨려졌지만 곧바로 빠져 나갔다
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2 && !ARG:1 && !(CFLAG:기정사실 & 합의_우후후)
		PRINTFORML 「읏……하아……미안, 아무것도 아냐……」
		PRINTFORMDL %CALLNAME:MASTER%에게 거절당한 %타겟은%, 눈물을 머금은 채로 시선을 돌려버렸다
		PRINTFORMDL 상당히 쇼크 받은 거 같았다
		PRINTW
		;타이프리세
		CFLAG:K17C_ID:K17C_CF_허덕여타입 = K17C_공격_애원계
		TCVAR:K17C_ID:K17C_TC_우후후주도권 = 0
		TCVAR:K17C_ID:K17C_TC_욕구불만도 = 0
		RETURN 1
	ENDIF
	
	;-------------------------------------------------
	;달랠 수 있었다(통상 종료시)
	;기입 체크(=0, 비표시, 1, 표시)
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		;약점 아직 남고 있어 본다
		IF CFLAG:약점잡음
			PRINTFORML 「오늘은 우선 이 정도로 용서해 줄게……」
			PRINTW
			RETURN 1
		ENDIF
		
		;감상전
		TRYCALL K17_우후후감상전( TCVAR:K17C_ID:K17C_TC_우후후주도권 , TCVAR:K17C_ID:K17C_TC_욕구불만도 )
		TCVAR:K17C_ID:K17C_TC_우후후주도권 = 0
		TCVAR:K17C_ID:K17C_TC_욕구불만도 = 0
		;타이프리세
		CFLAG:K17C_ID:K17C_CF_허덕여타입 = K17C_공격_애원계
		PRINTW
		RETURN 1
	ENDIF
	
ENDIF
RETURN 1


