;*********************************************************
@K17_CLL_COUNTER_REACTIONMESSAGE
SIF TARGET <= 0
	RETURN 0

IF !TCVAR:카운터행동 && !TCVAR:22
	PRINTW
	RETURN 0
ENDIF

CALL DATUI_MESSAGE

SIF TCVAR:피임 || TCVAR:카운터행동
	RESETCOLOR

SIF TCVAR:피임
	CALL SET_HININ(TARGET)
RESULT = 0
TRYCALLFORM EVENT_COUNTE_MESSAGE{TCVAR:카운터행동}
