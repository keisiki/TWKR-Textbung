;※※※※※※※※※※※※※※※※※※※※※※※※※※
;時止め判定処理
;ARG=時止め後状態（※イベント口上17番参照）
;射精箇所等には対応していないので, 改造したい人うぇるかむです
;※※※※※※※※※※※※※※※※※※※※※※※※※※
@M_KOJO_K103_PREVTW(ARG)
IF !CFLAG:18 && ARG == 2
	;面識無&絶頂
	TCVAR:350 = 9
ELSEIF !CFLAG:18 && ARG == 1
	;面識無&快感
	TCVAR:350 = 8
ELSEIF !CFLAG:18 && ARG == 3
	;面識無&パンツが無い
	TCVAR:350 = 7
ELSEIF !CFLAG:18 && ARG == 0
	;面識無
	TCVAR:350 = 6
ELSEIF !TCVAR:182 && ARG == 2
	;今日初&絶頂
	TCVAR:350 = 5
	CALL M_KOJO_K103_MEET
ELSEIF !TCVAR:182 && ARG == 1
	;今日初&快感
	TCVAR:350 = 4
	CALL M_KOJO_K103_MEET
ELSEIF !TCVAR:182 && ARG == 3
	;今日初&パンツが無い
	TCVAR:350 = 3
	CALL M_KOJO_K103_MEET
ELSEIF !TCVAR:182 && ARG == 0
	;今日初
	TCVAR:350 = 2
	CALL M_KOJO_K103_MEET
ELSE
	;目の前で時止め&解除したかの分岐
	;時止め直後
	IF TCVAR:351 != TCVAR:352
		;突然現れた
		SELECTCASE ARG
		CASE 2 ;絶頂
			TCVAR:350 = 14
		CASE 1 ;快感
			TCVAR:350 = 13
		CASE 3 ;パンツが無い
			TCVAR:350 = 12
		CASEELSE ;何もしてない
			TCVAR:350 = 11
		ENDSELECT
	ELSE
		;目の前での時止め&解除
		SELECTCASE ARG
		CASE 2 ;絶頂
			TCVAR:350 = 18
		CASE 1 ;快感
			TCVAR:350 = 17
		CASE 3 ;パンツが無い
			TCVAR:350 = 16
		CASEELSE ;何もしてない
			TCVAR:350 = 15
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

