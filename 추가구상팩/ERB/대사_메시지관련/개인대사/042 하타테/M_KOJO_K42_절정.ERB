;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;分岐作りに有用そうな変数などのメモ
;~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
;NOWEX:C절정 NOWEX:V절정 NOWEX:B절정 NOWEX:A절정 NOWEX:M절정
;各部位の絶頂強度. まず0~3が割り当てられ, 多重絶頂により最大16倍される. つまり単部位なら最大3, 5重絶頂で最大48
;NOWEX:TotalEX
;多重絶頂補正を受ける前の各部位絶頂強度の合計値. 1~15. このテンプレにおける分岐の基準. 

;TEQUIP:50
;Vに誰の竿が入ってるか. TEQUIP:50 = MASTERであなたが挿入中となる. 
;TEQUIP:51
;Aに誰の竿が入ってるか. TEQUIP:51 = MASTERであなたが挿入中となる. 
;NOWEX:사정
;사정량, 最大3. たとえばIF NOWEX:MASTER:사정 > 1だと, あなたが大量사정以上した場合となる. 
;SYNCED_ORGASM(TARGET)
;相손とあなたが同時に絶頂した場合真となる. 絶頂の部位は問わない. 

;-------------------------------------------------
;絶頂時の구上
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K42
#DIM TotalEX
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;분유
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:분유
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;사정した
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:사정
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;방뇨
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:방뇨
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「앗, 아아아아아～～～～～～～～앗…」
		PRINTFORML （오줌 싸버렸어…하면 안되는데도옷, 기분 조아아, 어째서…？）
		PRINTW 
	CASE 1
		PRINTFORML 「앗, 아아아아앗, 오줌싸개, 되어버려어…\/\/\/」
		PRINTW 
	CASE 2
		PRINTFORML 「싫엇, 멈춰엇%UNICODE(0x2665) *1%　멈춰어～～%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
		PRINTW 
	ENDSELECT
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示, 1, 表示）
LOCAL = 1
;-------------------------------------------------
;合計絶頂数で分岐
;これだけではどの部位が絶頂したかは区別されないため, 書き分けたければ任意で分岐を加筆されたし. 
IF LOCAL
	IF FLAG:70
	ELSE
		SELECTCASE NOWEX:TotalEX
			;強
			CASE 5 TO 15
				PRINTFORMW 「격렬해앳%UNICODE(0x2665) *1%　부서져엇, 부서져버려엇%UNICODE(0x2665) *1%」
			;中
			CASE 2 TO 4
				PRINTFORMW 「앗%UNICODE(0x2665) *1%　갓%UNICODE(0x2665) *1%　안 돼 안 돼 안 됏%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			;弱
			CASE 1
				PRINTFORMW 「싫엇, 이거 싫엇, 무서워, 날아가 날아가버려엇！」
				PRINTW 
		ENDSELECT

		IF NOWEX:C절정
			;로터
			SIF SELECTCOM == 40 && CFLAG:K42C_はたてと通販 != 2
				CFLAG:K42C_はたてと通販 = 1
			
			;Eマッサージャー
			SIF SELECTCOM == 41 && CFLAG:K42C_はたてと通販その２ != 2
				CFLAG:K42C_はたてと通販その２ = 1
			
			SIF TEQUIP:클리캡 && CFLAG:K42C_はたてと通販その３ != 2
				CFLAG:K42C_はたてと通販その３ = 1
			
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「아아아아앗！？　간닷%UNICODE(0x2665) *1% 간다간다가버려어%UNICODE(0x2665) *1%」
				PRINTW 
			CASE 1
				PRINTFORML 「아%UNICODE(0x2665) *1%　응%UNICODE(0x2665) *1%　간닷, 가앗…%UNICODE(0x2665) *1%」
				PRINTW 
			CASE 2
				PRINTFORML 「응읏…앗！？　응아앗%UNICODE(0x2665) *1%」
				PRINTFORML （가버렸어…나, 가버렸다구…）
				PRINTW 
			ENDSELECT
		ENDIF

		;はたてはV敏感持ち
		IF NOWEX:V절정
			SIF TEQUIP:바이브 && CFLAG:K42C_はたてと通販その４ != 2
				CFLAG:K42C_はたてと通販その４ = 1
			
			SELECTCASE RAND:6
			CASE 0
				PRINTFORML 「가…버려어어%UNICODE(0x2665) *1%　아아～～%UNICODE(0x2665) *1%　아아아～～～～～～앗%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			CASE 1
				PRINTFORML 「간닷%UNICODE(0x2665) *1%　가버려어%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
				PRINTFORML 「보지잇%UNICODE(0x2665) *1%　보지가앗%UNICODE(0x2665) *1%　하타테의 보지가아앗%UNICODE(0x2665) *1%
				IF TEQUIP:50 == MASTER
					IF CFLAG:1100 >= 1
						PRINTFORM 「달링의
					ELSE
						PRINTFORM 「%MASTERNAME:42%의
					ENDIF
				ENDIF
				PRINTFORML 자지로옷, 하타테의 보지 가버려엇%UNICODE(0x2665) *1%」
				PRINTFORML 「하타텟%UNICODE(0x2665) *1%　의 보지잇, 가버렷%UNICODE(0x2665) *1%　오%UNICODE(0x2665) *1%　오옷%UNICODE(0x2665) *1%　가버려써%UNICODE(0x2665) *1%　보지, 이제 가버려써어%UNICODE(0x2665) *1%」
				PRINTW 
			CASE 2
				PRINTFORML 「부서져엇%UNICODE(0x2665) *1%　보지잇%UNICODE(0x2665) *1%　기분이 너무 좋아서 부서져버려엇%UNICODE(0x2665) *1%」
				PRINTW 
			CASE 3
				PRINTFORML 「기분 조앗%UNICODE(0x2665) *1%　가버려…아앗%UNICODE(0x2665) *1%
				PRINTFORML 「간다 간다 간다아아앗%UNICODE(0x2665) *1%」
			CASE 4
				PRINTFORML 「날아가앗%UNICODE(0x2665) *1%　하타테의 신체엣, 날아가버려엇%UNICODE(0x2665) *1%」
				PRINTW 
			CASE 5
				PRINTFORML 「앗, 뭐야 이거！？　몰랏, 이런 거, 모른다구웃%UNICODE(0x2665) *1%」
				PRINTW 
			ENDSELECT
		ENDIF

		IF NOWEX:B절정
		
			SIF TEQUIP:유두캡 && CFLAG:K42C_はたてと通販その５ != 2
				CFLAG:K42C_はたてと通販その５ = 1
			
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「지금 가슴 안 됏%UNICODE(0x2665) *1%　가버렷%UNICODE(0x2665) *1%　가슴으로 가버려엇%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
			CASE 1
				PRINTFORML 「앗, 싫엇, 가슴으로, 가버려엇%UNICODE(0x2665) *1%　간다 간다앗%UNICODE(0x2665) *1%」
			CASE 2
				PRINTFORML 「～～～～～～～읏%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%　응%UNICODE(0x2665) *1%　응%UNICODE(0x2665) *1%」
				PRINTW 
			ENDSELECT
		ENDIF

		;はたてはA敏感持ち
		IF NOWEX:A절정
			;애널비즈
			SIF SELECTCOM:46 && CFLAG:K42C_はたてと通販その６ != 2
				CFLAG:K42C_はたてと通販その６ = 1

			SIF TEQUIP:애널바이브 && CFLAG:K42C_はたてと通販その７ != 2
				CFLAG:K42C_はたてと通販その７ = 1
			
			SELECTCASE RAND:3
			CASE 0
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「앗%UNICODE(0x2665) *1%　아아～～앗%UNICODE(0x2665) *1%%UNICODE(0x2665) *1 안%UNICODE(0x2665) *1%　대%UNICODE(0x2665) *1%　앳%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 1
					PRINTFORML 「하%UNICODE(0x2665) *1%　아아앗%UNICODE(0x2665) *1%　응앗%UNICODE(0x2665) *1%　아%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 2
					PRINTFORML 「엉덩잇%UNICODE(0x2665) *1%　부서져엇%UNICODE(0x2665) *1%」
					PRINTFORML 「바보가 되어버렷%UNICODE(0x2665) *1%　엉덩잇%UNICODE(0x2665) *1%　바보가 되어버렷%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 3
					PRINTFORML 「응아아아앗%UNICODE(0x2665) *1%　안 됏, 안 돼앳%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTFORML 「나, 엉덩이%UNICODE(0x2665) *1%　약하니까안%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			CASE 1
				SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「얏, 안 됏, 이쪽으로, 가고 싶지 않앗%UNICODE(0x2665) *1%　가앗%UNICODE(0x2665) *1%　가버려어어엇%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					PRINTW 
				CASE 1
					PRINTFORML 「아냣%UNICODE(0x2665) *1%　안 돼앳%UNICODE(0x2665) *1%　엉덩이, 안 돼앳%UNICODE(0x2665) *1%」
					PRINTW 
				ENDSELECT
			CASE 2
				PRINTFORML 「싫엇, 엉덩이, 엉덩이로, 가는 건！？　앗, 얏%UNICODE(0x2665) *1%　가버렷%UNICODE(0x2665) *1%」
				PRINTW 
			ENDSELECT
		ENDIF

		IF NOWEX:M절정
			SELECTCASE RAND:3
			CASE 0
				IF TEQUIP:TARGET:입접촉부위 == 1
					;구内사정されて絶頂
					IF GETBIT(TFLAG:1,4)
						PRINTFORML 「응%UNICODE(0x2665) *1%　구우우～～～～웃%UNICODE(0x2665) *1%　아, 우, 앗%UNICODE(0x2665) *1%」
						PRINTFORML （안 돼, 정액, 나와버리면, 바보가, 되어버렷%UNICODE(0x2665) *1%）
						PRINTFORML （목구멍 중앙에 나와서 가버렷%UNICODE(0x2665) *1%）
						PRINTW 
					;フェラ絶頂
					ELSE
						PRINTFORML 「우붓%UNICODE(0x2665) *1%　우긋, 응%UNICODE(0x2665) *1%　
					ENDIF
				;キスで絶頂
				ELSEIF TEQUIP:TARGET:입접촉부위 == 4
					PRINTFORML 「응훗, 응읏, 응응～～～～%UNICODE(0x2665) *1%　%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%%UNICODE(0x2665) *1%」
					IF TALENT:MASTER:성별 == 2
						PRINTFORML （조금 가슬가슬하지만 부드럽고 따뜻해）
						PRINTFORML （제일 좋아하는 %MASTERNAME:42%의 입술. %MASTERNAME:42%와(과)의 키스）
						PRINTFORML （더 갖고 싶어%UNICODE(0x2665) *1%　더 키스 해줘…%UNICODE(0x2665) *1%）
						PRINTW 
					ELSE
						PRINTFORML （%MASTERNAME:42%의 입술, 부드러워…%UNICODE(0x2665) *1%）
						PRINTFORML （동성의 키스는 이렇게 부드럽구나…%UNICODE(0x2665) *1%）
						PRINTFORML （머리가 멍해져서 아무 생각도 할 수 없게 되버려…%UNICODE(0x2665) *1%）
						PRINTW 
					ENDIF
				ENDIF
			CASE 1
				IF TEQUIP:TARGET:입접촉부위 == 1
					;구内사정されて絶頂
					IF GETBIT(TFLAG:1,4)
						SELECTCASE RAND:2
						CASE 0
							PRINTFORML 「음무우우웃, 읏, 응봇, 베…하, 아…%UNICODE(0x2665) *1%」
							PRINTFORML （입 안에 정액 냄새, 입 안에 가득 퍼져서, 기분나빠…）
							PRINTFORML （그런데 어째서 머리가 띵해져버리는 걸까, 영문을 모르게 되어버려…%UNICODE(0x2665) *1%）
							PRINTW 
						CASE 1
							PRINTFORML 「음무우우웃, 응, 응뭇%UNICODE(0x2665) *1%」
							PRINTFORML （아아, 또, 이 냄새야）
							PRINTFORML （정액, 내 머리속을 엉망진창으로 해버려…%UNICODE(0x2665) *1%）
						ENDSELECT
					;フェラ絶頂
					ELSE
						PRINTFORML 「음무우우웃, 응, 응뭇%UNICODE(0x2665) *1%」
						PRINTFORML （아아, 또, 이 냄새야）
						PRINTFORML （수컷의 냄새. 나를 망가뜨리는 냄새. 제일 좋아하는 맛과, 냄새…%UNICODE(0x2665) *1%）
						PRINTW 
					ENDIF
				;キスで絶頂
				ELSEIF TEQUIP:TARGET:입접촉부위 == 4
					PRINTFORML 「응훗, 읏, 응읏%UNICODE(0x2665) *1%」
					PRINTFORML （두근거리는 게 멈추질 않아, %MASTERNAME:42%에게 가슴 소리, 들려버려…%UNICODE(0x2665) *1%）
					PRINTFORML （더, 더 키스해줘. 내 가슴 속에, %MASTERNAME:42%(으)로 가득 채워줘%UNICODE(0x2665) *1%）
					PRINTW 
				ENDIF
			CASE 2
				;ちんぽねじ込まれて絶頂
				IF TEQUIP:TARGET:입접촉부위 == 1
					;구内사정されて絶頂
					IF GETBIT(TFLAG:1,4)
						IF TALENT:정애미각
							PRINTFORML 「음무우우웃, 응, 응뭇%UNICODE(0x2665) *1%　응곳%UNICODE(0x2665) *1%」
							PRINTFORML （정액, 냄새나고 진해%UNICODE(0x2665) *1%）
							PRINTFORML （더, 더 마시고 싶어%UNICODE(0x2665) *1%　더 싸줘%UNICODE(0x2665) *1%）
							PRINTW 
						ELSE
							PRINTFORML 「음무우우웃, 응, 응뭇, 고훗！？　앗, 갓」
							PRINTFORML （정액, 써, 냄새, 끔직해）
							PRINTFORML （어째서, 기분이 나쁘면서도 좋은지 모르겠어…）
							PRINTW 
						ENDIF
					;フェラ絶頂
					ELSE
						PRINTFORML 「옷, 우긋, 아우웃」
						PRINTFORML （내 몸, 어떻게 된 거야）
						PRINTFORML （남자의 자지, 이상한 맛에 끔찍한 냄새가 나는데도 물기만 하면 머리가 멍해져…）
						PRINTW 
					ENDIF
				;キスで絶頂
				ELSEIF TEQUIP:TARGET:입접촉부위 == 4
					PRINTFORML 「응훗, 응읏, 응응～～～～읏」
					PRINTFORML （상냥한 키스…왠지, 머리가, 멍해져…）
					PRINTW 
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

;-------------------------------------------------
;奴隷の수면絶頂時の구上
